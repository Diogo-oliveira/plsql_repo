DECLARE
    e_already_exists EXCEPTION;
    PRAGMA EXCEPTION_INIT(e_already_exists, -02260);
BEGIN
    EXECUTE IMMEDIATE 'ALTER TABLE NCH_EFFECTIVE_INTERVENTION ADD CONSTRAINT NEN_PK PRIMARY KEY (ID_NCH_EFFECTIVE, ID_INCP_INTERV_PLAN)';
EXCEPTION
    WHEN e_already_exists THEN
        dbms_output.put_line('table can have only one primary key');
END;
