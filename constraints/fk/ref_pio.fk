DECLARE
    l_sql   VARCHAR2(4000);
    l_count PLS_INTEGER;
BEGIN

    SELECT COUNT(1)
      INTO l_count
      FROM user_constraints u
     WHERE u.constraint_name = 'RPO_ERTX_FK';

    IF l_count = 0
    THEN
        -- creating data model
l_sql := q'[ALTER TABLE REF_PIO ADD CONSTRAINT RPO_ERTX_FK FOREIGN KEY (ID_EXTERNAL_REQUEST) REFERENCES P1_EXTERNAL_REQUEST (ID_EXTERNAL_REQUEST) ENABLE]';
        EXECUTE IMMEDIATE l_sql;
    END IF;

END;
