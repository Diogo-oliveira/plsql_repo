  when others then
         IF SQLCODE <> -2275
           AND SQLCODE <> -2264
        THEN
            raise_application_error(-20101,
                                    'SPIS_SR_POS_FK ' || SQLERRM);
        END IF; 
end;
/
-- CHANGE END: António Neto

