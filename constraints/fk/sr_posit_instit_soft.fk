  execute immediate 'ALTER TABLE SR_POSIT_INSTIT_SOFT ADD CONSTRAINT SPIS_SR_POS_AN0973_FK 
FOREIGN KEY (ID_SR_POSIT) REFERENCES SR_POSIT (ID_SR_POSIT) ENABLE NOVALIDATE';
