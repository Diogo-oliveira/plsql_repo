    BEGIN
           EXECUTE IMMEDIATE 'ALTER TABLE EPIS_DSTAG_PFACT_HIST ADD CONSTRAINT EDSPFH_CNCPTTR2_FK FOREIGN KEY(ID_VALUE, ID_VALUE_INST_OWNER) REFERENCES CONCEPT_TERM(ID_CONCEPT_TERM, ID_INST_OWNER)';
