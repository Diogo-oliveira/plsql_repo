           EXECUTE IMMEDIATE 'ALTER TABLE EPIS_DSTAG_PFACT_HIST ADD CONSTRAINT EDSPFH_CNCPTTR1_FK FOREIGN KEY(ID_FIELD, ID_FIELD_INST_OWNER) REFERENCES CONCEPT_TERM(ID_CONCEPT_TERM, ID_INST_OWNER)';
