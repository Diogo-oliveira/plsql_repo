         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
         l_ID_PROGRESS_NOTES(i) := col_in(i).ID_PROGRESS_NOTES;
         l_ID_EPIS_PROG_NOTES(i) := col_in(i).ID_EPIS_PROG_NOTES;
         l_ID_EPIS_COMPLAINT(i) := col_in(i).ID_EPIS_COMPLAINT;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE PROGRESS_NOTES SET
               ID_EPIS_RECOMEND = NVL (l_ID_EPIS_RECOMEND(i), ID_EPIS_RECOMEND),
               ID_EPIS_ANAMNESIS = NVL (l_ID_EPIS_ANAMNESIS(i), ID_EPIS_ANAMNESIS),
