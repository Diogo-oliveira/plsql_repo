               ID_EPIS_PROG_NOTES = NVL (l_ID_EPIS_PROG_NOTES(i), ID_EPIS_PROG_NOTES),
               ID_EPIS_COMPLAINT = NVL (l_ID_EPIS_COMPLAINT(i), ID_EPIS_COMPLAINT)
             WHERE
                ID_PROGRESS_NOTES = l_ID_PROGRESS_NOTES(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE PROGRESS_NOTES SET
               ID_EPIS_RECOMEND = l_ID_EPIS_RECOMEND(i),
               ID_EPIS_ANAMNESIS = l_ID_EPIS_ANAMNESIS(i),
