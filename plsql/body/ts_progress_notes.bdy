               ID_EPIS_COMPLAINT = l_ID_EPIS_COMPLAINT(i)
             WHERE
                ID_PROGRESS_NOTES = l_ID_PROGRESS_NOTES(i)
        returning rowid bulk collect into rows_out  ;
      END IF;
   END upd;


