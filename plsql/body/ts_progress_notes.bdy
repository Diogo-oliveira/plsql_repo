   BEGIN

   l_ID_EPIS_RECOMEND_n := sys.diutil.bool_to_int(ID_EPIS_RECOMEND_nin);
   l_ID_EPIS_ANAMNESIS_n := sys.diutil.bool_to_int(ID_EPIS_ANAMNESIS_nin);
