      l_ID_EPIS_PROG_NOTES ID_EPIS_PROG_NOTES_cc;
      l_ID_EPIS_COMPLAINT ID_EPIS_COMPLAINT_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_EPIS_RECOMEND(i) := col_in(i).ID_EPIS_RECOMEND;
         l_ID_EPIS_ANAMNESIS(i) := col_in(i).ID_EPIS_ANAMNESIS;
         l_ID_DIAGNOSIS(i) := col_in(i).ID_DIAGNOSIS;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
