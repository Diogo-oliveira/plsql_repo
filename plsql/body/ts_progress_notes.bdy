   l_ID_EPIS_PROG_NOTES_n := sys.diutil.bool_to_int(ID_EPIS_PROG_NOTES_nin);
   l_ID_EPIS_COMPLAINT_n := sys.diutil.bool_to_int(ID_EPIS_COMPLAINT_nin);


         UPDATE PROGRESS_NOTES SET
     ID_EPIS_RECOMEND = decode (l_ID_EPIS_RECOMEND_n,0,id_epis_recomend_in, NVL (id_epis_recomend_in, ID_EPIS_RECOMEND)),
     ID_EPIS_ANAMNESIS = decode (l_ID_EPIS_ANAMNESIS_n,0,id_epis_anamnesis_in, NVL (id_epis_anamnesis_in, ID_EPIS_ANAMNESIS)),
     ID_DIAGNOSIS = decode (l_ID_DIAGNOSIS_n,0,id_diagnosis_in, NVL (id_diagnosis_in, ID_DIAGNOSIS)),
     CREATE_TIME = decode (l_CREATE_TIME_n,0,create_time_in, NVL (create_time_in, CREATE_TIME)),
     CREATE_USER = decode (l_CREATE_USER_n,0,create_user_in, NVL (create_user_in, CREATE_USER)),
