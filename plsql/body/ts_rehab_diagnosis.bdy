    FUNCTION next_key(sequence_in IN VARCHAR2 := NULL) RETURN rehab_diagnosis.id_rehab_diagnosis%TYPE
    
     IS
        retval rehab_diagnosis.id_rehab_diagnosis%TYPE;
    
    BEGIN
        IF sequence_in IS NULL
        THEN
            SELECT seq_rehab_diagnosis.NEXTVAL
              INTO retval
              FROM dual;
        ELSE
            EXECUTE IMMEDIATE 'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
                INTO retval;
        END IF;
        RETURN retval;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'SEQUENCE-GENERATION-FAILURE',
                                            name1_in      => 'SEQUENCE',
                                            value1_in     => nvl(sequence_in, 'seq_REHAB_DIAGNOSIS'));
    END next_key;

