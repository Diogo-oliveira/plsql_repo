    PROCEDURE upd
    (
        col_in            IN cancel_reason_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_cancel_reason   id_cancel_reason_cc;
        l_code_cancel_reason code_cancel_reason_cc;
        l_id_content         id_content_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_cancel_reason(i) := col_in(i).id_cancel_reason;
            l_code_cancel_reason(i) := col_in(i).code_cancel_reason;
            l_id_content(i) := col_in(i).id_content;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE cancel_reason
                   SET code_cancel_reason = nvl(l_code_cancel_reason(i), code_cancel_reason),
                       id_content         = nvl(l_id_content(i), id_content)
                 WHERE id_cancel_reason = l_id_cancel_reason(i);
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE cancel_reason
                   SET code_cancel_reason = l_code_cancel_reason(i), id_content = l_id_content(i)
                 WHERE id_cancel_reason = l_id_cancel_reason(i);
        END IF;
    END upd;

