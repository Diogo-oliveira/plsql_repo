    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(cancel_reason_inout IN OUT cancel_reason%ROWTYPE) IS
    BEGIN
        cancel_reason_inout.id_cancel_reason   := NULL;
        cancel_reason_inout.code_cancel_reason := NULL;
        cancel_reason_inout.id_content         := NULL;
    END initrec;

    FUNCTION initrec RETURN cancel_reason%ROWTYPE IS
        l_cancel_reason cancel_reason%ROWTYPE;
    BEGIN
        RETURN l_cancel_reason;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN cancel_reason_tc IS
        data cancel_reason_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM cancel_reason
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN cancel_reason_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data cancel_reason_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

BEGIN
    NULL;
