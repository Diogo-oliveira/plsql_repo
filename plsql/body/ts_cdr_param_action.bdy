   PROCEDURE n_ins_clobs_in_chunks (
      id_cdr_parameter_in IN CDR_PARAM_ACTION.ID_CDR_PARAMETER%TYPE,
      id_cdr_action_in IN CDR_PARAM_ACTION.ID_CDR_ACTION%TYPE,
      id_cdr_param_action_in IN CDR_PARAM_ACTION.ID_CDR_PARAM_ACTION%TYPE DEFAULT NULL,
      event_span_in IN CDR_PARAM_ACTION.EVENT_SPAN%TYPE DEFAULT NULL,
      id_event_span_umea_in IN CDR_PARAM_ACTION.ID_EVENT_SPAN_UMEA%TYPE DEFAULT NULL,
      flg_first_time_in IN CDR_PARAM_ACTION.FLG_FIRST_TIME%TYPE DEFAULT 'N',
      create_user_in IN CDR_PARAM_ACTION.CREATE_USER%TYPE DEFAULT NULL,
      create_time_in IN CDR_PARAM_ACTION.CREATE_TIME%TYPE DEFAULT NULL,
      create_institution_in IN CDR_PARAM_ACTION.CREATE_INSTITUTION%TYPE DEFAULT NULL,
      update_user_in IN CDR_PARAM_ACTION.UPDATE_USER%TYPE DEFAULT NULL,
      update_time_in IN CDR_PARAM_ACTION.UPDATE_TIME%TYPE DEFAULT NULL,
      update_institution_in IN CDR_PARAM_ACTION.UPDATE_INSTITUTION%TYPE DEFAULT NULL,
      id_cdr_message_in IN CDR_PARAM_ACTION.ID_CDR_MESSAGE%TYPE DEFAULT NULL,
      handle_error_in IN BOOLEAN := TRUE,
      clob_columns_in IN varchar2_t,
      clob_pieces_in IN  varchar2_t
   ) IS
        l_MESSAGE clob;
      i                PLS_INTEGER;
      current_column varchar2(30) := '';
   BEGIN
      /* Be aware, PLSQL limitations on temporary clobs prevent multiple CLOB
         variables from pointing to the same temporary CLOB (it creates copies).
         Otherwise, the code could have been written a bit more compactly. */

      FOR i IN clob_columns_in.FIRST .. clob_columns_in.LAST
      LOOP
         /* Even when all clobs are null, DOA must send 1 row so skip it. */
         IF clob_columns_in (i) IS NOT NULL
         THEN
            IF current_column <> clob_columns_in (i) OR current_column IS NULL
            THEN
               current_column := LOWER (clob_columns_in (i));

               CASE current_column
                  WHEN 'message_in'
                  THEN
                     IF l_MESSAGE IS NULL
                     THEN
                       DBMS_LOB.createtemporary (
                         l_MESSAGE, TRUE, DBMS_LOB.CALL);
                     END IF;
               END CASE;
            END IF;

            CASE current_column
              WHEN 'message_in'
              THEN
                  DBMS_LOB.writeappend (l_MESSAGE
                                      , LENGTH (clob_pieces_in (i))
                                      , clob_pieces_in (i)
                                       );
            END CASE;
         END IF;
      END LOOP;

      ins (
         id_cdr_parameter_in => id_cdr_parameter_in,
         id_cdr_action_in => id_cdr_action_in,
         id_cdr_param_action_in => id_cdr_param_action_in,
         message_in => l_MESSAGE,
         event_span_in => event_span_in,
         id_event_span_umea_in => id_event_span_umea_in,
         flg_first_time_in => flg_first_time_in,
         create_user_in => create_user_in,
         create_time_in => create_time_in,
         create_institution_in => create_institution_in,
         update_user_in => update_user_in,
         update_time_in => update_time_in,
         update_institution_in => update_institution_in,
         id_cdr_message_in => id_cdr_message_in,
         handle_error_in => handle_error_in
         );

   END n_ins_clobs_in_chunks;

