      rows_in => rows_in
     ,handle_error_in => handle_error_in
     , rows_out => rows_out
   );
   END ins;


PROCEDURE upd (
      id_cdr_parameter_in IN CDR_PARAM_ACTION.ID_CDR_PARAMETER%TYPE,
      id_cdr_action_in IN CDR_PARAM_ACTION.ID_CDR_ACTION%TYPE,
      id_cdr_param_action_in IN CDR_PARAM_ACTION.ID_CDR_PARAM_ACTION%TYPE DEFAULT NULL,
      ID_CDR_PARAM_ACTION_nin IN BOOLEAN := TRUE,
      message_in IN CDR_PARAM_ACTION.MESSAGE%TYPE DEFAULT NULL,
      MESSAGE_nin IN BOOLEAN := TRUE,
      event_span_in IN CDR_PARAM_ACTION.EVENT_SPAN%TYPE DEFAULT NULL,
      EVENT_SPAN_nin IN BOOLEAN := TRUE,
      id_event_span_umea_in IN CDR_PARAM_ACTION.ID_EVENT_SPAN_UMEA%TYPE DEFAULT NULL,
      ID_EVENT_SPAN_UMEA_nin IN BOOLEAN := TRUE,
      flg_first_time_in IN CDR_PARAM_ACTION.FLG_FIRST_TIME%TYPE DEFAULT NULL,
      FLG_FIRST_TIME_nin IN BOOLEAN := TRUE,
      create_user_in IN CDR_PARAM_ACTION.CREATE_USER%TYPE DEFAULT NULL,
      CREATE_USER_nin IN BOOLEAN := TRUE,
      create_time_in IN CDR_PARAM_ACTION.CREATE_TIME%TYPE DEFAULT NULL,
      CREATE_TIME_nin IN BOOLEAN := TRUE,
      create_institution_in IN CDR_PARAM_ACTION.CREATE_INSTITUTION%TYPE DEFAULT NULL,
      CREATE_INSTITUTION_nin IN BOOLEAN := TRUE,
      update_user_in IN CDR_PARAM_ACTION.UPDATE_USER%TYPE DEFAULT NULL,
      UPDATE_USER_nin IN BOOLEAN := TRUE,
      update_time_in IN CDR_PARAM_ACTION.UPDATE_TIME%TYPE DEFAULT NULL,
      UPDATE_TIME_nin IN BOOLEAN := TRUE,
      update_institution_in IN CDR_PARAM_ACTION.UPDATE_INSTITUTION%TYPE DEFAULT NULL,
      UPDATE_INSTITUTION_nin IN BOOLEAN := TRUE,
      id_cdr_message_in IN CDR_PARAM_ACTION.ID_CDR_MESSAGE%TYPE DEFAULT NULL,
      ID_CDR_MESSAGE_nin IN BOOLEAN := TRUE,
     handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
   l_rows_out TABLE_VARCHAR;
   l_ID_CDR_PARAM_ACTION_n NUMBER(1);
   l_MESSAGE_n NUMBER(1);
   l_EVENT_SPAN_n NUMBER(1);
   l_ID_EVENT_SPAN_UMEA_n NUMBER(1);
   l_FLG_FIRST_TIME_n NUMBER(1);
   l_CREATE_USER_n NUMBER(1);
   l_CREATE_TIME_n NUMBER(1);
   l_CREATE_INSTITUTION_n NUMBER(1);
   l_UPDATE_USER_n NUMBER(1);
   l_UPDATE_TIME_n NUMBER(1);
   l_UPDATE_INSTITUTION_n NUMBER(1);
   l_ID_CDR_MESSAGE_n NUMBER(1);
   BEGIN

   l_ID_CDR_PARAM_ACTION_n := sys.diutil.bool_to_int(ID_CDR_PARAM_ACTION_nin);
   l_MESSAGE_n := sys.diutil.bool_to_int(MESSAGE_nin);
   l_EVENT_SPAN_n := sys.diutil.bool_to_int(EVENT_SPAN_nin);
   l_ID_EVENT_SPAN_UMEA_n := sys.diutil.bool_to_int(ID_EVENT_SPAN_UMEA_nin);
   l_FLG_FIRST_TIME_n := sys.diutil.bool_to_int(FLG_FIRST_TIME_nin);
   l_CREATE_USER_n := sys.diutil.bool_to_int(CREATE_USER_nin);
   l_CREATE_TIME_n := sys.diutil.bool_to_int(CREATE_TIME_nin);
   l_CREATE_INSTITUTION_n := sys.diutil.bool_to_int(CREATE_INSTITUTION_nin);
   l_UPDATE_USER_n := sys.diutil.bool_to_int(UPDATE_USER_nin);
   l_UPDATE_TIME_n := sys.diutil.bool_to_int(UPDATE_TIME_nin);
   l_UPDATE_INSTITUTION_n := sys.diutil.bool_to_int(UPDATE_INSTITUTION_nin);
   l_ID_CDR_MESSAGE_n := sys.diutil.bool_to_int(ID_CDR_MESSAGE_nin);


         UPDATE CDR_PARAM_ACTION SET
     ID_CDR_PARAM_ACTION = decode (l_ID_CDR_PARAM_ACTION_n,0,id_cdr_param_action_in, NVL (id_cdr_param_action_in, ID_CDR_PARAM_ACTION)),
     MESSAGE = decode (l_MESSAGE_n,0,message_in, NVL (message_in, MESSAGE)),
     EVENT_SPAN = decode (l_EVENT_SPAN_n,0,event_span_in, NVL (event_span_in, EVENT_SPAN)),
     ID_EVENT_SPAN_UMEA = decode (l_ID_EVENT_SPAN_UMEA_n,0,id_event_span_umea_in, NVL (id_event_span_umea_in, ID_EVENT_SPAN_UMEA)),
     FLG_FIRST_TIME = decode (l_FLG_FIRST_TIME_n,0,flg_first_time_in, NVL (flg_first_time_in, FLG_FIRST_TIME)),
     CREATE_USER = decode (l_CREATE_USER_n,0,create_user_in, NVL (create_user_in, CREATE_USER)),
     CREATE_TIME = decode (l_CREATE_TIME_n,0,create_time_in, NVL (create_time_in, CREATE_TIME)),
     CREATE_INSTITUTION = decode (l_CREATE_INSTITUTION_n,0,create_institution_in, NVL (create_institution_in, CREATE_INSTITUTION)),
     UPDATE_USER = decode (l_UPDATE_USER_n,0,update_user_in, NVL (update_user_in, UPDATE_USER)),
     UPDATE_TIME = decode (l_UPDATE_TIME_n,0,update_time_in, NVL (update_time_in, UPDATE_TIME)),
     UPDATE_INSTITUTION = decode (l_UPDATE_INSTITUTION_n,0,update_institution_in, NVL (update_institution_in, UPDATE_INSTITUTION)),
     ID_CDR_MESSAGE = decode (l_ID_CDR_MESSAGE_n,0,id_cdr_message_in, NVL (id_cdr_message_in, ID_CDR_MESSAGE))
          WHERE
             ID_CDR_PARAMETER = id_cdr_parameter_in AND
             ID_CDR_ACTION = id_cdr_action_in
         RETURNING ROWID BULK COLLECT INTO l_rows_out;


if(rows_out is null)
then
rows_out := table_varchar();
end if;

rows_out :=  rows_out MULTISET UNION DISTINCT l_rows_out;

   EXCEPTION
      WHEN DUP_VAL_ON_INDEX
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
           l_err_instance_id PLS_INTEGER;
        BEGIN
           get_constraint_info (l_owner, l_name);
           IF FALSE THEN NULL; -- Placeholder in case no unique indexes
           ELSIF l_name = 'CDRPA_UK'
           THEN
              pk_alert_exceptions.register_error (
                 error_name_in => 'DUPLICATE-VALUE'
                ,err_instance_id_out => l_err_instance_id
                ,text_in => 'Violation of unique index "CDRPA_UK"'
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'OWNER'
                ,value_in => l_owner
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'CONSTRAINT_NAME'
                ,value_in => l_name
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'TABLE_NAME'
                ,value_in => 'CDR_PARAM_ACTION'
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'ID_CDR_PARAM_ACTION'
                ,value_in => id_cdr_param_action_in
                ,validate_in => FALSE
                );
              pk_alert_exceptions.raise_error_instance (
                    err_instance_id_in => l_err_instance_id);
           ELSE
              pk_alert_exceptions.raise_error (
                    error_name_in => 'DUPLICATE-VALUE'
                    ,name1_in => 'OWNER'
                    ,value1_in => l_owner
                    ,name2_in => 'CONSTRAINT_NAME'
                    ,value2_in => l_name
                    ,name3_in => 'TABLE_NAME'
                    ,value3_in => 'CDR_PARAM_ACTION');
           END IF;
        END;
        END IF;
      WHEN e_check_constraint_failure
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
        BEGIN
           get_constraint_info (l_owner, l_name);
           pk_alert_exceptions.raise_error (
              error_name_in => 'CHECK-CONSTRAINT-FAILURE'
             ,name1_in => 'OWNER'
             ,value1_in => l_owner
             ,name2_in => 'CONSTRAINT_NAME'
             ,value2_in => l_name
             ,name3_in => 'TABLE_NAME'
             ,value3_in => 'CDR_PARAM_ACTION');
        END;
        END IF;
      WHEN e_integ_constraint_failure OR e_no_parent_key OR e_child_record_found
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
           l_id PLS_INTEGER;
           l_err_name VARCHAR2(32767) DEFAULT 'NO-PARENT-KEY-FOUND';
        BEGIN
           get_constraint_info (l_owner, l_name);
           IF SQLCODE = -2292 -- Child record found
           THEN
              l_err_name := 'CHILD-RECORD-FOUND' ;
           END IF;
           pk_alert_exceptions.register_error (
              error_name_in => l_err_name
             ,err_instance_id_out => l_id
             ,name1_in => 'OWNER', value1_in => l_owner
             ,name2_in => 'CONSTRAINT_NAME', value2_in => l_name
             ,name3_in => 'TABLE_NAME', value3_in => 'CDR_PARAM_ACTION');
           IF l_name = 'CDRPA_CDRA_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_ACTION'
               , value_in => id_cdr_action_in);
           END IF;
           IF l_name = 'CDRPA_CDRM_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_MESSAGE'
               , value_in => id_cdr_message_in);
           END IF;
           IF l_name = 'CDRPA_CDRP_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_PARAMETER'
               , value_in => id_cdr_parameter_in);
           END IF;
           IF l_name = 'CDRPA_UNITM_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_EVENT_SPAN_UMEA'
               , value_in => id_event_span_umea_in);
           END IF;
           pk_alert_exceptions.raise_error_instance (err_instance_id_in => l_id);
        END;
        END IF;
      WHEN e_null_column_value
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           v_errm VARCHAR2(2000) := DBMS_UTILITY.FORMAT_ERROR_STACK;
           dot1loc INTEGER;
           dot2loc INTEGER;
           parenloc INTEGER;
           c_owner ALL_CONSTRAINTS.OWNER%TYPE;
           c_tabname ALL_TABLES.TABLE_NAME%TYPE;
           c_colname ALL_TAB_COLUMNS.COLUMN_NAME%TYPE;
        BEGIN
           dot1loc := INSTR (v_errm, '.', 1, 1);
           dot2loc := INSTR (v_errm, '.', 1, 2);
           parenloc := INSTR (v_errm, '(');
           c_owner :=SUBSTR (v_errm, parenloc+1, dot1loc-parenloc-1);
           c_tabname := SUBSTR (v_errm, dot1loc+1, dot2loc-dot1loc-1);
           c_colname := SUBSTR (v_errm, dot2loc+1, INSTR (v_errm,')')-dot2loc-1);

           pk_alert_exceptions.raise_error (
                error_name_in => 'COLUMN-CANNOT-BE-NULL'
               ,name1_in => 'OWNER'
               ,value1_in => c_owner
               ,name2_in => 'TABLE_NAME'
               ,value2_in => c_tabname
               ,name3_in => 'COLUMN_NAME'
               ,value3_in => c_colname);
        END;
        END IF;
   END upd;


