    --Update with collection (with rows_out)
    PROCEDURE upd
    (
        col_in            IN pn_data_block_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_pn_data_block        id_pn_data_block_cc;
        l_flg_type                flg_type_cc;
        l_data_area               data_area_cc;
        l_flg_available           flg_available_cc;
        l_internal_name           internal_name_cc;
        l_code_pn_data_block      code_pn_data_block_cc;
        l_original_block          original_block_cc;
        l_title_code              title_code_cc;
        l_sample_text_code        sample_text_code_cc;
        l_create_user             create_user_cc;
        l_create_time             create_time_cc;
        l_create_institution      create_institution_cc;
        l_update_user             update_user_cc;
        l_update_time             update_time_cc;
        l_update_institution      update_institution_cc;
        l_code_pn_data_block_hist code_pn_data_block_hist_cc;
        l_id_pndb_parent          id_pndb_parent_cc;
        l_id_doc_area             id_doc_area_cc;
        l_id_swf_file_viewer      id_swf_file_viewer_cc;
        l_id_sys_button_viewer    id_sys_button_viewer_cc;
        l_flg_wf_viewer           flg_wf_viewer_cc;
        l_id_summary_page         id_summary_page_cc;
        l_sample_text_comment     sample_text_comment_cc;
        l_id_task_type            id_task_type_cc;
        l_id_swf_file_detail      id_swf_file_detail_cc;
        l_cancel_reason_area      cancel_reason_area_cc;
        l_sample_text_cancel      sample_text_cancel_cc;
        l_multi_option_column     multi_option_column_cc;
        l_id_mtos_score           id_mtos_score_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_pn_data_block(i) := col_in(i).id_pn_data_block;
            l_flg_type(i) := col_in(i).flg_type;
            l_data_area(i) := col_in(i).data_area;
            l_flg_available(i) := col_in(i).flg_available;
            l_internal_name(i) := col_in(i).internal_name;
            l_code_pn_data_block(i) := col_in(i).code_pn_data_block;
            l_original_block(i) := col_in(i).original_block;
            l_title_code(i) := col_in(i).title_code;
            l_sample_text_code(i) := col_in(i).sample_text_code;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_code_pn_data_block_hist(i) := col_in(i).code_pn_data_block_hist;
            l_id_pndb_parent(i) := col_in(i).id_pndb_parent;
            l_id_doc_area(i) := col_in(i).id_doc_area;
            l_id_swf_file_viewer(i) := col_in(i).id_swf_file_viewer;
            l_id_sys_button_viewer(i) := col_in(i).id_sys_button_viewer;
            l_flg_wf_viewer(i) := col_in(i).flg_wf_viewer;
            l_id_summary_page(i) := col_in(i).id_summary_page;
            l_sample_text_comment(i) := col_in(i).sample_text_comment;
            l_id_task_type(i) := col_in(i).id_task_type;
            l_id_swf_file_detail(i) := col_in(i).id_swf_file_detail;
            l_cancel_reason_area(i) := col_in(i).cancel_reason_area;
            l_sample_text_cancel(i) := col_in(i).sample_text_cancel;
            l_multi_option_column(i) := col_in(i).multi_option_column;
            l_id_mtos_score(i) := col_in(i).id_mtos_score;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE pn_data_block
                   SET flg_type                = nvl(l_flg_type(i), flg_type),
                       data_area               = nvl(l_data_area(i), data_area),
                       flg_available           = nvl(l_flg_available(i), flg_available),
                       internal_name           = nvl(l_internal_name(i), internal_name),
                       code_pn_data_block      = nvl(l_code_pn_data_block(i), code_pn_data_block),
                       original_block          = nvl(l_original_block(i), original_block),
                       title_code              = nvl(l_title_code(i), title_code),
                       sample_text_code        = nvl(l_sample_text_code(i), sample_text_code),
                       create_user             = nvl(l_create_user(i), create_user),
                       create_time             = nvl(l_create_time(i), create_time),
                       create_institution      = nvl(l_create_institution(i), create_institution),
                       update_user             = nvl(l_update_user(i), update_user),
                       update_time             = nvl(l_update_time(i), update_time),
                       update_institution      = nvl(l_update_institution(i), update_institution),
                       code_pn_data_block_hist = nvl(l_code_pn_data_block_hist(i), code_pn_data_block_hist),
                       id_pndb_parent          = nvl(l_id_pndb_parent(i), id_pndb_parent),
                       id_doc_area             = nvl(l_id_doc_area(i), id_doc_area),
                       id_swf_file_viewer      = nvl(l_id_swf_file_viewer(i), id_swf_file_viewer),
                       id_sys_button_viewer    = nvl(l_id_sys_button_viewer(i), id_sys_button_viewer),
                       flg_wf_viewer           = nvl(l_flg_wf_viewer(i), flg_wf_viewer),
                       id_summary_page         = nvl(l_id_summary_page(i), id_summary_page),
                       sample_text_comment     = nvl(l_sample_text_comment(i), sample_text_comment),
                       id_task_type            = nvl(l_id_task_type(i), id_task_type),
                       id_swf_file_detail      = nvl(l_id_swf_file_detail(i), id_swf_file_detail),
                       cancel_reason_area      = nvl(l_cancel_reason_area(i), cancel_reason_area),
                       sample_text_cancel      = nvl(l_sample_text_cancel(i), sample_text_cancel),
                       multi_option_column     = nvl(l_multi_option_column(i), multi_option_column),
                       id_mtos_score           = nvl(l_id_mtos_score(i), id_mtos_score)
                 WHERE id_pn_data_block = l_id_pn_data_block(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE pn_data_block
                   SET flg_type                = l_flg_type(i),
                       data_area               = l_data_area(i),
                       flg_available           = l_flg_available(i),
                       internal_name           = l_internal_name(i),
                       code_pn_data_block      = l_code_pn_data_block(i),
                       original_block          = l_original_block(i),
                       title_code              = l_title_code(i),
                       sample_text_code        = l_sample_text_code(i),
                       create_user             = l_create_user(i),
                       create_time             = l_create_time(i),
                       create_institution      = l_create_institution(i),
                       update_user             = l_update_user(i),
                       update_time             = l_update_time(i),
                       update_institution      = l_update_institution(i),
                       code_pn_data_block_hist = l_code_pn_data_block_hist(i),
                       id_pndb_parent          = l_id_pndb_parent(i),
                       id_doc_area             = l_id_doc_area(i),
                       id_swf_file_viewer      = l_id_swf_file_viewer(i),
                       id_sys_button_viewer    = l_id_sys_button_viewer(i),
                       flg_wf_viewer           = l_flg_wf_viewer(i),
                       id_summary_page         = l_id_summary_page(i),
                       sample_text_comment     = l_sample_text_comment(i),
                       id_task_type            = l_id_task_type(i),
                       id_swf_file_detail      = l_id_swf_file_detail(i),
                       cancel_reason_area      = l_cancel_reason_area(i),
                       sample_text_cancel      = l_sample_text_cancel(i),
                       multi_option_column     = l_multi_option_column(i),
                       id_mtos_score           = l_id_mtos_score(i)
                 WHERE id_pn_data_block = l_id_pn_data_block(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    
    EXCEPTION
        WHEN e_forall_error THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                FOR indx IN 1 .. SQL%bulk_exceptions.count
                LOOP
                    pk_alertlog.log_info('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
                END LOOP;
            END IF;
        WHEN OTHERS THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                pk_alertlog.log_info('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
            END IF;
        
    END upd;

    --Update with collection (without rows_out)
    PROCEDURE upd
    (
        col_in            IN pn_data_block_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE
    ) IS
        rows_out table_varchar;
    BEGIN
