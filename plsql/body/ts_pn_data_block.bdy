                                       ' CODE_PN_DATA_BLOCK_HIST = decode (' || l_code_pn_data_block_hist_n ||
                                       ',0,:CODE_PN_DATA_BLOCK_HIST_in, NVL (:CODE_PN_DATA_BLOCK_HIST_in, CODE_PN_DATA_BLOCK_HIST)) ' || ',' ||
                                       ' ID_PNDB_PARENT = decode (' || l_id_pndb_parent_n ||
                                       ',0,:ID_PNDB_PARENT_in, NVL (:ID_PNDB_PARENT_in, ID_PNDB_PARENT)) ' || ',' ||
                                       ' ID_DOC_AREA = decode (' || l_id_doc_area_n ||
                                       ',0,:ID_DOC_AREA_in, NVL (:ID_DOC_AREA_in, ID_DOC_AREA)) ' || ',' ||
                                       ' ID_SWF_FILE_VIEWER = decode (' || l_id_swf_file_viewer_n ||
                                       ',0,:ID_SWF_FILE_VIEWER_in, NVL (:ID_SWF_FILE_VIEWER_in, ID_SWF_FILE_VIEWER)) ' || ',' ||
                                       ' ID_SYS_BUTTON_VIEWER = decode (' || l_id_sys_button_viewer_n ||
                                       ',0,:ID_SYS_BUTTON_VIEWER_in, NVL (:ID_SYS_BUTTON_VIEWER_in, ID_SYS_BUTTON_VIEWER)) ' || ',' ||
                                       ' FLG_WF_VIEWER = decode (' || l_flg_wf_viewer_n ||
                                       ',0,:FLG_WF_VIEWER_in, NVL (:FLG_WF_VIEWER_in, FLG_WF_VIEWER)) ' || ',' ||
                                       ' ID_SUMMARY_PAGE = decode (' || l_id_summary_page_n ||
                                       ',0,:ID_SUMMARY_PAGE_in, NVL (:ID_SUMMARY_PAGE_in, ID_SUMMARY_PAGE)) ' || ',' ||
                                       ' SAMPLE_TEXT_COMMENT = decode (' || l_sample_text_comment_n ||
                                       ',0,:SAMPLE_TEXT_COMMENT_in, NVL (:SAMPLE_TEXT_COMMENT_in, SAMPLE_TEXT_COMMENT)) ' || ',' ||
                                       ' ID_TASK_TYPE = decode (' || l_id_task_type_n ||
                                       ',0,:ID_TASK_TYPE_in, NVL (:ID_TASK_TYPE_in, ID_TASK_TYPE)) ' || ',' ||
                                       ' ID_SWF_FILE_DETAIL = decode (' || l_id_swf_file_detail_n ||
                                       ',0,:ID_SWF_FILE_DETAIL_in, NVL (:ID_SWF_FILE_DETAIL_in, ID_SWF_FILE_DETAIL)) ' || ',' ||
                                       ' CANCEL_REASON_AREA = decode (' || l_cancel_reason_area_n ||
                                       ',0,:CANCEL_REASON_AREA_in, NVL (:CANCEL_REASON_AREA_in, CANCEL_REASON_AREA)) ' || ',' ||
                                       ' SAMPLE_TEXT_CANCEL = decode (' || l_sample_text_cancel_n ||
                                       ',0,:SAMPLE_TEXT_CANCEL_in, NVL (:SAMPLE_TEXT_CANCEL_in, SAMPLE_TEXT_CANCEL)) ' || ',' ||
                                       ' MULTI_OPTION_COLUMN = decode (' || l_multi_option_column_n ||
                                       ',0,:MULTI_OPTION_COLUMN_in, NVL (:MULTI_OPTION_COLUMN_in, MULTI_OPTION_COLUMN)) ' || ',' ||
                                       ' ID_MTOS_SCORE = decode (' || l_id_mtos_score_n ||
                                       ',0,:ID_MTOS_SCORE_in, NVL (:ID_MTOS_SCORE_in, ID_MTOS_SCORE)) ' || ' where ' ||
                                       nvl(where_in, '(1=1)') || ' RETURNING ROWID BULK COLLECT INTO :l_rows_out';
        EXECUTE IMMEDIATE 'BEGIN ' || l_sql || '; END;'
            USING IN flg_type_in, data_area_in, flg_available_in, internal_name_in, code_pn_data_block_in, original_block_in, title_code_in, sample_text_code_in, create_user_in, create_time_in, create_institution_in, update_user_in, update_time_in, update_institution_in, code_pn_data_block_hist_in, id_pndb_parent_in, id_doc_area_in, id_swf_file_viewer_in, id_sys_button_viewer_in, flg_wf_viewer_in, id_summary_page_in, sample_text_comment_in, id_task_type_in, id_swf_file_detail_in, cancel_reason_area_in, sample_text_cancel_in, multi_option_column_in, id_mtos_score_in, OUT l_rows_out;
        IF (rows_out IS NULL)
        THEN
            rows_out := table_varchar();
        END IF;
        rows_out := rows_out MULTISET UNION DISTINCT l_rows_out;
    
    EXCEPTION
        WHEN e_null_column_value
             OR e_existing_fky_reference
             OR e_check_constraint_failure
             OR e_no_parent_key
             OR e_child_record_found
             OR e_forall_error
             OR e_integ_constraint_failure THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                pk_alertlog.log_info('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
            END IF;
        
    END upd;

