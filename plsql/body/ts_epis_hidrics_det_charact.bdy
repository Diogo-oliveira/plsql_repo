                       id_epis_hid_ftxt_char = nvl(l_id_epis_hid_ftxt_char(i), id_epis_hid_ftxt_char)
                 WHERE id_epis_hidrics_det = l_id_epis_hidrics_det(i)
                   AND id_hidrics_charact = l_id_hidrics_charact(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE epis_hidrics_det_charact
                   SET create_user           = l_create_user(i),
                       create_time           = l_create_time(i),
