   FUNCTION next_key (sequence_in IN VARCHAR2 := NULL) RETURN DOC_MACRO_VERSION.ID_DOC_MACRO_VERSION%TYPE

   IS
     retval DOC_MACRO_VERSION.ID_DOC_MACRO_VERSION%TYPE;

   BEGIN
      IF sequence_in IS NULL
      THEN
         SELECT seq_DOC_MACRO_VERSION.NEXTVAL INTO retval FROM dual;
      ELSE
         EXECUTE IMMEDIATE
            'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
            INTO retval;
      END IF;
      RETURN retval;
   EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'SEQUENCE-GENERATION-FAILURE'
           ,name1_in => 'SEQUENCE'
           ,value1_in => NVL (sequence_in, 'seq_DOC_MACRO_VERSION')
           );
   END next_key;

