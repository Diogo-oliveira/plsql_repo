    PROCEDURE upd
    (
        col_in            IN epis_report_disclosure_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_epis_report       id_epis_report_cc;
        l_dt_register          dt_register_cc;
        l_id_prof_disclosure   id_prof_disclosure_cc;
        l_dt_request           dt_request_cc;
        l_dt_disclosure        dt_disclosure_cc;
        l_disclosure_recipient disclosure_recipient_cc;
        l_recipient_address    recipient_address_cc;
        l_id_sample_text       id_sample_text_cc;
        l_free_text_purp_disc  free_text_purp_disc_cc;
        l_notes                notes_cc;
        l_create_user          create_user_cc;
        l_create_time          create_time_cc;
        l_create_institution   create_institution_cc;
        l_update_user          update_user_cc;
        l_update_time          update_time_cc;
        l_update_institution   update_institution_cc;
        l_flg_disc_recipient   flg_disc_recipient_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_epis_report(i) := col_in(i).id_epis_report;
            l_dt_register(i) := col_in(i).dt_register;
            l_id_prof_disclosure(i) := col_in(i).id_prof_disclosure;
            l_dt_request(i) := col_in(i).dt_request;
            l_dt_disclosure(i) := col_in(i).dt_disclosure;
            l_disclosure_recipient(i) := col_in(i).disclosure_recipient;
            l_recipient_address(i) := col_in(i).recipient_address;
            l_id_sample_text(i) := col_in(i).id_sample_text;
            l_free_text_purp_disc(i) := col_in(i).free_text_purp_disc;
            l_notes(i) := col_in(i).notes;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_flg_disc_recipient(i) := col_in(i).flg_disc_recipient;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE epis_report_disclosure
                   SET dt_register          = nvl(l_dt_register(i), dt_register),
                       id_prof_disclosure   = nvl(l_id_prof_disclosure(i), id_prof_disclosure),
                       dt_request           = nvl(l_dt_request(i), dt_request),
                       dt_disclosure        = nvl(l_dt_disclosure(i), dt_disclosure),
                       disclosure_recipient = nvl(l_disclosure_recipient(i), disclosure_recipient),
                       recipient_address    = nvl(l_recipient_address(i), recipient_address),
                       id_sample_text       = nvl(l_id_sample_text(i), id_sample_text),
                       free_text_purp_disc  = nvl(l_free_text_purp_disc(i), free_text_purp_disc),
                       notes                = nvl(l_notes(i), notes),
                       create_user          = nvl(l_create_user(i), create_user),
                       create_time          = nvl(l_create_time(i), create_time),
                       create_institution   = nvl(l_create_institution(i), create_institution),
                       update_user          = nvl(l_update_user(i), update_user),
                       update_time          = nvl(l_update_time(i), update_time),
                       update_institution   = nvl(l_update_institution(i), update_institution),
                       flg_disc_recipient   = nvl(l_flg_disc_recipient(i), flg_disc_recipient)
                 WHERE id_epis_report = l_id_epis_report(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE epis_report_disclosure
                   SET dt_register          = l_dt_register(i),
                       id_prof_disclosure   = l_id_prof_disclosure(i),
                       dt_request           = l_dt_request(i),
                       dt_disclosure        = l_dt_disclosure(i),
                       disclosure_recipient = l_disclosure_recipient(i),
                       recipient_address    = l_recipient_address(i),
                       id_sample_text       = l_id_sample_text(i),
                       free_text_purp_disc  = l_free_text_purp_disc(i),
                       notes                = l_notes(i),
                       create_user          = l_create_user(i),
                       create_time          = l_create_time(i),
                       create_institution   = l_create_institution(i),
                       update_user          = l_update_user(i),
                       update_time          = l_update_time(i),
                       update_institution   = l_update_institution(i),
                       flg_disc_recipient   = l_flg_disc_recipient(i)
                 WHERE id_epis_report = l_id_epis_report(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    END upd;

    PROCEDURE upd
    (
        col_in            IN epis_report_disclosure_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE
    ) IS
        rows_out table_varchar;
    BEGIN
        upd(col_in, ignore_if_null_in, handle_error_in, rows_out);
    END upd;

    FUNCTION dynupdstr
    (
        colname_in IN all_tab_columns.column_name%TYPE,
        where_in   IN VARCHAR2 := NULL
    )
    
     RETURN VARCHAR2 IS
    BEGIN
        RETURN 'BEGIN UPDATE EPIS_REPORT_DISCLOSURE
             SET ' || colname_in || ' = :value
           WHERE ' || nvl(where_in, '1=1') || ' RETURNING ROWID BULK COLLECT INTO :rows_out; END;';
    END dynupdstr;

