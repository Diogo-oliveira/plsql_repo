        id_prof_last_update_nin IN BOOLEAN := TRUE,
        dt_last_update_in       IN pat_prob_unaware.dt_last_update%TYPE DEFAULT NULL,
        dt_last_update_nin      IN BOOLEAN := TRUE,
        id_cancel_reason_in     IN pat_prob_unaware.id_cancel_reason%TYPE DEFAULT NULL,
        id_cancel_reason_nin    IN BOOLEAN := TRUE,
        cancel_notes_in         IN pat_prob_unaware.cancel_notes%TYPE DEFAULT NULL,
        cancel_notes_nin        IN BOOLEAN := TRUE,
