                                               nvl(id_resched_reason_in, id_resched_reason)),
               id_prof_resched        = decode(l_id_prof_resched_n,
                                               0,
                                               id_prof_resched_in,
                                               nvl(id_prof_resched_in, id_prof_resched)),
               dt_resched_date        = decode(l_dt_resched_date_n,
                                               0,
                                               dt_resched_date_in,
                                               nvl(dt_resched_date_in, dt_resched_date)),
               resched_notes          = decode(l_resched_notes_n,
                                               0,
