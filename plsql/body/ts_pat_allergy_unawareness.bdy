                       cancel_notes           = nvl(l_cancel_notes(i), cancel_notes),
                       dt_cancel              = nvl(l_dt_cancel(i), dt_cancel)
                 WHERE id_pat_allergy_unawareness = l_id_pat_allergy_unawareness(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE pat_allergy_unawareness
                   SET id_allergy_unawareness = l_id_allergy_unawareness(i),
                       id_professional        = l_id_professional(i),
                       id_patient             = l_id_patient(i),
