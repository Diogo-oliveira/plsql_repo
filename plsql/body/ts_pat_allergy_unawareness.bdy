        l_cancel_notes               cancel_notes_cc;
        l_dt_cancel                  dt_cancel_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_pat_allergy_unawareness(i) := col_in(i).id_pat_allergy_unawareness;
            l_id_allergy_unawareness(i) := col_in(i).id_allergy_unawareness;
