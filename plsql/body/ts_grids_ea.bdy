            l_id_fast_track_er_law(i) := col_in(i).id_fast_track_er_law;
            l_flg_has_transfer(i) := col_in(i).flg_has_transfer;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE grids_ea
                   SET id_visit                = nvl(l_id_visit(i), id_visit),
                       id_clinical_service     = nvl(l_id_clinical_service(i), id_clinical_service),
