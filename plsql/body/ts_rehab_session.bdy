        id_professional_nin        IN BOOLEAN := TRUE,
        id_episode_in              IN rehab_session.id_episode%TYPE DEFAULT NULL,
        id_episode_nin             IN BOOLEAN := TRUE,
        flg_status_in              IN rehab_session.flg_status%TYPE DEFAULT NULL,
        flg_status_nin             IN BOOLEAN := TRUE,
        id_exec_professional_in    IN rehab_session.id_exec_professional%TYPE DEFAULT NULL,
        id_exec_professional_nin   IN BOOLEAN := TRUE,
        dt_begin_in                IN rehab_session.dt_begin%TYPE DEFAULT NULL,
        dt_begin_nin               IN BOOLEAN := TRUE,
        dt_end_in                  IN rehab_session.dt_end%TYPE DEFAULT NULL,
        dt_end_nin                 IN BOOLEAN := TRUE,
        duration_in                IN rehab_session.duration%TYPE DEFAULT NULL,
        duration_nin               IN BOOLEAN := TRUE,
        notes_in                   IN rehab_session.notes%TYPE DEFAULT NULL,
        notes_nin                  IN BOOLEAN := TRUE,
        id_cancel_reason_in        IN rehab_session.id_cancel_reason%TYPE DEFAULT NULL,
        id_cancel_reason_nin       IN BOOLEAN := TRUE,
        dt_cancel_in               IN rehab_session.dt_cancel%TYPE DEFAULT NULL,
        dt_cancel_nin              IN BOOLEAN := TRUE,
        id_cancel_professional_in  IN rehab_session.id_cancel_professional%TYPE DEFAULT NULL,
        id_cancel_professional_nin IN BOOLEAN := TRUE,
        notes_cancel_in            IN rehab_session.notes_cancel%TYPE DEFAULT NULL,
        notes_cancel_nin           IN BOOLEAN := TRUE,
        create_user_in             IN rehab_session.create_user%TYPE DEFAULT NULL,
        create_user_nin            IN BOOLEAN := TRUE,
        create_time_in             IN rehab_session.create_time%TYPE DEFAULT NULL,
        create_time_nin            IN BOOLEAN := TRUE,
        create_institution_in      IN rehab_session.create_institution%TYPE DEFAULT NULL,
        create_institution_nin     IN BOOLEAN := TRUE,
        update_user_in             IN rehab_session.update_user%TYPE DEFAULT NULL,
        update_user_nin            IN BOOLEAN := TRUE,
        update_time_in             IN rehab_session.update_time%TYPE DEFAULT NULL,
        update_time_nin            IN BOOLEAN := TRUE,
        update_institution_in      IN rehab_session.update_institution%TYPE DEFAULT NULL,
        update_institution_nin     IN BOOLEAN := TRUE,
        handle_error_in            IN BOOLEAN := TRUE,
        rows_out                   IN OUT table_varchar
    ) IS
        l_rows_out                 table_varchar;
        l_dt_rehab_session_n       NUMBER(1);
        l_id_rehab_presc_n         NUMBER(1);
        l_id_professional_n        NUMBER(1);
        l_id_episode_n             NUMBER(1);
        l_flg_status_n             NUMBER(1);
        l_id_exec_professional_n   NUMBER(1);
        l_dt_begin_n               NUMBER(1);
        l_dt_end_n                 NUMBER(1);
        l_duration_n               NUMBER(1);
        l_notes_n                  NUMBER(1);
        l_id_cancel_reason_n       NUMBER(1);
        l_dt_cancel_n              NUMBER(1);
        l_id_cancel_professional_n NUMBER(1);
        l_notes_cancel_n           NUMBER(1);
        l_create_user_n            NUMBER(1);
        l_create_time_n            NUMBER(1);
        l_create_institution_n     NUMBER(1);
        l_update_user_n            NUMBER(1);
        l_update_time_n            NUMBER(1);
        l_update_institution_n     NUMBER(1);
    BEGIN
    
        l_dt_rehab_session_n       := sys.diutil.bool_to_int(dt_rehab_session_nin);
        l_id_rehab_presc_n         := sys.diutil.bool_to_int(id_rehab_presc_nin);
        l_id_professional_n        := sys.diutil.bool_to_int(id_professional_nin);
        l_id_episode_n             := sys.diutil.bool_to_int(id_episode_nin);
        l_flg_status_n             := sys.diutil.bool_to_int(flg_status_nin);
        l_id_exec_professional_n   := sys.diutil.bool_to_int(id_exec_professional_nin);
        l_dt_begin_n               := sys.diutil.bool_to_int(dt_begin_nin);
        l_dt_end_n                 := sys.diutil.bool_to_int(dt_end_nin);
        l_duration_n               := sys.diutil.bool_to_int(duration_nin);
        l_notes_n                  := sys.diutil.bool_to_int(notes_nin);
        l_id_cancel_reason_n       := sys.diutil.bool_to_int(id_cancel_reason_nin);
        l_dt_cancel_n              := sys.diutil.bool_to_int(dt_cancel_nin);
        l_id_cancel_professional_n := sys.diutil.bool_to_int(id_cancel_professional_nin);
        l_notes_cancel_n           := sys.diutil.bool_to_int(notes_cancel_nin);
        l_create_user_n            := sys.diutil.bool_to_int(create_user_nin);
        l_create_time_n            := sys.diutil.bool_to_int(create_time_nin);
        l_create_institution_n     := sys.diutil.bool_to_int(create_institution_nin);
        l_update_user_n            := sys.diutil.bool_to_int(update_user_nin);
        l_update_time_n            := sys.diutil.bool_to_int(update_time_nin);
        l_update_institution_n     := sys.diutil.bool_to_int(update_institution_nin);
    
        UPDATE rehab_session
           SET dt_rehab_session       = decode(l_dt_rehab_session_n,
                                               0,
                                               dt_rehab_session_in,
                                               nvl(dt_rehab_session_in, dt_rehab_session)),
               id_rehab_presc         = decode(l_id_rehab_presc_n,
                                               0,
                                               id_rehab_presc_in,
                                               nvl(id_rehab_presc_in, id_rehab_presc)),
               id_professional        = decode(l_id_professional_n,
                                               0,
                                               id_professional_in,
                                               nvl(id_professional_in, id_professional)),
               id_episode             = decode(l_id_episode_n, 0, id_episode_in, nvl(id_episode_in, id_episode)),
               flg_status             = decode(l_flg_status_n, 0, flg_status_in, nvl(flg_status_in, flg_status)),
               id_exec_professional   = decode(l_id_exec_professional_n,
