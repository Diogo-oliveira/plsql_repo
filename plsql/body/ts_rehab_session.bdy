    PROCEDURE upd
    (
        id_rehab_session_in        IN rehab_session.id_rehab_session%TYPE,
        dt_rehab_session_in        IN rehab_session.dt_rehab_session%TYPE DEFAULT NULL,
        dt_rehab_session_nin       IN BOOLEAN := TRUE,
        id_rehab_presc_in          IN rehab_session.id_rehab_presc%TYPE DEFAULT NULL,
        id_rehab_presc_nin         IN BOOLEAN := TRUE,
