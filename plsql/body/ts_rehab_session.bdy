    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN rehab_session_tc IS
        data rehab_session_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM rehab_session
         WHERE ROWID IN (SELECT /*+opt_estimate(table,t,scale_rows=0.0000001))*/
                          *
                           FROM TABLE(rows_in) t);
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN rehab_session_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
