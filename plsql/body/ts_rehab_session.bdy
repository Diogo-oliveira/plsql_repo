    PROCEDURE upd
    (
        col_in            IN rehab_session_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_rehab_session       id_rehab_session_cc;
        l_dt_rehab_session       dt_rehab_session_cc;
        l_id_rehab_presc         id_rehab_presc_cc;
        l_id_professional        id_professional_cc;
        l_id_episode             id_episode_cc;
        l_flg_status             flg_status_cc;
        l_id_exec_professional   id_exec_professional_cc;
        l_dt_begin               dt_begin_cc;
        l_dt_end                 dt_end_cc;
        l_duration               duration_cc;
        l_notes                  notes_cc;
        l_id_cancel_reason       id_cancel_reason_cc;
        l_dt_cancel              dt_cancel_cc;
        l_id_cancel_professional id_cancel_professional_cc;
        l_notes_cancel           notes_cancel_cc;
        l_create_user            create_user_cc;
        l_create_time            create_time_cc;
        l_create_institution     create_institution_cc;
        l_update_user            update_user_cc;
        l_update_time            update_time_cc;
        l_update_institution     update_institution_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_id_rehab_session(i) := col_in(i).id_rehab_session;
            l_dt_rehab_session(i) := col_in(i).dt_rehab_session;
            l_id_rehab_presc(i) := col_in(i).id_rehab_presc;
            l_id_professional(i) := col_in(i).id_professional;
            l_id_episode(i) := col_in(i).id_episode;
            l_flg_status(i) := col_in(i).flg_status;
            l_id_exec_professional(i) := col_in(i).id_exec_professional;
            l_dt_begin(i) := col_in(i).dt_begin;
            l_dt_end(i) := col_in(i).dt_end;
            l_duration(i) := col_in(i).duration;
            l_notes(i) := col_in(i).notes;
            l_id_cancel_reason(i) := col_in(i).id_cancel_reason;
            l_dt_cancel(i) := col_in(i).dt_cancel;
            l_id_cancel_professional(i) := col_in(i).id_cancel_professional;
            l_notes_cancel(i) := col_in(i).notes_cancel;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE rehab_session
                   SET dt_rehab_session       = nvl(l_dt_rehab_session(i), dt_rehab_session),
                       id_rehab_presc         = nvl(l_id_rehab_presc(i), id_rehab_presc),
                       id_professional        = nvl(l_id_professional(i), id_professional),
                       id_episode             = nvl(l_id_episode(i), id_episode),
                       flg_status             = nvl(l_flg_status(i), flg_status),
                       id_exec_professional   = nvl(l_id_exec_professional(i), id_exec_professional),
                       dt_begin               = nvl(l_dt_begin(i), dt_begin),
                       dt_end                 = nvl(l_dt_end(i), dt_end),
                       duration               = nvl(l_duration(i), duration),
                       notes                  = nvl(l_notes(i), notes),
                       id_cancel_reason       = nvl(l_id_cancel_reason(i), id_cancel_reason),
                       dt_cancel              = nvl(l_dt_cancel(i), dt_cancel),
                       id_cancel_professional = nvl(l_id_cancel_professional(i), id_cancel_professional),
                       notes_cancel           = nvl(l_notes_cancel(i), notes_cancel),
                       create_user            = nvl(l_create_user(i), create_user),
                       create_time            = nvl(l_create_time(i), create_time),
                       create_institution     = nvl(l_create_institution(i), create_institution),
                       update_user            = nvl(l_update_user(i), update_user),
                       update_time            = nvl(l_update_time(i), update_time),
                       update_institution     = nvl(l_update_institution(i), update_institution)
                 WHERE id_rehab_session = l_id_rehab_session(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE rehab_session
                   SET dt_rehab_session       = l_dt_rehab_session(i),
                       id_rehab_presc         = l_id_rehab_presc(i),
                       id_professional        = l_id_professional(i),
                       id_episode             = l_id_episode(i),
                       flg_status             = l_flg_status(i),
                       id_exec_professional   = l_id_exec_professional(i),
                       dt_begin               = l_dt_begin(i),
                       dt_end                 = l_dt_end(i),
                       duration               = l_duration(i),
                       notes                  = l_notes(i),
                       id_cancel_reason       = l_id_cancel_reason(i),
                       dt_cancel              = l_dt_cancel(i),
                       id_cancel_professional = l_id_cancel_professional(i),
                       notes_cancel           = l_notes_cancel(i),
                       create_user            = l_create_user(i),
                       create_time            = l_create_time(i),
