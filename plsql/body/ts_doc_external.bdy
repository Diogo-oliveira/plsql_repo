    -- Insert a collection of rows using FORALL; all primary key values
    -- must have already been generated, or are handled in triggers
    PROCEDURE ins
    (
        rows_in         IN doc_external_tc,
        handle_error_in IN BOOLEAN := FALSE,
        rows_out        OUT table_varchar
    ) IS
    BEGIN
        IF rows_in.count = 0
        THEN
            NULL;
        ELSE
            FORALL indx IN rows_in.first .. rows_in.last SAVE EXCEPTIONS
                INSERT INTO doc_external
                    (id_doc_external,
                     id_doc_type,
                     num_doc,
                     dt_emited,
                     notes,
                     dt_digit,
                     id_doc_ori_type,
                     id_doc_destination,
                     dt_expire,
                     id_external_request,
                     desc_doc_type,
                     desc_doc_ori_type,
                     desc_doc_destination,
                     id_episode,
                     id_patient,
                     flg_status,
                     local_emited,
                     id_institution,
                     flg_sent_by,
                     flg_received,
                     id_doc_original,
                     desc_doc_original,
                     id_professional,
                     title,
                     dt_inserted,
                     dt_updated,
                     id_professional_upd,
                     id_prof_perf_by,
                     desc_perf_by,
                     id_grupo,
                     organ_shipper,
                     id_language,
                     id_specialty,
                     author,
                     dt_last_identification,
                     doc_oid,
                     desc_language)
                VALUES
                    (rows_in(indx).id_doc_external,
                     rows_in(indx).id_doc_type,
                     rows_in(indx).num_doc,
                     rows_in(indx).dt_emited,
                     rows_in(indx).notes,
                     rows_in(indx).dt_digit,
                     rows_in(indx).id_doc_ori_type,
                     rows_in(indx).id_doc_destination,
                     rows_in(indx).dt_expire,
                     rows_in(indx).id_external_request,
                     rows_in(indx).desc_doc_type,
                     rows_in(indx).desc_doc_ori_type,
                     rows_in(indx).desc_doc_destination,
                     rows_in(indx).id_episode,
                     rows_in(indx).id_patient,
                     rows_in(indx).flg_status,
                     rows_in(indx).local_emited,
                     rows_in(indx).id_institution,
                     rows_in(indx).flg_sent_by,
                     rows_in(indx).flg_received,
                     rows_in(indx).id_doc_original,
                     rows_in(indx).desc_doc_original,
                     rows_in(indx).id_professional,
                     rows_in(indx).title,
                     rows_in(indx).dt_inserted,
                     rows_in(indx).dt_updated,
                     rows_in(indx).id_professional_upd,
                     rows_in(indx).id_prof_perf_by,
                     rows_in(indx).desc_perf_by,
                     rows_in(indx).id_grupo,
                     rows_in(indx).organ_shipper,
                     rows_in(indx).id_language,
                     rows_in(indx).id_specialty,
                     rows_in(indx).author,
                     rows_in(indx).dt_last_identification,
                     rows_in(indx).doc_oid,
                     rows_in(indx).desc_language)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    
    EXCEPTION
        WHEN e_forall_error THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                FOR indx IN 1 .. SQL%bulk_exceptions.count
                LOOP
                    pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
                END LOOP;
            END IF;
        WHEN OTHERS THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
            END IF;
        
    END ins;

    -- Insert a collection of rows using FORALL; all primary key values
