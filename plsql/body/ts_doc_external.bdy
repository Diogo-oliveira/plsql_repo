                       desc_language          = nvl(l_desc_language(i), desc_language)
                 WHERE id_doc_external = l_id_doc_external(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE doc_external
                   SET id_doc_type            = l_id_doc_type(i),
                       num_doc                = l_num_doc(i),
                       dt_emited              = l_dt_emited(i),
                       notes                  = l_notes(i),
                       dt_digit               = l_dt_digit(i),
                       id_doc_ori_type        = l_id_doc_ori_type(i),
                       id_doc_destination     = l_id_doc_destination(i),
                       dt_expire              = l_dt_expire(i),
                       id_external_request    = l_id_external_request(i),
                       desc_doc_type          = l_desc_doc_type(i),
                       desc_doc_ori_type      = l_desc_doc_ori_type(i),
                       desc_doc_destination   = l_desc_doc_destination(i),
                       id_episode             = l_id_episode(i),
                       id_patient             = l_id_patient(i),
                       flg_status             = l_flg_status(i),
                       local_emited           = l_local_emited(i),
                       id_institution         = l_id_institution(i),
                       flg_sent_by            = l_flg_sent_by(i),
                       flg_received           = l_flg_received(i),
                       id_doc_original        = l_id_doc_original(i),
                       desc_doc_original      = l_desc_doc_original(i),
                       id_professional        = l_id_professional(i),
                       title                  = l_title(i),
                       dt_inserted            = l_dt_inserted(i),
                       dt_updated             = l_dt_updated(i),
                       id_professional_upd    = l_id_professional_upd(i),
                       id_prof_perf_by        = l_id_prof_perf_by(i),
                       desc_perf_by           = l_desc_perf_by(i),
                       id_grupo               = l_id_grupo(i),
                       create_user            = l_create_user(i),
                       create_time            = l_create_time(i),
                       create_institution     = l_create_institution(i),
                       update_user            = l_update_user(i),
                       update_time            = l_update_time(i),
                       update_institution     = l_update_institution(i),
                       organ_shipper          = l_organ_shipper(i),
                       id_language            = l_id_language(i),
                       id_specialty           = l_id_specialty(i),
                       author                 = l_author(i),
                       dt_last_identification = l_dt_last_identification(i),
                       doc_oid                = l_doc_oid(i),
                       desc_language          = l_desc_language(i)
                 WHERE id_doc_external = l_id_doc_external(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    
    EXCEPTION
        WHEN e_forall_error THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                FOR indx IN 1 .. SQL%bulk_exceptions.count
                LOOP
                    pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
                END LOOP;
            END IF;
        WHEN OTHERS THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
            END IF;
        
    END upd;

