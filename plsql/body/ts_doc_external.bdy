    --Update with collection (with rows_out)
    PROCEDURE upd
    (
        col_in            IN doc_external_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := FALSE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_doc_external        id_doc_external_cc;
        l_id_doc_type            id_doc_type_cc;
        l_num_doc                num_doc_cc;
        l_dt_emited              dt_emited_cc;
        l_notes                  notes_cc;
        l_dt_digit               dt_digit_cc;
        l_id_doc_ori_type        id_doc_ori_type_cc;
        l_id_doc_destination     id_doc_destination_cc;
        l_dt_expire              dt_expire_cc;
        l_id_external_request    id_external_request_cc;
        l_desc_doc_type          desc_doc_type_cc;
        l_desc_doc_ori_type      desc_doc_ori_type_cc;
        l_desc_doc_destination   desc_doc_destination_cc;
        l_id_episode             id_episode_cc;
        l_id_patient             id_patient_cc;
        l_flg_status             flg_status_cc;
        l_local_emited           local_emited_cc;
        l_id_institution         id_institution_cc;
        l_flg_sent_by            flg_sent_by_cc;
        l_flg_received           flg_received_cc;
        l_id_doc_original        id_doc_original_cc;
        l_desc_doc_original      desc_doc_original_cc;
        l_id_professional        id_professional_cc;
        l_title                  title_cc;
        l_dt_inserted            dt_inserted_cc;
        l_dt_updated             dt_updated_cc;
        l_id_professional_upd    id_professional_upd_cc;
        l_id_prof_perf_by        id_prof_perf_by_cc;
        l_desc_perf_by           desc_perf_by_cc;
        l_id_grupo               id_grupo_cc;
        l_create_user            create_user_cc;
        l_create_time            create_time_cc;
        l_create_institution     create_institution_cc;
        l_update_user            update_user_cc;
        l_update_time            update_time_cc;
        l_update_institution     update_institution_cc;
        l_organ_shipper          organ_shipper_cc;
        l_id_language            id_language_cc;
        l_id_specialty           id_specialty_cc;
        l_author                 author_cc;
        l_dt_last_identification dt_last_identification_cc;
        l_doc_oid                doc_oid_cc;
        l_desc_language          desc_language_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_doc_external(i) := col_in(i).id_doc_external;
            l_id_doc_type(i) := col_in(i).id_doc_type;
            l_num_doc(i) := col_in(i).num_doc;
            l_dt_emited(i) := col_in(i).dt_emited;
            l_notes(i) := col_in(i).notes;
            l_dt_digit(i) := col_in(i).dt_digit;
            l_id_doc_ori_type(i) := col_in(i).id_doc_ori_type;
            l_id_doc_destination(i) := col_in(i).id_doc_destination;
            l_dt_expire(i) := col_in(i).dt_expire;
            l_id_external_request(i) := col_in(i).id_external_request;
            l_desc_doc_type(i) := col_in(i).desc_doc_type;
            l_desc_doc_ori_type(i) := col_in(i).desc_doc_ori_type;
            l_desc_doc_destination(i) := col_in(i).desc_doc_destination;
            l_id_episode(i) := col_in(i).id_episode;
            l_id_patient(i) := col_in(i).id_patient;
            l_flg_status(i) := col_in(i).flg_status;
            l_local_emited(i) := col_in(i).local_emited;
            l_id_institution(i) := col_in(i).id_institution;
            l_flg_sent_by(i) := col_in(i).flg_sent_by;
            l_flg_received(i) := col_in(i).flg_received;
            l_id_doc_original(i) := col_in(i).id_doc_original;
            l_desc_doc_original(i) := col_in(i).desc_doc_original;
            l_id_professional(i) := col_in(i).id_professional;
            l_title(i) := col_in(i).title;
            l_dt_inserted(i) := col_in(i).dt_inserted;
            l_dt_updated(i) := col_in(i).dt_updated;
            l_id_professional_upd(i) := col_in(i).id_professional_upd;
            l_id_prof_perf_by(i) := col_in(i).id_prof_perf_by;
            l_desc_perf_by(i) := col_in(i).desc_perf_by;
            l_id_grupo(i) := col_in(i).id_grupo;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_organ_shipper(i) := col_in(i).organ_shipper;
            l_id_language(i) := col_in(i).id_language;
            l_id_specialty(i) := col_in(i).id_specialty;
            l_author(i) := col_in(i).author;
            l_dt_last_identification(i) := col_in(i).dt_last_identification;
            l_doc_oid(i) := col_in(i).doc_oid;
            l_desc_language(i) := col_in(i).desc_language;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE doc_external
                   SET id_doc_type            = nvl(l_id_doc_type(i), id_doc_type),
                       num_doc                = nvl(l_num_doc(i), num_doc),
                       dt_emited              = nvl(l_dt_emited(i), dt_emited),
                       notes                  = nvl(l_notes(i), notes),
                       dt_digit               = nvl(l_dt_digit(i), dt_digit),
                       id_doc_ori_type        = nvl(l_id_doc_ori_type(i), id_doc_ori_type),
                       id_doc_destination     = nvl(l_id_doc_destination(i), id_doc_destination),
                       dt_expire              = nvl(l_dt_expire(i), dt_expire),
                       id_external_request    = nvl(l_id_external_request(i), id_external_request),
                       desc_doc_type          = nvl(l_desc_doc_type(i), desc_doc_type),
                       desc_doc_ori_type      = nvl(l_desc_doc_ori_type(i), desc_doc_ori_type),
                       desc_doc_destination   = nvl(l_desc_doc_destination(i), desc_doc_destination),
                       id_episode             = nvl(l_id_episode(i), id_episode),
                       id_patient             = nvl(l_id_patient(i), id_patient),
                       flg_status             = nvl(l_flg_status(i), flg_status),
                       local_emited           = nvl(l_local_emited(i), local_emited),
                       id_institution         = nvl(l_id_institution(i), id_institution),
                       flg_sent_by            = nvl(l_flg_sent_by(i), flg_sent_by),
                       flg_received           = nvl(l_flg_received(i), flg_received),
                       id_doc_original        = nvl(l_id_doc_original(i), id_doc_original),
                       desc_doc_original      = nvl(l_desc_doc_original(i), desc_doc_original),
                       id_professional        = nvl(l_id_professional(i), id_professional),
                       title                  = nvl(l_title(i), title),
                       dt_inserted            = nvl(l_dt_inserted(i), dt_inserted),
                       dt_updated             = nvl(l_dt_updated(i), dt_updated),
                       id_professional_upd    = nvl(l_id_professional_upd(i), id_professional_upd),
                       id_prof_perf_by        = nvl(l_id_prof_perf_by(i), id_prof_perf_by),
                       desc_perf_by           = nvl(l_desc_perf_by(i), desc_perf_by),
                       id_grupo               = nvl(l_id_grupo(i), id_grupo),
                       create_user            = nvl(l_create_user(i), create_user),
                       create_time            = nvl(l_create_time(i), create_time),
                       create_institution     = nvl(l_create_institution(i), create_institution),
                       update_user            = nvl(l_update_user(i), update_user),
                       update_time            = nvl(l_update_time(i), update_time),
                       update_institution     = nvl(l_update_institution(i), update_institution),
                       organ_shipper          = nvl(l_organ_shipper(i), organ_shipper),
                       id_language            = nvl(l_id_language(i), id_language),
                       id_specialty           = nvl(l_id_specialty(i), id_specialty),
                       author                 = nvl(l_author(i), author),
                       dt_last_identification = nvl(l_dt_last_identification(i), dt_last_identification),
