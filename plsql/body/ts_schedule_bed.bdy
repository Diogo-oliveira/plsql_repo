                    l_id       PLS_INTEGER;
                    l_err_name VARCHAR2(32767) DEFAULT 'NO-PARENT-KEY-FOUND';
                BEGIN
                    get_constraint_info(l_owner, l_name);
                    IF SQLCODE = -2292 -- Child record found
                    THEN
                        l_err_name := 'CHILD-RECORD-FOUND';
                    END IF;
                    pk_alert_exceptions.register_error(error_name_in       => l_err_name,
                                                       err_instance_id_out => l_id,
                                                       name1_in            => 'OWNER',
                                                       value1_in           => l_owner,
                                                       name2_in            => 'CONSTRAINT_NAME',
                                                       value2_in           => l_name,
                                                       name3_in            => 'TABLE_NAME',
                                                       value3_in           => 'SCHEDULE_BED');
                    pk_alert_exceptions.raise_error_instance(err_instance_id_in => l_id);
                END;
            END IF;
    END del_by_col;

    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(schedule_bed_inout IN OUT schedule_bed%ROWTYPE) IS
    BEGIN
        schedule_bed_inout.id_schedule        := NULL;
        schedule_bed_inout.id_bed             := NULL;
        schedule_bed_inout.id_waiting_list    := NULL;
        schedule_bed_inout.flg_temporary      := 'Y';
        schedule_bed_inout.flg_conflict       := 'N';
        schedule_bed_inout.create_user        := NULL;
        schedule_bed_inout.create_institution := NULL;
        schedule_bed_inout.update_user        := NULL;
        schedule_bed_inout.update_institution := NULL;
        schedule_bed_inout.create_time        := NULL;
        schedule_bed_inout.update_time        := NULL;
    END initrec;

    FUNCTION initrec RETURN schedule_bed%ROWTYPE IS
        l_schedule_bed schedule_bed%ROWTYPE;
    BEGIN
        l_schedule_bed.flg_temporary := 'Y';
        l_schedule_bed.flg_conflict  := 'N';
        RETURN l_schedule_bed;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN schedule_bed_tc IS
        data schedule_bed_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM schedule_bed
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN schedule_bed_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data schedule_bed_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

BEGIN
    NULL;
END ts_schedule_bed;
