                                                    name3_in      => 'TABLE_NAME',
                                                    value3_in     => 'SCHEDULE_BED');
                END;
            END IF;
        WHEN e_integ_constraint_failure
             OR e_no_parent_key
             OR e_child_record_found THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                DECLARE
