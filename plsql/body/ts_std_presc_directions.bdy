               DIRECTIONS_DESCRIPTION = NVL (l_DIRECTIONS_DESCRIPTION(i), DIRECTIONS_DESCRIPTION)
             WHERE
                ID_PRESC_DIRECTIONS = l_ID_PRESC_DIRECTIONS(i)
         ;
