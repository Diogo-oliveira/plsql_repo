         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE STD_PRESC_DIRECTIONS SET
               FLG_TAKE_TYPE = NVL (l_FLG_TAKE_TYPE(i), FLG_TAKE_TYPE),
               ID_ROUTE = NVL (l_ID_ROUTE(i), ID_ROUTE),
