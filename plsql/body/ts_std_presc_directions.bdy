   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_PRESC_DIRECTIONS(i) := col_in(i).ID_PRESC_DIRECTIONS;
         l_FLG_TAKE_TYPE(i) := col_in(i).FLG_TAKE_TYPE;
