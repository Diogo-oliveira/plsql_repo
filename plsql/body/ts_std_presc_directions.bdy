PROCEDURE upd (
      id_presc_directions_in IN STD_PRESC_DIRECTIONS.ID_PRESC_DIRECTIONS%TYPE,
      flg_take_type_in IN STD_PRESC_DIRECTIONS.FLG_TAKE_TYPE%TYPE DEFAULT NULL,
