               DIRECTIONS_DESCRIPTION = l_DIRECTIONS_DESCRIPTION(i)
             WHERE
                ID_PRESC_DIRECTIONS = l_ID_PRESC_DIRECTIONS(i)
         ;
      END IF;
   END upd;


