   PROCEDURE upd (
      col_in IN NOC_OUTCOME_tc,
      ignore_if_null_in IN BOOLEAN := TRUE
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
      l_ID_NOC_OUTCOME ID_NOC_OUTCOME_cc;
      l_ID_TERMINOLOGY_VERSION ID_TERMINOLOGY_VERSION_cc;
      l_OUTCOME_CODE OUTCOME_CODE_cc;
      l_REFERENCES REFERENCES_cc;
      l_ID_INST_OWNER ID_INST_OWNER_cc;
      l_ID_CONCEPT_VERSION ID_CONCEPT_VERSION_cc;
      l_ID_CONCEPT_TERM ID_CONCEPT_TERM_cc;
      l_ID_NOC_CLASS ID_NOC_CLASS_cc;
      l_ID_NOC_SCALE ID_NOC_SCALE_cc;
      l_CREATE_USER CREATE_USER_cc;
      l_CREATE_TIME CREATE_TIME_cc;
      l_CREATE_INSTITUTION CREATE_INSTITUTION_cc;
      l_UPDATE_USER UPDATE_USER_cc;
      l_UPDATE_TIME UPDATE_TIME_cc;
      l_UPDATE_INSTITUTION UPDATE_INSTITUTION_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_NOC_OUTCOME(i) := col_in(i).ID_NOC_OUTCOME;
         l_ID_TERMINOLOGY_VERSION(i) := col_in(i).ID_TERMINOLOGY_VERSION;
         l_OUTCOME_CODE(i) := col_in(i).OUTCOME_CODE;
         l_REFERENCES(i) := col_in(i).REFERENCES;
         l_ID_INST_OWNER(i) := col_in(i).ID_INST_OWNER;
         l_ID_CONCEPT_VERSION(i) := col_in(i).ID_CONCEPT_VERSION;
         l_ID_CONCEPT_TERM(i) := col_in(i).ID_CONCEPT_TERM;
         l_ID_NOC_CLASS(i) := col_in(i).ID_NOC_CLASS;
         l_ID_NOC_SCALE(i) := col_in(i).ID_NOC_SCALE;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE NOC_OUTCOME SET
               ID_TERMINOLOGY_VERSION = NVL (l_ID_TERMINOLOGY_VERSION(i), ID_TERMINOLOGY_VERSION),
               OUTCOME_CODE = NVL (l_OUTCOME_CODE(i), OUTCOME_CODE),
               REFERENCES = NVL (l_REFERENCES(i), REFERENCES),
               ID_INST_OWNER = NVL (l_ID_INST_OWNER(i), ID_INST_OWNER),
               ID_CONCEPT_VERSION = NVL (l_ID_CONCEPT_VERSION(i), ID_CONCEPT_VERSION),
               ID_CONCEPT_TERM = NVL (l_ID_CONCEPT_TERM(i), ID_CONCEPT_TERM),
               ID_NOC_CLASS = NVL (l_ID_NOC_CLASS(i), ID_NOC_CLASS),
               ID_NOC_SCALE = NVL (l_ID_NOC_SCALE(i), ID_NOC_SCALE),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
