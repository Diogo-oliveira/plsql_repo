        pat_necessity_inout.id_necessity     := NULL;
        pat_necessity_inout.flg_status       := NULL;
        pat_necessity_inout.id_institution   := NULL;
        pat_necessity_inout.id_episode       := NULL;
    END initrec;

    FUNCTION initrec RETURN pat_necessity%ROWTYPE IS
        l_pat_necessity pat_necessity%ROWTYPE;
    BEGIN
        RETURN l_pat_necessity;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN pat_necessity_tc IS
        data pat_necessity_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM pat_necessity
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN pat_necessity_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data pat_necessity_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

BEGIN
    NULL;
END ts_pat_necessity;
