                 ' where ' || nvl(where_in, '(1=1)') || ' RETURNING ROWID BULK COLLECT INTO :l_rows_out';
    
        EXECUTE IMMEDIATE 'BEGIN ' || l_sql || '; END;'
            USING IN id_episode_in, id_professional_in, flg_type_in, barcode_in, num_days_expire_in, flg_time_in, flg_status_in, id_prof_cancel_in, notes_cancel_in, id_episode_origin_in, id_episode_destination_in, id_protocols_in, id_prev_episode_in, dt_drug_prescription_tstz_in, dt_begin_tstz_in, dt_cancel_tstz_in, id_patient_in, create_user_in, create_time_in, create_institution_in, update_user_in, update_time_in, update_institution_in, id_parent_in, OUT l_rows_out;
    
        IF (rows_out IS NULL)
