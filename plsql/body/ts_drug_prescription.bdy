    PROCEDURE upd
    (
        id_drug_prescription_in       IN drug_prescription.id_drug_prescription%TYPE,
        id_episode_in                 IN drug_prescription.id_episode%TYPE DEFAULT NULL,
        id_episode_nin                IN BOOLEAN := TRUE,
