    FUNCTION next_key(sequence_in IN VARCHAR2 := NULL) RETURN epis_encounter.id_epis_encounter%TYPE
    
     IS
        retval epis_encounter.id_epis_encounter%TYPE;
    
    BEGIN
        IF sequence_in IS NULL
        THEN
            SELECT seq_epis_encounter.NEXTVAL
              INTO retval
              FROM dual;
        ELSE
            EXECUTE IMMEDIATE 'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
                INTO retval;
        END IF;
        RETURN retval;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'SEQUENCE-GENERATION-FAILURE',
                                            name1_in      => 'SEQUENCE',
                                            value1_in     => nvl(sequence_in, 'seq_EPIS_ENCOUNTER'));
    END next_key;

