    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(vaccine_det_inout IN OUT vaccine_det%ROWTYPE) IS
    BEGIN
        vaccine_det_inout.id_vaccine_det := NULL;
        vaccine_det_inout.medid          := NULL;
        vaccine_det_inout.flg_ci         := NULL;
        vaccine_det_inout.flg_type       := NULL;
        vaccine_det_inout.id_patient     := NULL;
        vaccine_det_inout.id_episode     := NULL;
    END initrec;

    FUNCTION initrec RETURN vaccine_det%ROWTYPE IS
        l_vaccine_det vaccine_det%ROWTYPE;
    BEGIN
        RETURN l_vaccine_det;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN vaccine_det_tc IS
        data vaccine_det_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM vaccine_det
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN vaccine_det_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data vaccine_det_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

