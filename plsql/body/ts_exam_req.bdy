    FUNCTION next_key(sequence_in IN VARCHAR2 := NULL) RETURN exam_req.id_exam_req%TYPE
    
     IS
        retval exam_req.id_exam_req%TYPE;
    
    BEGIN
        IF sequence_in IS NULL
        THEN
            SELECT seq_exam_req.nextval
              INTO retval
              FROM dual;
        ELSE
            EXECUTE IMMEDIATE 'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
                INTO retval;
        END IF;
        RETURN retval;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'SEQUENCE-GENERATION-FAILURE',
                                            name1_in      => 'SEQUENCE',
                                            value1_in     => nvl(sequence_in, 'seq_EXAM_REQ'));
    END next_key;

