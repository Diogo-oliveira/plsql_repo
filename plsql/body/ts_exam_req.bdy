    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN exam_req_tc IS
        data exam_req_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM exam_req
         WHERE ROWID IN (SELECT /*+opt_estimate(table,t,scale_rows=0.0000001))*/
                          *
                           FROM TABLE(rows_in) t);
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN exam_req_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data exam_req_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

