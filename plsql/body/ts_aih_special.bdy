    --Update with collection (with rows_out)
    PROCEDURE upd
    (
        col_in            IN aih_special_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_aih_special       id_aih_special_cc;
        l_id_episode           id_episode_cc;
        l_id_patient           id_patient_cc;
        l_id_professional      id_professional_cc;
        l_id_institution       id_institution_cc;
        l_flg_status           flg_status_cc;
        l_dt_order_tstz        dt_order_tstz_cc;
        l_dt_cancel_tstz       dt_cancel_tstz_cc;
        l_id_prof_cancel       id_prof_cancel_cc;
        l_notes_cancel         notes_cancel_cc;
        l_id_solic_type        id_solic_type_cc;
        l_id_inst_exec         id_inst_exec_cc;
        l_inst_exec_oth        inst_exec_oth_cc;
        l_inst_cnes            inst_cnes_cc;
        l_resp_name            resp_name_cc;
        l_resp_phone           resp_phone_cc;
        l_id_proc_diag_old     id_proc_diag_old_cc;
        l_id_proc_io_diag_old  id_proc_io_diag_old_cc;
        l_id_proc_alert_old    id_proc_alert_old_cc;
        l_id_proc_io_adiag_old id_proc_io_adiag_old_cc;
        l_id_princ_diag        id_princ_diag_cc;
        l_id_princ_io_diag     id_princ_io_diag_cc;
        l_id_princ_alert       id_princ_alert_cc;
        l_id_princ_io_adiag    id_princ_io_adiag_cc;
        l_id_proc_diag         id_proc_diag_cc;
        l_id_proc_io_diag      id_proc_io_diag_cc;
        l_id_proc_alert        id_proc_alert_cc;
        l_id_proc_io_adiag     id_proc_io_adiag_cc;
        l_id_proc_diag_chg     id_proc_diag_chg_cc;
        l_id_proc_io_diag_chg  id_proc_io_diag_chg_cc;
        l_id_proc_alert_chg    id_proc_alert_chg_cc;
        l_id_proc_io_adiag_chg id_proc_io_adiag_chg_cc;
        l_id_sec_diag          id_sec_diag_cc;
        l_id_sec_io_diag       id_sec_io_diag_cc;
        l_id_sec_alert         id_sec_alert_cc;
        l_id_sec_io_adiag      id_sec_io_adiag_cc;
        l_id_uti               id_uti_cc;
        l_reason               reason_cc;
        l_create_user          create_user_cc;
        l_create_time          create_time_cc;
        l_create_institution   create_institution_cc;
        l_update_user          update_user_cc;
        l_update_time          update_time_cc;
        l_update_institution   update_institution_cc;
        l_id_prof_last_update  id_prof_last_update_cc;
        l_dt_last_update_tstz  dt_last_update_tstz_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_aih_special(i) := col_in(i).id_aih_special;
            l_id_episode(i) := col_in(i).id_episode;
            l_id_patient(i) := col_in(i).id_patient;
            l_id_professional(i) := col_in(i).id_professional;
            l_id_institution(i) := col_in(i).id_institution;
            l_flg_status(i) := col_in(i).flg_status;
            l_dt_order_tstz(i) := col_in(i).dt_order_tstz;
            l_dt_cancel_tstz(i) := col_in(i).dt_cancel_tstz;
            l_id_prof_cancel(i) := col_in(i).id_prof_cancel;
            l_notes_cancel(i) := col_in(i).notes_cancel;
            l_id_solic_type(i) := col_in(i).id_solic_type;
            l_id_inst_exec(i) := col_in(i).id_inst_exec;
            l_inst_exec_oth(i) := col_in(i).inst_exec_oth;
            l_inst_cnes(i) := col_in(i).inst_cnes;
            l_resp_name(i) := col_in(i).resp_name;
            l_resp_phone(i) := col_in(i).resp_phone;
            l_id_proc_diag_old(i) := col_in(i).id_proc_diag_old;
            l_id_proc_io_diag_old(i) := col_in(i).id_proc_io_diag_old;
            l_id_proc_alert_old(i) := col_in(i).id_proc_alert_old;
            l_id_proc_io_adiag_old(i) := col_in(i).id_proc_io_adiag_old;
            l_id_princ_diag(i) := col_in(i).id_princ_diag;
            l_id_princ_io_diag(i) := col_in(i).id_princ_io_diag;
            l_id_princ_alert(i) := col_in(i).id_princ_alert;
            l_id_princ_io_adiag(i) := col_in(i).id_princ_io_adiag;
            l_id_proc_diag(i) := col_in(i).id_proc_diag;
            l_id_proc_io_diag(i) := col_in(i).id_proc_io_diag;
            l_id_proc_alert(i) := col_in(i).id_proc_alert;
            l_id_proc_io_adiag(i) := col_in(i).id_proc_io_adiag;
            l_id_proc_diag_chg(i) := col_in(i).id_proc_diag_chg;
            l_id_proc_io_diag_chg(i) := col_in(i).id_proc_io_diag_chg;
            l_id_proc_alert_chg(i) := col_in(i).id_proc_alert_chg;
            l_id_proc_io_adiag_chg(i) := col_in(i).id_proc_io_adiag_chg;
            l_id_sec_diag(i) := col_in(i).id_sec_diag;
            l_id_sec_io_diag(i) := col_in(i).id_sec_io_diag;
            l_id_sec_alert(i) := col_in(i).id_sec_alert;
            l_id_sec_io_adiag(i) := col_in(i).id_sec_io_adiag;
            l_id_uti(i) := col_in(i).id_uti;
            l_reason(i) := col_in(i).reason;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_id_prof_last_update(i) := col_in(i).id_prof_last_update;
            l_dt_last_update_tstz(i) := col_in(i).dt_last_update_tstz;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
