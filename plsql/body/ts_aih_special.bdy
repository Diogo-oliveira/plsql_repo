                                    ' where ' || nvl(where_in, '(1=1)') ||
                                    ' RETURNING ROWID BULK COLLECT INTO :l_rows_out';
        EXECUTE IMMEDIATE 'BEGIN ' || l_sql || '; END;'
            USING IN id_episode_in, id_patient_in, id_professional_in, id_institution_in, flg_status_in, dt_order_tstz_in, dt_cancel_tstz_in, id_prof_cancel_in, notes_cancel_in, id_solic_type_in, id_inst_exec_in, inst_exec_oth_in, inst_cnes_in, resp_name_in, resp_phone_in, id_proc_diag_old_in, id_proc_io_diag_old_in, id_proc_alert_old_in, id_proc_io_adiag_old_in, id_princ_diag_in, id_princ_io_diag_in, id_princ_alert_in, id_princ_io_adiag_in, id_proc_diag_in, id_proc_io_diag_in, id_proc_alert_in, id_proc_io_adiag_in, id_proc_diag_chg_in, id_proc_io_diag_chg_in, id_proc_alert_chg_in, id_proc_io_adiag_chg_in, id_sec_diag_in, id_sec_io_diag_in, id_sec_alert_in, id_sec_io_adiag_in, id_uti_in, reason_in, create_user_in, create_time_in, create_institution_in, update_user_in, update_time_in, update_institution_in, id_prof_last_update_in, dt_last_update_tstz_in, OUT l_rows_out;
