    FUNCTION next_key(sequence_in IN VARCHAR2 := NULL) RETURN analysis_result_par_hist.id_analysis_result_par_hist%TYPE
    
     IS
        retval analysis_result_par_hist.id_analysis_result_par_hist%TYPE;
    
    BEGIN
        IF sequence_in IS NULL
        THEN
            SELECT seq_analysis_result_par_hist.nextval
              INTO retval
              FROM dual;
        ELSE
            EXECUTE IMMEDIATE 'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
                INTO retval;
        END IF;
        RETURN retval;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'SEQUENCE-GENERATION-FAILURE',
                                            name1_in      => 'SEQUENCE',
                                            value1_in     => nvl(sequence_in, 'seq_ANALYSIS_RESULT_PAR_HIST'));
    END next_key;

