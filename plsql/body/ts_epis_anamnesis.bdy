   FUNCTION next_key (sequence_in IN VARCHAR2 := NULL) RETURN EPIS_ANAMNESIS.ID_EPIS_ANAMNESIS%TYPE

   IS
     retval EPIS_ANAMNESIS.ID_EPIS_ANAMNESIS%TYPE;

   BEGIN
      IF sequence_in IS NULL
      THEN
         SELECT seq_EPIS_ANAMNESIS.NEXTVAL INTO retval FROM dual;
      ELSE
         EXECUTE IMMEDIATE
            'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
            INTO retval;
      END IF;
      RETURN retval;
   EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'SEQUENCE-GENERATION-FAILURE'
           ,name1_in => 'SEQUENCE'
           ,value1_in => NVL (sequence_in, 'seq_EPIS_ANAMNESIS')
           );
   END next_key;

