   FUNCTION next_key (sequence_in IN VARCHAR2 := NULL) RETURN EPIS_PROG_NOTES.ID_EPIS_PROG_NOTES%TYPE

   IS
     retval EPIS_PROG_NOTES.ID_EPIS_PROG_NOTES%TYPE;

   BEGIN
      IF sequence_in IS NULL
      THEN
         SELECT seq_EPIS_PROG_NOTES.NEXTVAL INTO retval FROM dual;
      ELSE
         EXECUTE IMMEDIATE
            'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
            INTO retval;
      END IF;
      RETURN retval;
   EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'SEQUENCE-GENERATION-FAILURE'
