   FUNCTION next_key (sequence_in IN VARCHAR2 := NULL) RETURN SUPPLY_REQUEST.ID_SUPPLY_REQUEST%TYPE

   IS
     retval SUPPLY_REQUEST.ID_SUPPLY_REQUEST%TYPE;

   BEGIN
      IF sequence_in IS NULL
      THEN
         SELECT seq_SUPPLY_REQUEST.NEXTVAL INTO retval FROM dual;
      ELSE
         EXECUTE IMMEDIATE
            'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
            INTO retval;
      END IF;
      RETURN retval;
   EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'SEQUENCE-GENERATION-FAILURE'
           ,name1_in => 'SEQUENCE'
           ,value1_in => NVL (sequence_in, 'seq_SUPPLY_REQUEST')
           );
   END next_key;

