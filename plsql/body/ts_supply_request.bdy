                  ,NAME_IN => 'DT_REQUEST _' || indx
                  ,value_in => rows_in(indx).DT_REQUEST
                  ,validate_in => FALSE
                );

                pk_alert_exceptions.add_context (
                   err_instance_id_in => l_err_instance_id
                  ,NAME_IN => 'FLG_STATUS _' || indx
                  ,value_in => rows_in(indx).FLG_STATUS
                  ,validate_in => FALSE
                );

                pk_alert_exceptions.add_context (
                   err_instance_id_in => l_err_instance_id
                  ,NAME_IN => 'FLG_REASON _' || indx
                  ,value_in => rows_in(indx).FLG_REASON
                  ,validate_in => FALSE
                );

                pk_alert_exceptions.add_context (
                   err_instance_id_in => l_err_instance_id
                  ,NAME_IN => 'FLG_PROF_PREP _' || indx
                  ,value_in => rows_in(indx).FLG_PROF_PREP
