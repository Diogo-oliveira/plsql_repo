   )
   IS
   BEGIN
      IF rows_in.COUNT = 0
      THEN
         NULL;
      ELSE
         FORALL indx IN rows_in.FIRST .. rows_in.LAST
            SAVE EXCEPTIONS
            INSERT INTO REHAB_EPIS_ENCOUNTER VALUES rows_in (indx) RETURNING ROWID BULK COLLECT INTO rows_out;
      END IF;
