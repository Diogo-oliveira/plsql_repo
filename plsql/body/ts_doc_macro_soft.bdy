                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'TABLE_NAME'
                ,value_in => 'DOC_MACRO_SOFT'
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'ID_DOC_MACRO'
                ,value_in => id_doc_macro_in
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
