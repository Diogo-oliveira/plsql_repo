PROCEDURE upd (
      id_adm_preparation_in IN ADM_PREPARATION.ID_ADM_PREPARATION%TYPE,
      flg_available_in IN ADM_PREPARATION.FLG_AVAILABLE%TYPE DEFAULT NULL,
      FLG_AVAILABLE_nin IN BOOLEAN := TRUE,
