   FUNCTION get_data_rowid(
        rows_in IN TABLE_VARCHAR
        ) RETURN ADM_PREPARATION_tc
   IS
        data ADM_PREPARATION_tc;
   BEGIN
        select * bulk collect into data from ADM_PREPARATION where rowid in (select /*+opt_estimate(table,t,scale_rows=0.0000001))*/ * from table(rows_in) t);
        return data;
        EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'get_data_rowid'
           );
   END get_data_rowid;


   FUNCTION get_data_rowid_pat(
        rows_in IN TABLE_VARCHAR
        ) RETURN ADM_PREPARATION_tc
   is
        PRAGMA AUTONOMOUS_TRANSACTION;
        data ADM_PREPARATION_tc;
   BEGIN
        data := get_data_rowid(rows_in);
        commit;
        return data;
        EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'get_data_rowid'
           );
        rollback;
    END get_data_rowid_pat;

BEGIN
   NULL;
END TS_ADM_PREPARATION;
