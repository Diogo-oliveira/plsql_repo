PROCEDURE upd (
      id_cdr_inst_param_in IN CDR_INST_PARAM.ID_CDR_INST_PARAM%TYPE,
      id_cdr_instance_in IN CDR_INST_PARAM.ID_CDR_INSTANCE%TYPE DEFAULT NULL,
      ID_CDR_INSTANCE_nin IN BOOLEAN := TRUE,
      id_cdr_parameter_in IN CDR_INST_PARAM.ID_CDR_PARAMETER%TYPE DEFAULT NULL,
      ID_CDR_PARAMETER_nin IN BOOLEAN := TRUE,
      id_element_in IN CDR_INST_PARAM.ID_ELEMENT%TYPE DEFAULT NULL,
      ID_ELEMENT_nin IN BOOLEAN := TRUE,
      validity_in IN CDR_INST_PARAM.VALIDITY%TYPE DEFAULT NULL,
      VALIDITY_nin IN BOOLEAN := TRUE,
      id_validity_umea_in IN CDR_INST_PARAM.ID_VALIDITY_UMEA%TYPE DEFAULT NULL,
      ID_VALIDITY_UMEA_nin IN BOOLEAN := TRUE,
      val_min_in IN CDR_INST_PARAM.VAL_MIN%TYPE DEFAULT NULL,
      VAL_MIN_nin IN BOOLEAN := TRUE,
      val_max_in IN CDR_INST_PARAM.VAL_MAX%TYPE DEFAULT NULL,
      VAL_MAX_nin IN BOOLEAN := TRUE,
      id_domain_umea_in IN CDR_INST_PARAM.ID_DOMAIN_UMEA%TYPE DEFAULT NULL,
      ID_DOMAIN_UMEA_nin IN BOOLEAN := TRUE,
      route_id_in IN CDR_INST_PARAM.ROUTE_ID%TYPE DEFAULT NULL,
      ROUTE_ID_nin IN BOOLEAN := TRUE,
      create_user_in IN CDR_INST_PARAM.CREATE_USER%TYPE DEFAULT NULL,
      CREATE_USER_nin IN BOOLEAN := TRUE,
      create_time_in IN CDR_INST_PARAM.CREATE_TIME%TYPE DEFAULT NULL,
      CREATE_TIME_nin IN BOOLEAN := TRUE,
      create_institution_in IN CDR_INST_PARAM.CREATE_INSTITUTION%TYPE DEFAULT NULL,
      CREATE_INSTITUTION_nin IN BOOLEAN := TRUE,
      update_user_in IN CDR_INST_PARAM.UPDATE_USER%TYPE DEFAULT NULL,
      UPDATE_USER_nin IN BOOLEAN := TRUE,
      update_time_in IN CDR_INST_PARAM.UPDATE_TIME%TYPE DEFAULT NULL,
      UPDATE_TIME_nin IN BOOLEAN := TRUE,
      update_institution_in IN CDR_INST_PARAM.UPDATE_INSTITUTION%TYPE DEFAULT NULL,
      UPDATE_INSTITUTION_nin IN BOOLEAN := TRUE,
     handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
   l_rows_out TABLE_VARCHAR;
   l_ID_CDR_INSTANCE_n NUMBER(1);
   l_ID_CDR_PARAMETER_n NUMBER(1);
   l_ID_ELEMENT_n NUMBER(1);
   l_VALIDITY_n NUMBER(1);
   l_ID_VALIDITY_UMEA_n NUMBER(1);
   l_VAL_MIN_n NUMBER(1);
   l_VAL_MAX_n NUMBER(1);
   l_ID_DOMAIN_UMEA_n NUMBER(1);
   l_ROUTE_ID_n NUMBER(1);
   l_CREATE_USER_n NUMBER(1);
   l_CREATE_TIME_n NUMBER(1);
   l_CREATE_INSTITUTION_n NUMBER(1);
   l_UPDATE_USER_n NUMBER(1);
   l_UPDATE_TIME_n NUMBER(1);
   l_UPDATE_INSTITUTION_n NUMBER(1);
   BEGIN

   l_ID_CDR_INSTANCE_n := sys.diutil.bool_to_int(ID_CDR_INSTANCE_nin);
   l_ID_CDR_PARAMETER_n := sys.diutil.bool_to_int(ID_CDR_PARAMETER_nin);
   l_ID_ELEMENT_n := sys.diutil.bool_to_int(ID_ELEMENT_nin);
   l_VALIDITY_n := sys.diutil.bool_to_int(VALIDITY_nin);
   l_ID_VALIDITY_UMEA_n := sys.diutil.bool_to_int(ID_VALIDITY_UMEA_nin);
   l_VAL_MIN_n := sys.diutil.bool_to_int(VAL_MIN_nin);
   l_VAL_MAX_n := sys.diutil.bool_to_int(VAL_MAX_nin);
   l_ID_DOMAIN_UMEA_n := sys.diutil.bool_to_int(ID_DOMAIN_UMEA_nin);
   l_ROUTE_ID_n := sys.diutil.bool_to_int(ROUTE_ID_nin);
   l_CREATE_USER_n := sys.diutil.bool_to_int(CREATE_USER_nin);
   l_CREATE_TIME_n := sys.diutil.bool_to_int(CREATE_TIME_nin);
   l_CREATE_INSTITUTION_n := sys.diutil.bool_to_int(CREATE_INSTITUTION_nin);
   l_UPDATE_USER_n := sys.diutil.bool_to_int(UPDATE_USER_nin);
   l_UPDATE_TIME_n := sys.diutil.bool_to_int(UPDATE_TIME_nin);
   l_UPDATE_INSTITUTION_n := sys.diutil.bool_to_int(UPDATE_INSTITUTION_nin);


         UPDATE CDR_INST_PARAM SET
     ID_CDR_INSTANCE = decode (l_ID_CDR_INSTANCE_n,0,id_cdr_instance_in, NVL (id_cdr_instance_in, ID_CDR_INSTANCE)),
     ID_CDR_PARAMETER = decode (l_ID_CDR_PARAMETER_n,0,id_cdr_parameter_in, NVL (id_cdr_parameter_in, ID_CDR_PARAMETER)),
     ID_ELEMENT = decode (l_ID_ELEMENT_n,0,id_element_in, NVL (id_element_in, ID_ELEMENT)),
     VALIDITY = decode (l_VALIDITY_n,0,validity_in, NVL (validity_in, VALIDITY)),
     ID_VALIDITY_UMEA = decode (l_ID_VALIDITY_UMEA_n,0,id_validity_umea_in, NVL (id_validity_umea_in, ID_VALIDITY_UMEA)),
     VAL_MIN = decode (l_VAL_MIN_n,0,val_min_in, NVL (val_min_in, VAL_MIN)),
     VAL_MAX = decode (l_VAL_MAX_n,0,val_max_in, NVL (val_max_in, VAL_MAX)),
     ID_DOMAIN_UMEA = decode (l_ID_DOMAIN_UMEA_n,0,id_domain_umea_in, NVL (id_domain_umea_in, ID_DOMAIN_UMEA)),
     ROUTE_ID = decode (l_ROUTE_ID_n,0,route_id_in, NVL (route_id_in, ROUTE_ID)),
     CREATE_USER = decode (l_CREATE_USER_n,0,create_user_in, NVL (create_user_in, CREATE_USER)),
     CREATE_TIME = decode (l_CREATE_TIME_n,0,create_time_in, NVL (create_time_in, CREATE_TIME)),
     CREATE_INSTITUTION = decode (l_CREATE_INSTITUTION_n,0,create_institution_in, NVL (create_institution_in, CREATE_INSTITUTION)),
     UPDATE_USER = decode (l_UPDATE_USER_n,0,update_user_in, NVL (update_user_in, UPDATE_USER)),
     UPDATE_TIME = decode (l_UPDATE_TIME_n,0,update_time_in, NVL (update_time_in, UPDATE_TIME)),
     UPDATE_INSTITUTION = decode (l_UPDATE_INSTITUTION_n,0,update_institution_in, NVL (update_institution_in, UPDATE_INSTITUTION))
          WHERE
             ID_CDR_INST_PARAM = id_cdr_inst_param_in
         RETURNING ROWID BULK COLLECT INTO l_rows_out;


if(rows_out is null)
then
rows_out := table_varchar();
end if;

rows_out :=  rows_out MULTISET UNION DISTINCT l_rows_out;

   EXCEPTION
      WHEN DUP_VAL_ON_INDEX
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
           l_err_instance_id PLS_INTEGER;
        BEGIN
           get_constraint_info (l_owner, l_name);
           IF FALSE THEN NULL; -- Placeholder in case no unique indexes
           ELSE
              pk_alert_exceptions.raise_error (
                    error_name_in => 'DUPLICATE-VALUE'
                    ,name1_in => 'OWNER'
                    ,value1_in => l_owner
                    ,name2_in => 'CONSTRAINT_NAME'
                    ,value2_in => l_name
                    ,name3_in => 'TABLE_NAME'
                    ,value3_in => 'CDR_INST_PARAM');
           END IF;
        END;
        END IF;
      WHEN e_check_constraint_failure
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
        BEGIN
           get_constraint_info (l_owner, l_name);
           pk_alert_exceptions.raise_error (
              error_name_in => 'CHECK-CONSTRAINT-FAILURE'
             ,name1_in => 'OWNER'
             ,value1_in => l_owner
             ,name2_in => 'CONSTRAINT_NAME'
             ,value2_in => l_name
             ,name3_in => 'TABLE_NAME'
             ,value3_in => 'CDR_INST_PARAM');
        END;
        END IF;
      WHEN e_integ_constraint_failure OR e_no_parent_key OR e_child_record_found
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
           l_id PLS_INTEGER;
           l_err_name VARCHAR2(32767) DEFAULT 'NO-PARENT-KEY-FOUND';
        BEGIN
           get_constraint_info (l_owner, l_name);
           IF SQLCODE = -2292 -- Child record found
           THEN
              l_err_name := 'CHILD-RECORD-FOUND' ;
           END IF;
           pk_alert_exceptions.register_error (
              error_name_in => l_err_name
             ,err_instance_id_out => l_id
             ,name1_in => 'OWNER', value1_in => l_owner
             ,name2_in => 'CONSTRAINT_NAME', value2_in => l_name
             ,name3_in => 'TABLE_NAME', value3_in => 'CDR_INST_PARAM');
           IF l_name = 'CDRIP_CDRI_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_INSTANCE'
               , value_in => id_cdr_instance_in);
           END IF;
           IF l_name = 'CDRIP_CDRP_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_PARAMETER'
               , value_in => id_cdr_parameter_in);
           END IF;
           IF l_name = 'CDRIP_DOMAIN_UNITM_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_DOMAIN_UMEA'
               , value_in => id_domain_umea_in);
           END IF;
           IF l_name = 'CDRIP_VAL_UNITM_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_VALIDITY_UMEA'
               , value_in => id_validity_umea_in);
           END IF;
           pk_alert_exceptions.raise_error_instance (err_instance_id_in => l_id);
        END;
        END IF;
      WHEN e_null_column_value
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           v_errm VARCHAR2(2000) := DBMS_UTILITY.FORMAT_ERROR_STACK;
           dot1loc INTEGER;
           dot2loc INTEGER;
           parenloc INTEGER;
           c_owner ALL_CONSTRAINTS.OWNER%TYPE;
           c_tabname ALL_TABLES.TABLE_NAME%TYPE;
           c_colname ALL_TAB_COLUMNS.COLUMN_NAME%TYPE;
        BEGIN
           dot1loc := INSTR (v_errm, '.', 1, 1);
           dot2loc := INSTR (v_errm, '.', 1, 2);
           parenloc := INSTR (v_errm, '(');
           c_owner :=SUBSTR (v_errm, parenloc+1, dot1loc-parenloc-1);
           c_tabname := SUBSTR (v_errm, dot1loc+1, dot2loc-dot1loc-1);
           c_colname := SUBSTR (v_errm, dot2loc+1, INSTR (v_errm,')')-dot2loc-1);

           pk_alert_exceptions.raise_error (
                error_name_in => 'COLUMN-CANNOT-BE-NULL'
               ,name1_in => 'OWNER'
               ,value1_in => c_owner
               ,name2_in => 'TABLE_NAME'
               ,value2_in => c_tabname
               ,name3_in => 'COLUMN_NAME'
               ,value3_in => c_colname);
        END;
        END IF;
   END upd;


   PROCEDURE upd (
