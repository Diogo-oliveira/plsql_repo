      )
   IS
      l_ID_ICF ID_ICF_cc;
      l_ID_INTERV_EVALUATION ID_INTERV_EVALUATION_cc;
      l_NOTES NOTES_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_ICF(i) := col_in(i).ID_ICF;
         l_ID_INTERV_EVALUATION(i) := col_in(i).ID_INTERV_EVALUATION;
         l_NOTES(i) := col_in(i).NOTES;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE INTERV_EVALUATION_ICF SET
               NOTES = NVL (l_NOTES(i), NOTES)
             WHERE
                ID_ICF = l_ID_ICF(i) AND
                ID_INTERV_EVALUATION = l_ID_INTERV_EVALUATION(i)
         ;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE INTERV_EVALUATION_ICF SET
               NOTES = l_NOTES(i)
             WHERE
                ID_ICF = l_ID_ICF(i) AND
                ID_INTERV_EVALUATION = l_ID_INTERV_EVALUATION(i)
         ;
      END IF;
   END upd;


