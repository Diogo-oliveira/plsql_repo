    PROCEDURE upd
    (
        col_in            IN pat_vacc_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_vacc            id_vacc_cc;
        l_flg_available      flg_available_cc;
        l_id_patient         id_patient_cc;
        l_dt_pat_vacc        dt_pat_vacc_cc;
        l_id_professional    id_professional_cc;
        l_dt_last_change     dt_last_change_cc;
        l_id_episode         id_episode_cc;
        l_create_user        create_user_cc;
        l_create_time        create_time_cc;
        l_create_institution create_institution_cc;
        l_update_user        update_user_cc;
        l_update_time        update_time_cc;
        l_update_institution update_institution_cc;
        l_flg_status         flg_status_cc;
        l_id_prof_status     id_prof_status_cc;
        l_dt_status          dt_status_cc;
        l_notes              notes_cc;
        l_id_reason          id_reason_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_vacc(i) := col_in(i).id_vacc;
            l_flg_available(i) := col_in(i).flg_available;
            l_id_patient(i) := col_in(i).id_patient;
            l_dt_pat_vacc(i) := col_in(i).dt_pat_vacc;
            l_id_professional(i) := col_in(i).id_professional;
            l_dt_last_change(i) := col_in(i).dt_last_change;
            l_id_episode(i) := col_in(i).id_episode;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_flg_status(i) := col_in(i).flg_status;
            l_id_prof_status(i) := col_in(i).id_prof_status;
            l_dt_status(i) := col_in(i).dt_status;
            l_notes(i) := col_in(i).notes;
            l_id_reason(i) := col_in(i).id_reason;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE pat_vacc
                   SET flg_available      = nvl(l_flg_available(i), flg_available),
                       dt_pat_vacc        = nvl(l_dt_pat_vacc(i), dt_pat_vacc),
                       id_professional    = nvl(l_id_professional(i), id_professional),
                       dt_last_change     = nvl(l_dt_last_change(i), dt_last_change),
                       id_episode         = nvl(l_id_episode(i), id_episode),
                       create_user        = nvl(l_create_user(i), create_user),
                       create_time        = nvl(l_create_time(i), create_time),
                       create_institution = nvl(l_create_institution(i), create_institution),
                       update_user        = nvl(l_update_user(i), update_user),
                       update_time        = nvl(l_update_time(i), update_time),
                       update_institution = nvl(l_update_institution(i), update_institution),
                       flg_status         = nvl(l_flg_status(i), flg_status),
                       id_prof_status     = nvl(l_id_prof_status(i), id_prof_status),
                       dt_status          = nvl(l_dt_status(i), dt_status),
                       notes              = nvl(l_notes(i), notes),
                       id_reason          = nvl(l_id_reason(i), id_reason)
                 WHERE id_vacc = l_id_vacc(i)
                   AND id_patient = l_id_patient(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE pat_vacc
                   SET flg_available      = l_flg_available(i),
                       dt_pat_vacc        = l_dt_pat_vacc(i),
                       id_professional    = l_id_professional(i),
                       dt_last_change     = l_dt_last_change(i),
                       id_episode         = l_id_episode(i),
                       create_user        = l_create_user(i),
                       create_time        = l_create_time(i),
                       create_institution = l_create_institution(i),
                       update_user        = l_update_user(i),
                       update_time        = l_update_time(i),
                       update_institution = l_update_institution(i),
                       flg_status         = l_flg_status(i),
                       id_prof_status     = l_id_prof_status(i),
                       dt_status          = l_dt_status(i),
                       notes              = l_notes(i),
                       id_reason          = l_id_reason(i)
                 WHERE id_vacc = l_id_vacc(i)
                   AND id_patient = l_id_patient(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    END upd;

