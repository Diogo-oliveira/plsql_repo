    PROCEDURE upd
    (
        col_in            IN pat_family_prof_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_pat_family_prof id_pat_family_prof_cc;
        l_id_pat_family      id_pat_family_cc;
        l_id_institution     id_institution_cc;
        l_id_professional    id_professional_cc;
        l_reason_type        reason_type_cc;
        l_reason             reason_cc;
        l_id_patient         id_patient_cc;
        l_dt_begin_tstz      dt_begin_tstz_cc;
        l_dt_end_tstz        dt_end_tstz_cc;
        l_flg_status         flg_status_cc;
        l_id_episode         id_episode_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_id_pat_family_prof(i) := col_in(i).id_pat_family_prof;
            l_id_pat_family(i) := col_in(i).id_pat_family;
            l_id_institution(i) := col_in(i).id_institution;
            l_id_professional(i) := col_in(i).id_professional;
            l_reason_type(i) := col_in(i).reason_type;
            l_reason(i) := col_in(i).reason;
            l_id_patient(i) := col_in(i).id_patient;
            l_dt_begin_tstz(i) := col_in(i).dt_begin_tstz;
            l_dt_end_tstz(i) := col_in(i).dt_end_tstz;
            l_flg_status(i) := col_in(i).flg_status;
            l_id_episode(i) := col_in(i).id_episode;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE pat_family_prof
                   SET id_pat_family   = nvl(l_id_pat_family(i), id_pat_family),
                       id_institution  = nvl(l_id_institution(i), id_institution),
                       id_professional = nvl(l_id_professional(i), id_professional),
                       reason_type     = nvl(l_reason_type(i), reason_type),
                       reason          = nvl(l_reason(i), reason),
                       id_patient      = nvl(l_id_patient(i), id_patient),
                       dt_begin_tstz   = nvl(l_dt_begin_tstz(i), dt_begin_tstz),
                       dt_end_tstz     = nvl(l_dt_end_tstz(i), dt_end_tstz),
                       flg_status      = nvl(l_flg_status(i), flg_status),
                       id_episode      = nvl(l_id_episode(i), id_episode)
                 WHERE id_pat_family_prof = l_id_pat_family_prof(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE pat_family_prof
                   SET id_pat_family   = l_id_pat_family(i),
                       id_institution  = l_id_institution(i),
                       id_professional = l_id_professional(i),
                       reason_type     = l_reason_type(i),
                       reason          = l_reason(i),
                       id_patient      = l_id_patient(i),
                       dt_begin_tstz   = l_dt_begin_tstz(i),
                       dt_end_tstz     = l_dt_end_tstz(i),
                       flg_status      = l_flg_status(i),
                       id_episode      = l_id_episode(i)
                 WHERE id_pat_family_prof = l_id_pat_family_prof(i);
        END IF;
    END upd;

