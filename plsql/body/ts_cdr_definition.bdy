   PROCEDURE upd (
      col_in IN CDR_DEFINITION_tc,
      ignore_if_null_in IN BOOLEAN := TRUE
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
      l_ID_CDR_DEFINITION ID_CDR_DEFINITION_cc;
      l_CODE_NAME CODE_NAME_cc;
      l_CODE_DESCRIPTION CODE_DESCRIPTION_cc;
      l_FLG_STATUS FLG_STATUS_cc;
      l_FLG_ORIGIN FLG_ORIGIN_cc;
      l_ID_CDR_TYPE ID_CDR_TYPE_cc;
      l_ID_INSTITUTION ID_INSTITUTION_cc;
      l_ID_PROF_CREATE ID_PROF_CREATE_cc;
      l_ID_CANCEL_INFO_DET ID_CANCEL_INFO_DET_cc;
      l_CREATE_USER CREATE_USER_cc;
      l_CREATE_TIME CREATE_TIME_cc;
      l_CREATE_INSTITUTION CREATE_INSTITUTION_cc;
      l_UPDATE_USER UPDATE_USER_cc;
      l_UPDATE_TIME UPDATE_TIME_cc;
      l_UPDATE_INSTITUTION UPDATE_INSTITUTION_cc;
      l_ID_CONTENT ID_CONTENT_cc;
      l_FLG_AVAILABLE FLG_AVAILABLE_cc;
      l_FLG_GENERIC FLG_GENERIC_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_CDR_DEFINITION(i) := col_in(i).ID_CDR_DEFINITION;
         l_CODE_NAME(i) := col_in(i).CODE_NAME;
         l_CODE_DESCRIPTION(i) := col_in(i).CODE_DESCRIPTION;
         l_FLG_STATUS(i) := col_in(i).FLG_STATUS;
         l_FLG_ORIGIN(i) := col_in(i).FLG_ORIGIN;
         l_ID_CDR_TYPE(i) := col_in(i).ID_CDR_TYPE;
         l_ID_INSTITUTION(i) := col_in(i).ID_INSTITUTION;
         l_ID_PROF_CREATE(i) := col_in(i).ID_PROF_CREATE;
         l_ID_CANCEL_INFO_DET(i) := col_in(i).ID_CANCEL_INFO_DET;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
         l_ID_CONTENT(i) := col_in(i).ID_CONTENT;
         l_FLG_AVAILABLE(i) := col_in(i).FLG_AVAILABLE;
         l_FLG_GENERIC(i) := col_in(i).FLG_GENERIC;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE CDR_DEFINITION SET
               CODE_NAME = NVL (l_CODE_NAME(i), CODE_NAME),
               CODE_DESCRIPTION = NVL (l_CODE_DESCRIPTION(i), CODE_DESCRIPTION),
               FLG_STATUS = NVL (l_FLG_STATUS(i), FLG_STATUS),
               FLG_ORIGIN = NVL (l_FLG_ORIGIN(i), FLG_ORIGIN),
               ID_CDR_TYPE = NVL (l_ID_CDR_TYPE(i), ID_CDR_TYPE),
               ID_INSTITUTION = NVL (l_ID_INSTITUTION(i), ID_INSTITUTION),
               ID_PROF_CREATE = NVL (l_ID_PROF_CREATE(i), ID_PROF_CREATE),
               ID_CANCEL_INFO_DET = NVL (l_ID_CANCEL_INFO_DET(i), ID_CANCEL_INFO_DET),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
               UPDATE_INSTITUTION = NVL (l_UPDATE_INSTITUTION(i), UPDATE_INSTITUTION),
               ID_CONTENT = NVL (l_ID_CONTENT(i), ID_CONTENT),
               FLG_AVAILABLE = NVL (l_FLG_AVAILABLE(i), FLG_AVAILABLE),
               FLG_GENERIC = NVL (l_FLG_GENERIC(i), FLG_GENERIC)
             WHERE
                ID_CDR_DEFINITION = l_ID_CDR_DEFINITION(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE CDR_DEFINITION SET
               CODE_NAME = l_CODE_NAME(i),
               CODE_DESCRIPTION = l_CODE_DESCRIPTION(i),
               FLG_STATUS = l_FLG_STATUS(i),
               FLG_ORIGIN = l_FLG_ORIGIN(i),
               ID_CDR_TYPE = l_ID_CDR_TYPE(i),
               ID_INSTITUTION = l_ID_INSTITUTION(i),
               ID_PROF_CREATE = l_ID_PROF_CREATE(i),
               ID_CANCEL_INFO_DET = l_ID_CANCEL_INFO_DET(i),
               CREATE_USER = l_CREATE_USER(i),
               CREATE_TIME = l_CREATE_TIME(i),
               CREATE_INSTITUTION = l_CREATE_INSTITUTION(i),
               UPDATE_USER = l_UPDATE_USER(i),
               UPDATE_TIME = l_UPDATE_TIME(i),
               UPDATE_INSTITUTION = l_UPDATE_INSTITUTION(i),
               ID_CONTENT = l_ID_CONTENT(i),
               FLG_AVAILABLE = l_FLG_AVAILABLE(i),
               FLG_GENERIC = l_FLG_GENERIC(i)
             WHERE
                ID_CDR_DEFINITION = l_ID_CDR_DEFINITION(i)
        returning rowid bulk collect into rows_out  ;
      END IF;
   END upd;


