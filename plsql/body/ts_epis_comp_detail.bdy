    END ins;

    PROCEDURE upd
    (
        id_epis_comp_detail_in   IN epis_comp_detail.id_epis_comp_detail%TYPE,
        id_epis_complication_in  IN epis_comp_detail.id_epis_complication%TYPE DEFAULT NULL,
        id_epis_complication_nin IN BOOLEAN := TRUE,
        id_comp_axe_in           IN epis_comp_detail.id_comp_axe%TYPE DEFAULT NULL,
        id_comp_axe_nin          IN BOOLEAN := TRUE,
        id_sys_list_in           IN epis_comp_detail.id_sys_list%TYPE DEFAULT NULL,
        id_sys_list_nin          IN BOOLEAN := TRUE,
        dt_context_in            IN epis_comp_detail.dt_context%TYPE DEFAULT NULL,
        dt_context_nin           IN BOOLEAN := TRUE,
        id_context_prof_in       IN epis_comp_detail.id_context_prof%TYPE DEFAULT NULL,
        id_context_prof_nin      IN BOOLEAN := TRUE,
        id_context_prof_spec_in  IN epis_comp_detail.id_context_prof_spec%TYPE DEFAULT NULL,
        id_context_prof_spec_nin IN BOOLEAN := TRUE,
        dt_epis_comp_detail_in   IN epis_comp_detail.dt_epis_comp_detail%TYPE DEFAULT NULL,
        dt_epis_comp_detail_nin  IN BOOLEAN := TRUE,
        id_epis_comp_hist_in     IN epis_comp_detail.id_epis_comp_hist%TYPE DEFAULT NULL,
        id_epis_comp_hist_nin    IN BOOLEAN := TRUE,
        create_user_in           IN epis_comp_detail.create_user%TYPE DEFAULT NULL,
        create_user_nin          IN BOOLEAN := TRUE,
        create_time_in           IN epis_comp_detail.create_time%TYPE DEFAULT NULL,
        create_time_nin          IN BOOLEAN := TRUE,
        create_institution_in    IN epis_comp_detail.create_institution%TYPE DEFAULT NULL,
        create_institution_nin   IN BOOLEAN := TRUE,
        update_user_in           IN epis_comp_detail.update_user%TYPE DEFAULT NULL,
        update_user_nin          IN BOOLEAN := TRUE,
        update_time_in           IN epis_comp_detail.update_time%TYPE DEFAULT NULL,
        update_time_nin          IN BOOLEAN := TRUE,
        update_institution_in    IN epis_comp_detail.update_institution%TYPE DEFAULT NULL,
        update_institution_nin   IN BOOLEAN := TRUE,
        id_context_new_in        IN epis_comp_detail.id_context_new%TYPE DEFAULT NULL,
        id_context_new_nin       IN BOOLEAN := TRUE,
        handle_error_in          IN BOOLEAN := TRUE,
        rows_out                 IN OUT table_varchar
    ) IS
        l_rows_out               table_varchar;
        l_id_epis_complication_n NUMBER(1);
        l_id_comp_axe_n          NUMBER(1);
        l_id_sys_list_n          NUMBER(1);
        l_dt_context_n           NUMBER(1);
        l_id_context_prof_n      NUMBER(1);
        l_id_context_prof_spec_n NUMBER(1);
        l_dt_epis_comp_detail_n  NUMBER(1);
        l_id_epis_comp_hist_n    NUMBER(1);
        l_create_user_n          NUMBER(1);
        l_create_time_n          NUMBER(1);
        l_create_institution_n   NUMBER(1);
        l_update_user_n          NUMBER(1);
        l_update_time_n          NUMBER(1);
        l_update_institution_n   NUMBER(1);
        l_id_context_new_n       NUMBER(1);
    BEGIN
    
        l_id_epis_complication_n := sys.diutil.bool_to_int(id_epis_complication_nin);
        l_id_comp_axe_n          := sys.diutil.bool_to_int(id_comp_axe_nin);
        l_id_sys_list_n          := sys.diutil.bool_to_int(id_sys_list_nin);
        l_dt_context_n           := sys.diutil.bool_to_int(dt_context_nin);
        l_id_context_prof_n      := sys.diutil.bool_to_int(id_context_prof_nin);
        l_id_context_prof_spec_n := sys.diutil.bool_to_int(id_context_prof_spec_nin);
        l_dt_epis_comp_detail_n  := sys.diutil.bool_to_int(dt_epis_comp_detail_nin);
        l_id_epis_comp_hist_n    := sys.diutil.bool_to_int(id_epis_comp_hist_nin);
        l_create_user_n          := sys.diutil.bool_to_int(create_user_nin);
        l_create_time_n          := sys.diutil.bool_to_int(create_time_nin);
        l_create_institution_n   := sys.diutil.bool_to_int(create_institution_nin);
        l_update_user_n          := sys.diutil.bool_to_int(update_user_nin);
        l_update_time_n          := sys.diutil.bool_to_int(update_time_nin);
        l_update_institution_n   := sys.diutil.bool_to_int(update_institution_nin);
        l_id_context_new_n       := sys.diutil.bool_to_int(id_context_new_nin);
    
        UPDATE epis_comp_detail
           SET id_epis_complication = decode(l_id_epis_complication_n,
                                             0,
                                             id_epis_complication_in,
                                             nvl(id_epis_complication_in, id_epis_complication)),
               id_comp_axe          = decode(l_id_comp_axe_n, 0, id_comp_axe_in, nvl(id_comp_axe_in, id_comp_axe)),
               id_sys_list          = decode(l_id_sys_list_n, 0, id_sys_list_in, nvl(id_sys_list_in, id_sys_list)),
               dt_context           = decode(l_dt_context_n, 0, dt_context_in, nvl(dt_context_in, dt_context)),
               id_context_prof      = decode(l_id_context_prof_n,
                                             0,
                                             id_context_prof_in,
                                             nvl(id_context_prof_in, id_context_prof)),
               id_context_prof_spec = decode(l_id_context_prof_spec_n,
                                             0,
                                             id_context_prof_spec_in,
                                             nvl(id_context_prof_spec_in, id_context_prof_spec)),
               dt_epis_comp_detail  = decode(l_dt_epis_comp_detail_n,
                                             0,
                                             dt_epis_comp_detail_in,
                                             nvl(dt_epis_comp_detail_in, dt_epis_comp_detail)),
               id_epis_comp_hist    = decode(l_id_epis_comp_hist_n,
                                             0,
                                             id_epis_comp_hist_in,
                                             nvl(id_epis_comp_hist_in, id_epis_comp_hist)),
               create_user          = decode(l_create_user_n, 0, create_user_in, nvl(create_user_in, create_user)),
               create_time          = decode(l_create_time_n, 0, create_time_in, nvl(create_time_in, create_time)),
               create_institution   = decode(l_create_institution_n,
                                             0,
                                             create_institution_in,
                                             nvl(create_institution_in, create_institution)),
               update_user          = decode(l_update_user_n, 0, update_user_in, nvl(update_user_in, update_user)),
               update_time          = decode(l_update_time_n, 0, update_time_in, nvl(update_time_in, update_time)),
               update_institution   = decode(l_update_institution_n,
                                             0,
                                             update_institution_in,
                                             nvl(update_institution_in, update_institution)),
               id_context_new       = decode(l_id_context_new_n,
                                             0,
                                             id_context_new_in,
                                             nvl(id_context_new_in, id_context_new))
         WHERE id_epis_comp_detail = id_epis_comp_detail_in
        RETURNING ROWID BULK COLLECT INTO l_rows_out;
    
        IF (rows_out IS NULL)
        THEN
            rows_out := table_varchar();
        END IF;
    
        rows_out := rows_out MULTISET UNION DISTINCT l_rows_out;
    
    EXCEPTION
        WHEN dup_val_on_index THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                DECLARE
                    l_owner           all_constraints.owner%TYPE;
                    l_name            all_constraints.constraint_name%TYPE;
                    l_err_instance_id PLS_INTEGER;
                BEGIN
                    get_constraint_info(l_owner, l_name);
                    IF FALSE
                    THEN
                        NULL; -- Placeholder in case no unique indexes
                    ELSE
                        pk_alert_exceptions.raise_error(error_name_in => 'DUPLICATE-VALUE',
                                                        name1_in      => 'OWNER',
                                                        value1_in     => l_owner,
                                                        name2_in      => 'CONSTRAINT_NAME',
                                                        value2_in     => l_name,
                                                        name3_in      => 'TABLE_NAME',
                                                        value3_in     => 'EPIS_COMP_DETAIL');
                    END IF;
                END;
            END IF;
        WHEN e_check_constraint_failure THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                DECLARE
                    l_owner all_constraints.owner%TYPE;
                    l_name  all_constraints.constraint_name%TYPE;
                BEGIN
                    get_constraint_info(l_owner, l_name);
                    pk_alert_exceptions.raise_error(error_name_in => 'CHECK-CONSTRAINT-FAILURE',
                                                    name1_in      => 'OWNER',
                                                    value1_in     => l_owner,
                                                    name2_in      => 'CONSTRAINT_NAME',
                                                    value2_in     => l_name,
                                                    name3_in      => 'TABLE_NAME',
                                                    value3_in     => 'EPIS_COMP_DETAIL');
                END;
            END IF;
        WHEN e_integ_constraint_failure
             OR e_no_parent_key
             OR e_child_record_found THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                DECLARE
                    l_owner    all_constraints.owner%TYPE;
                    l_name     all_constraints.constraint_name%TYPE;
                    l_id       PLS_INTEGER;
                    l_err_name VARCHAR2(32767) DEFAULT 'NO-PARENT-KEY-FOUND';
                BEGIN
                    get_constraint_info(l_owner, l_name);
                    IF SQLCODE = -2292 -- Child record found
                    THEN
                        l_err_name := 'CHILD-RECORD-FOUND';
                    END IF;
                    pk_alert_exceptions.register_error(error_name_in       => l_err_name,
                                                       err_instance_id_out => l_id,
                                                       name1_in            => 'OWNER',
                                                       value1_in           => l_owner,
                                                       name2_in            => 'CONSTRAINT_NAME',
