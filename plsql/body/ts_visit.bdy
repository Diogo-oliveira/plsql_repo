    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(visit_inout IN OUT visit%ROWTYPE) IS
    BEGIN
        visit_inout.id_visit           := NULL;
        visit_inout.flg_status         := NULL;
        visit_inout.id_external_cause  := NULL;
        visit_inout.id_patient         := NULL;
        visit_inout.id_origin          := NULL;
        visit_inout.id_institution     := NULL;
        visit_inout.barcode            := NULL;
        visit_inout.dt_begin_tstz      := NULL;
        visit_inout.dt_end_tstz        := NULL;
        visit_inout.flg_migration      := 'A';
        visit_inout.dt_creation        := NULL;
        visit_inout.create_user        := NULL;
        visit_inout.create_time        := NULL;
        visit_inout.create_institution := NULL;
        visit_inout.update_user        := NULL;
        visit_inout.update_time        := NULL;
        visit_inout.update_institution := NULL;
    END initrec;

    FUNCTION initrec RETURN visit%ROWTYPE IS
        l_visit visit%ROWTYPE;
    BEGIN
        l_visit.flg_migration := 'A';
        RETURN l_visit;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN visit_tc IS
        data visit_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM visit
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN visit_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data visit_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

BEGIN
    NULL;
END ts_visit;
