    FUNCTION next_key(sequence_in IN VARCHAR2 := NULL) RETURN visit.id_visit%TYPE
    
     IS
        retval visit.id_visit%TYPE;
    
    BEGIN
        IF sequence_in IS NULL
        THEN
            SELECT seq_visit.NEXTVAL
              INTO retval
              FROM dual;
        ELSE
            EXECUTE IMMEDIATE 'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
                INTO retval;
        END IF;
        RETURN retval;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'SEQUENCE-GENERATION-FAILURE',
                                            name1_in      => 'SEQUENCE',
                                            value1_in     => nvl(sequence_in, 'seq_VISIT'));
    END next_key;

