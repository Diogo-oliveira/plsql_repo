    PROCEDURE n_ins_clobs_in_chunks
    (
        id_interv_question_response_in IN interv_question_response.id_interv_question_response%TYPE,
        id_episode_in                  IN interv_question_response.id_episode%TYPE DEFAULT NULL,
        id_interv_presc_det_in         IN interv_question_response.id_interv_presc_det%TYPE DEFAULT NULL,
        flg_time_in                    IN interv_question_response.flg_time%TYPE DEFAULT NULL,
        id_questionnaire_in            IN interv_question_response.id_questionnaire%TYPE DEFAULT NULL,
        id_response_in                 IN interv_question_response.id_response%TYPE DEFAULT NULL,
        id_prof_last_update_in         IN interv_question_response.id_prof_last_update%TYPE DEFAULT NULL,
        dt_last_update_tstz_in         IN interv_question_response.dt_last_update_tstz%TYPE DEFAULT NULL,
        create_user_in                 IN interv_question_response.create_user%TYPE DEFAULT NULL,
        create_time_in                 IN interv_question_response.create_time%TYPE DEFAULT NULL,
        create_institution_in          IN interv_question_response.create_institution%TYPE DEFAULT NULL,
        update_user_in                 IN interv_question_response.update_user%TYPE DEFAULT NULL,
        update_time_in                 IN interv_question_response.update_time%TYPE DEFAULT NULL,
        update_institution_in          IN interv_question_response.update_institution%TYPE DEFAULT NULL,
        handle_error_in                IN BOOLEAN := TRUE,
        clob_columns_in                IN varchar2_t,
        clob_pieces_in                 IN varchar2_t
    ) IS
        l_notes        CLOB;
        i              PLS_INTEGER;
        current_column VARCHAR2(30) := '';
    BEGIN
        /* Be aware, PLSQL limitations on temporary clobs prevent multiple CLOB
        variables from pointing to the same temporary CLOB (it creates copies).
        Otherwise, the code could have been written a bit more compactly. */
    
        FOR i IN clob_columns_in.first .. clob_columns_in.last
        LOOP
            /* Even when all clobs are null, DOA must send 1 row so skip it. */
            IF clob_columns_in(i) IS NOT NULL
            THEN
                IF current_column <> clob_columns_in(i)
                   OR current_column IS NULL
                THEN
                    current_column := lower(clob_columns_in(i));
                
                    CASE current_column
                        WHEN 'notes_in' THEN
                            IF l_notes IS NULL
                            THEN
                                dbms_lob.createtemporary(l_notes, TRUE, dbms_lob.call);
                            END IF;
                    END CASE;
                END IF;
            
                CASE current_column
                    WHEN 'notes_in' THEN
                        dbms_lob.writeappend(l_notes, length(clob_pieces_in(i)), clob_pieces_in(i));
                END CASE;
            END IF;
        END LOOP;
    
        ins(id_interv_question_response_in => id_interv_question_response_in,
            id_episode_in                  => id_episode_in,
            id_interv_presc_det_in         => id_interv_presc_det_in,
            flg_time_in                    => flg_time_in,
            id_questionnaire_in            => id_questionnaire_in,
            id_response_in                 => id_response_in,
            notes_in                       => l_notes,
            id_prof_last_update_in         => id_prof_last_update_in,
            dt_last_update_tstz_in         => dt_last_update_tstz_in,
            create_user_in                 => create_user_in,
            create_time_in                 => create_time_in,
            create_institution_in          => create_institution_in,
            update_user_in                 => update_user_in,
            update_time_in                 => update_time_in,
            update_institution_in          => update_institution_in,
            handle_error_in                => handle_error_in);
    
    END n_ins_clobs_in_chunks;

