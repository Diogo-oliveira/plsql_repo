    FUNCTION next_key(sequence_in IN VARCHAR2 := NULL) RETURN interv_question_response.id_interv_question_response%TYPE
    
     IS
        retval interv_question_response.id_interv_question_response%TYPE;
    
    BEGIN
        IF sequence_in IS NULL
        THEN
            SELECT seq_interv_question_response.nextval
              INTO retval
              FROM dual;
        ELSE
            EXECUTE IMMEDIATE 'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
                INTO retval;
        END IF;
        RETURN retval;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'SEQUENCE-GENERATION-FAILURE',
