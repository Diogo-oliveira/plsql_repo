    PROCEDURE upd
    (
        col_in            IN pat_child_feed_dev_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_pat_child_feed_dev id_pat_child_feed_dev_cc;
        l_dt_pat_child_feed_dev dt_pat_child_feed_dev_cc;
        l_id_patient            id_patient_cc;
        l_id_child_feed_dev     id_child_feed_dev_cc;
        l_id_professional       id_professional_cc;
        l_child_age             child_age_cc;
        l_dt_cancel             dt_cancel_cc;
        l_id_prof_cancel        id_prof_cancel_cc;
        l_flg_status            flg_status_cc;
        l_id_episode            id_episode_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_id_pat_child_feed_dev(i) := col_in(i).id_pat_child_feed_dev;
            l_dt_pat_child_feed_dev(i) := col_in(i).dt_pat_child_feed_dev;
            l_id_patient(i) := col_in(i).id_patient;
            l_id_child_feed_dev(i) := col_in(i).id_child_feed_dev;
            l_id_professional(i) := col_in(i).id_professional;
            l_child_age(i) := col_in(i).child_age;
            l_dt_cancel(i) := col_in(i).dt_cancel;
            l_id_prof_cancel(i) := col_in(i).id_prof_cancel;
            l_flg_status(i) := col_in(i).flg_status;
            l_id_episode(i) := col_in(i).id_episode;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE pat_child_feed_dev
                   SET dt_pat_child_feed_dev = nvl(l_dt_pat_child_feed_dev(i), dt_pat_child_feed_dev),
                       id_patient            = nvl(l_id_patient(i), id_patient),
                       id_child_feed_dev     = nvl(l_id_child_feed_dev(i), id_child_feed_dev),
                       id_professional       = nvl(l_id_professional(i), id_professional),
                       child_age             = nvl(l_child_age(i), child_age),
                       dt_cancel             = nvl(l_dt_cancel(i), dt_cancel),
                       id_prof_cancel        = nvl(l_id_prof_cancel(i), id_prof_cancel),
                       flg_status            = nvl(l_flg_status(i), flg_status),
                       id_episode            = nvl(l_id_episode(i), id_episode)
                 WHERE id_pat_child_feed_dev = l_id_pat_child_feed_dev(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE pat_child_feed_dev
                   SET dt_pat_child_feed_dev = l_dt_pat_child_feed_dev(i),
                       id_patient            = l_id_patient(i),
                       id_child_feed_dev     = l_id_child_feed_dev(i),
                       id_professional       = l_id_professional(i),
                       child_age             = l_child_age(i),
                       dt_cancel             = l_dt_cancel(i),
                       id_prof_cancel        = l_id_prof_cancel(i),
                       flg_status            = l_flg_status(i),
                       id_episode            = l_id_episode(i)
                 WHERE id_pat_child_feed_dev = l_id_pat_child_feed_dev(i);
        END IF;
    END upd;

