    FUNCTION dyndelstr(where_in IN VARCHAR2) RETURN VARCHAR2 IS
    BEGIN
        IF where_in IS NULL
        THEN
            RETURN 'DELETE FROM supply_workflow_hist';
        ELSE
            RETURN 'DELETE FROM supply_workflow_hist WHERE ' || where_in;
        END IF;
    END dyndelstr;
