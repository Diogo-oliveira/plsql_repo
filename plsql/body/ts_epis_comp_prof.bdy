        l_dt_epis_comp_prof_n    := sys.diutil.bool_to_int(dt_epis_comp_prof_nin);
    
        UPDATE epis_comp_prof
           SET id_epis_complication = decode(l_id_epis_complication_n,
                                             0,
                                             id_epis_complication_in,
                                             nvl(id_epis_complication_in, id_epis_complication)),
               id_professional      = decode(l_id_professional_n,
