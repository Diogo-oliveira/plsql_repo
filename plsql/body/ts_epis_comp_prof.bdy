    PROCEDURE upd
    (
        col_in            IN epis_comp_prof_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_epis_comp_prof    id_epis_comp_prof_cc;
        l_id_epis_complication id_epis_complication_cc;
        l_id_professional      id_professional_cc;
        l_id_epis_comp_hist    id_epis_comp_hist_cc;
        l_create_user          create_user_cc;
        l_create_time          create_time_cc;
        l_create_institution   create_institution_cc;
        l_update_user          update_user_cc;
        l_update_time          update_time_cc;
        l_update_institution   update_institution_cc;
        l_dt_epis_comp_prof    dt_epis_comp_prof_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_id_epis_comp_prof(i) := col_in(i).id_epis_comp_prof;
            l_id_epis_complication(i) := col_in(i).id_epis_complication;
            l_id_professional(i) := col_in(i).id_professional;
            l_id_epis_comp_hist(i) := col_in(i).id_epis_comp_hist;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_dt_epis_comp_prof(i) := col_in(i).dt_epis_comp_prof;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE epis_comp_prof
                   SET id_epis_complication = nvl(l_id_epis_complication(i), id_epis_complication),
                       id_professional      = nvl(l_id_professional(i), id_professional),
                       id_epis_comp_hist    = nvl(l_id_epis_comp_hist(i), id_epis_comp_hist),
                       create_user          = nvl(l_create_user(i), create_user),
                       create_time          = nvl(l_create_time(i), create_time),
                       create_institution   = nvl(l_create_institution(i), create_institution),
                       update_user          = nvl(l_update_user(i), update_user),
                       update_time          = nvl(l_update_time(i), update_time),
                       update_institution   = nvl(l_update_institution(i), update_institution),
                       dt_epis_comp_prof    = nvl(l_dt_epis_comp_prof(i), dt_epis_comp_prof)
                 WHERE id_epis_comp_prof = l_id_epis_comp_prof(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE epis_comp_prof
                   SET id_epis_complication = l_id_epis_complication(i),
                       id_professional      = l_id_professional(i),
                       id_epis_comp_hist    = l_id_epis_comp_hist(i),
                       create_user          = l_create_user(i),
                       create_time          = l_create_time(i),
                       create_institution   = l_create_institution(i),
                       update_user          = l_update_user(i),
                       update_time          = l_update_time(i),
                       update_institution   = l_update_institution(i),
                       dt_epis_comp_prof    = l_dt_epis_comp_prof(i)
                 WHERE id_epis_comp_prof = l_id_epis_comp_prof(i);
        END IF;
    END upd;

