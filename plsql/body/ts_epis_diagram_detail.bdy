    --Update with collection (with rows_out)
    PROCEDURE upd
    (
        col_in            IN epis_diagram_detail_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := FALSE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_epis_diagram_detail id_epis_diagram_detail_cc;
        l_id_diagram_lay_imag    id_diagram_lay_imag_cc;
        l_id_diagram_tools       id_diagram_tools_cc;
        l_position_x             position_x_cc;
        l_position_y             position_y_cc;
        l_value                  value_cc;
        l_notes_cancel           notes_cancel_cc;
        l_id_prof_cancel         id_prof_cancel_cc;
        l_adw_last_update        adw_last_update_cc;
        l_flg_status             flg_status_cc;
        l_id_professional        id_professional_cc;
        l_id_epis_diagram_layout id_epis_diagram_layout_cc;
        l_dt_diagram_detail_tstz dt_diagram_detail_tstz_cc;
        l_dt_cancel_tstz         dt_cancel_tstz_cc;
        l_create_user            create_user_cc;
        l_create_time            create_time_cc;
        l_create_institution     create_institution_cc;
        l_update_user            update_user_cc;
        l_update_time            update_time_cc;
        l_update_institution     update_institution_cc;
        l_coordinates            coordinates_cc;
        l_color                  color_cc;
        l_id_cancel_reason       id_cancel_reason_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_epis_diagram_detail(i) := col_in(i).id_epis_diagram_detail;
            l_id_diagram_lay_imag(i) := col_in(i).id_diagram_lay_imag;
            l_id_diagram_tools(i) := col_in(i).id_diagram_tools;
            l_position_x(i) := col_in(i).position_x;
            l_position_y(i) := col_in(i).position_y;
            l_value(i) := col_in(i).value;
            l_notes_cancel(i) := col_in(i).notes_cancel;
            l_id_prof_cancel(i) := col_in(i).id_prof_cancel;
            l_adw_last_update(i) := col_in(i).adw_last_update;
            l_flg_status(i) := col_in(i).flg_status;
            l_id_professional(i) := col_in(i).id_professional;
            l_id_epis_diagram_layout(i) := col_in(i).id_epis_diagram_layout;
            l_dt_diagram_detail_tstz(i) := col_in(i).dt_diagram_detail_tstz;
            l_dt_cancel_tstz(i) := col_in(i).dt_cancel_tstz;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_coordinates(i) := col_in(i).coordinates;
            l_color(i) := col_in(i).color;
            l_id_cancel_reason(i) := col_in(i).id_cancel_reason;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE epis_diagram_detail
                   SET id_diagram_lay_imag    = nvl(l_id_diagram_lay_imag(i), id_diagram_lay_imag),
                       id_diagram_tools       = nvl(l_id_diagram_tools(i), id_diagram_tools),
                       position_x             = nvl(l_position_x(i), position_x),
                       position_y             = nvl(l_position_y(i), position_y),
                       VALUE                  = nvl(l_value(i), VALUE),
                       notes_cancel           = nvl(l_notes_cancel(i), notes_cancel),
                       id_prof_cancel         = nvl(l_id_prof_cancel(i), id_prof_cancel),
                       adw_last_update        = nvl(l_adw_last_update(i), adw_last_update),
                       flg_status             = nvl(l_flg_status(i), flg_status),
                       id_professional        = nvl(l_id_professional(i), id_professional),
                       id_epis_diagram_layout = nvl(l_id_epis_diagram_layout(i), id_epis_diagram_layout),
                       dt_diagram_detail_tstz = nvl(l_dt_diagram_detail_tstz(i), dt_diagram_detail_tstz),
                       dt_cancel_tstz         = nvl(l_dt_cancel_tstz(i), dt_cancel_tstz),
                       create_user            = nvl(l_create_user(i), create_user),
                       create_time            = nvl(l_create_time(i), create_time),
                       create_institution     = nvl(l_create_institution(i), create_institution),
                       update_user            = nvl(l_update_user(i), update_user),
                       update_time            = nvl(l_update_time(i), update_time),
                       update_institution     = nvl(l_update_institution(i), update_institution),
                       coordinates            = nvl(l_coordinates(i), coordinates),
                       color                  = nvl(l_color(i), color),
                       id_cancel_reason       = nvl(l_id_cancel_reason(i), id_cancel_reason)
                 WHERE id_epis_diagram_detail = l_id_epis_diagram_detail(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE epis_diagram_detail
                   SET id_diagram_lay_imag    = l_id_diagram_lay_imag(i),
                       id_diagram_tools       = l_id_diagram_tools(i),
                       position_x             = l_position_x(i),
                       position_y             = l_position_y(i),
                       VALUE                  = l_value(i),
                       notes_cancel           = l_notes_cancel(i),
                       id_prof_cancel         = l_id_prof_cancel(i),
                       adw_last_update        = l_adw_last_update(i),
                       flg_status             = l_flg_status(i),
                       id_professional        = l_id_professional(i),
                       id_epis_diagram_layout = l_id_epis_diagram_layout(i),
                       dt_diagram_detail_tstz = l_dt_diagram_detail_tstz(i),
                       dt_cancel_tstz         = l_dt_cancel_tstz(i),
                       create_user            = l_create_user(i),
                       create_time            = l_create_time(i),
                       create_institution     = l_create_institution(i),
                       update_user            = l_update_user(i),
                       update_time            = l_update_time(i),
                       update_institution     = l_update_institution(i),
                       coordinates            = l_coordinates(i),
                       color                  = l_color(i),
                       id_cancel_reason       = l_id_cancel_reason(i)
                 WHERE id_epis_diagram_detail = l_id_epis_diagram_detail(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    
    EXCEPTION
        WHEN e_forall_error THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                FOR indx IN 1 .. SQL%bulk_exceptions.count
                LOOP
                    pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
                END LOOP;
            END IF;
        WHEN OTHERS THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
            END IF;
        
    END upd;

