    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(wtl_epis_inout IN OUT wtl_epis%ROWTYPE) IS
    BEGIN
        wtl_epis_inout.id_episode         := NULL;
        wtl_epis_inout.id_waiting_list    := NULL;
        wtl_epis_inout.id_epis_type       := NULL;
        wtl_epis_inout.id_schedule        := NULL;
        wtl_epis_inout.flg_status         := 'N';
        wtl_epis_inout.create_user        := NULL;
        wtl_epis_inout.create_time        := NULL;
        wtl_epis_inout.create_institution := NULL;
        wtl_epis_inout.update_user        := NULL;
        wtl_epis_inout.update_time        := NULL;
        wtl_epis_inout.update_institution := NULL;
    END initrec;

    FUNCTION initrec RETURN wtl_epis%ROWTYPE IS
        l_wtl_epis wtl_epis%ROWTYPE;
    BEGIN
        l_wtl_epis.flg_status := 'N';
        RETURN l_wtl_epis;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN wtl_epis_tc IS
        data wtl_epis_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM wtl_epis
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN wtl_epis_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data wtl_epis_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

