                       drug_transp        = l_drug_transp(i),
                       intervention       = l_intervention(i),
                       monitorization     = l_monitorization(i),
                       nurse_activity     = l_nurse_activity(i),
                       teach_req          = l_teach_req(i),
                       movement           = l_movement(i),
                       clin_rec_req       = l_clin_rec_req(i),
                       clin_rec_transp    = l_clin_rec_transp(i),
                       vaccine            = l_vaccine(i),
                       hemo_req           = l_hemo_req(i),
                       material_req       = l_material_req(i),
                       icnp_intervention  = l_icnp_intervention(i),
                       positioning        = l_positioning(i),
                       hidrics_reg        = l_hidrics_reg(i),
                       scale_value        = l_scale_value(i),
                       prescription_n     = l_prescription_n(i),
                       prescription_p     = l_prescription_p(i),
                       discharge_pend     = l_discharge_pend(i),
                       create_user        = l_create_user(i),
