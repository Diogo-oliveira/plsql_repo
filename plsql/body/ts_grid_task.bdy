                       update_institution = l_update_institution(i),
                       supplies           = l_supplies(i),
                       noc_outcome        = l_noc_outcome(i),
                       noc_indicator      = l_noc_indicator(i),
                       nic_activity       = l_nic_activity(i),
                       opinion_state      = l_opinion_state(i),
                       oth_exam_d         = l_oth_exam_d(i),
                       oth_exam_n         = l_oth_exam_n(i),
                       img_exam_d         = l_img_exam_d(i),
                       img_exam_n         = l_img_exam_n(i),
                       disp_task          = l_disp_task(i),
                       disp_ivroom        = l_disp_ivroom(i),
                       common_order       = l_common_order(i),
                       medical_order      = l_medical_order(i)
                 WHERE id_grid_task = l_id_grid_task(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    
    EXCEPTION
        WHEN e_forall_error THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                FOR indx IN 1 .. SQL%bulk_exceptions.count
                LOOP
                    pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
                END LOOP;
            END IF;
        WHEN OTHERS THEN
            IF NOT handle_error_in
            THEN
                RAISE;
            ELSE
                pk_alertlog.log_error('DML error ignored: ' || SQLCODE || ': ' || SQLERRM);
            END IF;
        
    END upd;

