            l_oth_exam_n(i) := col_in(i).oth_exam_n;
            l_img_exam_d(i) := col_in(i).img_exam_d;
            l_img_exam_n(i) := col_in(i).img_exam_n;
            l_disp_task(i) := col_in(i).disp_task;
            l_disp_ivroom(i) := col_in(i).disp_ivroom;
            l_common_order(i) := col_in(i).common_order;
            l_medical_order(i) := col_in(i).medical_order;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE grid_task
                   SET id_episode         = nvl(l_id_episode(i), id_episode),
                       analysis_d         = nvl(l_analysis_d(i), analysis_d),
