               disp_task          = decode(l_disp_task_n, 0, disp_task_in, nvl(disp_task_in, disp_task)),
               disp_ivroom        = decode(l_disp_ivroom_n, 0, disp_ivroom_in, nvl(disp_ivroom_in, disp_ivroom)),
