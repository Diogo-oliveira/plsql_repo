      l_combination_compound COMBINATION_COMPOUND%ROWTYPE;
   BEGIN
      RETURN l_combination_compound;
   END initrec;


   FUNCTION get_data_rowid(
        rows_in IN TABLE_VARCHAR
        ) RETURN COMBINATION_COMPOUND_tc
   IS
        data COMBINATION_COMPOUND_tc;
   BEGIN
        select * bulk collect into data from COMBINATION_COMPOUND where rowid in (select * from table(rows_in));
        return data;
        EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'get_data_rowid'
           );
   END get_data_rowid;


   FUNCTION get_data_rowid_pat(
        rows_in IN TABLE_VARCHAR
        ) RETURN COMBINATION_COMPOUND_tc
   is
        PRAGMA AUTONOMOUS_TRANSACTION;
        data COMBINATION_COMPOUND_tc;
   BEGIN
        data := get_data_rowid(rows_in);
        commit;
        return data;
        EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'get_data_rowid'
           );
        rollback;
    END get_data_rowid_pat;

BEGIN
   NULL;
END TS_COMBINATION_COMPOUND;
