    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(clin_record_inout IN OUT clin_record%ROWTYPE) IS
    BEGIN
        clin_record_inout.id_clin_record    := NULL;
        clin_record_inout.flg_status        := NULL;
        clin_record_inout.id_patient        := NULL;
        clin_record_inout.id_institution    := NULL;
        clin_record_inout.id_pat_family     := NULL;
        clin_record_inout.num_clin_record   := NULL;
        clin_record_inout.id_instit_enroled := NULL;
        clin_record_inout.id_episode        := NULL;
    END initrec;

    FUNCTION initrec RETURN clin_record%ROWTYPE IS
        l_clin_record clin_record%ROWTYPE;
    BEGIN
        RETURN l_clin_record;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN clin_record_tc IS
        data clin_record_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM clin_record
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN clin_record_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data clin_record_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

BEGIN
    NULL;
