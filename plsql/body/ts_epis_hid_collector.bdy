   PROCEDURE upd (
      col_in IN EPIS_HID_COLLECTOR_tc,
      ignore_if_null_in IN BOOLEAN := TRUE
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
      l_ID_EPIS_HID_COLLECTOR ID_EPIS_HID_COLLECTOR_cc;
      l_ID_EPIS_HIDRICS ID_EPIS_HIDRICS_cc;
      l_ID_EPIS_HIDRICS_LINE ID_EPIS_HIDRICS_LINE_cc;
      l_FLG_LEVEL_CONTROL FLG_LEVEL_CONTROL_cc;
      l_DT_BEGIN DT_BEGIN_cc;
      l_DT_END DT_END_cc;
      l_CURR_LEVEL CURR_LEVEL_cc;
      l_FLG_RESTART FLG_RESTART_cc;
