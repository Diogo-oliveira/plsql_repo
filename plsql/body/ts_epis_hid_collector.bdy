      l_CREATE_USER CREATE_USER_cc;
      l_CREATE_TIME CREATE_TIME_cc;
      l_CREATE_INSTITUTION CREATE_INSTITUTION_cc;
      l_UPDATE_USER UPDATE_USER_cc;
      l_UPDATE_TIME UPDATE_TIME_cc;
      l_UPDATE_INSTITUTION UPDATE_INSTITUTION_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_EPIS_HID_COLLECTOR(i) := col_in(i).ID_EPIS_HID_COLLECTOR;
         l_ID_EPIS_HIDRICS(i) := col_in(i).ID_EPIS_HIDRICS;
         l_ID_EPIS_HIDRICS_LINE(i) := col_in(i).ID_EPIS_HIDRICS_LINE;
         l_FLG_LEVEL_CONTROL(i) := col_in(i).FLG_LEVEL_CONTROL;
         l_DT_BEGIN(i) := col_in(i).DT_BEGIN;
         l_DT_END(i) := col_in(i).DT_END;
         l_CURR_LEVEL(i) := col_in(i).CURR_LEVEL;
         l_FLG_RESTART(i) := col_in(i).FLG_RESTART;
         l_ID_PROF(i) := col_in(i).ID_PROF;
         l_DT_EPIS_HID_COLLECTOR(i) := col_in(i).DT_EPIS_HID_COLLECTOR;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE EPIS_HID_COLLECTOR SET
               ID_EPIS_HIDRICS = NVL (l_ID_EPIS_HIDRICS(i), ID_EPIS_HIDRICS),
               ID_EPIS_HIDRICS_LINE = NVL (l_ID_EPIS_HIDRICS_LINE(i), ID_EPIS_HIDRICS_LINE),
               FLG_LEVEL_CONTROL = NVL (l_FLG_LEVEL_CONTROL(i), FLG_LEVEL_CONTROL),
               DT_BEGIN = NVL (l_DT_BEGIN(i), DT_BEGIN),
               DT_END = NVL (l_DT_END(i), DT_END),
               CURR_LEVEL = NVL (l_CURR_LEVEL(i), CURR_LEVEL),
               FLG_RESTART = NVL (l_FLG_RESTART(i), FLG_RESTART),
               ID_PROF = NVL (l_ID_PROF(i), ID_PROF),
               DT_EPIS_HID_COLLECTOR = NVL (l_DT_EPIS_HID_COLLECTOR(i), DT_EPIS_HID_COLLECTOR),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
               UPDATE_INSTITUTION = NVL (l_UPDATE_INSTITUTION(i), UPDATE_INSTITUTION)
             WHERE
                ID_EPIS_HID_COLLECTOR = l_ID_EPIS_HID_COLLECTOR(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE EPIS_HID_COLLECTOR SET
               ID_EPIS_HIDRICS = l_ID_EPIS_HIDRICS(i),
               ID_EPIS_HIDRICS_LINE = l_ID_EPIS_HIDRICS_LINE(i),
               FLG_LEVEL_CONTROL = l_FLG_LEVEL_CONTROL(i),
               DT_BEGIN = l_DT_BEGIN(i),
               DT_END = l_DT_END(i),
               CURR_LEVEL = l_CURR_LEVEL(i),
               FLG_RESTART = l_FLG_RESTART(i),
               ID_PROF = l_ID_PROF(i),
               DT_EPIS_HID_COLLECTOR = l_DT_EPIS_HID_COLLECTOR(i),
               CREATE_USER = l_CREATE_USER(i),
               CREATE_TIME = l_CREATE_TIME(i),
               CREATE_INSTITUTION = l_CREATE_INSTITUTION(i),
               UPDATE_USER = l_UPDATE_USER(i),
               UPDATE_TIME = l_UPDATE_TIME(i),
               UPDATE_INSTITUTION = l_UPDATE_INSTITUTION(i)
             WHERE
                ID_EPIS_HID_COLLECTOR = l_ID_EPIS_HID_COLLECTOR(i)
        returning rowid bulk collect into rows_out  ;
      END IF;
   END upd;


