        ) RETURN CDR_EXTERNAL_tc
   IS
        data CDR_EXTERNAL_tc;
   BEGIN
        select * bulk collect into data from CDR_EXTERNAL where rowid in (select /*+opt_estimate(table,t,scale_rows=0.0000001))*/ * from table(rows_in) t);
        return data;
        EXCEPTION
      WHEN OTHERS THEN
