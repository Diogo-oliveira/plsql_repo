   PROCEDURE n_ins_clobs_in_chunks (
      id_cdr_external_in IN CDR_EXTERNAL.ID_CDR_EXTERNAL%TYPE,
      id_cdr_call_in IN CDR_EXTERNAL.ID_CDR_CALL%TYPE DEFAULT NULL,
      id_cdr_type_in IN CDR_EXTERNAL.ID_CDR_TYPE%TYPE DEFAULT NULL,
      id_cdr_severity_in IN CDR_EXTERNAL.ID_CDR_SEVERITY%TYPE DEFAULT NULL,
      title_in IN CDR_EXTERNAL.TITLE%TYPE DEFAULT NULL,
      create_user_in IN CDR_EXTERNAL.CREATE_USER%TYPE DEFAULT NULL,
      create_time_in IN CDR_EXTERNAL.CREATE_TIME%TYPE DEFAULT NULL,
      create_institution_in IN CDR_EXTERNAL.CREATE_INSTITUTION%TYPE DEFAULT NULL,
      update_user_in IN CDR_EXTERNAL.UPDATE_USER%TYPE DEFAULT NULL,
      update_time_in IN CDR_EXTERNAL.UPDATE_TIME%TYPE DEFAULT NULL,
      update_institution_in IN CDR_EXTERNAL.UPDATE_INSTITUTION%TYPE DEFAULT NULL,
      id_product_in IN CDR_EXTERNAL.ID_PRODUCT%TYPE DEFAULT NULL,
      id_product_level_in IN CDR_EXTERNAL.ID_PRODUCT_LEVEL%TYPE DEFAULT NULL,
      id_product_supplier_in IN CDR_EXTERNAL.ID_PRODUCT_SUPPLIER%TYPE DEFAULT NULL,
      handle_error_in IN BOOLEAN := TRUE,
      clob_columns_in IN varchar2_t,
      clob_pieces_in IN  varchar2_t
   ) IS
        l_COMMENT_DESC clob;
      i                PLS_INTEGER;
      current_column varchar2(30) := '';
   BEGIN
      /* Be aware, PLSQL limitations on temporary clobs prevent multiple CLOB
         variables from pointing to the same temporary CLOB (it creates copies).
         Otherwise, the code could have been written a bit more compactly. */

      FOR i IN clob_columns_in.FIRST .. clob_columns_in.LAST
      LOOP
         /* Even when all clobs are null, DOA must send 1 row so skip it. */
         IF clob_columns_in (i) IS NOT NULL
         THEN
            IF current_column <> clob_columns_in (i) OR current_column IS NULL
            THEN
               current_column := LOWER (clob_columns_in (i));

               CASE current_column
                  WHEN 'comment_desc_in'
                  THEN
                     IF l_COMMENT_DESC IS NULL
                     THEN
                       DBMS_LOB.createtemporary (
                         l_COMMENT_DESC, TRUE, DBMS_LOB.CALL);
                     END IF;
               END CASE;
            END IF;

            CASE current_column
              WHEN 'comment_desc_in'
              THEN
                  DBMS_LOB.writeappend (l_COMMENT_DESC
                                      , LENGTH (clob_pieces_in (i))
                                      , clob_pieces_in (i)
                                       );
            END CASE;
         END IF;
      END LOOP;

      ins (
         id_cdr_external_in => id_cdr_external_in,
         id_cdr_call_in => id_cdr_call_in,
         id_cdr_type_in => id_cdr_type_in,
         id_cdr_severity_in => id_cdr_severity_in,
         title_in => title_in,
         comment_desc_in => l_COMMENT_DESC,
         create_user_in => create_user_in,
         create_time_in => create_time_in,
         create_institution_in => create_institution_in,
         update_user_in => update_user_in,
         update_time_in => update_time_in,
         update_institution_in => update_institution_in,
         id_product_in => id_product_in,
         id_product_level_in => id_product_level_in,
         id_product_supplier_in => id_product_supplier_in,
         handle_error_in => handle_error_in
         );

   END n_ins_clobs_in_chunks;

