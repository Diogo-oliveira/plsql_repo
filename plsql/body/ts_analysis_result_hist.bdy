        id_sample_type_in           IN analysis_result_hist.id_sample_type%TYPE DEFAULT NULL,
        id_sample_type_nin          IN BOOLEAN := TRUE,
        id_exam_cat_in              IN analysis_result_hist.id_exam_cat%TYPE DEFAULT NULL,
        id_exam_cat_nin             IN BOOLEAN := TRUE,
