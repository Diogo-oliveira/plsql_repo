        flg_orig_analysis_in       IN analysis_result_hist.flg_orig_analysis%TYPE DEFAULT NULL,
        loinc_code_in              IN analysis_result_hist.loinc_code%TYPE DEFAULT NULL,
        create_user_in             IN analysis_result_hist.create_user%TYPE DEFAULT NULL,
        create_time_in             IN analysis_result_hist.create_time%TYPE DEFAULT NULL,
        create_institution_in      IN analysis_result_hist.create_institution%TYPE DEFAULT NULL,
        update_user_in             IN analysis_result_hist.update_user%TYPE DEFAULT NULL,
        update_time_in             IN analysis_result_hist.update_time%TYPE DEFAULT NULL,
        update_institution_in      IN analysis_result_hist.update_institution%TYPE DEFAULT NULL,
        handle_error_in            IN BOOLEAN := TRUE,
        clob_columns_in            IN varchar2_t,
        clob_pieces_in             IN varchar2_t
    ) IS
        l_notes        CLOB;
        i              PLS_INTEGER;
        current_column VARCHAR2(30) := '';
    BEGIN
        /* Be aware, PLSQL limitations on temporary clobs prevent multiple CLOB
        variables from pointing to the same temporary CLOB (it creates copies).
        Otherwise, the code could have been written a bit more compactly. */
    
        FOR i IN clob_columns_in.first .. clob_columns_in.last
        LOOP
            /* Even when all clobs are null, DOA must send 1 row so skip it. */
            IF clob_columns_in(i) IS NOT NULL
            THEN
                IF current_column <> clob_columns_in(i)
                   OR current_column IS NULL
                THEN
                    current_column := lower(clob_columns_in(i));
                
                    CASE current_column
                        WHEN 'notes_in' THEN
                            IF l_notes IS NULL
                            THEN
                                dbms_lob.createtemporary(l_notes, TRUE, dbms_lob.call);
                            END IF;
                    END CASE;
                END IF;
            
                CASE current_column
                    WHEN 'notes_in' THEN
                        dbms_lob.writeappend(l_notes, length(clob_pieces_in(i)), clob_pieces_in(i));
                END CASE;
            END IF;
        END LOOP;
    
        ins(dt_analysis_result_hist_in => dt_analysis_result_hist_in,
            id_analysis_result_in      => id_analysis_result_in,
            id_analysis_req_det_in     => id_analysis_req_det_in,
