    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(lens_soft_inst_inout IN OUT lens_soft_inst%ROWTYPE) IS
    BEGIN
        lens_soft_inst_inout.id_lens        := NULL;
        lens_soft_inst_inout.id_software    := NULL;
        lens_soft_inst_inout.id_institution := NULL;
        lens_soft_inst_inout.flg_available  := NULL;
        lens_soft_inst_inout.rank           := NULL;
    END initrec;

    FUNCTION initrec RETURN lens_soft_inst%ROWTYPE IS
        l_lens_soft_inst lens_soft_inst%ROWTYPE;
    BEGIN
        RETURN l_lens_soft_inst;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN lens_soft_inst_tc IS
        data lens_soft_inst_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM lens_soft_inst
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN lens_soft_inst_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data lens_soft_inst_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

