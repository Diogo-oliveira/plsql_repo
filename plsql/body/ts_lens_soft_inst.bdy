    PROCEDURE upd
    (
        col_in            IN lens_soft_inst_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_lens        id_lens_cc;
        l_id_software    id_software_cc;
        l_id_institution id_institution_cc;
        l_flg_available  flg_available_cc;
        l_rank           rank_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_id_lens(i) := col_in(i).id_lens;
            l_id_software(i) := col_in(i).id_software;
            l_id_institution(i) := col_in(i).id_institution;
            l_flg_available(i) := col_in(i).flg_available;
            l_rank(i) := col_in(i).rank;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE lens_soft_inst
                   SET flg_available = nvl(l_flg_available(i), flg_available), rank = nvl(l_rank(i), rank)
                 WHERE id_lens = l_id_lens(i)
                   AND id_software = l_id_software(i)
                   AND id_institution = l_id_institution(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE lens_soft_inst
                   SET flg_available = l_flg_available(i), rank = l_rank(i)
                 WHERE id_lens = l_id_lens(i)
                   AND id_software = l_id_software(i)
                   AND id_institution = l_id_institution(i);
        END IF;
    END upd;

