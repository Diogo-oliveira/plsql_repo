    PROCEDURE ins (
      rows_in IN NNN_EPIS_ACTIVITY_DET_H_tc
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out OUT TABLE_VARCHAR
   )
   IS
   BEGIN
      IF rows_in.COUNT = 0
      THEN
         NULL;
      ELSE
       FORALL indx IN rows_in.FIRST .. rows_in.LAST
            SAVE EXCEPTIONS
            INSERT INTO NNN_EPIS_ACTIVITY_DET_H (
               ID_NNN_EPIS_ACTIVITY_DET_H,
               ID_NNN_EPIS_ACTIVITY_DET,
               ID_NNN_EPIS_ACTIVITY,
               ID_PATIENT,
               ID_EPISODE,
               ID_VISIT,
               ID_PROFESSIONAL,
               ID_CANCEL_REASON,
               CANCEL_NOTES,
               DT_PLAN,
               ID_ORDER_RECURR_PLAN,
               EXEC_NUMBER,
               NOTES,
               ID_EPIS_DOCUMENTATION,
               VITAL_SIGN_READ_LIST,
               FLG_STATUS,
               DT_VAL_TIME_START,
               DT_VAL_TIME_END,
               DT_TRS_TIME_START,
               DT_TRS_TIME_END,
               CREATE_USER,
               CREATE_TIME,
               CREATE_INSTITUTION,
               UPDATE_USER,
               UPDATE_TIME,
               UPDATE_INSTITUTION
               )
            VALUES (
               rows_in(indx).ID_NNN_EPIS_ACTIVITY_DET_H,
               rows_in(indx).ID_NNN_EPIS_ACTIVITY_DET,
               rows_in(indx).ID_NNN_EPIS_ACTIVITY,
               rows_in(indx).ID_PATIENT,
               rows_in(indx).ID_EPISODE,
               rows_in(indx).ID_VISIT,
               rows_in(indx).ID_PROFESSIONAL,
               rows_in(indx).ID_CANCEL_REASON,
               rows_in(indx).CANCEL_NOTES,
               rows_in(indx).DT_PLAN,
               rows_in(indx).ID_ORDER_RECURR_PLAN,
               rows_in(indx).EXEC_NUMBER,
               rows_in(indx).NOTES,
               rows_in(indx).ID_EPIS_DOCUMENTATION,
               rows_in(indx).VITAL_SIGN_READ_LIST,
               rows_in(indx).FLG_STATUS,
               rows_in(indx).DT_VAL_TIME_START,
               rows_in(indx).DT_VAL_TIME_END,
               rows_in(indx).DT_TRS_TIME_START,
               rows_in(indx).DT_TRS_TIME_END,
               rows_in(indx).CREATE_USER,
               rows_in(indx).CREATE_TIME,
               rows_in(indx).CREATE_INSTITUTION,
               rows_in(indx).UPDATE_USER,
               rows_in(indx).UPDATE_TIME,
               rows_in(indx).UPDATE_INSTITUTION
               ) RETURNING ROWID BULK COLLECT INTO rows_out;

      END IF;
   EXCEPTION
     WHEN e_forall_error
     THEN
        -- In Oracle9i and above, SAVE EXCEPTIONS will direct control
        -- here if any error occurs. We can then save all the error
        -- information out to the error instance.
       IF NOT handle_error_in THEN RAISE;
       ELSE
          <<bulk_handler>>
          DECLARE
             l_err_instance_id NUMBER;
          BEGIN
             -- For each error, write to the log.
             FOR indx IN 1 .. SQL%BULK_EXCEPTIONS.COUNT
             LOOP
                pk_alert_exceptions.register_error (
                    error_name_in => 'FORALL-INSERT-FAILURE'
                   ,err_instance_id_out => l_err_instance_id
