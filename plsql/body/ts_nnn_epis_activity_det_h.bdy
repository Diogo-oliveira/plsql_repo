      dt_plan_in IN NNN_EPIS_ACTIVITY_DET_H.DT_PLAN%TYPE DEFAULT NULL,
      id_order_recurr_plan_in IN NNN_EPIS_ACTIVITY_DET_H.ID_ORDER_RECURR_PLAN%TYPE DEFAULT NULL,
      exec_number_in IN NNN_EPIS_ACTIVITY_DET_H.EXEC_NUMBER%TYPE DEFAULT NULL,
      id_epis_documentation_in IN NNN_EPIS_ACTIVITY_DET_H.ID_EPIS_DOCUMENTATION%TYPE DEFAULT NULL,
      vital_sign_read_list_in IN NNN_EPIS_ACTIVITY_DET_H.VITAL_SIGN_READ_LIST%TYPE DEFAULT NULL,
      flg_status_in IN NNN_EPIS_ACTIVITY_DET_H.FLG_STATUS%TYPE DEFAULT NULL,
