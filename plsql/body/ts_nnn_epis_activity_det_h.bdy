   FUNCTION next_key (sequence_in IN VARCHAR2 := NULL) RETURN NNN_EPIS_ACTIVITY_DET_H.ID_NNN_EPIS_ACTIVITY_DET_H%TYPE

   IS
     retval NNN_EPIS_ACTIVITY_DET_H.ID_NNN_EPIS_ACTIVITY_DET_H%TYPE;

   BEGIN
      IF sequence_in IS NULL
      THEN
         SELECT seq_NNN_EPIS_ACTIVITY_DET_H.NEXTVAL INTO retval FROM dual;
      ELSE
         EXECUTE IMMEDIATE
            'SELECT ' || sequence_in || '.NEXTVAL FROM dual'
            INTO retval;
      END IF;
      RETURN retval;
   EXCEPTION
      WHEN OTHERS THEN
        pk_alert_exceptions.raise_error (
           error_name_in => 'SEQUENCE-GENERATION-FAILURE'
           ,name1_in => 'SEQUENCE'
           ,value1_in => NVL (sequence_in, 'seq_NNN_EPIS_ACTIVITY_DET_H')
           );
   END next_key;

