   PROCEDURE upd (
      col_in IN ANNOUNCED_ARRIVAL_HIST_tc,
      ignore_if_null_in IN BOOLEAN := TRUE
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
      l_ID_ANNOUNCED_ARRIVAL_HIST ID_ANNOUNCED_ARRIVAL_HIST_cc;
      l_ID_ANNOUNCED_ARRIVAL ID_ANNOUNCED_ARRIVAL_cc;
      l_ID_PRE_HOSP_ACCIDENT ID_PRE_HOSP_ACCIDENT_cc;
      l_ID_EPISODE ID_EPISODE_cc;
      l_FLG_EPI_TYPE FLG_EPI_TYPE_cc;
      l_TYPE_INJURY TYPE_INJURY_cc;
      l_CONDITION CONDITION_cc;
      l_REFERRED_BY REFERRED_BY_cc;
      l_ID_SPECIALITY ID_SPECIALITY_cc;
      l_ID_CLINICAL_SERVICE ID_CLINICAL_SERVICE_cc;
      l_ID_ED_PHYSICIAN ID_ED_PHYSICIAN_cc;
      l_DT_EXPECTED_ARRIVAL DT_EXPECTED_ARRIVAL_cc;
      l_FLG_STATUS FLG_STATUS_cc;
      l_DT_ANNOUNCED_ARRIVAL DT_ANNOUNCED_ARRIVAL_cc;
      l_CREATE_USER CREATE_USER_cc;
      l_CREATE_TIME CREATE_TIME_cc;
      l_CREATE_INSTITUTION CREATE_INSTITUTION_cc;
      l_UPDATE_USER UPDATE_USER_cc;
      l_UPDATE_TIME UPDATE_TIME_cc;
      l_UPDATE_INSTITUTION UPDATE_INSTITUTION_cc;
      l_ID_CANCEL_REASON ID_CANCEL_REASON_cc;
      l_CANCEL_NOTES CANCEL_NOTES_cc;
      l_ID_PATIENT ID_PATIENT_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_ANNOUNCED_ARRIVAL_HIST(i) := col_in(i).ID_ANNOUNCED_ARRIVAL_HIST;
         l_ID_ANNOUNCED_ARRIVAL(i) := col_in(i).ID_ANNOUNCED_ARRIVAL;
         l_ID_PRE_HOSP_ACCIDENT(i) := col_in(i).ID_PRE_HOSP_ACCIDENT;
         l_ID_EPISODE(i) := col_in(i).ID_EPISODE;
         l_FLG_EPI_TYPE(i) := col_in(i).FLG_EPI_TYPE;
         l_TYPE_INJURY(i) := col_in(i).TYPE_INJURY;
         l_CONDITION(i) := col_in(i).CONDITION;
         l_REFERRED_BY(i) := col_in(i).REFERRED_BY;
         l_ID_SPECIALITY(i) := col_in(i).ID_SPECIALITY;
         l_ID_CLINICAL_SERVICE(i) := col_in(i).ID_CLINICAL_SERVICE;
         l_ID_ED_PHYSICIAN(i) := col_in(i).ID_ED_PHYSICIAN;
         l_DT_EXPECTED_ARRIVAL(i) := col_in(i).DT_EXPECTED_ARRIVAL;
         l_FLG_STATUS(i) := col_in(i).FLG_STATUS;
         l_DT_ANNOUNCED_ARRIVAL(i) := col_in(i).DT_ANNOUNCED_ARRIVAL;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
         l_ID_CANCEL_REASON(i) := col_in(i).ID_CANCEL_REASON;
         l_CANCEL_NOTES(i) := col_in(i).CANCEL_NOTES;
         l_ID_PATIENT(i) := col_in(i).ID_PATIENT;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE ANNOUNCED_ARRIVAL_HIST SET
               ID_ANNOUNCED_ARRIVAL = NVL (l_ID_ANNOUNCED_ARRIVAL(i), ID_ANNOUNCED_ARRIVAL),
               ID_PRE_HOSP_ACCIDENT = NVL (l_ID_PRE_HOSP_ACCIDENT(i), ID_PRE_HOSP_ACCIDENT),
               ID_EPISODE = NVL (l_ID_EPISODE(i), ID_EPISODE),
               FLG_EPI_TYPE = NVL (l_FLG_EPI_TYPE(i), FLG_EPI_TYPE),
               TYPE_INJURY = NVL (l_TYPE_INJURY(i), TYPE_INJURY),
               CONDITION = NVL (l_CONDITION(i), CONDITION),
               REFERRED_BY = NVL (l_REFERRED_BY(i), REFERRED_BY),
               ID_SPECIALITY = NVL (l_ID_SPECIALITY(i), ID_SPECIALITY),
               ID_CLINICAL_SERVICE = NVL (l_ID_CLINICAL_SERVICE(i), ID_CLINICAL_SERVICE),
               ID_ED_PHYSICIAN = NVL (l_ID_ED_PHYSICIAN(i), ID_ED_PHYSICIAN),
               DT_EXPECTED_ARRIVAL = NVL (l_DT_EXPECTED_ARRIVAL(i), DT_EXPECTED_ARRIVAL),
               FLG_STATUS = NVL (l_FLG_STATUS(i), FLG_STATUS),
               DT_ANNOUNCED_ARRIVAL = NVL (l_DT_ANNOUNCED_ARRIVAL(i), DT_ANNOUNCED_ARRIVAL),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
               UPDATE_INSTITUTION = NVL (l_UPDATE_INSTITUTION(i), UPDATE_INSTITUTION),
               ID_CANCEL_REASON = NVL (l_ID_CANCEL_REASON(i), ID_CANCEL_REASON),
               CANCEL_NOTES = NVL (l_CANCEL_NOTES(i), CANCEL_NOTES),
               ID_PATIENT = NVL (l_ID_PATIENT(i), ID_PATIENT)
             WHERE
                ID_ANNOUNCED_ARRIVAL_HIST = l_ID_ANNOUNCED_ARRIVAL_HIST(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE ANNOUNCED_ARRIVAL_HIST SET
               ID_ANNOUNCED_ARRIVAL = l_ID_ANNOUNCED_ARRIVAL(i),
               ID_PRE_HOSP_ACCIDENT = l_ID_PRE_HOSP_ACCIDENT(i),
               ID_EPISODE = l_ID_EPISODE(i),
               FLG_EPI_TYPE = l_FLG_EPI_TYPE(i),
               TYPE_INJURY = l_TYPE_INJURY(i),
               CONDITION = l_CONDITION(i),
               REFERRED_BY = l_REFERRED_BY(i),
               ID_SPECIALITY = l_ID_SPECIALITY(i),
               ID_CLINICAL_SERVICE = l_ID_CLINICAL_SERVICE(i),
               ID_ED_PHYSICIAN = l_ID_ED_PHYSICIAN(i),
               DT_EXPECTED_ARRIVAL = l_DT_EXPECTED_ARRIVAL(i),
               FLG_STATUS = l_FLG_STATUS(i),
               DT_ANNOUNCED_ARRIVAL = l_DT_ANNOUNCED_ARRIVAL(i),
               CREATE_USER = l_CREATE_USER(i),
               CREATE_TIME = l_CREATE_TIME(i),
               CREATE_INSTITUTION = l_CREATE_INSTITUTION(i),
               UPDATE_USER = l_UPDATE_USER(i),
               UPDATE_TIME = l_UPDATE_TIME(i),
               UPDATE_INSTITUTION = l_UPDATE_INSTITUTION(i),
               ID_CANCEL_REASON = l_ID_CANCEL_REASON(i),
               CANCEL_NOTES = l_CANCEL_NOTES(i),
               ID_PATIENT = l_ID_PATIENT(i)
             WHERE
                ID_ANNOUNCED_ARRIVAL_HIST = l_ID_ANNOUNCED_ARRIVAL_HIST(i)
        returning rowid bulk collect into rows_out  ;
      END IF;
   END upd;


