   PROCEDURE upd (
      col_in IN EVENT_MOST_FREQ_tc,
      ignore_if_null_in IN BOOLEAN := TRUE
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
      l_ID_EVENT_MOST_FREQ ID_EVENT_MOST_FREQ_cc;
      l_ID_GROUP ID_GROUP_cc;
      l_FLG_GROUP FLG_GROUP_cc;
      l_ID_PATIENT ID_PATIENT_cc;
      l_VALUE VALUE_cc;
      l_ID_UNIT_MEASURE ID_UNIT_MEASURE_cc;
      l_ID_PAT_PREGNANCY ID_PAT_PREGNANCY_cc;
      l_ID_PROF_READ ID_PROF_READ_cc;
      l_ID_INSTITUTION_READ ID_INSTITUTION_READ_cc;
      l_ID_SOFTWARE_READ ID_SOFTWARE_READ_cc;
      l_DT_EVENT_MOST_FREQ_TSTZ DT_EVENT_MOST_FREQ_TSTZ_cc;
      l_ID_EPISODE ID_EPISODE_cc;
      l_CREATE_USER CREATE_USER_cc;
      l_CREATE_TIME CREATE_TIME_cc;
      l_CREATE_INSTITUTION CREATE_INSTITUTION_cc;
      l_UPDATE_USER UPDATE_USER_cc;
      l_UPDATE_TIME UPDATE_TIME_cc;
      l_UPDATE_INSTITUTION UPDATE_INSTITUTION_cc;
      l_ID_PAT_PREGN_FETUS ID_PAT_PREGN_FETUS_cc;
      l_FLG_STATUS FLG_STATUS_cc;
      l_DT_CANCEL DT_CANCEL_cc;
      l_ID_PROF_CANCEL ID_PROF_CANCEL_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_EVENT_MOST_FREQ(i) := col_in(i).ID_EVENT_MOST_FREQ;
         l_ID_GROUP(i) := col_in(i).ID_GROUP;
         l_FLG_GROUP(i) := col_in(i).FLG_GROUP;
         l_ID_PATIENT(i) := col_in(i).ID_PATIENT;
         l_VALUE(i) := col_in(i).VALUE;
         l_ID_UNIT_MEASURE(i) := col_in(i).ID_UNIT_MEASURE;
         l_ID_PAT_PREGNANCY(i) := col_in(i).ID_PAT_PREGNANCY;
         l_ID_PROF_READ(i) := col_in(i).ID_PROF_READ;
         l_ID_INSTITUTION_READ(i) := col_in(i).ID_INSTITUTION_READ;
         l_ID_SOFTWARE_READ(i) := col_in(i).ID_SOFTWARE_READ;
         l_DT_EVENT_MOST_FREQ_TSTZ(i) := col_in(i).DT_EVENT_MOST_FREQ_TSTZ;
         l_ID_EPISODE(i) := col_in(i).ID_EPISODE;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
         l_ID_PAT_PREGN_FETUS(i) := col_in(i).ID_PAT_PREGN_FETUS;
         l_FLG_STATUS(i) := col_in(i).FLG_STATUS;
         l_DT_CANCEL(i) := col_in(i).DT_CANCEL;
         l_ID_PROF_CANCEL(i) := col_in(i).ID_PROF_CANCEL;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE EVENT_MOST_FREQ SET
               ID_GROUP = NVL (l_ID_GROUP(i), ID_GROUP),
               FLG_GROUP = NVL (l_FLG_GROUP(i), FLG_GROUP),
               ID_PATIENT = NVL (l_ID_PATIENT(i), ID_PATIENT),
               VALUE = NVL (l_VALUE(i), VALUE),
               ID_UNIT_MEASURE = NVL (l_ID_UNIT_MEASURE(i), ID_UNIT_MEASURE),
               ID_PAT_PREGNANCY = NVL (l_ID_PAT_PREGNANCY(i), ID_PAT_PREGNANCY),
               ID_PROF_READ = NVL (l_ID_PROF_READ(i), ID_PROF_READ),
               ID_INSTITUTION_READ = NVL (l_ID_INSTITUTION_READ(i), ID_INSTITUTION_READ),
               ID_SOFTWARE_READ = NVL (l_ID_SOFTWARE_READ(i), ID_SOFTWARE_READ),
               DT_EVENT_MOST_FREQ_TSTZ = NVL (l_DT_EVENT_MOST_FREQ_TSTZ(i), DT_EVENT_MOST_FREQ_TSTZ),
               ID_EPISODE = NVL (l_ID_EPISODE(i), ID_EPISODE),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
               UPDATE_INSTITUTION = NVL (l_UPDATE_INSTITUTION(i), UPDATE_INSTITUTION),
               ID_PAT_PREGN_FETUS = NVL (l_ID_PAT_PREGN_FETUS(i), ID_PAT_PREGN_FETUS),
               FLG_STATUS = NVL (l_FLG_STATUS(i), FLG_STATUS),
               DT_CANCEL = NVL (l_DT_CANCEL(i), DT_CANCEL),
               ID_PROF_CANCEL = NVL (l_ID_PROF_CANCEL(i), ID_PROF_CANCEL)
             WHERE
                ID_EVENT_MOST_FREQ = l_ID_EVENT_MOST_FREQ(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE EVENT_MOST_FREQ SET
               ID_GROUP = l_ID_GROUP(i),
               FLG_GROUP = l_FLG_GROUP(i),
               ID_PATIENT = l_ID_PATIENT(i),
               VALUE = l_VALUE(i),
               ID_UNIT_MEASURE = l_ID_UNIT_MEASURE(i),
               ID_PAT_PREGNANCY = l_ID_PAT_PREGNANCY(i),
               ID_PROF_READ = l_ID_PROF_READ(i),
               ID_INSTITUTION_READ = l_ID_INSTITUTION_READ(i),
               ID_SOFTWARE_READ = l_ID_SOFTWARE_READ(i),
               DT_EVENT_MOST_FREQ_TSTZ = l_DT_EVENT_MOST_FREQ_TSTZ(i),
               ID_EPISODE = l_ID_EPISODE(i),
               CREATE_USER = l_CREATE_USER(i),
               CREATE_TIME = l_CREATE_TIME(i),
               CREATE_INSTITUTION = l_CREATE_INSTITUTION(i),
               UPDATE_USER = l_UPDATE_USER(i),
               UPDATE_TIME = l_UPDATE_TIME(i),
               UPDATE_INSTITUTION = l_UPDATE_INSTITUTION(i),
               ID_PAT_PREGN_FETUS = l_ID_PAT_PREGN_FETUS(i),
               FLG_STATUS = l_FLG_STATUS(i),
               DT_CANCEL = l_DT_CANCEL(i),
               ID_PROF_CANCEL = l_ID_PROF_CANCEL(i)
             WHERE
                ID_EVENT_MOST_FREQ = l_ID_EVENT_MOST_FREQ(i)
        returning rowid bulk collect into rows_out  ;
      END IF;
   END upd;


