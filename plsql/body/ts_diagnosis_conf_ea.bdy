    PROCEDURE upd
    (
        col_in            IN diagnosis_conf_ea_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_flg_terminology         flg_terminology_cc;
        l_id_language             id_language_cc;
        l_id_task_type            id_task_type_cc;
        l_task_type_internal_name task_type_internal_name_cc;
        l_id_institution          id_institution_cc;
        l_id_software             id_software_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_flg_terminology(i) := col_in(i).flg_terminology;
            l_id_language(i) := col_in(i).id_language;
            l_id_task_type(i) := col_in(i).id_task_type;
            l_task_type_internal_name(i) := col_in(i).task_type_internal_name;
            l_id_institution(i) := col_in(i).id_institution;
            l_id_software(i) := col_in(i).id_software;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE diagnosis_conf_ea
                   SET id_language             = nvl(l_id_language(i), id_language),
                       task_type_internal_name = nvl(l_task_type_internal_name(i), task_type_internal_name)
                 WHERE flg_terminology = l_flg_terminology(i)
                   AND id_task_type = l_id_task_type(i)
                   AND id_institution = l_id_institution(i)
                   AND id_software = l_id_software(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE diagnosis_conf_ea
                   SET id_language = l_id_language(i), task_type_internal_name = l_task_type_internal_name(i)
                 WHERE flg_terminology = l_flg_terminology(i)
                   AND id_task_type = l_id_task_type(i)
                   AND id_institution = l_id_institution(i)
                   AND id_software = l_id_software(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    END upd;

    PROCEDURE upd
    (
        col_in            IN diagnosis_conf_ea_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE
    ) IS
        rows_out table_varchar;
    BEGIN
        upd(col_in, ignore_if_null_in, handle_error_in, rows_out);
    END upd;

