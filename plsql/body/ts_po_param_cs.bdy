        po_param_cs_inout.create_user         := NULL;
        po_param_cs_inout.create_time         := NULL;
        po_param_cs_inout.create_institution  := NULL;
        po_param_cs_inout.update_user         := NULL;
        po_param_cs_inout.update_time         := NULL;
