        l_update_user_n            NUMBER(1);
        l_update_time_n            NUMBER(1);
        l_update_institution_n     NUMBER(1);
        l_id_epis_diagnosis_n      NUMBER(1);
        id_adm_req_diagnosis_in    adm_req_diagnosis.id_adm_req_diagnosis%TYPE;
    BEGIN
    
        l_id_adm_request_n         := sys.diutil.bool_to_int(id_adm_request_nin);
        l_id_professional_diag_n   := sys.diutil.bool_to_int(id_professional_diag_nin);
