    ) IS
        l_sql                      VARCHAR2(32767);
        l_rows_out                 table_varchar;
        l_id_adm_request_n         NUMBER(1);
        l_id_professional_diag_n   NUMBER(1);
        l_flg_status_n             NUMBER(1);
