    PROCEDURE upd
    (
        col_in            IN adm_req_diagnosis_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_adm_req_diagnosis   id_adm_req_diagnosis_cc;
        l_id_adm_request         id_adm_request_cc;
        l_id_professional_diag   id_professional_diag_cc;
        l_flg_status             flg_status_cc;
        l_flg_diag_status        flg_diag_status_cc;
        l_dt_epis_diagnosis      dt_epis_diagnosis_cc;
        l_notes                  notes_cc;
        l_id_professional_update id_professional_update_cc;
        l_dt_update              dt_update_cc;
        l_create_user            create_user_cc;
        l_create_time            create_time_cc;
        l_create_institution     create_institution_cc;
        l_update_user            update_user_cc;
        l_update_time            update_time_cc;
        l_update_institution     update_institution_cc;
        l_id_epis_diagnosis      id_epis_diagnosis_cc;
    BEGIN
        FOR i IN col_in.first .. col_in.last
        LOOP
            l_id_adm_req_diagnosis(i) := col_in(i).id_adm_req_diagnosis;
            l_id_adm_request(i) := col_in(i).id_adm_request;
            l_id_professional_diag(i) := col_in(i).id_professional_diag;
            l_flg_status(i) := col_in(i).flg_status;
            l_flg_diag_status(i) := col_in(i).flg_diag_status;
            l_dt_epis_diagnosis(i) := col_in(i).dt_epis_diagnosis;
            l_notes(i) := col_in(i).notes;
            l_id_professional_update(i) := col_in(i).id_professional_update;
            l_dt_update(i) := col_in(i).dt_update;
            l_create_user(i) := col_in(i).create_user;
            l_create_time(i) := col_in(i).create_time;
            l_create_institution(i) := col_in(i).create_institution;
            l_update_user(i) := col_in(i).update_user;
            l_update_time(i) := col_in(i).update_time;
            l_update_institution(i) := col_in(i).update_institution;
            l_id_epis_diagnosis(i) := col_in(i).id_epis_diagnosis;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.first .. col_in.last
                UPDATE adm_req_diagnosis
                   SET id_adm_request         = nvl(l_id_adm_request(i), id_adm_request),
                       id_professional_diag   = nvl(l_id_professional_diag(i), id_professional_diag),
                       flg_status             = nvl(l_flg_status(i), flg_status),
                       flg_diag_status        = nvl(l_flg_diag_status(i), flg_diag_status),
                       dt_epis_diagnosis      = nvl(l_dt_epis_diagnosis(i), dt_epis_diagnosis),
                       notes                  = nvl(l_notes(i), notes),
                       id_professional_update = nvl(l_id_professional_update(i), id_professional_update),
                       dt_update              = nvl(l_dt_update(i), dt_update),
                       create_user            = nvl(l_create_user(i), create_user),
                       create_time            = nvl(l_create_time(i), create_time),
                       create_institution     = nvl(l_create_institution(i), create_institution),
                       update_user            = nvl(l_update_user(i), update_user),
                       update_time            = nvl(l_update_time(i), update_time),
                       update_institution     = nvl(l_update_institution(i), update_institution),
                       id_epis_diagnosis      = nvl(l_id_epis_diagnosis(i), id_epis_diagnosis)
                 WHERE id_adm_req_diagnosis = l_id_adm_req_diagnosis(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        ELSE
            FORALL i IN col_in.first .. col_in.last
                UPDATE adm_req_diagnosis
                   SET id_adm_request         = l_id_adm_request(i),
                       id_professional_diag   = l_id_professional_diag(i),
                       flg_status             = l_flg_status(i),
                       flg_diag_status        = l_flg_diag_status(i),
                       dt_epis_diagnosis      = l_dt_epis_diagnosis(i),
                       notes                  = l_notes(i),
                       id_professional_update = l_id_professional_update(i),
                       dt_update              = l_dt_update(i),
                       create_user            = l_create_user(i),
                       create_time            = l_create_time(i),
                       create_institution     = l_create_institution(i),
                       update_user            = l_update_user(i),
                       update_time            = l_update_time(i),
                       update_institution     = l_update_institution(i),
                       id_epis_diagnosis      = l_id_epis_diagnosis(i)
                 WHERE id_adm_req_diagnosis = l_id_adm_req_diagnosis(i)
                RETURNING ROWID BULK COLLECT INTO rows_out;
        END IF;
    END upd;

    PROCEDURE upd
    (
        col_in            IN adm_req_diagnosis_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE
    ) IS
        rows_out table_varchar;
    BEGIN
        upd(col_in, ignore_if_null_in, handle_error_in, rows_out);
