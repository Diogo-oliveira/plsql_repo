               ID_PROF_WRITES = NVL (l_ID_PROF_WRITES(i), ID_PROF_WRITES),
               ID_PROF_UPD = NVL (l_ID_PROF_UPD(i), ID_PROF_UPD),
               ID_PROF_CANCEL = NVL (l_ID_PROF_CANCEL(i), ID_PROF_CANCEL),
               NOTES_CANCEL = NVL (l_NOTES_CANCEL(i), NOTES_CANCEL),
