   BEGIN
      ins (
      rows_in => rows_in
     ,handle_error_in => handle_error_in
     , rows_out => rows_out
   );
   END ins;


PROCEDURE upd (
      id_cdr_event_in IN CDR_EVENT.ID_CDR_EVENT%TYPE,
      id_cdr_call_in IN CDR_EVENT.ID_CDR_CALL%TYPE DEFAULT NULL,
      ID_CDR_CALL_nin IN BOOLEAN := TRUE,
      id_cdr_inst_par_action_in IN CDR_EVENT.ID_CDR_INST_PAR_ACTION%TYPE DEFAULT NULL,
      ID_CDR_INST_PAR_ACTION_nin IN BOOLEAN := TRUE,
      flg_hidden_in IN CDR_EVENT.FLG_HIDDEN%TYPE DEFAULT NULL,
      FLG_HIDDEN_nin IN BOOLEAN := TRUE,
      flg_session_in IN CDR_EVENT.FLG_SESSION%TYPE DEFAULT NULL,
      FLG_SESSION_nin IN BOOLEAN := TRUE,
      id_prof_answer_in IN CDR_EVENT.ID_PROF_ANSWER%TYPE DEFAULT NULL,
      ID_PROF_ANSWER_nin IN BOOLEAN := TRUE,
      dt_answer_in IN CDR_EVENT.DT_ANSWER%TYPE DEFAULT NULL,
      DT_ANSWER_nin IN BOOLEAN := TRUE,
      flg_answer_in IN CDR_EVENT.FLG_ANSWER%TYPE DEFAULT NULL,
      FLG_ANSWER_nin IN BOOLEAN := TRUE,
      notes_answer_in IN CDR_EVENT.NOTES_ANSWER%TYPE DEFAULT NULL,
      NOTES_ANSWER_nin IN BOOLEAN := TRUE,
      create_user_in IN CDR_EVENT.CREATE_USER%TYPE DEFAULT NULL,
      CREATE_USER_nin IN BOOLEAN := TRUE,
      create_time_in IN CDR_EVENT.CREATE_TIME%TYPE DEFAULT NULL,
      CREATE_TIME_nin IN BOOLEAN := TRUE,
      create_institution_in IN CDR_EVENT.CREATE_INSTITUTION%TYPE DEFAULT NULL,
      CREATE_INSTITUTION_nin IN BOOLEAN := TRUE,
      update_user_in IN CDR_EVENT.UPDATE_USER%TYPE DEFAULT NULL,
      UPDATE_USER_nin IN BOOLEAN := TRUE,
      update_time_in IN CDR_EVENT.UPDATE_TIME%TYPE DEFAULT NULL,
      UPDATE_TIME_nin IN BOOLEAN := TRUE,
      update_institution_in IN CDR_EVENT.UPDATE_INSTITUTION%TYPE DEFAULT NULL,
      UPDATE_INSTITUTION_nin IN BOOLEAN := TRUE,
      id_cdr_answer_in IN CDR_EVENT.ID_CDR_ANSWER%TYPE DEFAULT NULL,
      ID_CDR_ANSWER_nin IN BOOLEAN := TRUE,
      id_cdr_external_in IN CDR_EVENT.ID_CDR_EXTERNAL%TYPE DEFAULT NULL,
      ID_CDR_EXTERNAL_nin IN BOOLEAN := TRUE,
      domain_value_in IN CDR_EVENT.DOMAIN_VALUE%TYPE DEFAULT NULL,
      DOMAIN_VALUE_nin IN BOOLEAN := TRUE,
      domain_free_text_in IN CDR_EVENT.DOMAIN_FREE_TEXT%TYPE DEFAULT NULL,
      DOMAIN_FREE_TEXT_nin IN BOOLEAN := TRUE,
     handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
   l_rows_out TABLE_VARCHAR;
   l_ID_CDR_CALL_n NUMBER(1);
   l_ID_CDR_INST_PAR_ACTION_n NUMBER(1);
   l_FLG_HIDDEN_n NUMBER(1);
   l_FLG_SESSION_n NUMBER(1);
   l_ID_PROF_ANSWER_n NUMBER(1);
   l_DT_ANSWER_n NUMBER(1);
   l_FLG_ANSWER_n NUMBER(1);
   l_NOTES_ANSWER_n NUMBER(1);
   l_CREATE_USER_n NUMBER(1);
   l_CREATE_TIME_n NUMBER(1);
   l_CREATE_INSTITUTION_n NUMBER(1);
   l_UPDATE_USER_n NUMBER(1);
   l_UPDATE_TIME_n NUMBER(1);
   l_UPDATE_INSTITUTION_n NUMBER(1);
   l_ID_CDR_ANSWER_n NUMBER(1);
   l_ID_CDR_EXTERNAL_n NUMBER(1);
   l_DOMAIN_VALUE_n NUMBER(1);
   l_DOMAIN_FREE_TEXT_n NUMBER(1);
   BEGIN

   l_ID_CDR_CALL_n := sys.diutil.bool_to_int(ID_CDR_CALL_nin);
   l_ID_CDR_INST_PAR_ACTION_n := sys.diutil.bool_to_int(ID_CDR_INST_PAR_ACTION_nin);
   l_FLG_HIDDEN_n := sys.diutil.bool_to_int(FLG_HIDDEN_nin);
   l_FLG_SESSION_n := sys.diutil.bool_to_int(FLG_SESSION_nin);
   l_ID_PROF_ANSWER_n := sys.diutil.bool_to_int(ID_PROF_ANSWER_nin);
   l_DT_ANSWER_n := sys.diutil.bool_to_int(DT_ANSWER_nin);
   l_FLG_ANSWER_n := sys.diutil.bool_to_int(FLG_ANSWER_nin);
   l_NOTES_ANSWER_n := sys.diutil.bool_to_int(NOTES_ANSWER_nin);
   l_CREATE_USER_n := sys.diutil.bool_to_int(CREATE_USER_nin);
   l_CREATE_TIME_n := sys.diutil.bool_to_int(CREATE_TIME_nin);
   l_CREATE_INSTITUTION_n := sys.diutil.bool_to_int(CREATE_INSTITUTION_nin);
   l_UPDATE_USER_n := sys.diutil.bool_to_int(UPDATE_USER_nin);
   l_UPDATE_TIME_n := sys.diutil.bool_to_int(UPDATE_TIME_nin);
   l_UPDATE_INSTITUTION_n := sys.diutil.bool_to_int(UPDATE_INSTITUTION_nin);
   l_ID_CDR_ANSWER_n := sys.diutil.bool_to_int(ID_CDR_ANSWER_nin);
   l_ID_CDR_EXTERNAL_n := sys.diutil.bool_to_int(ID_CDR_EXTERNAL_nin);
   l_DOMAIN_VALUE_n := sys.diutil.bool_to_int(DOMAIN_VALUE_nin);
   l_DOMAIN_FREE_TEXT_n := sys.diutil.bool_to_int(DOMAIN_FREE_TEXT_nin);


         UPDATE CDR_EVENT SET
     ID_CDR_CALL = decode (l_ID_CDR_CALL_n,0,id_cdr_call_in, NVL (id_cdr_call_in, ID_CDR_CALL)),
     ID_CDR_INST_PAR_ACTION = decode (l_ID_CDR_INST_PAR_ACTION_n,0,id_cdr_inst_par_action_in, NVL (id_cdr_inst_par_action_in, ID_CDR_INST_PAR_ACTION)),
     FLG_HIDDEN = decode (l_FLG_HIDDEN_n,0,flg_hidden_in, NVL (flg_hidden_in, FLG_HIDDEN)),
     FLG_SESSION = decode (l_FLG_SESSION_n,0,flg_session_in, NVL (flg_session_in, FLG_SESSION)),
     ID_PROF_ANSWER = decode (l_ID_PROF_ANSWER_n,0,id_prof_answer_in, NVL (id_prof_answer_in, ID_PROF_ANSWER)),
     DT_ANSWER = decode (l_DT_ANSWER_n,0,dt_answer_in, NVL (dt_answer_in, DT_ANSWER)),
     FLG_ANSWER = decode (l_FLG_ANSWER_n,0,flg_answer_in, NVL (flg_answer_in, FLG_ANSWER)),
     NOTES_ANSWER = decode (l_NOTES_ANSWER_n,0,notes_answer_in, NVL (notes_answer_in, NOTES_ANSWER)),
     CREATE_USER = decode (l_CREATE_USER_n,0,create_user_in, NVL (create_user_in, CREATE_USER)),
     CREATE_TIME = decode (l_CREATE_TIME_n,0,create_time_in, NVL (create_time_in, CREATE_TIME)),
     CREATE_INSTITUTION = decode (l_CREATE_INSTITUTION_n,0,create_institution_in, NVL (create_institution_in, CREATE_INSTITUTION)),
     UPDATE_USER = decode (l_UPDATE_USER_n,0,update_user_in, NVL (update_user_in, UPDATE_USER)),
     UPDATE_TIME = decode (l_UPDATE_TIME_n,0,update_time_in, NVL (update_time_in, UPDATE_TIME)),
     UPDATE_INSTITUTION = decode (l_UPDATE_INSTITUTION_n,0,update_institution_in, NVL (update_institution_in, UPDATE_INSTITUTION)),
     ID_CDR_ANSWER = decode (l_ID_CDR_ANSWER_n,0,id_cdr_answer_in, NVL (id_cdr_answer_in, ID_CDR_ANSWER)),
     ID_CDR_EXTERNAL = decode (l_ID_CDR_EXTERNAL_n,0,id_cdr_external_in, NVL (id_cdr_external_in, ID_CDR_EXTERNAL)),
     DOMAIN_VALUE = decode (l_DOMAIN_VALUE_n,0,domain_value_in, NVL (domain_value_in, DOMAIN_VALUE)),
     DOMAIN_FREE_TEXT = decode (l_DOMAIN_FREE_TEXT_n,0,domain_free_text_in, NVL (domain_free_text_in, DOMAIN_FREE_TEXT))
          WHERE
             ID_CDR_EVENT = id_cdr_event_in
         RETURNING ROWID BULK COLLECT INTO l_rows_out;


if(rows_out is null)
then
rows_out := table_varchar();
end if;

rows_out :=  rows_out MULTISET UNION DISTINCT l_rows_out;

   EXCEPTION
      WHEN DUP_VAL_ON_INDEX
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
           l_err_instance_id PLS_INTEGER;
        BEGIN
           get_constraint_info (l_owner, l_name);
           IF FALSE THEN NULL; -- Placeholder in case no unique indexes
           ELSIF l_name = 'CDRE_UK_CIPA'
           THEN
              pk_alert_exceptions.register_error (
                 error_name_in => 'DUPLICATE-VALUE'
                ,err_instance_id_out => l_err_instance_id
                ,text_in => 'Violation of unique index "CDRE_UK_CIPA"'
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'OWNER'
                ,value_in => l_owner
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'CONSTRAINT_NAME'
                ,value_in => l_name
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'TABLE_NAME'
                ,value_in => 'CDR_EVENT'
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'ID_CDR_EVENT'
                ,value_in => id_cdr_event_in
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'ID_CDR_INST_PAR_ACTION'
                ,value_in => id_cdr_inst_par_action_in
                ,validate_in => FALSE
                );
              pk_alert_exceptions.raise_error_instance (
                    err_instance_id_in => l_err_instance_id);
           ELSIF l_name = 'CDRE_UK_EXTERNAL'
           THEN
              pk_alert_exceptions.register_error (
                 error_name_in => 'DUPLICATE-VALUE'
                ,err_instance_id_out => l_err_instance_id
                ,text_in => 'Violation of unique index "CDRE_UK_EXTERNAL"'
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'OWNER'
                ,value_in => l_owner
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'CONSTRAINT_NAME'
                ,value_in => l_name
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'TABLE_NAME'
                ,value_in => 'CDR_EVENT'
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'ID_CDR_EVENT'
                ,value_in => id_cdr_event_in
                ,validate_in => FALSE
                );
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_err_instance_id
                ,NAME_IN => 'ID_CDR_EXTERNAL'
                ,value_in => id_cdr_external_in
                ,validate_in => FALSE
                );
              pk_alert_exceptions.raise_error_instance (
                    err_instance_id_in => l_err_instance_id);
           ELSE
              pk_alert_exceptions.raise_error (
                    error_name_in => 'DUPLICATE-VALUE'
                    ,name1_in => 'OWNER'
                    ,value1_in => l_owner
                    ,name2_in => 'CONSTRAINT_NAME'
                    ,value2_in => l_name
                    ,name3_in => 'TABLE_NAME'
                    ,value3_in => 'CDR_EVENT');
           END IF;
        END;
        END IF;
      WHEN e_check_constraint_failure
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
        BEGIN
           get_constraint_info (l_owner, l_name);
           pk_alert_exceptions.raise_error (
              error_name_in => 'CHECK-CONSTRAINT-FAILURE'
             ,name1_in => 'OWNER'
             ,value1_in => l_owner
             ,name2_in => 'CONSTRAINT_NAME'
             ,value2_in => l_name
             ,name3_in => 'TABLE_NAME'
             ,value3_in => 'CDR_EVENT');
        END;
        END IF;
      WHEN e_integ_constraint_failure OR e_no_parent_key OR e_child_record_found
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           l_owner ALL_CONSTRAINTS.OWNER%TYPE;
           l_name ALL_CONSTRAINTS.CONSTRAINT_NAME%TYPE;
           l_id PLS_INTEGER;
           l_err_name VARCHAR2(32767) DEFAULT 'NO-PARENT-KEY-FOUND';
        BEGIN
           get_constraint_info (l_owner, l_name);
           IF SQLCODE = -2292 -- Child record found
           THEN
              l_err_name := 'CHILD-RECORD-FOUND' ;
           END IF;
           pk_alert_exceptions.register_error (
              error_name_in => l_err_name
             ,err_instance_id_out => l_id
             ,name1_in => 'OWNER', value1_in => l_owner
             ,name2_in => 'CONSTRAINT_NAME', value2_in => l_name
             ,name3_in => 'TABLE_NAME', value3_in => 'CDR_EVENT');
           IF l_name = 'CDRE_CDRAW_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_ANSWER'
               , value_in => id_cdr_answer_in);
           END IF;
           IF l_name = 'CDRE_CDRIPA_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_INST_PAR_ACTION'
               , value_in => id_cdr_inst_par_action_in);
           END IF;
           IF l_name = 'CDRE_CDRL_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_CDR_CALL'
               , value_in => id_cdr_call_in);
           END IF;
           IF l_name = 'CDRE_PROF_FK'
           THEN
              -- Add a context value for each column
              pk_alert_exceptions.add_context (
                 err_instance_id_in => l_id
               , name_in => 'ID_PROF_ANSWER'
               , value_in => id_prof_answer_in);
           END IF;
           pk_alert_exceptions.raise_error_instance (err_instance_id_in => l_id);
        END;
        END IF;
      WHEN e_null_column_value
      THEN
        IF NOT handle_error_in THEN RAISE;
        ELSE
        DECLARE
           v_errm VARCHAR2(2000) := DBMS_UTILITY.FORMAT_ERROR_STACK;
           dot1loc INTEGER;
           dot2loc INTEGER;
           parenloc INTEGER;
           c_owner ALL_CONSTRAINTS.OWNER%TYPE;
           c_tabname ALL_TABLES.TABLE_NAME%TYPE;
           c_colname ALL_TAB_COLUMNS.COLUMN_NAME%TYPE;
        BEGIN
           dot1loc := INSTR (v_errm, '.', 1, 1);
           dot2loc := INSTR (v_errm, '.', 1, 2);
           parenloc := INSTR (v_errm, '(');
           c_owner :=SUBSTR (v_errm, parenloc+1, dot1loc-parenloc-1);
           c_tabname := SUBSTR (v_errm, dot1loc+1, dot2loc-dot1loc-1);
           c_colname := SUBSTR (v_errm, dot2loc+1, INSTR (v_errm,')')-dot2loc-1);

           pk_alert_exceptions.raise_error (
                error_name_in => 'COLUMN-CANNOT-BE-NULL'
               ,name1_in => 'OWNER'
               ,value1_in => c_owner
               ,name2_in => 'TABLE_NAME'
               ,value2_in => c_tabname
               ,name3_in => 'COLUMN_NAME'
               ,value3_in => c_colname);
        END;
        END IF;
   END upd;


