      create_institution_in IN CDR_EVENT.CREATE_INSTITUTION%TYPE DEFAULT NULL,
      update_user_in IN CDR_EVENT.UPDATE_USER%TYPE DEFAULT NULL,
      update_time_in IN CDR_EVENT.UPDATE_TIME%TYPE DEFAULT NULL,
      update_institution_in IN CDR_EVENT.UPDATE_INSTITUTION%TYPE DEFAULT NULL,
      id_cdr_answer_in IN CDR_EVENT.ID_CDR_ANSWER%TYPE DEFAULT NULL,
      id_cdr_external_in IN CDR_EVENT.ID_CDR_EXTERNAL%TYPE DEFAULT NULL,
      domain_value_in IN CDR_EVENT.DOMAIN_VALUE%TYPE DEFAULT NULL,
      domain_free_text_in IN CDR_EVENT.DOMAIN_FREE_TEXT%TYPE DEFAULT NULL,
      handle_error_in IN BOOLEAN := TRUE,
      clob_columns_in IN varchar2_t,
      clob_pieces_in IN  varchar2_t
   ) IS
        l_NOTES_ANSWER clob;
      i                PLS_INTEGER;
      current_column varchar2(30) := '';
   BEGIN
      /* Be aware, PLSQL limitations on temporary clobs prevent multiple CLOB
         variables from pointing to the same temporary CLOB (it creates copies).
         Otherwise, the code could have been written a bit more compactly. */

      FOR i IN clob_columns_in.FIRST .. clob_columns_in.LAST
      LOOP
         /* Even when all clobs are null, DOA must send 1 row so skip it. */
         IF clob_columns_in (i) IS NOT NULL
         THEN
            IF current_column <> clob_columns_in (i) OR current_column IS NULL
            THEN
               current_column := LOWER (clob_columns_in (i));

               CASE current_column
                  WHEN 'notes_answer_in'
                  THEN
                     IF l_NOTES_ANSWER IS NULL
                     THEN
                       DBMS_LOB.createtemporary (
                         l_NOTES_ANSWER, TRUE, DBMS_LOB.CALL);
                     END IF;
               END CASE;
            END IF;

            CASE current_column
              WHEN 'notes_answer_in'
              THEN
                  DBMS_LOB.writeappend (l_NOTES_ANSWER
                                      , LENGTH (clob_pieces_in (i))
                                      , clob_pieces_in (i)
                                       );
            END CASE;
         END IF;
      END LOOP;

      ins (
         id_cdr_event_in => id_cdr_event_in,
         id_cdr_call_in => id_cdr_call_in,
         id_cdr_inst_par_action_in => id_cdr_inst_par_action_in,
         flg_hidden_in => flg_hidden_in,
         flg_session_in => flg_session_in,
         id_prof_answer_in => id_prof_answer_in,
         dt_answer_in => dt_answer_in,
         flg_answer_in => flg_answer_in,
         notes_answer_in => l_NOTES_ANSWER,
         create_user_in => create_user_in,
         create_time_in => create_time_in,
         create_institution_in => create_institution_in,
         update_user_in => update_user_in,
         update_time_in => update_time_in,
         update_institution_in => update_institution_in,
         id_cdr_answer_in => id_cdr_answer_in,
         id_cdr_external_in => id_cdr_external_in,
         domain_value_in => domain_value_in,
         domain_free_text_in => domain_free_text_in,
         handle_error_in => handle_error_in
         );

   END n_ins_clobs_in_chunks;

