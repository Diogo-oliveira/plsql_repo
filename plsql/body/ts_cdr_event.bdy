               )
            VALUES (
               rows_in(indx).ID_CDR_CALL,
               rows_in(indx).ID_CDR_INST_PAR_ACTION,
               rows_in(indx).FLG_HIDDEN,
               rows_in(indx).FLG_SESSION,
