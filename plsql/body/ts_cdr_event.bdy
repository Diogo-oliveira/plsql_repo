   PROCEDURE upd (
      col_in IN CDR_EVENT_tc,
      ignore_if_null_in IN BOOLEAN := TRUE
     ,handle_error_in IN BOOLEAN := TRUE
     , rows_out IN OUT TABLE_VARCHAR
      )
   IS
      l_ID_CDR_CALL ID_CDR_CALL_cc;
      l_ID_CDR_INST_PAR_ACTION ID_CDR_INST_PAR_ACTION_cc;
      l_FLG_HIDDEN FLG_HIDDEN_cc;
      l_FLG_SESSION FLG_SESSION_cc;
      l_ID_PROF_ANSWER ID_PROF_ANSWER_cc;
      l_DT_ANSWER DT_ANSWER_cc;
      l_FLG_ANSWER FLG_ANSWER_cc;
      l_NOTES_ANSWER NOTES_ANSWER_cc;
      l_CREATE_USER CREATE_USER_cc;
      l_CREATE_TIME CREATE_TIME_cc;
      l_CREATE_INSTITUTION CREATE_INSTITUTION_cc;
      l_UPDATE_USER UPDATE_USER_cc;
      l_UPDATE_TIME UPDATE_TIME_cc;
      l_UPDATE_INSTITUTION UPDATE_INSTITUTION_cc;
      l_ID_CDR_ANSWER ID_CDR_ANSWER_cc;
      l_ID_CDR_EVENT ID_CDR_EVENT_cc;
      l_ID_CDR_EXTERNAL ID_CDR_EXTERNAL_cc;
      l_DOMAIN_VALUE DOMAIN_VALUE_cc;
      l_DOMAIN_FREE_TEXT DOMAIN_FREE_TEXT_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_CDR_CALL(i) := col_in(i).ID_CDR_CALL;
         l_ID_CDR_INST_PAR_ACTION(i) := col_in(i).ID_CDR_INST_PAR_ACTION;
         l_FLG_HIDDEN(i) := col_in(i).FLG_HIDDEN;
         l_FLG_SESSION(i) := col_in(i).FLG_SESSION;
         l_ID_PROF_ANSWER(i) := col_in(i).ID_PROF_ANSWER;
         l_DT_ANSWER(i) := col_in(i).DT_ANSWER;
         l_FLG_ANSWER(i) := col_in(i).FLG_ANSWER;
         l_NOTES_ANSWER(i) := col_in(i).NOTES_ANSWER;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
         l_ID_CDR_ANSWER(i) := col_in(i).ID_CDR_ANSWER;
         l_ID_CDR_EVENT(i) := col_in(i).ID_CDR_EVENT;
         l_ID_CDR_EXTERNAL(i) := col_in(i).ID_CDR_EXTERNAL;
         l_DOMAIN_VALUE(i) := col_in(i).DOMAIN_VALUE;
         l_DOMAIN_FREE_TEXT(i) := col_in(i).DOMAIN_FREE_TEXT;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE CDR_EVENT SET
               ID_CDR_CALL = NVL (l_ID_CDR_CALL(i), ID_CDR_CALL),
               ID_CDR_INST_PAR_ACTION = NVL (l_ID_CDR_INST_PAR_ACTION(i), ID_CDR_INST_PAR_ACTION),
               FLG_HIDDEN = NVL (l_FLG_HIDDEN(i), FLG_HIDDEN),
               FLG_SESSION = NVL (l_FLG_SESSION(i), FLG_SESSION),
               ID_PROF_ANSWER = NVL (l_ID_PROF_ANSWER(i), ID_PROF_ANSWER),
               DT_ANSWER = NVL (l_DT_ANSWER(i), DT_ANSWER),
               FLG_ANSWER = NVL (l_FLG_ANSWER(i), FLG_ANSWER),
               NOTES_ANSWER = NVL (l_NOTES_ANSWER(i), NOTES_ANSWER),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
               UPDATE_INSTITUTION = NVL (l_UPDATE_INSTITUTION(i), UPDATE_INSTITUTION),
               ID_CDR_ANSWER = NVL (l_ID_CDR_ANSWER(i), ID_CDR_ANSWER),
               ID_CDR_EXTERNAL = NVL (l_ID_CDR_EXTERNAL(i), ID_CDR_EXTERNAL),
               DOMAIN_VALUE = NVL (l_DOMAIN_VALUE(i), DOMAIN_VALUE),
               DOMAIN_FREE_TEXT = NVL (l_DOMAIN_FREE_TEXT(i), DOMAIN_FREE_TEXT)
             WHERE
                ID_CDR_EVENT = l_ID_CDR_EVENT(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE CDR_EVENT SET
               ID_CDR_CALL = l_ID_CDR_CALL(i),
               ID_CDR_INST_PAR_ACTION = l_ID_CDR_INST_PAR_ACTION(i),
               FLG_HIDDEN = l_FLG_HIDDEN(i),
               FLG_SESSION = l_FLG_SESSION(i),
               ID_PROF_ANSWER = l_ID_PROF_ANSWER(i),
               DT_ANSWER = l_DT_ANSWER(i),
               FLG_ANSWER = l_FLG_ANSWER(i),
               NOTES_ANSWER = l_NOTES_ANSWER(i),
               CREATE_USER = l_CREATE_USER(i),
               CREATE_TIME = l_CREATE_TIME(i),
               CREATE_INSTITUTION = l_CREATE_INSTITUTION(i),
               UPDATE_USER = l_UPDATE_USER(i),
               UPDATE_TIME = l_UPDATE_TIME(i),
               UPDATE_INSTITUTION = l_UPDATE_INSTITUTION(i),
               ID_CDR_ANSWER = l_ID_CDR_ANSWER(i),
               ID_CDR_EXTERNAL = l_ID_CDR_EXTERNAL(i),
               DOMAIN_VALUE = l_DOMAIN_VALUE(i),
               DOMAIN_FREE_TEXT = l_DOMAIN_FREE_TEXT(i)
             WHERE
                ID_CDR_EVENT = l_ID_CDR_EVENT(i)
        returning rowid bulk collect into rows_out  ;
      END IF;
   END upd;


