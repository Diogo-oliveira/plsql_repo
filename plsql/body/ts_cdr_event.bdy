     || ' ID_CDR_INST_PAR_ACTION = decode (' || l_ID_CDR_INST_PAR_ACTION_n || ',0,:id_cdr_inst_par_action_in, NVL (:id_cdr_inst_par_action_in, ID_CDR_INST_PAR_ACTION)) '|| ','
