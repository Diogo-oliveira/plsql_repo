   PROCEDURE n_ins_clobs_in_chunks (
      id_cdr_event_in IN CDR_EVENT.ID_CDR_EVENT%TYPE,
      id_cdr_call_in IN CDR_EVENT.ID_CDR_CALL%TYPE DEFAULT NULL,
      id_cdr_inst_par_action_in IN CDR_EVENT.ID_CDR_INST_PAR_ACTION%TYPE DEFAULT NULL,
      flg_hidden_in IN CDR_EVENT.FLG_HIDDEN%TYPE DEFAULT 'N',
      flg_session_in IN CDR_EVENT.FLG_SESSION%TYPE DEFAULT 'N',
      id_prof_answer_in IN CDR_EVENT.ID_PROF_ANSWER%TYPE DEFAULT NULL,
      dt_answer_in IN CDR_EVENT.DT_ANSWER%TYPE DEFAULT NULL,
      flg_answer_in IN CDR_EVENT.FLG_ANSWER%TYPE DEFAULT 'U',
      create_user_in IN CDR_EVENT.CREATE_USER%TYPE DEFAULT NULL,
