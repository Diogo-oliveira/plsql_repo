     || ' ID_CDR_ANSWER = decode (' || l_ID_CDR_ANSWER_n || ',0,:id_cdr_answer_in, NVL (:id_cdr_answer_in, ID_CDR_ANSWER)) '|| ','
     || ' ID_CDR_EXTERNAL = decode (' || l_ID_CDR_EXTERNAL_n || ',0,:id_cdr_external_in, NVL (:id_cdr_external_in, ID_CDR_EXTERNAL)) '|| ','
