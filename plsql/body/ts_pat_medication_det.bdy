      l_FLG_UNRELEVANT_MED FLG_UNRELEVANT_MED_cc;
      l_UNRELEVANT_MED_NOTES UNRELEVANT_MED_NOTES_cc;
   BEGIN
      FOR i IN col_in.FIRST .. col_in.LAST loop
         l_ID_PAT_MEDICATION_DET(i) := col_in(i).ID_PAT_MEDICATION_DET;
         l_ID_PATIENT(i) := col_in(i).ID_PATIENT;
         l_FLG_NO_MED(i) := col_in(i).FLG_NO_MED;
         l_FLG_UNKNOWN_MED(i) := col_in(i).FLG_UNKNOWN_MED;
         l_ADW_LAST_UPDATE(i) := col_in(i).ADW_LAST_UPDATE;
         l_ID_PROFESSIONAL(i) := col_in(i).ID_PROFESSIONAL;
         l_NO_MED_NOTES(i) := col_in(i).NO_MED_NOTES;
         l_UNKNOWN_MED_NOTES(i) := col_in(i).UNKNOWN_MED_NOTES;
         l_DT_PAT_MEDICATION_DET_TSTZ(i) := col_in(i).DT_PAT_MEDICATION_DET_TSTZ;
         l_ID_EPISODE(i) := col_in(i).ID_EPISODE;
         l_CREATE_USER(i) := col_in(i).CREATE_USER;
         l_CREATE_TIME(i) := col_in(i).CREATE_TIME;
         l_CREATE_INSTITUTION(i) := col_in(i).CREATE_INSTITUTION;
         l_UPDATE_USER(i) := col_in(i).UPDATE_USER;
         l_UPDATE_TIME(i) := col_in(i).UPDATE_TIME;
         l_UPDATE_INSTITUTION(i) := col_in(i).UPDATE_INSTITUTION;
         l_FLG_UNRELEVANT_MED(i) := col_in(i).FLG_UNRELEVANT_MED;
         l_UNRELEVANT_MED_NOTES(i) := col_in(i).UNRELEVANT_MED_NOTES;
      END LOOP;
      IF NVL (ignore_if_null_in, FALSE)
      THEN
         -- Set any columns to their current values
         -- if incoming value is NULL.
         -- Put WHEN clause on column-level triggers!
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE PAT_MEDICATION_DET SET
               ID_PATIENT = NVL (l_ID_PATIENT(i), ID_PATIENT),
               FLG_NO_MED = NVL (l_FLG_NO_MED(i), FLG_NO_MED),
               FLG_UNKNOWN_MED = NVL (l_FLG_UNKNOWN_MED(i), FLG_UNKNOWN_MED),
               ADW_LAST_UPDATE = NVL (l_ADW_LAST_UPDATE(i), ADW_LAST_UPDATE),
               ID_PROFESSIONAL = NVL (l_ID_PROFESSIONAL(i), ID_PROFESSIONAL),
               NO_MED_NOTES = NVL (l_NO_MED_NOTES(i), NO_MED_NOTES),
               UNKNOWN_MED_NOTES = NVL (l_UNKNOWN_MED_NOTES(i), UNKNOWN_MED_NOTES),
               DT_PAT_MEDICATION_DET_TSTZ = NVL (l_DT_PAT_MEDICATION_DET_TSTZ(i), DT_PAT_MEDICATION_DET_TSTZ),
               ID_EPISODE = NVL (l_ID_EPISODE(i), ID_EPISODE),
               CREATE_USER = NVL (l_CREATE_USER(i), CREATE_USER),
               CREATE_TIME = NVL (l_CREATE_TIME(i), CREATE_TIME),
               CREATE_INSTITUTION = NVL (l_CREATE_INSTITUTION(i), CREATE_INSTITUTION),
               UPDATE_USER = NVL (l_UPDATE_USER(i), UPDATE_USER),
               UPDATE_TIME = NVL (l_UPDATE_TIME(i), UPDATE_TIME),
               UPDATE_INSTITUTION = NVL (l_UPDATE_INSTITUTION(i), UPDATE_INSTITUTION),
               FLG_UNRELEVANT_MED = NVL (l_FLG_UNRELEVANT_MED(i), FLG_UNRELEVANT_MED),
               UNRELEVANT_MED_NOTES = NVL (l_UNRELEVANT_MED_NOTES(i), UNRELEVANT_MED_NOTES)
             WHERE
                ID_PAT_MEDICATION_DET = l_ID_PAT_MEDICATION_DET(i)
          returning rowid bulk collect into rows_out;
      ELSE
         FORALL i IN col_in.FIRST .. col_in.LAST
            UPDATE PAT_MEDICATION_DET SET
               ID_PATIENT = l_ID_PATIENT(i),
               FLG_NO_MED = l_FLG_NO_MED(i),
               FLG_UNKNOWN_MED = l_FLG_UNKNOWN_MED(i),
               ADW_LAST_UPDATE = l_ADW_LAST_UPDATE(i),
               ID_PROFESSIONAL = l_ID_PROFESSIONAL(i),
               NO_MED_NOTES = l_NO_MED_NOTES(i),
               UNKNOWN_MED_NOTES = l_UNKNOWN_MED_NOTES(i),
               DT_PAT_MEDICATION_DET_TSTZ = l_DT_PAT_MEDICATION_DET_TSTZ(i),
               ID_EPISODE = l_ID_EPISODE(i),
               CREATE_USER = l_CREATE_USER(i),
               CREATE_TIME = l_CREATE_TIME(i),
               CREATE_INSTITUTION = l_CREATE_INSTITUTION(i),
               UPDATE_USER = l_UPDATE_USER(i),
               UPDATE_TIME = l_UPDATE_TIME(i),
               UPDATE_INSTITUTION = l_UPDATE_INSTITUTION(i),
               FLG_UNRELEVANT_MED = l_FLG_UNRELEVANT_MED(i),
               UNRELEVANT_MED_NOTES = l_UNRELEVANT_MED_NOTES(i)
             WHERE
                ID_PAT_MEDICATION_DET = l_ID_PAT_MEDICATION_DET(i)
          returning rowid bulk collect into rows_out;
      END IF;
   END upd;


