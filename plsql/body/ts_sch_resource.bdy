    -- Initialize a record with default values for columns in the table.
    PROCEDURE initrec(sch_resource_inout IN OUT sch_resource%ROWTYPE) IS
    BEGIN
        sch_resource_inout.id_sch_resource        := NULL;
        sch_resource_inout.id_schedule            := NULL;
        sch_resource_inout.id_institution         := 2;
        sch_resource_inout.id_professional        := NULL;
        sch_resource_inout.dt_sch_resource_tstz   := current_timestamp;
        sch_resource_inout.create_user            := NULL;
        sch_resource_inout.create_time            := NULL;
        sch_resource_inout.create_institution     := NULL;
        sch_resource_inout.update_user            := NULL;
        sch_resource_inout.update_time            := NULL;
        sch_resource_inout.update_institution     := NULL;
        sch_resource_inout.flg_leader             := NULL;
        sch_resource_inout.id_sch_consult_vacancy := NULL;
    END initrec;

    FUNCTION initrec RETURN sch_resource%ROWTYPE IS
        l_sch_resource sch_resource%ROWTYPE;
    BEGIN
        l_sch_resource.id_institution       := 2;
        l_sch_resource.dt_sch_resource_tstz := current_timestamp;
        RETURN l_sch_resource;
    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN sch_resource_tc IS
        data sch_resource_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM sch_resource
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN sch_resource_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data sch_resource_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

BEGIN
    NULL;
END ts_sch_resource;
