        cur_value_in    IN area_key_nextval.cur_value%TYPE DEFAULT NULL,
        handle_error_in IN BOOLEAN := TRUE,
        rows_out        OUT table_varchar
    ) IS
    BEGIN
        upd(area_in         => area_in,
            key_in          => key_in,
            cur_value_in    => cur_value_in,
            handle_error_in => handle_error_in,
            rows_out        => rows_out);
        IF SQL%ROWCOUNT = 0
        THEN
            ins(area_in         => area_in,
                key_in          => key_in,
                cur_value_in    => cur_value_in,
                handle_error_in => handle_error_in,
                rows_out        => rows_out);
