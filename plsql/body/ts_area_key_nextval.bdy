    PROCEDURE upd_ins
    (
        area_in         IN area_key_nextval.area%TYPE,
        key_in          IN area_key_nextval.key%TYPE,
        cur_value_in    IN area_key_nextval.cur_value%TYPE DEFAULT NULL,
        handle_error_in IN BOOLEAN := TRUE
    ) IS
        rows_out table_varchar;
    BEGIN
        upd_ins(area_in, key_in, cur_value_in, handle_error_in, rows_out);
    END upd_ins;

    PROCEDURE upd
    (
        col_in            IN area_key_nextval_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_area      area_cc;
        l_key       key_cc;
        l_cur_value cur_value_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_area(i) := col_in(i).area;
            l_key(i) := col_in(i).key;
            l_cur_value(i) := col_in(i).cur_value;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE area_key_nextval
                   SET cur_value = nvl(l_cur_value(i), cur_value)
                 WHERE area = l_area(i)
                   AND key = l_key(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE area_key_nextval
                   SET cur_value = l_cur_value(i)
                 WHERE area = l_area(i)
                   AND key = l_key(i);
        END IF;
    END upd;

