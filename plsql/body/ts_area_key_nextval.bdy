        area_key_nextval_inout.key       := NULL;
        area_key_nextval_inout.cur_value := NULL;
    END initrec;

    FUNCTION initrec RETURN area_key_nextval%ROWTYPE IS
        l_area_key_nextval area_key_nextval%ROWTYPE;
    BEGIN
