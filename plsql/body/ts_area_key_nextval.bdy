    END initrec;

    FUNCTION get_data_rowid(rows_in IN table_varchar) RETURN area_key_nextval_tc IS
        data area_key_nextval_tc;
    BEGIN
        SELECT * BULK COLLECT
          INTO data
          FROM area_key_nextval
         WHERE ROWID IN (SELECT *
                           FROM TABLE(rows_in));
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
    END get_data_rowid;

    FUNCTION get_data_rowid_pat(rows_in IN table_varchar) RETURN area_key_nextval_tc IS
        PRAGMA AUTONOMOUS_TRANSACTION;
        data area_key_nextval_tc;
    BEGIN
        data := get_data_rowid(rows_in);
        COMMIT;
        RETURN data;
    EXCEPTION
        WHEN OTHERS THEN
            pk_alert_exceptions.raise_error(error_name_in => 'get_data_rowid');
            ROLLBACK;
    END get_data_rowid_pat;

