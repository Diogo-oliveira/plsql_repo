    PROCEDURE upd
    (
        col_in            IN lens_presc_det_tc,
        ignore_if_null_in IN BOOLEAN := TRUE,
        handle_error_in   IN BOOLEAN := TRUE,
        rows_out          IN OUT table_varchar
    ) IS
        l_id_lens_presc        id_lens_presc_cc;
        l_id_lens_presc_hist   id_lens_presc_hist_cc;
        l_id_advanced_input    id_advanced_input_cc;
        l_id_adv_inp_field_det id_adv_inp_field_det_cc;
        l_value                value_cc;
    BEGIN
        FOR i IN col_in.FIRST .. col_in.LAST
        LOOP
            l_id_lens_presc(i) := col_in(i).id_lens_presc;
            l_id_lens_presc_hist(i) := col_in(i).id_lens_presc_hist;
            l_id_advanced_input(i) := col_in(i).id_advanced_input;
            l_id_adv_inp_field_det(i) := col_in(i).id_adv_inp_field_det;
            l_value(i) := col_in(i).VALUE;
        END LOOP;
        IF nvl(ignore_if_null_in, FALSE)
        THEN
            -- Set any columns to their current values
            -- if incoming value is NULL.
            -- Put WHEN clause on column-level triggers!
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE lens_presc_det
                   SET VALUE = nvl(l_value(i), VALUE)
                 WHERE id_lens_presc = l_id_lens_presc(i)
                   AND id_advanced_input = l_id_advanced_input(i)
                   AND id_adv_inp_field_det = l_id_adv_inp_field_det(i)
                   AND id_lens_presc_hist = l_id_lens_presc_hist(i);
        ELSE
            FORALL i IN col_in.FIRST .. col_in.LAST
                UPDATE lens_presc_det
                   SET VALUE = l_value(i)
                 WHERE id_lens_presc = l_id_lens_presc(i)
                   AND id_advanced_input = l_id_advanced_input(i)
                   AND id_adv_inp_field_det = l_id_adv_inp_field_det(i)
                   AND id_lens_presc_hist = l_id_lens_presc_hist(i);
        END IF;
    END upd;

