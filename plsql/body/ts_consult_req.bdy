            l_status_flg(i) := col_in(i).status_flg;
            l_status_icon(i) := col_in(i).status_icon;
            l_status_msg(i) := col_in(i).status_msg;
            l_status_str(i) := col_in(i).status_str;
            l_reason_for_visit(i) := col_in(i).reason_for_visit;
