    BEGIN
        IF rows_in.count = 0
        THEN
            NULL;
        ELSE
            FORALL indx IN rows_in.first .. rows_in.last SAVE EXCEPTIONS
                INSERT INTO vital_sign_unit_measure
                    (id_vital_sign_unit_measure,
                     id_vital_sign,
                     id_unit_measure,
                     id_institution,
                     id_software,
                     val_min,
                     val_max,
                     format_num,
                     decimals,
                     adw_last_update,
                     create_user,
                     create_time,
                     create_institution,
                     update_user,
                     update_time,
                     update_institution,
