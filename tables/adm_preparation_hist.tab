-- Create table
create table ADM_PREPARATION_HIST
(
  ID_ADM_PREPARATION        NUMBER(24) not null,
  FLG_AVAILABLE             VARCHAR2(1) not null,
  CREATE_USER               VARCHAR2(24),
  CREATE_TIME               TIMESTAMP(6) WITH LOCAL TIME ZONE,
  CREATE_INSTITUTION        NUMBER(24),
  UPDATE_USER               VARCHAR2(24),
  UPDATE_TIME               TIMESTAMP(6) WITH LOCAL TIME ZONE,
  UPDATE_INSTITUTION        NUMBER(24),
  ID_INSTITUTION            NUMBER(24) not null,
  CODE_ADM_PREPARATION      VARCHAR2(400),
  FLG_STATUS                VARCHAR2(1 CHAR),
  FLG_PARAMETERIZATION_TYPE VARCHAR2(1 CHAR),
  ID_PROFESSIONAL           NUMBER(24),
  DT_CREATION               TIMESTAMP(6) WITH LOCAL TIME ZONE,
  DT_LAST_UPDATE            TIMESTAMP(6) WITH LOCAL TIME ZONE,
  DESC_ADM_PREPARATION      VARCHAR2(1000 CHAR),
  ID_ADM_PREPARATION_HIST   NUMBER(24) not null
);

COMMENT ON TABLE ADM_PREPARATION_HIST IS 'Preparations history';
COMMENT ON COLUMN ADM_PREPARATION_HIST.FLG_STATUS IS 'Status of this data: A - active, E - edited, C - canceled.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.FLG_PARAMETERIZATION_TYPE IS 'Type of parameterization used to create this data: C - configurations team, directy in the database, B - backoffice.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.ID_PROFESSIONAL IS 'Id professional that creates the data, when using backoffice.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.DT_CREATION IS 'Creation date.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.DT_LAST_UPDATE IS 'Last date when this data was updated.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.DESC_ADM_PREPARATION IS 'Preparation description.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.ID_ADM_PREPARATION IS 'Primary key';
COMMENT ON COLUMN ADM_PREPARATION_HIST.FLG_AVAILABLE IS 'Flag indicating content availability';
COMMENT ON COLUMN ADM_PREPARATION_HIST.ID_INSTITUTION IS 'Institution id';
COMMENT ON COLUMN ADM_PREPARATION_HIST.CODE_ADM_PREPARATION IS 'Code for translation';
COMMENT ON COLUMN ADM_PREPARATION_HIST.flg_status IS 'Status of this data: A - active, E - edited, C - canceled.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.flg_parameterization_type IS 'Type of parameterization used to create this data: C - configurations team, directy in the database, B - backoffice.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.id_professional IS 'Id professional that creates the data, when using backoffice.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.dt_creation IS 'Creation date.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.dt_last_update IS 'Last date when this data was updated.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.desc_adm_preparation IS 'Preparation description.';
COMMENT ON COLUMN ADM_PREPARATION_HIST.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN ADM_PREPARATION_HIST.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN ADM_PREPARATION_HIST.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN ADM_PREPARATION_HIST.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN ADM_PREPARATION_HIST.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN ADM_PREPARATION_HIST.UPDATE_INSTITUTION IS 'Update Institution';
