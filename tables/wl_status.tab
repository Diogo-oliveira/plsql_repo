

  CREATE TABLE WL_STATUS
   (	ID_WL_STATUS NUMBER(24,0) NOT NULL ENABLE,
	RANK NUMBER(3,0) NOT NULL ENABLE,
	FLG_STATUS VARCHAR2(1)
   );


COMMENT ON TABLE wl_status IS 'Estados do atendimento do utente, em qualquer fase do processo' ;

COMMENT ON COLUMN wl_status.rank IS 'Ordem sequencial dos estados' ;
COMMENT ON COLUMN wl_status.flg_status IS 'Estados' ;
