

  CREATE TABLE MATR_DEP_CLIN_SERV
   (	ID_MATR_DEP_CLIN_SERV NUMBER(24,0) NOT NULL ENABLE,
	ID_MATERIAL NUMBER(24,0) NOT NULL ENABLE,
	ID_DEP_CLIN_SERV NUMBER(24,0) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(12,0) NOT NULL ENABLE,
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE matr_dep_clin_serv IS 'Associação do material aos serviços clínicos e tipos de serviço prestados' ;

COMMENT ON COLUMN matr_dep_clin_serv.id_matr_dep_clin_serv IS 'ID único' ;
COMMENT ON COLUMN matr_dep_clin_serv.id_material IS 'ID do material' ;
COMMENT ON COLUMN matr_dep_clin_serv.id_dep_clin_serv IS 'ID do departamento clínico' ;
