

  CREATE TABLE GROUPS_HIST
   (	ID_GROUP_HIST NUMBER(24,0),
	ID_GROUP NUMBER(24,0),
	NAME VARCHAR2(200),
	BEGIN_DATE TIMESTAMP (6) WITH LOCAL TIME ZONE,
	END_DATE TIMESTAMP (6) WITH LOCAL TIME ZONE,
	ID_PROF_CHANGE NUMBER(24,0),
	CREATE_USER VARCHAR2(24),
	CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	CREATE_INSTITUTION NUMBER(24,0),
	UPDATE_USER VARCHAR2(24),
	UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	UPDATE_INSTITUTION NUMBER(24,0)
   ) ;

   COMMENT ON COLUMN GROUPS_HIST.ID_GROUP_HIST IS 'History table identification';



   COMMENT ON COLUMN GROUPS_HIST.ID_GROUP IS 'Group identification';



   COMMENT ON COLUMN GROUPS_HIST.NAME IS 'Group name';



   COMMENT ON COLUMN GROUPS_HIST.BEGIN_DATE IS 'Registry creation date';



   COMMENT ON COLUMN GROUPS_HIST.END_DATE IS 'Registry outdated date';



   COMMENT ON COLUMN GROUPS_HIST.ID_PROF_CHANGE IS 'Id professional change';



   COMMENT ON COLUMN GROUPS_HIST.CREATE_USER IS 'Creation User';



   COMMENT ON COLUMN GROUPS_HIST.CREATE_TIME IS 'Creation Time';



   COMMENT ON COLUMN GROUPS_HIST.CREATE_INSTITUTION IS 'Creation Institution';



   COMMENT ON COLUMN GROUPS_HIST.UPDATE_USER IS 'Update User';



   COMMENT ON COLUMN GROUPS_HIST.UPDATE_TIME IS 'Update Time';



   COMMENT ON COLUMN GROUPS_HIST.UPDATE_INSTITUTION IS 'Update Institution';



   COMMENT ON TABLE GROUPS_HIST  IS 'History groups';

   comment on column GROUPS_HIST.ID_PROF_CHANGE  is 'Professional change identification';

