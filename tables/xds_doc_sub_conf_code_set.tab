COMMENT ON COLUMN XDS_DOC_SUB_CONF_CODE_SET.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN XDS_DOC_SUB_CONF_CODE_SET.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN XDS_DOC_SUB_CONF_CODE_SET.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN XDS_DOC_SUB_CONF_CODE_SET.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN XDS_DOC_SUB_CONF_CODE_SET.UPDATE_INSTITUTION IS 'Update Institution';
