COMMENT ON COLUMN drug_presc_det.dt_begin IS 'Data de inicio' ;
COMMENT ON COLUMN drug_presc_det.dt_end IS 'Data de fim' ;
COMMENT ON COLUMN drug_presc_det.notes IS 'Notas da prescric?o' ;
COMMENT ON COLUMN drug_presc_det.flg_take_type IS 'Tipo de plano de tomas: N - normal, S - SOS, U - unitário, C - contínuo, A - ad eternum' ;
COMMENT ON COLUMN drug_presc_det.qty IS 'Quantidade prescrita' ;
COMMENT ON COLUMN drug_presc_det.rate IS 'Ritmo de administrac?o do medicamento' ;
COMMENT ON COLUMN drug_presc_det.flg_status IS 'Estado da prescrição: D - pendente, R - requisitado, E - em curso, F - concluído, C - anulado, I - interrompido' ;
COMMENT ON COLUMN drug_presc_det.dt_cancel IS 'Data de cancelamento' ;
COMMENT ON COLUMN drug_presc_det.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN drug_presc_det.notes_justif IS 'Texto de justificação' ;
COMMENT ON COLUMN drug_presc_det.interval IS 'Intervalo entre tomas' ;
COMMENT ON COLUMN drug_presc_det.takes IS 'Nº de tomas' ;

COMMENT ON COLUMN drug_presc_det.dosage IS 'Dosagem' ;
COMMENT ON COLUMN drug_presc_det.id_drug_justification IS 'ID da justificação.' ;
COMMENT ON COLUMN drug_presc_det.flg_ci IS 'Chamada de atenção de contra-indicações: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN drug_presc_det.flg_cheaper IS 'Chamada de atenção de medicamentos mais baratos: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN drug_presc_det.flg_justif IS 'Chamada de atenção de justificação de prescrição: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN drug_presc_det.flg_attention IS 'Flag que indica a chamada de atenção prioritária: CI - contra-indicações; B - medicamentos mais baratos; J - justificação de prescrição; IM - interacções medicamentosas' ;
COMMENT ON COLUMN drug_presc_det.flg_attention_print IS 'Flag que indica se o profissional prosseguiu (Y) ou não (N) com a impressão apesar das chamadas de atenção.' ;
