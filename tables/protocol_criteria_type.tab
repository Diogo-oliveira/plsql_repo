
CREATE TABLE PROTOCOL_CRITERIA_TYPE
 (ID_PROTOCOL_CRITERIA_TYPE NUMBER(24) NOT NULL
 ,CODE_PROTOCOL_CRITERIA_TYPE VARCHAR2(200)
 ,FLG_AVAILABLE VARCHAR2(1)
 ,ADW_LAST_UPDATE DATE
 );
 
COMMENT ON COLUMN PROTOCOL_CRITERIA_TYPE.ID_PROTOCOL_CRITERIA_TYPE IS 'ID of protocol criteria type';
COMMENT ON COLUMN PROTOCOL_CRITERIA_TYPE.CODE_PROTOCOL_CRITERIA_TYPE IS 'Code criteria type for translation';
COMMENT ON COLUMN PROTOCOL_CRITERIA_TYPE.FLG_AVAILABLE IS 'Available Flag';
COMMENT ON COLUMN PROTOCOL_CRITERIA_TYPE.ADW_LAST_UPDATE IS 'Data warehouse column';

