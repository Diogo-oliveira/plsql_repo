

  CREATE TABLE SCH_PROF_OUTP
   (	ID_SCH_PROF_OUTP NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	ID_SCHEDULE_OUTP NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE sch_prof_outp IS 'Profissionais agendados (agendamentos de consultas)' ;
COMMENT ON COLUMN SCH_PROF_OUTP.ID_PROFESSIONAL IS 'Professional identifier';
COMMENT ON COLUMN SCH_PROF_OUTP.ID_SCH_PROF_OUTP IS 'Professional-Consult appointment association identifier';
COMMENT ON COLUMN SCH_PROF_OUTP.ID_SCHEDULE_OUTP IS 'Consult appointment identifier';
COMMENT ON TABLE SCH_PROF_OUTP IS 'Contains the association between professionals and consult appointments.';

