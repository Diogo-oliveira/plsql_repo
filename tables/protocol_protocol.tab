
CREATE TABLE PROTOCOL_PROTOCOL
 (ID_PROTOCOL_PROTOCOL NUMBER(24) NOT NULL
 ,DESC_PROTOCOL_PROTOCOL VARCHAR2(1000)
 ,ID_NESTED_PROTOCOL NUMBER(24)
 );

COMMENT ON COLUMN PROTOCOL_PROTOCOL.ID_PROTOCOL_PROTOCOL IS 'ID of protocol_protocol';
COMMENT ON COLUMN PROTOCOL_PROTOCOL.DESC_PROTOCOL_PROTOCOL IS 'Description of protocol';
COMMENT ON COLUMN PROTOCOL_PROTOCOL.ID_NESTED_PROTOCOL IS 'ID of nested protocol';

