
CREATE TABLE PROTOCOL_FREQUENT
 (ID_PROTOCOL_FREQUENT NUMBER(24) NOT NULL
 ,RANK NUMBER(6)
 ,ID_PROTOCOL NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(12) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 );

COMMENT ON COLUMN PROTOCOL_FREQUENT.ID_PROTOCOL_FREQUENT IS 'Protocol FREQUENT ID';
COMMENT ON COLUMN PROTOCOL_FREQUENT.RANK IS 'Rank of frequent protocol';
COMMENT ON COLUMN PROTOCOL_FREQUENT.ID_PROTOCOL IS 'Frequent protocol ID';
COMMENT ON COLUMN PROTOCOL_FREQUENT.ID_INSTITUTION IS 'Institution associated to frequent protocol';
COMMENT ON COLUMN PROTOCOL_FREQUENT.ID_SOFTWARE IS 'software associated to frequent protocol';

