

  CREATE TABLE SR_PROF_SHIFT
   (	ID_SR_PROF_SHIFT NUMBER(24,0) NOT NULL ENABLE,
	CODE_SR_PROF_SHIFT VARCHAR2(200),
	FLG_AVAILABLE VARCHAR2(1),
	RANK NUMBER(3,0),
	ID_INSTITUTION NUMBER(12,0) NOT NULL ENABLE,
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE,
	HOUR_START VARCHAR2(6),
	HOUR_END VARCHAR2(6)
   );


COMMENT ON COLUMN sr_prof_shift.id_sr_prof_shift IS 'Id único' ;
COMMENT ON COLUMN sr_prof_shift.code_sr_prof_shift IS 'Código para obtenção da descrição na tabela translation' ;
COMMENT ON COLUMN sr_prof_shift.flg_available IS 'Disponibilidade na instituição' ;
COMMENT ON COLUMN sr_prof_shift.rank IS 'Ordem de visualização' ;
COMMENT ON COLUMN sr_prof_shift.id_institution IS 'ID da instituição' ;
COMMENT ON COLUMN sr_prof_shift.id_software IS 'ID do software' ;
COMMENT ON COLUMN sr_prof_shift.hour_start IS 'Hora de início do turno' ;
COMMENT ON COLUMN sr_prof_shift.hour_end IS 'Hora de fim do turno' ;


