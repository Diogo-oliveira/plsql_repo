
  CREATE TABLE EMB_DEP_CLIN_SERV_IT
   (	ID_EMB_DEP_CLIN_SERV NUMBER(24,0) NOT NULL ENABLE,
	EMB_ID NUMBER(10,0),
	QTY NUMBER(2,0),
	DOSAGE VARCHAR2(100),
	GENERICO VARCHAR2(1),
	ID_DEP_CLIN_SERV NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0),
	RANK NUMBER(2,0) NOT NULL ENABLE,
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE,
	ID_DIETARY_DRUG NUMBER(24,0),
	ID_MANIPULATED NUMBER(24,0)
   );


COMMENT ON TABLE emb_dep_clin_serv_it IS 'Embalagens mais frequentes' ;

COMMENT ON COLUMN emb_dep_clin_serv_it.id_emb_dep_clin_serv IS 'Chave primária.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.emb_id IS 'ID da embalagem.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.qty IS 'Quantidades mais frequentes dos medicamentos mais frequentes.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.dosage IS 'Posologias mais frequentes dos medicamentos mais frequentes.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.generico IS 'Indicação se autoriza genérico (Y) ou não (N).' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.id_dep_clin_serv IS 'ID do departamento e serviço clínico para o qual a embalagem é mais frequente.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.rank IS 'Rank.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.id_software IS 'ID do software.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.id_dietary_drug IS 'ID do dietético.' ;
COMMENT ON COLUMN emb_dep_clin_serv_it.id_manipulated IS 'ID do manipulado.' ;

