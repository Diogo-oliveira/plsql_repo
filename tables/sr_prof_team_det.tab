

  CREATE TABLE SR_PROF_TEAM_DET
   (	ID_SR_PROF_TEAM_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_SURGERY_RECORD NUMBER(24,0),
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	ID_PROF_TEAM_LEADER NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_BEGIN DATE,
	DT_END DATE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	ID_CATEGORY_SUB NUMBER(12,0) NOT NULL ENABLE,
	ID_PROF_TEAM NUMBER(24,0),
	FLG_STATUS VARCHAR2(1),

	ID_PROF_REG NUMBER(24,0),
	DT_REG DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	DT_CANCEL DATE
   );


COMMENT ON COLUMN sr_prof_team_det.id_sr_prof_team_det IS 'ID único' ;
COMMENT ON COLUMN sr_prof_team_det.id_surgery_record IS 'ID do registo de intervenção' ;
COMMENT ON COLUMN sr_prof_team_det.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_prof_team_det.dt_begin IS 'Data de início de participação na intervenção' ;
COMMENT ON COLUMN sr_prof_team_det.dt_end IS 'Data de fim de participação na intervenção' ;
COMMENT ON COLUMN sr_prof_team_det.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN sr_prof_team_det.flg_status IS 'Estado do registo (A-Activo, C-Cancelado)' ;
COMMENT ON COLUMN sr_prof_team_det.id_prof_reg IS 'ID do último profissional que alterou o registo' ;
COMMENT ON COLUMN sr_prof_team_det.dt_reg IS 'Data da última alteração no registo' ;
COMMENT ON COLUMN sr_prof_team_det.id_prof_cancel IS 'ID do profissional que cancelou o registo' ;
COMMENT ON COLUMN sr_prof_team_det.dt_cancel IS 'Data de cancelamento do registo' ;

