COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.RANK IS 'Used to configure the visualization order';
COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN PN_SOAP_BLOCK_CFG.UPDATE_INSTITUTION IS 'Update Institution';
