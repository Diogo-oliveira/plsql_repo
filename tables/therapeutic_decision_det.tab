-- Creating Table 'THERAPEUTIC_DECISION_DET'
CREATE TABLE THERAPEUTIC_DECISION_DET
 (ID_THERAPEUTIC_DEC_DET NUMBER(24) NOT NULL
 ,ID_THERAPEUTIC_DECISION NUMBER(24) NOT NULL
 ,ID_PROFESSIONAL NUMBER(24) NOT NULL
 ,FLG_STATUS VARCHAR2(1)
 ,DT_CREATION TIMESTAMP WITH LOCAL TIME ZONE
 ,FLG_PRESENCE VARCHAR2(1)
 ,DT_PRESENCE TIMESTAMP WITH LOCAL TIME ZONE
 ,ID_PROF_PRESENCE NUMBER(24) NOT NULL
 ,FLG_OPINION VARCHAR2(1)
 ,DT_OPINION TIMESTAMP WITH LOCAL TIME ZONE
 ,FLG_LEADER              VARCHAR2(1)
 ,CREATE_USER             VARCHAR2(24)
 ,CREATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION      NUMBER(24)
 ,UPDATE_USER             VARCHAR2(24)
 ,UPDATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION      NUMBER(24)
 
 );

COMMENT ON TABLE THERAPEUTIC_DECISION_DET IS 'Professionals from a decision therapeutic consult';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.ID_THERAPEUTIC_DEC_DET IS 'ID';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.ID_THERAPEUTIC_DECISION IS 'ID of therapeutic decision';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.ID_PROFESSIONAL IS 'id professional of consult';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.FLG_STATUS IS 'Status of register A- Active; O - Outdated';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.DT_CREATION IS 'Creation Date';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.FLG_PRESENCE IS 'Flag that indicates the presence of the professional in the consult P - Present; F - Failed';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.DT_PRESENCE IS 'Date when presence was registered';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.ID_PROF_PRESENCE IS 'id of professional that register the presence';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.FLG_OPINION IS 'flg that indicates if professional agrees with decision; Y - Agree';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.DT_OPINION IS 'Date when opinion was registered';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.FLG_LEADER IS 'Professional leader flag - (Y)es / (N)o';

COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN THERAPEUTIC_DECISION_DET.UPDATE_INSTITUTION IS 'Update Institution';
