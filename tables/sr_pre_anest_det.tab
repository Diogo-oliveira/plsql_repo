

  CREATE TABLE SR_PRE_ANEST_DET
   (	ID_SR_PRE_ANEST_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_PRE_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_PRE_ANEST NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	VALUE VARCHAR2(240),
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_REG DATE
   );


COMMENT ON TABLE sr_pre_anest_det IS 'Tabela de avaliações pré-anestésicas' ;

COMMENT ON COLUMN sr_pre_anest_det.id_sr_pre_anest_det IS 'ID único' ;
COMMENT ON COLUMN sr_pre_anest_det.id_sr_pre_eval_visit IS 'ID da visita pré-anestésica' ;
COMMENT ON COLUMN sr_pre_anest_det.id_sr_pre_anest IS 'ID do item da avaliação' ;
COMMENT ON COLUMN sr_pre_anest_det.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_pre_anest_det.value IS 'Valor avaliado' ;
COMMENT ON COLUMN sr_pre_anest_det.id_professional IS 'ID do profissional que efectuou o registo' ;
COMMENT ON COLUMN sr_pre_anest_det.dt_reg IS 'Data de registo' ;
