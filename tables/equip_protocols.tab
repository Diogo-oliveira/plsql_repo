

  CREATE TABLE EQUIP_PROTOCOLS
   (	ID_EQUIP_PROTOCOLS NUMBER(24,0) NOT NULL ENABLE,
	ID_PROTOCOLS NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_EQUIP NUMBER(24,0) NOT NULL ENABLE,
	QTY_REQ NUMBER(9,3) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1),
	RANK NUMBER(6,0),
	ADW_LAST_UPDATE DATE,
	ID_SURG_PERIOD NUMBER(3,0)
   );


COMMENT ON COLUMN equip_protocols.id_equip_protocols IS 'Id único' ;
COMMENT ON COLUMN equip_protocols.id_protocols IS 'ID do protocolo' ;
COMMENT ON COLUMN equip_protocols.id_sr_equip IS 'ID do equipamento, material ou hemoderivado' ;
COMMENT ON COLUMN equip_protocols.qty_req IS 'Quantidade a requisitar' ;
COMMENT ON COLUMN equip_protocols.flg_available IS 'Disponibilidade' ;
COMMENT ON COLUMN equip_protocols.rank IS 'Ordem de visualização' ;
COMMENT ON COLUMN equip_protocols.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN equip_protocols.id_surg_period IS 'ID do período cirúrgico' ;
