
CREATE TABLE PROTOCOL_PROCESS_TASK_DET
 (ID_PROTOCOL_PROCESS_TASK_DET NUMBER(24) NOT NULL
 ,ID_PROTOCOL_PROCESS_ELEM NUMBER(24) NOT NULL
 ,FLG_DETAIL_TYPE VARCHAR2(1) NOT NULL
 ,VALUE_TYPE VARCHAR2(1) NOT NULL
 ,NVALUE NUMBER(8,3)
 ,DVALUE TIMESTAMP WITH LOCAL TIME ZONE
 ,VVALUE VARCHAR2(200)
 );

COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.ID_PROTOCOL_PROCESS_TASK_DET IS 'Protocol process task detail ID';
COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.ID_PROTOCOL_PROCESS_ELEM IS 'Protocol process tasks associated to detail';
COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.FLG_DETAIL_TYPE IS 'Type of detail';
COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.VALUE_TYPE IS 'Detail value type: Date / Number / Varchar';
COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.NVALUE IS 'Numeric value';
COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.DVALUE IS 'Date value';
COMMENT ON COLUMN PROTOCOL_PROCESS_TASK_DET.VVALUE IS 'Varchar value';

