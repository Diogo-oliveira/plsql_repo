

  CREATE TABLE SR_RECEIVE_PROC_DET
   (	ID_SR_RECEIVE_PROC_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_RECEIVE_PROC NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	CHKLIST_VALUE_YN VARCHAR2(1),
	ID_PROFESSIONAL NUMBER(24,0),
	LAST_UPD_DATE DATE
   );


COMMENT ON COLUMN sr_receive_proc_det.id_sr_receive_proc_det IS 'ID único' ;
COMMENT ON COLUMN sr_receive_proc_det.id_sr_receive_proc IS 'ID único' ;
COMMENT ON COLUMN sr_receive_proc_det.id_episode IS 'ID do agendamento' ;
COMMENT ON COLUMN sr_receive_proc_det.chklist_value_yn IS 'Valor do item de acolhimento. Valores possíveis: Y- Sim, N- Não, A- Não aplicável, X- nenhum valor seleccionado' ;
