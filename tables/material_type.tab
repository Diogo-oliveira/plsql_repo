

  CREATE TABLE MATERIAL_TYPE
   (	ID_MATERIAL_TYPE NUMBER(24,0) NOT NULL ENABLE,
	CODE_MATERIAL_TYPE VARCHAR2(200) NOT NULL ENABLE,
	FLG_MOBILE VARCHAR2(1) NOT NULL ENABLE,
	INTERVAL NUMBER(6,0),
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE material_type IS 'Tipos de equipamentos' ;

COMMENT ON COLUMN material_type.id_material_type IS 'Id único' ;
COMMENT ON COLUMN material_type.code_material_type IS 'Código para nome' ;
COMMENT ON COLUMN material_type.flg_mobile IS 'É móvel (Y/N)' ;
COMMENT ON COLUMN material_type.interval IS 'Intervalo de tempo entre agendamentos / requisições, em minutos.' ;
COMMENT ON COLUMN material_type.rank IS 'Ordem de visualização' ;
COMMENT ON COLUMN material_type.adw_last_update IS 'Data da última alteração' ;
