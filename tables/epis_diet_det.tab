-- Creating Table 'EPIS_DIET_DET'
CREATE TABLE EPIS_DIET_DET
 (ID_EPIS_DIET_DET NUMBER(24) NOT NULL
 ,ID_EPIS_DIET_REQ NUMBER(24) NOT NULL
 ,NOTES VARCHAR2(4000)
 ,ID_DIET_SCHEDULE NUMBER(24) NOT NULL
 ,DT_DIET_SCHEDULE TIMESTAMP WITH LOCAL TIME ZONE
 ,ID_DIET NUMBER(24) NOT NULL
 ,QUANTITY NUMBER
 ,ID_UNIT_MEASURE NUMBER(24)
 ,CREATE_USER             VARCHAR2(24)
 ,CREATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION      NUMBER(24)
 ,UPDATE_USER             VARCHAR2(24)
 ,UPDATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION      NUMBER(24)
 
 );

COMMENT ON TABLE EPIS_DIET_DET IS 'Requisitions diets detail';

COMMENT ON COLUMN EPIS_DIET_DET.ID_EPIS_DIET_DET IS 'Record ID';

COMMENT ON COLUMN EPIS_DIET_DET.ID_EPIS_DIET_REQ IS 'Epis_diet_req id';

COMMENT ON COLUMN EPIS_DIET_DET.NOTES IS 'Notes';

COMMENT ON COLUMN EPIS_DIET_DET.ID_DIET_SCHEDULE IS 'Diet Schedule id';

COMMENT ON COLUMN EPIS_DIET_DET.DT_DIET_SCHEDULE IS 'Meal hour';

COMMENT ON COLUMN EPIS_DIET_DET.ID_DIET IS 'Diet id';

COMMENT ON COLUMN EPIS_DIET_DET.QUANTITY IS 'Quantity';

COMMENT ON COLUMN EPIS_DIET_DET.ID_UNIT_MEASURE IS 'Quantity unit measure';

COMMENT ON COLUMN EPIS_DIET_DET.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN EPIS_DIET_DET.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN EPIS_DIET_DET.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN EPIS_DIET_DET.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN EPIS_DIET_DET.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN EPIS_DIET_DET.UPDATE_INSTITUTION IS 'Update Institution';
