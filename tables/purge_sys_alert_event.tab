

  CREATE TABLE "ALERT"."PURGE_SYS_ALERT_EVENT"
   (	"ID_PURGE_SYS_ALERT_EVENT" NUMBER(24,0),
	"ID_SYS_ALERT" NUMBER(24,0),
	"ID_SOFTWARE" NUMBER(24,0),
	"ID_INSTITUTION" NUMBER(24,0),
	"ID_PATIENT" NUMBER(24,0),
	"ID_VISIT" NUMBER(24,0),
	"ID_EPISODE" NUMBER(24,0),
	"ID_RECORD" NUMBER(24,0),
	"DT_RECORD" TIMESTAMP (6) WITH LOCAL TIME ZONE,
	"ID_PROFESSIONAL" NUMBER(24,0),
	"ID_ROOM" NUMBER(24,0),
	"ID_CLINICAL_SERVICE" NUMBER(12,0),
	"FLG_VISIBLE" VARCHAR2(1),
	"REPLACE1" VARCHAR2(200),
	"REPLACE2" VARCHAR2(4000),
	"ID_DEP_CLIN_SERV" NUMBER(24,0),
	"LAST_UPDATE_DATE" DATE
   ) ;


