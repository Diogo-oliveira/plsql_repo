   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_SYS_ALERT" IS 'Alert Id';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_SOFTWARE" IS 'Software Id';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_INSTITUTION" IS 'Institution Id';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_PATIENT" IS 'Patient Id';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_VISIT" IS 'Visit Id';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_EPISODE" IS 'Episode Id';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_RECORD" IS 'Record id for the record that generated the event';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."DT_RECORD" IS 'Event date';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_PROFESSIONAL" IS 'Target Professional Id. Should only be used if the event is aimed at this professional.';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_ROOM" IS 'Target Room Id. Should only be used if the event is aimed at the professionals allocated to this room.';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."ID_CLINICAL_SERVICE" IS 'Target Clinical Service Id. Should only be used if the event is aimed at the professionals allocated to this Clinical Service.';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."FLG_VISIBLE" IS 'Y - Available to the user; N - Not available (To manage duplications)';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."REPLACE1" IS 'coluna de valor a apresentar da descrição do alerta. ex: código para obtenção da descrição de um exame';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."REPLACE2" IS 'coluna de valor a apresentar da descrição do alerta. ex: número de minutos em atraso ';



   COMMENT ON COLUMN "ALERT"."PURGE_SYS_ALERT_EVENT"."LAST_UPDATE_DATE" IS 'Update date';




