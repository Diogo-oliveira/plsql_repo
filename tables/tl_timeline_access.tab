   COMMENT ON COLUMN "ALERT"."TL_TIMELINE_ACCESS"."ID_TL_TIMELINE_ACCESS" IS 'ID da escala horizontal superior';



   COMMENT ON COLUMN "ALERT"."TL_TIMELINE_ACCESS"."ID_TL_TIMELINE" IS 'ID da timeline';



