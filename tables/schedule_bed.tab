CREATE TABLE SCHEDULE_BED
 (ID_SCHEDULE NUMBER(24) NOT NULL
 ,ID_BED NUMBER(24) NOT NULL
 ,ID_WAITING_LIST NUMBER(24)
 ,FLG_TEMPORARY VARCHAR2(1) DEFAULT '''Y'''
 ,FLG_CONFLICT VARCHAR2(1) DEFAULT '''N''' NOT NULL
 ,CREATE_USER VARCHAR2(24)
 ,CREATE_TIME TIMESTAMP WITH TIME ZONE
 ,CREATE_INSTITUTION NUMBER(24)
 ,UPDATE_USER VARCHAR2(24)
 ,UPDATE_TIME TIMESTAMP WITH TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
 );

COMMENT ON TABLE SCHEDULE_BED IS 'Contains specific information for admission schedules';
COMMENT ON COLUMN SCHEDULE_BED.ID_SCHEDULE IS 'schedule id (primary key)';
COMMENT ON COLUMN SCHEDULE_BED.ID_BED IS 'bed id';
COMMENT ON COLUMN SCHEDULE_BED.ID_WAITING_LIST IS 'waiting list id that originated this schedule';
COMMENT ON COLUMN SCHEDULE_BED.FLG_TEMPORARY IS 'Y=temporary, not confirmed by planner. N=definitive';
COMMENT ON COLUMN SCHEDULE_BED.FLG_CONFLICT IS 'Y=this schedule in overlapping with another. N=no conflict';
COMMENT ON COLUMN SCHEDULE_BED.CREATE_USER IS 'Creation user';
COMMENT ON COLUMN SCHEDULE_BED.CREATE_TIME IS 'creation time';
COMMENT ON COLUMN SCHEDULE_BED.CREATE_INSTITUTION IS 'creation institution';
COMMENT ON COLUMN SCHEDULE_BED.UPDATE_USER IS 'update user';
COMMENT ON COLUMN SCHEDULE_BED.UPDATE_TIME IS 'update time';
COMMENT ON COLUMN SCHEDULE_BED.UPDATE_INSTITUTION IS 'update institution';

--END

