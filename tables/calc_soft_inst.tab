CREATE TABLE CALC_SOFT_INST
 (ID_CALCULATOR NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1) NOT NULL
 )
/
COMMENT ON TABLE CALC_SOFT_INST IS 'Configuration table to define which calculator will be available for configuration in the backoffice.'
/
COMMENT ON COLUMN CALC_SOFT_INST.ID_CALCULATOR IS 'Calculator Id'
/
COMMENT ON COLUMN CALC_SOFT_INST.ID_SOFTWARE IS 'Software Id'
/
COMMENT ON COLUMN CALC_SOFT_INST.ID_INSTITUTION IS 'Institution Id'
/
COMMENT ON COLUMN CALC_SOFT_INST.FLG_AVAILABLE IS 'Availability in the Backoffce. Y-Yes N-No'
/
