

  CREATE TABLE PROFILE_TEMPL_ACCESS
   (	ID_PROFILE_TEMPL_ACCESS NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFILE_TEMPLATE NUMBER(12,0) NOT NULL ENABLE,
	RANK NUMBER(6,0),
	ID_SYS_BUTTON_PROP NUMBER(24,0) NOT NULL ENABLE,
	FLG_CREATE VARCHAR2(1),
	FLG_CANCEL VARCHAR2(1),
	FLG_SEARCH VARCHAR2(1),
	FLG_PRINT VARCHAR2(1),
	FLG_OK VARCHAR2(1),
	FLG_DETAIL VARCHAR2(1),
	FLG_CONTENT VARCHAR2(1),

	FLG_HELP VARCHAR2(1),
	ID_SYS_SHORTCUT NUMBER(24,0),
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE,
	ID_SHORTCUT_PK NUMBER(24,0),
	ID_SOFTWARE_CONTEXT NUMBER(24,0) NOT NULL ENABLE,
	FLG_GRAPH VARCHAR2(1),
	FLG_VISION VARCHAR2(1),
	FLG_DIGITAL VARCHAR2(240),
	FLG_FREQ VARCHAR2(1),
	FLG_NO VARCHAR2(240)
   );


COMMENT ON TABLE profile_templ_access IS 'Acessos dos templates aos botões.As flags não são preenchidas para os botões com deepnavs.' ;

COMMENT ON COLUMN profile_templ_access.flg_cancel IS 'Cancelamento: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_search IS 'Pesquisa: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_print IS 'Impressão: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_ok IS 'Avançar: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_detail IS 'Detalhe: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_content IS 'Conteúdo: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_help IS 'Ajuda: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.id_software_context IS 'Quando se acede numa aplicação (ex:OUTP) a funcionalidades de outra (ex: ORIS), a coluna ID_SOFTWARE deve ser preenchida com a aplicação acedida (ex:OUTP), mas ID_SOFTWARE_CONTEXT é preenchida com o ID da outra aplicação (ex: ORIS)' ;
COMMENT ON COLUMN profile_templ_access.flg_graph IS 'Ajuda: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_vision IS 'Ajuda: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_digital IS 'Ajuda: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_freq IS 'Ajuda: N - não aplicável, I - inactivo, A - activo' ;
COMMENT ON COLUMN profile_templ_access.flg_no IS 'Ajuda: N - não aplicável, I - inactivo, A - activo' ;

COMMENT ON COLUMN profile_templ_access.rank IS 'Ordem de apresentação' ;
COMMENT ON COLUMN profile_templ_access.flg_create IS 'Criação: N - não aplicável, I - inactivo, A - activo' ;

alter table profile_templ_access add position number(3);

alter table profile_templ_access add toolbar_level number(2);

COMMENT ON COLUMN profile_templ_access.position      IS 'Posição que o botão ocupa na toolbar superior' ;
COMMENT ON COLUMN profile_templ_access.toolbar_level IS 'Nível em que que está incluído o botão (toolbar superior)' ;



alter table profile_templ_access 
add (FLG_ACTION VARCHAR2(1));

COMMENT ON COLUMN PROFILE_TEMPL_ACCESS.FLG_ACTION IS 'Acções: N - não aplicável, I - inactivo, A - activo';


alter table profile_templ_access 
add (FLG_VIEW VARCHAR2(1));

COMMENT ON COLUMN PROFILE_TEMPL_ACCESS.FLG_VIEW IS 'Views: N - not applicable, I - inactive, A - active';

