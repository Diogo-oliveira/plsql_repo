

  CREATE TABLE DOC_ORIGINAL
   (	ID_DOC_ORIGINAL NUMBER(12,0) NOT NULL ENABLE,
	CODE_DOC_ORIGINAL VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_OTHER VARCHAR2(1) NOT NULL ENABLE
   );


COMMENT ON TABLE doc_original IS 'Tipos de originais dos documentos.' ;

COMMENT ON COLUMN doc_original.id_doc_original IS 'Chave primária' ;
COMMENT ON COLUMN doc_original.code_doc_original IS 'Código de identificação para a descrição multi-língua' ;
COMMENT ON COLUMN doc_original.flg_available IS 'Registo disponível: Y - disponível, N - Não disponível.' ;
COMMENT ON COLUMN doc_original.rank IS 'Valor para ordenação de display do registo' ;
COMMENT ON COLUMN doc_original.adw_last_update IS 'Data da última alteração do registo' ;
COMMENT ON COLUMN doc_original.flg_other IS 'Y- Permite escrita manual. N não permite' ;

