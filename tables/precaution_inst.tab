CREATE TABLE PRECAUTION_INST
 (ID_PRECAUTION NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 );

COMMENT ON TABLE PRECAUTION_INST IS 'Precaution institution parametrization';

COMMENT ON COLUMN PRECAUTION_INST.ID_PRECAUTION IS 'Precaution identifier';
COMMENT ON COLUMN PRECAUTION_INST.ID_INSTITUTION IS 'Institution identifier (0 - All)';

ALTER TABLE PRECAUTION_INST ADD (
   CREATE_USER VARCHAR2(24),
   CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
   CREATE_INSTITUTION NUMBER(24,0),
   UPDATE_USER VARCHAR2(24),
   UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
   UPDATE_INSTITUTION NUMBER(24,0)
);
 
COMMENT ON COLUMN PRECAUTION_INST.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN PRECAUTION_INST.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN PRECAUTION_INST.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN PRECAUTION_INST.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN PRECAUTION_INST.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN PRECAUTION_INST.UPDATE_INSTITUTION IS 'Update Institution';
