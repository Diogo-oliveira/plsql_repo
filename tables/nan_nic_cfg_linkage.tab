IS
  'Institution ID' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.ID_NAN_DIAGNOSIS
IS
  'NANDA Diagnosis ID' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.ID_NIC_INTERVENTION
IS
  'NIC Intervention ID' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.FLG_LNK_STATUS
IS
  'Link status: (A)ctive, (C)ancelled' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NAN_NIC_CFG_LINKAGE.UPDATE_INSTITUTION
IS
  'Update Institution' ;
  
