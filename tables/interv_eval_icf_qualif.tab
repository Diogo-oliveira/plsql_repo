-- Creating Table 'interv_eval_icf_qualif'
CREATE TABLE interv_eval_icf_qualif
 (ID_ICF_QUALIFICATION NUMBER(24) NOT NULL
 ,ID_ICF_QUALIFICATION_SCALE NUMBER(24) NOT NULL
 ,ID_ICF NUMBER(24) NOT NULL
 ,ID_INTERV_EVALUATION NUMBER(24) NOT NULL
 ,FLG_LEVEL NUMBER(2) NOT NULL
 );
COMMENT ON TABLE INTERV_EVAL_ICF_QUALIF is 'Physiotherapy evaluation detail, ICF qualification';
COMMENT ON COLUMN interv_eval_icf_qualif.ID_ICF_QUALIFICATION IS 'ID ICF QUALIFICATION';
COMMENT ON COLUMN interv_eval_icf_qualif.ID_ICF_QUALIFICATION_SCALE IS 'ID qualification scale';
COMMENT ON COLUMN interv_eval_icf_qualif.ID_ICF IS 'ID ICF';
COMMENT ON COLUMN interv_eval_icf_qualif.ID_INTERV_EVALUATION IS 'ID INTERV EVALUATION';
COMMENT ON COLUMN interv_eval_icf_qualif.FLG_LEVEL IS 'level of qualification';

