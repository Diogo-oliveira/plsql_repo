

-- CHANGED BY: Diogo Oliveira
-- CHANGE DATE: 21/08/2019 16:37
-- CHANGE REASON: [EMR-19010] - New area for Medical Orders (DDL)
CREATE TABLE COMM_ORDER_PLAN
(
ID_COMM_ORDER_PLAN    NUMBER(24),
ID_COMM_ORDER_REQ   NUMBER(24),
ID_PROF_TAKE    NUMBER(24),
NOTES   VARCHAR2(4000),
FLG_STATUS    VARCHAR2(1),
ID_PROF_CANCEL    NUMBER(24),
NOTES_CANCEL    VARCHAR2(4000),
ID_WOUND_TREAT    NUMBER(24),
ID_EPISODE_WRITE    NUMBER(24),
DT_PLAN_TSTZ    TIMESTAMP(6) WITH LOCAL TIME ZONE,
DT_TAKE_TSTZ    TIMESTAMP(6) WITH LOCAL TIME ZONE,
DT_CANCEL_TSTZ    TIMESTAMP(6) WITH LOCAL TIME ZONE,
ID_PROF_PERFORMED   NUMBER(24),
START_TIME    TIMESTAMP(6) WITH LOCAL TIME ZONE,
END_TIME    TIMESTAMP(6) WITH LOCAL TIME ZONE,
DT_COMM_ORDER_PLAN    TIMESTAMP(6) WITH LOCAL TIME ZONE,
CREATE_USER   VARCHAR2(24),
CREATE_TIME   TIMESTAMP(6) WITH LOCAL TIME ZONE,
CREATE_INSTITUTION    NUMBER(24),
UPDATE_USER   VARCHAR2(24),
UPDATE_TIME   TIMESTAMP(6) WITH LOCAL TIME ZONE,
UPDATE_INSTITUTION    NUMBER(24),
FLG_SUPPLIES_REG    VARCHAR2(1 CHAR),
ID_CANCEL_REASON    NUMBER(24),
ID_CDR_EVENT    NUMBER(24),
ID_EPIS_DOCUMENTATION   NUMBER(24),
EXEC_NUMBER   NUMBER(24),
ID_PROF_LAST_UPDATE   NUMBER(24),
DT_LAST_UPDATE_TSTZ   TIMESTAMP(6) WITH LOCAL TIME ZONE
);

COMMENT ON COLUMN COMM_ORDER_PLAN.ID_COMM_ORDER_PLAN IS 'Primary key';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_COMM_ORDER_REQ IS 'Order''s order detail ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_PROF_TAKE IS 'Execution professional ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.NOTES IS 'Notes';
COMMENT ON COLUMN COMM_ORDER_PLAN.FLG_STATUS IS 'Flag that indicates the order''s execution status';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_PROF_CANCEL IS 'Professional that cancels the order';
COMMENT ON COLUMN COMM_ORDER_PLAN.NOTES_CANCEL IS 'Cancelation notes';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_WOUND_TREAT IS 'Dressing treatment ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_EPISODE_WRITE IS 'Execution episode ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.DT_PLAN_TSTZ IS 'Planned date';
COMMENT ON COLUMN COMM_ORDER_PLAN.DT_TAKE_TSTZ IS 'Execution date';
COMMENT ON COLUMN COMM_ORDER_PLAN.DT_CANCEL_TSTZ IS 'Date that indicates when the order was cancelled';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_PROF_PERFORMED IS 'Professional that executed the order';
COMMENT ON COLUMN COMM_ORDER_PLAN.START_TIME IS 'Start date of the order execution';
COMMENT ON COLUMN COMM_ORDER_PLAN.END_TIME IS 'End date of the order execution';
COMMENT ON COLUMN COMM_ORDER_PLAN.DT_COMM_ORDER_PLAN IS 'Date that indicates when the execution was registered';
COMMENT ON COLUMN COMM_ORDER_PLAN.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN COMM_ORDER_PLAN.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN COMM_ORDER_PLAN.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN COMM_ORDER_PLAN.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN COMM_ORDER_PLAN.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN COMM_ORDER_PLAN.UPDATE_INSTITUTION IS 'Update Institution';
COMMENT ON COLUMN COMM_ORDER_PLAN.FLG_SUPPLIES_REG IS 'Flag that indicates supplies consumption (Y/N)';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_CANCEL_REASON IS 'Cancel reason ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_CDR_EVENT IS 'Clinical decision rule ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_EPIS_DOCUMENTATION IS 'Order''s perform ID (documentation ID)';
COMMENT ON COLUMN COMM_ORDER_PLAN.EXEC_NUMBER IS 'Order of the execution (reports)';
COMMENT ON COLUMN COMM_ORDER_PLAN.ID_PROF_LAST_UPDATE IS 'Last update professional ID';
COMMENT ON COLUMN COMM_ORDER_PLAN.DT_LAST_UPDATE_TSTZ IS 'Last update date';
-- CHANGE END: Diogo Oliveira

