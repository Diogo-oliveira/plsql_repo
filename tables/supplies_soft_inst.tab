CREATE TABLE supplies_soft_inst
 (  id_supplies_soft_inst NUMBER(24,0) NOT NULL ENABLE,
    id_supplies NUMBER(24,0) NOT NULL ENABLE,
    id_institution NUMBER(24,0) NOT NULL ENABLE,		
    id_software NUMBER(24,0) NOT NULL ENABLE,		
    barcode VARCHAR2(30),
    id_unit_measure NUMBER(24,0),
    rank NUMBER(20,0)
 );
 
comment on table supplies_soft_inst is 'Parametrização dos materias disponíveis na requisição de MCDTs';
comment on column supplies_soft_inst.id_supplies is 'ID do material';
comment on column supplies_soft_inst.id_institution is 'ID da instituição';
comment on column supplies_soft_inst.id_software is 'ID do software';
comment on column supplies_soft_inst.barcode is 'Código de barras associado ao material';
comment on column supplies_soft_inst.id_unit_measure is 'Unidade de medida';
comment on column supplies_soft_inst.rank is 'Parâmetro de ordenação';

