

  CREATE TABLE PROF_FUNC
   (	ID_PROF_FUNC NUMBER(24,0) NOT NULL ENABLE,
	ID_FUNCTIONALITY NUMBER(12,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	ID_DEP_CLIN_SERV NUMBER(24,0),
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE prof_func IS 'Profissionais associados a funcionalidades.Por ex., no caso da funcionalidade dos pedidos de parecer, ficam registados nesta tabela os profissionais a quem se pode pedir opinião (prof. listados para escolha de destino do pedido).' ;

COMMENT ON COLUMN prof_func.id_dep_clin_serv IS 'Dep. + serv. clínico onde o profissional tem acesso à funcionalidade' ;


