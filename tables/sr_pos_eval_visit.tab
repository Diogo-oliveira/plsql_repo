

  CREATE TABLE SR_POS_EVAL_VISIT
   (	ID_SR_POS_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	DT_REG DATE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1),
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(2000)
   );


COMMENT ON TABLE sr_pos_eval_visit IS 'Tabela de visitas pós-operatórias' ;

COMMENT ON COLUMN sr_pos_eval_visit.id_sr_pos_eval_visit IS 'ID da visita pós-operatória' ;
COMMENT ON COLUMN sr_pos_eval_visit.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_pos_eval_visit.dt_reg IS 'Data de criação da visita' ;
COMMENT ON COLUMN sr_pos_eval_visit.id_professional IS 'ID do professional que efectuou o registo' ;
COMMENT ON COLUMN sr_pos_eval_visit.dt_cancel IS 'Data de cancelamento da visita' ;
COMMENT ON COLUMN sr_pos_eval_visit.id_prof_cancel IS 'ID do profissional que cancelou' ;
COMMENT ON COLUMN sr_pos_eval_visit.notes_cancel IS 'Notas de cancelamento' ;
