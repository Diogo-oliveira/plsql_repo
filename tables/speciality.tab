

  CREATE TABLE SPECIALITY
   (	ID_SPECIALITY NUMBER(12,0) NOT NULL ENABLE,
	CODE_SPECIALITY VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE speciality IS 'Especialidades' ;

COMMENT ON COLUMN speciality.code_speciality IS 'Código para nome' ;
COMMENT ON COLUMN speciality.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;


