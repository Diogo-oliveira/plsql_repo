

  CREATE TABLE DOC_ELEMENT_QUANTIF
   (	ID_DOC_ELEMENT_QUANTIF NUMBER(24,0) NOT NULL ENABLE,
	ID_DOC_ELEMENT_CRIT NUMBER(24,0) NOT NULL ENABLE,
	ID_DOC_QUANTIFICATION NUMBER(24,0) NOT NULL ENABLE,
	ID_DOC_ELEMENT_QUALIF NUMBER(24,0) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN doc_element_quantif.id_doc_element_quantif IS 'Chave primária' ;
COMMENT ON COLUMN doc_element_quantif.id_doc_element_crit IS 'Critério do elemento' ;
COMMENT ON COLUMN doc_element_quantif.id_doc_quantification IS 'Quantificação' ;
COMMENT ON COLUMN doc_element_quantif.id_doc_element_qualif IS 'Relação elemento/qualificação' ;
COMMENT ON COLUMN doc_element_quantif.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN doc_element_quantif.adw_last_update IS 'Data da Última alteração' ;

