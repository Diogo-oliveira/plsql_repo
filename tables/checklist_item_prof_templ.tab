/** Tables */
CREATE TABLE CHECKLIST_ITEM_PROF_TEMPL(
    FLG_CONTENT_CREATOR VARCHAR2(1 CHAR) NOT NULL,
INTERNAL_NAME VARCHAR2(1000 CHAR) NOT NULL,
VERSION NUMBER(6) NOT NULL,
ITEM NUMBER(6) NOT NULL,
ID_PROFILE_TEMPLATE NUMBER(24) NOT NULL,
ID_CHECKLIST_ITEM NUMBER(24) NOT NULL,
CREATE_USER VARCHAR2(24 CHAR),
    CREATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
CREATE_INSTITUTION NUMBER(24),
UPDATE_USER VARCHAR2(24 CHAR),
UPDATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
UPDATE_INSTITUTION NUMBER(24)
);

COMMENT ON TABLE CHECKLIST_ITEM_PROF_TEMPL IS 'Authorized profiles for checklist item';

COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.FLG_CONTENT_CREATOR IS 'Content creator: Checklist created by (A)LERT, Checklist created by (I)nstitution';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.INTERNAL_NAME IS 'Checklist''s internal name (original name)';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.VERSION IS 'An integer to represent the version number of the checklist template';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.ITEM IS 'Item ID within a checklist';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.ID_PROFILE_TEMPLATE IS 'Authorized profile to answer this checklist item';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.ID_CHECKLIST_ITEM IS 'Checklist item';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.create_user IS 'Creation User';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.create_time IS 'Creation Time';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.create_institution IS 'Creation Institution';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.update_user IS 'Update User';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.update_time IS 'Update Time';
COMMENT ON COLUMN CHECKLIST_ITEM_PROF_TEMPL.update_institution IS 'Update Institution';
-- CHANGE END: Ariel Machado