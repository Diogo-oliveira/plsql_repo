CREATE TABLE EXAM_TYPE_TEMPLATE
 (ID_EXAM_TYPE_TEMPLATE NUMBER(24) NOT NULL
 ,ID_EXAM_TYPE NUMBER(24) NOT NULL
 ,ID_DOC_TEMPLATE NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 ,PERIOD NUMBER(1) NOT NULL
 ,SCREEN_NAME VARCHAR2(4000)
 );
 
COMMENT ON TABLE EXAM_TYPE_TEMPLATE IS 'Tabela com templates conforme tipo de exame e periodo da gravidez';
COMMENT ON COLUMN EXAM_TYPE_TEMPLATE.ID_EXAM_TYPE_TEMPLATE IS 'Chave primária';
COMMENT ON COLUMN EXAM_TYPE_TEMPLATE.ID_EXAM_TYPE          IS 'Tipo de exame';
COMMENT ON COLUMN EXAM_TYPE_TEMPLATE.ID_DOC_TEMPLATE       IS 'Template associado';
COMMENT ON COLUMN EXAM_TYPE_TEMPLATE.ID_INSTITUTION        IS 'Instituição';
COMMENT ON COLUMN EXAM_TYPE_TEMPLATE.ID_SOFTWARE           IS 'Software';
COMMENT ON COLUMN EXAM_TYPE_TEMPLATE.SCREEN_NAME IS 'Ecrã a carregar para o tipo de exame definido';
