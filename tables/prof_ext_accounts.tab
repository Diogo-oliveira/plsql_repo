

  CREATE TABLE PROF_EXT_ACCOUNTS
   (	ID_PROFESSIONAL_EXT NUMBER(24,0),
	ID_ACCOUNT NUMBER(24,0),
	VALUE VARCHAR2(200),
	ID_INSTITUTION NUMBER(24,0)
   ) ;


   COMMENT ON COLUMN PROF_EXT_ACCOUNTS.ID_PROFESSIONAL_EXT IS 'External professional ID';



   COMMENT ON COLUMN PROF_EXT_ACCOUNTS.ID_ACCOUNT IS 'Affiliation ID';



   COMMENT ON COLUMN PROF_EXT_ACCOUNTS.VALUE IS 'Value';



   COMMENT ON COLUMN PROF_EXT_ACCOUNTS.ID_INSTITUTION IS 'Institution ID';



   COMMENT ON TABLE PROF_EXT_ACCOUNTS  IS 'External professionals affiliations';


