CREATE TABLE LENS_ADVANCED_INPUT
 (ID_LENS NUMBER(24) NOT NULL
 ,ID_ADVANCED_INPUT NUMBER(6) NOT NULL
 )
/

COMMENT ON TABLE LENS_ADVANCED_INPUT IS 'Association between Lens and advanced inputs'
/

COMMENT ON COLUMN LENS_ADVANCED_INPUT.ID_LENS IS 'Lens Id'
/

COMMENT ON COLUMN LENS_ADVANCED_INPUT.ID_ADVANCED_INPUT IS 'Advanced Input ID to be used when prescribing this kind of lens'
/
