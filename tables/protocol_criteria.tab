
CREATE TABLE PROTOCOL_CRITERIA
 (ID_PROTOCOL_CRITERIA NUMBER(24) NOT NULL
 ,ID_PROTOCOL NUMBER(24) NOT NULL
 ,CRITERIA_TYPE VARCHAR2(1) NOT NULL
 ,GENDER VARCHAR2(1)
 ,MIN_AGE NUMBER(6)
 ,MAX_AGE NUMBER(6)
 ,MIN_WEIGHT NUMBER(10,3)
 ,MAX_WEIGHT NUMBER(10,3)
 ,ID_WEIGHT_UNIT_MEASURE NUMBER(24)
 ,MIN_HEIGHT NUMBER(10,3)
 ,MAX_HEIGHT NUMBER(10,3)
 ,ID_HEIGHT_UNIT_MEASURE NUMBER(24)
 ,IMC_MIN NUMBER(10,3)
 ,IMC_MAX NUMBER(10,3)
 ,ID_BLOOD_PRESSURE_UNIT_MEASURE NUMBER(24)
 ,MIN_BLOOD_PRESSURE_S NUMBER(10,3)
 ,MAX_BLOOD_PRESSURE_S NUMBER(10,3)
 ,MIN_BLOOD_PRESSURE_D NUMBER(10,3)
 ,MAX_BLOOD_PRESSURE_D NUMBER(10,3)
 );

COMMENT ON COLUMN PROTOCOL_CRITERIA.ID_PROTOCOL_CRITERIA IS 'ID of protocol criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.ID_PROTOCOL IS 'ID of protocol';
COMMENT ON COLUMN PROTOCOL_CRITERIA.CRITERIA_TYPE IS 'Criteria type: inlcusion/exclusion';
COMMENT ON COLUMN PROTOCOL_CRITERIA.GENDER IS 'Gender :  Undefined / Male / Female';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MIN_AGE IS 'Minimum age criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MAX_AGE IS 'Maximum age criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MIN_WEIGHT IS 'Minimum weight criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MAX_WEIGHT IS 'Maximum weight criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.ID_WEIGHT_UNIT_MEASURE IS 'Unit of measure for weight';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MIN_HEIGHT IS 'Minimum height criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MAX_HEIGHT IS 'Maximum height criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.ID_HEIGHT_UNIT_MEASURE IS 'Unit of measure for height';
COMMENT ON COLUMN PROTOCOL_CRITERIA.IMC_MIN IS 'Minimum IMC criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.IMC_MAX IS 'Maximum IMC criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.ID_BLOOD_PRESSURE_UNIT_MEASURE IS 'Unit of measure for blood pressure';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MIN_BLOOD_PRESSURE_S IS 'Minimum systolic blood pressure criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MAX_BLOOD_PRESSURE_S IS 'Maximum systolic blood pressure criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MIN_BLOOD_PRESSURE_D IS 'Minimum diastolic blood pressure criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA.MAX_BLOOD_PRESSURE_D IS 'Maximum diastolic blood pressure criteria';

