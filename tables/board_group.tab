

  CREATE TABLE BOARD_GROUP
   (	ID_BOARD_GROUP NUMBER(12,0) NOT NULL ENABLE,
	CODE_BOARD_GROUP VARCHAR2(200) NOT NULL ENABLE,
	CODE_HELP_BOARD_GROUP VARCHAR2(200),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE board_group IS 'Grupos de fluxogramas' ;

COMMENT ON COLUMN board_group.code_board_group IS 'Código para tradução dos grupos de fluxogramas' ;
COMMENT ON COLUMN board_group.code_help_board_group IS 'Código para tradução da explicação do grupo' ;
COMMENT ON COLUMN board_group.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN board_group.rank IS 'Ordem de visualização' ;
