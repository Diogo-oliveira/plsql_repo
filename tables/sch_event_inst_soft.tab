-- CHANGED BY: Telmo Castro
-- CHANGE DATE: 30-07-2010
-- CHANGE REASON: ALERT-114571

CREATE TABLE SCH_EVENT_INST_SOFT (
ID_SCH_EVENT NUMBER(24) NOT NULL,
ID_INSTITUTION NUMBER(24) DEFAULT 0 NOT NULL,
ID_SOFTWARE NUMBER(24) DEFAULT 0 NOT NULL,
FLG_AVAILABLE VARCHAR2(1 CHAR) DEFAULT 'Y' NOT NULL,
CREATE_USER VARCHAR2(24 CHAR),
CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE,
CREATE_INSTITUTION NUMBER(24),
UPDATE_USER VARCHAR2(24 CHAR),
UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE,
UPDATE_INSTITUTION NUMBER(24)
);

COMMENT ON TABLE SCH_EVENT_INST_SOFT IS 'parameterization table for scheduler events';

COMMENT ON COLUMN SCH_EVENT_INST_SOFT.ID_SCH_EVENT IS 'event id configured';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.ID_INSTITUTION IS 'event available in this institution. 0 = all';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.ID_SOFTWARE IS 'event available in this software. 0 = all';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.FLG_AVAILABLE IS 'Y = this row is active; N = this row is inactive';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.CREATE_USER IS 'creation user';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.CREATE_TIME IS 'creation time';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN SCH_EVENT_INST_SOFT.UPDATE_INSTITUTION IS 'Update Institution';
-- change end: Telmo

