

  CREATE TABLE SYS_ALERT_TYPE
   (	ID_SYS_ALERT_TYPE NUMBER(6,0) NOT NULL ENABLE,
	CODE_SYS_ALERT_TYPE VARCHAR2(200) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE sys_alert_type IS 'Contém os diferentes tipos de alertas (ex: major, minor, follow-up)' ;

COMMENT ON COLUMN sys_alert_type.id_sys_alert_type IS 'ID único' ;
COMMENT ON COLUMN sys_alert_type.code_sys_alert_type IS 'Código para obtenção da descrição' ;
COMMENT ON COLUMN sys_alert_type.rank IS 'Ordem para visualização' ;
COMMENT ON COLUMN sys_alert_type.adw_last_update IS 'Data da última alteração' ;

-- CMF 11-01-2016
COMMENT ON TABLE sys_alert_type IS 'Types of alerts' ;

COMMENT ON COLUMN sys_alert_type.id_sys_alert_type IS 'Primary key' ;
COMMENT ON COLUMN sys_alert_type.code_sys_alert_type IS 'Code for translating purpose' ;
COMMENT ON COLUMN sys_alert_type.rank IS 'value for ordering purpose' ;
COMMENT ON COLUMN sys_alert_type.adw_last_update IS 'DEPRECATED. Do not use.' ;
