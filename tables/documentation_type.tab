

  CREATE TABLE DOCUMENTATION_TYPE
   (	ID_DOCUMENTATION_TYPE NUMBER(24,0) NOT NULL ENABLE,
	INTERNAL_NAME VARCHAR2(240),
	ID_GLOBAL NUMBER(24,0),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN documentation_type.id_documentation_type IS 'Chave primária' ;
COMMENT ON COLUMN documentation_type.internal_name IS 'Nome interno' ;
COMMENT ON COLUMN documentation_type.id_global IS 'Chave de relacionamento' ;
COMMENT ON COLUMN documentation_type.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN documentation_type.adw_last_update IS 'Data da última alteração' ;


ALTER TABLE DOCUMENTATION_TYPE ADD (ID_CONTENT VARCHAR2(200));
COMMENT ON COLUMN DOCUMENTATION_TYPE.ID_CONTENT  IS 'Identificador único de conteúdo';