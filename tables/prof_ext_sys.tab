

  CREATE TABLE PROF_EXT_SYS
   (	ID_PROF_EXT_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_EXTERNAL_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	VALUE VARCHAR2(30) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE prof_ext_sys IS 'Correspondência dos identificadores únicos dos profissionais, no Alert e nos sistemas externos' ;

COMMENT ON COLUMN prof_ext_sys.value IS 'Identificador único do profissional, no sistema externo indicado' ;
