 ,ID_PROTOCOL_CRITERIA NUMBER(24) NOT NULL
 ,ID_LINK_OTHER_CRITERIA NUMBER(24)
 ,ID_LINK_OTHER_CRITERIA_TYPE NUMBER(24) NOT NULL
 );

COMMENT ON COLUMN PROTOCOL_CRITERIA_LINK.ID_PROTOCOL_CRITERIA_LINK IS 'ID of protocol criteria link';
COMMENT ON COLUMN PROTOCOL_CRITERIA_LINK.ID_PROTOCOL_CRITERIA IS 'ID of protocol criteria';
COMMENT ON COLUMN PROTOCOL_CRITERIA_LINK.ID_LINK_OTHER_CRITERIA IS 'ID of other criteria link';
COMMENT ON COLUMN PROTOCOL_CRITERIA_LINK.ID_LINK_OTHER_CRITERIA_TYPE IS 'Type of other criteria link:  Task types - declared in package';

