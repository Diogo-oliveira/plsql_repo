CREATE TABLE DOC_ELEMENT_DOMAIN
(
    CODE_ELEMENT_DOMAIN VARCHAR2(200),
    ID_LANGUAGE NUMBER(6,0),
    VAL VARCHAR2(30),
    DESC_VAL VARCHAR2(200),
    RANK NUMBER(6,0),
    IMG_NAME VARCHAR2(200),
    FLG_AVAILABLE VARCHAR2(1),
    ADW_LAST_UPDATE DATE
);

COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.CODE_ELEMENT_DOMAIN IS 'Domain ID for a multi-choice';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.ID_LANGUAGE IS 'Language';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.VAL IS 'Domain value';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.DESC_VAL IS 'Description of value';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.RANK IS 'Order';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.IMG_NAME IS 'Icon';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.FLG_AVAILABLE IS 'Indicates if this record can be used ''Y'' - yes, ''N'' - no';
COMMENT ON COLUMN DOC_ELEMENT_DOMAIN.ADW_LAST_UPDATE IS 'Record modification date';
COMMENT ON TABLE DOC_ELEMENT_DOMAIN  IS 'Domain values for multi-choice data elements';

