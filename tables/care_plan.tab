

  CREATE TABLE "ALERT"."CARE_PLAN"
   (	"ID_CARE_PLAN" NUMBER(12,0),
	"ID_PROF" NUMBER(24,0),
	"DT_CARE_PLAN" TIMESTAMP (6) WITH LOCAL TIME ZONE,
	"ID_PATIENT" NUMBER(24,0),
	"FLG_STATUS" VARCHAR2(2),
	"STR_STATUS" VARCHAR2(100),
	"NAME" VARCHAR2(200),
	"ID_CARE_PLAN_TYPE" NUMBER(12,0),
	"DT_BEGIN" TIMESTAMP (6) WITH LOCAL TIME ZONE,
	"DT_END" TIMESTAMP (6) WITH LOCAL TIME ZONE,
	"SUBJECT_TYPE" VARCHAR2(2),
	"ID_SUBJECT" NUMBER(24,0),
	"ID_PROF_COORDINATOR" NUMBER(24,0),
	"GOALS" VARCHAR2(4000),
	"NOTES" VARCHAR2(4000),
	"ID_PROF_CANCEL" NUMBER(24,0),
	"DT_CANCEL" TIMESTAMP (6) WITH LOCAL TIME ZONE,
	"NOTES_CANCEL" VARCHAR2(4000)
   ) ;


