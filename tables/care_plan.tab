COMMENT ON TABLE CARE_PLAN IS 'Tabela de registo dos Care Plans';
COMMENT ON COLUMN CARE_PLAN.ID_CARE_PLAN IS 'Id do Care Plan';
COMMENT ON COLUMN CARE_PLAN.ID_PROF IS 'Id do profissional que registou o Care Plan';
COMMENT ON COLUMN CARE_PLAN.DT_CARE_PLAN IS 'Data de registo do Care Plan';
COMMENT ON COLUMN CARE_PLAN.ID_PATIENT IS 'Id do paciente';
COMMENT ON COLUMN CARE_PLAN.FLG_STATUS IS 'Flag que indica o estado do Care Plan';
COMMENT ON COLUMN CARE_PLAN.STR_STATUS IS 'String que indica o estado do Care Plan';
COMMENT ON COLUMN CARE_PLAN.NAME IS 'Nome do Care Plan';
COMMENT ON COLUMN CARE_PLAN.ID_CARE_PLAN_TYPE IS 'Id do tipo do Care Plan';
COMMENT ON COLUMN CARE_PLAN.DT_BEGIN IS 'Data de início do Care Plan';
COMMENT ON COLUMN CARE_PLAN.DT_END IS 'Data de fim do Care Plan';
COMMENT ON COLUMN CARE_PLAN.SUBJECT_TYPE IS 'Flag que indica o tipo de patologia do Care Plan: A - Alergias; D - Diagnósticos confirmados; R - Doenças Relevantes';
COMMENT ON COLUMN CARE_PLAN.ID_SUBJECT IS 'Id da patologia do Care Plan';
COMMENT ON COLUMN CARE_PLAN.ID_PROF_COORDINATOR IS 'Id do coordenador do Care Plan';
COMMENT ON COLUMN CARE_PLAN.GOALS IS 'Objectivos do Care Plan';
COMMENT ON COLUMN CARE_PLAN.NOTES IS 'Notas do Care Plan';
COMMENT ON COLUMN CARE_PLAN.ID_PROF_CANCEL IS 'Id do profissional que cancelou o Care Plan';
COMMENT ON COLUMN CARE_PLAN.DT_CANCEL IS 'Data de cancelamento do Care Plan';
COMMENT ON COLUMN CARE_PLAN.NOTES_CANCEL IS 'Notas de cancelamento do Care Plan';


