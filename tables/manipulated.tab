

  CREATE TABLE MANIPULATED
   (	ID_MANIPULATED NUMBER(24,0) NOT NULL ENABLE,
	CODE_MANIPULATED VARCHAR2(200) NOT NULL ENABLE,
	ID_MANIPULATED_GROUP NUMBER(24,0) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE manipulated IS 'Manipulados.' ;

COMMENT ON COLUMN manipulated.id_manipulated IS 'Chave primária.' ;
COMMENT ON COLUMN manipulated.code_manipulated IS 'Nome do constituinte' ;
COMMENT ON COLUMN manipulated.id_manipulated_group IS 'ID  do grupo.' ;
COMMENT ON COLUMN manipulated.flg_type IS 'Manipulado para a farmácia exterior (E) ou para a farmácia da instituição (I).' ;
COMMENT ON COLUMN manipulated.adw_last_update IS 'Data da última alteração.' ;
