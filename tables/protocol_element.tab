
CREATE TABLE PROTOCOL_ELEMENT
 (ID_PROTOCOL_ELEMENT NUMBER(24) NOT NULL
 ,ID_PROTOCOL NUMBER(24) NOT NULL
 ,ID_ELEMENT NUMBER(24)
 ,ELEMENT_TYPE VARCHAR2(1)
 ,DESC_ELEMENT VARCHAR2(1000)
 ,X_COORDINATE NUMBER
 ,Y_COORDINATE NUMBER
 ,FLG_AVAILABLE VARCHAR2(1)
 );
 
COMMENT ON COLUMN PROTOCOL_ELEMENT.ID_PROTOCOL_ELEMENT IS 'ID of protocol element';
COMMENT ON COLUMN PROTOCOL_ELEMENT.ID_ELEMENT IS 'Code protocol for translation';
COMMENT ON COLUMN PROTOCOL_ELEMENT.ELEMENT_TYPE IS 'Element type (text,question,task)';
COMMENT ON COLUMN PROTOCOL_ELEMENT.DESC_ELEMENT IS 'Description of protocol element';
COMMENT ON COLUMN PROTOCOL_ELEMENT.X_COORDINATE IS 'X Coordinate of element in grid';
COMMENT ON COLUMN PROTOCOL_ELEMENT.Y_COORDINATE IS 'Y Coordinate of element in grid';
COMMENT ON COLUMN PROTOCOL_ELEMENT.FLG_AVAILABLE IS 'Available Flag';

