-- Creating Table 'TEAM_TYPE_INST_SOFT'
CREATE TABLE TEAM_TYPE_INST_SOFT
 (ID_TEAM_TYPE_INST_SOFT NUMBER(24) NOT NULL
 ,ID_TEAM_TYPE NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1)
 ,CREATE_USER             VARCHAR2(24)
 ,CREATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION      NUMBER(24)
 ,UPDATE_USER             VARCHAR2(24)
 ,UPDATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION      NUMBER(24)
 
 );

COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.ID_TEAM_TYPE_INST_SOFT IS 'ID';

COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.ID_TEAM_TYPE IS 'ID Team type';

COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.ID_INSTITUTION IS 'ID institution';

COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.ID_SOFTWARE IS 'ID software';

COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.FLG_AVAILABLE IS 'if it is available';

COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN TEAM_TYPE_INST_SOFT.UPDATE_INSTITUTION IS 'Update Institution';
