

  CREATE TABLE HOME
   (	ID_HOME NUMBER(24,0) NOT NULL ENABLE,
	ID_PAT_FAMILY NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_REGISTRY DATE NOT NULL ENABLE,
	NUM_ROOMS NUMBER(2,0),
	NUM_BEDROOMS NUMBER(2,0),
	NUM_PERSON_ROOM NUMBER(2,0),
	FLG_WC_TYPE VARCHAR2(1),
	FLG_WC_LOCATION VARCHAR2(1),
	FLG_WC_OUT VARCHAR2(1),
	FLG_WATER_DISTRIB VARCHAR2(1),

	FLG_WATER_ORIGIN VARCHAR2(1),
	FLG_CONSERV VARCHAR2(1),
	FLG_OWNER VARCHAR2(1),
	FLG_HAB_TYPE VARCHAR2(1),
	FLG_LIGHT VARCHAR2(1),
	FLG_HEAT VARCHAR2(1),
	ARQUITECT_BARRIER VARCHAR2(4000)
   );


COMMENT ON TABLE home IS 'Condições habitacionais' ;

COMMENT ON COLUMN home.id_pat_family IS 'Chave primaria' ;
COMMENT ON COLUMN home.num_rooms IS 'Nº de divisões da casa' ;
COMMENT ON COLUMN home.num_bedrooms IS 'Nº de quartos' ;
COMMENT ON COLUMN home.num_person_room IS 'Nº de pessoas por quarto' ;
COMMENT ON COLUMN home.flg_wc_type IS 'C - completo, I - incompleto, N - não tem' ;
COMMENT ON COLUMN home.flg_wc_location IS 'H - na habitação, F - fora da habitação, N - inexistente' ;
COMMENT ON COLUMN home.flg_wc_out IS 'Modo de lançamento no exterior: R - rede pública, F - fossa séptica, O - outro, N - nenhum' ;
COMMENT ON COLUMN home.flg_water_distrib IS 'D - domiciliária, T - torneira no quintal, F - fontanário (< 100m de casa), M - fontanário (> 100m de casa)' ;
COMMENT ON COLUMN home.flg_water_origin IS 'P - pública, S - semi-pública, T - particular' ;
COMMENT ON COLUMN home.flg_conserv IS 'B - bom, D - degradado, R - razoável,' ;
COMMENT ON COLUMN home.flg_owner IS 'A - arrendada, C - cedida, F - família, P - própria, O - outro' ;
COMMENT ON COLUMN home.flg_hab_type IS 'A - andar, Q - quarto, B - barraca, C - abarracada, R - rural, T - tradicional, V - vivenda, U - urbana, S - sem abrigo' ;
COMMENT ON COLUMN home.flg_light IS 'Existência de luz' ;

COMMENT ON COLUMN home.flg_heat IS 'C-  central, L - local, N - nenhum' ;
COMMENT ON COLUMN home.arquitect_barrier IS 'Barreiras arquitectónicas' ;

alter table home
add (dt_registry_tstz timestamp with local time zone); 

alter table home
add (flg_hab_location varchar2(1));

comment on column home.flg_hab_location is 'Zona de habitação: (R)ural, (S)uburbana ou (U)rbana';


alter table home
add (notes varchar2(4000));

comment on column home.notes is 'Notas';



