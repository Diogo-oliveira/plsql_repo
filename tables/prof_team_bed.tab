
create table prof_team_bed
(
  ID_PROF_TEAM_ROOM   NUMBER(24) NOT NULL,
  ID_BED              NUMBER(24) NOT NULL
);


COMMENT ON TABLE prof_team_bed IS 'Beds assigned to a specific team.';
COMMENT ON COLUMN prof_team_bed.ID_PROF_TEAM_ROOM IS 'Assignment of a room to a team';
COMMENT ON COLUMN prof_team_bed.ID_BED IS 'Bed ID';

