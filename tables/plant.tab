
  CREATE TABLE PLANT
   (	ID_PLANT NUMBER(24,0) NOT NULL ENABLE,
	CODE_PLANT VARCHAR2(200) NOT NULL ENABLE,
	IMAGE_PLANT LONG RAW NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(12,0),
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );



COMMENT ON COLUMN plant.id_plant IS 'Chave primária' ;
COMMENT ON COLUMN plant.code_plant IS 'Descrição da planta' ;
COMMENT ON COLUMN plant.image_plant IS 'Data da última alteração' ;
COMMENT ON COLUMN plant.adw_last_update IS 'Imagem da planta' ;

