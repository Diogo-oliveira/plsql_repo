  COMMENT ON COLUMN NNN_EPIS_DIAG_EVAL.ID_VISIT
IS
  'Visit ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAG_EVAL.ID_PROFESSIONAL
IS
  'Professional ID who did the evaluation' ;
  COMMENT ON COLUMN NNN_EPIS_DIAG_EVAL.ID_CANCEL_REASON
IS
  'Cancel reason ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAG_EVAL.CANCEL_NOTES
IS
  'Cancelation notes' ;
  COMMENT ON COLUMN NNN_EPIS_DIAG_EVAL.FLG_STATUS
