CREATE TABLE EPIS_COMP_DETAIL_AXE
(
  ID_EPIS_COMP_DETAIL_AXE NUMBER(24) NOT NULL,
  ID_EPIS_COMP_DETAIL     NUMBER(24) NOT NULL,
  ID_PARENT_COMP_AXE      NUMBER(24) NOT NULL,
  ID_COMP_AXE             NUMBER(24) NOT NULL,
  CREATE_USER             VARCHAR2(24 CHAR),
  CREATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE,
  CREATE_INSTITUTION      NUMBER(24),
  UPDATE_USER             VARCHAR2(24 CHAR),
  UPDATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE,
  UPDATE_INSTITUTION      NUMBER(24)  
);

ALTER TABLE EPIS_COMP_DETAIL_AXE MOVE TABLESPACE TABLE_M;

COMMENT ON TABLE EPIS_COMP_DETAIL_AXE IS 'Episode comp axe detail information';
COMMENT ON COLUMN EPIS_COMP_DETAIL_AXE.ID_EPIS_COMP_DETAIL_AXE IS 'Primary key.';
COMMENT ON COLUMN EPIS_COMP_DETAIL_AXE.ID_EPIS_COMP_DETAIL IS 'Epis complication detail id.';
COMMENT ON COLUMN EPIS_COMP_DETAIL_AXE.ID_PARENT_COMP_AXE IS 'Epis_comp_detail.id_comp_axe; For instance External factor ID';
COMMENT ON COLUMN EPIS_COMP_DETAIL_AXE.ID_COMP_AXE IS 'Child comp axe id; Sort of external factor medication';
