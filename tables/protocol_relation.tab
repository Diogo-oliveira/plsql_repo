
CREATE TABLE PROTOCOL_RELATION
 (ID_PROTOCOL_RELATION NUMBER(24) NOT NULL
 ,ID_PROTOCOL NUMBER(24) NOT NULL
 ,ID_PROTOCOL_ELEMENT_PARENT NUMBER(24) NOT NULL
 ,ID_PROTOCOL_CONNECTOR NUMBER(24) NOT NULL
 ,ID_PROTOCOL_ELEMENT NUMBER(24) NOT NULL
 ,DESC_RELATION VARCHAR2(200)
 ,FLG_AVAILABLE VARCHAR2(1)
 );

COMMENT ON COLUMN PROTOCOL_RELATION.ID_PROTOCOL_RELATION IS 'ID of protocol relation';
COMMENT ON COLUMN PROTOCOL_RELATION.ID_PROTOCOL IS 'ID of protocol';
COMMENT ON COLUMN PROTOCOL_RELATION.ID_PROTOCOL_ELEMENT_PARENT IS 'ID of parent element';
COMMENT ON COLUMN PROTOCOL_RELATION.ID_PROTOCOL_CONNECTOR IS 'ID of connector';
COMMENT ON COLUMN PROTOCOL_RELATION.ID_PROTOCOL_ELEMENT IS 'ID of element';
COMMENT ON COLUMN PROTOCOL_RELATION.DESC_RELATION IS 'Desccription of relation';
COMMENT ON COLUMN PROTOCOL_RELATION.FLG_AVAILABLE IS 'Available flag';

