-- Creating Table 'ICNP_TASK_COMP_SOFT_INST'
BEGIN

EXECUTE IMMEDIATE 'CREATE TABLE ICNP_TASK_COMP_SOFT_INST
 (ID_TASK NUMBER(24) NOT NULL
 ,ID_TASK_TYPE NUMBER(24) NOT NULL
 ,ID_COMPOSITION NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) DEFAULT 0 NOT NULL
 ,ID_INSTITUTION NUMBER(24) DEFAULT 0 NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1 CHAR) DEFAULT ''Y'' NOT NULL
 ,CREATE_USER VARCHAR2(24 CHAR)
 ,CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION NUMBER(24,0)
 ,UPDATE_USER VARCHAR2(24 CHAR)
 ,UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24,0)
 )';
EXCEPTION
        WHEN OTHERS THEN
            dbms_output.put_line('WARNING: Object already exists.');
END;
/

COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.ID_TASK IS 'id task (exam, procedure...)';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.ID_TASK_TYPE IS 'id task type';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.ID_COMPOSITION IS 'id composition';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.ID_SOFTWARE IS 'id software';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.ID_INSTITUTION IS 'id institution';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.FLG_AVAILABLE IS 'it is available';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN ICNP_TASK_COMP_SOFT_INST.UPDATE_INSTITUTION IS 'Update Institution';
