-- CHANGE REASON: [ALERT-290969] Nursing Care Plan: NANDA, NIC, NOC - Data Model
BEGIN
  pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'NAN_CLASS', i_obj_type => 'TABLE', i_flg_category => 'CNT', i_flg_alert_default => 'N', i_delete_script => '', i_flg_default_child => 'N', i_flg_nzd => 'N', i_responsible_team => 'NURSING');
END;
/
CREATE TABLE NAN_CLASS
  (
    ID_NAN_CLASS           NUMBER (24) NOT NULL ,
    ID_TERMINOLOGY_VERSION NUMBER (24) NOT NULL ,
    CLASS_CODE             VARCHAR2 (200 CHAR) NOT NULL ,
    CODE_NAME              VARCHAR2 (200 CHAR) AS ( 'NAN_CLASS.CODE_NAME.'
    ||TO_CHAR(ID_NAN_CLASS) ) VIRTUAL NOT NULL ,
    CODE_DEFINITION VARCHAR2 (200 CHAR) AS ( 'NAN_CLASS.CODE_DEFINITION.'
    ||TO_CHAR(ID_NAN_CLASS) ) VIRTUAL NOT NULL ,
    RANK               NUMBER (6) ,
    ID_INST_OWNER      NUMBER (24) ,
    ID_CONCEPT_VERSION NUMBER (24) ,
    ID_CONCEPT_TERM    NUMBER (24) ,
    ID_NAN_DOMAIN      NUMBER (24) NOT NULL ,
    CREATE_USER        VARCHAR2 (30 CHAR) ,
    CREATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION NUMBER (24) ,
    UPDATE_USER        VARCHAR2 (30 CHAR) ,
    UPDATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION NUMBER (24)
  )
  TABLESPACE ALERT_DATA ;
COMMENT ON TABLE NAN_CLASS
IS
  'NANDA - Nursing Diagnosis Class' ;
  COMMENT ON COLUMN NAN_CLASS.ID_NAN_CLASS
IS
  'Class ID (surrogate key)' ;
  COMMENT ON COLUMN NAN_CLASS.ID_TERMINOLOGY_VERSION
IS
  'NANDA Version ID' ;
  COMMENT ON COLUMN NAN_CLASS.CLASS_CODE
IS
  'NANDA Class code' ;
  COMMENT ON COLUMN NAN_CLASS.CODE_NAME
IS
  'Translation code for NANDA Class label' ;
  COMMENT ON COLUMN NAN_CLASS.CODE_DEFINITION
IS
  'Translation code for NANDA Class definition' ;
  COMMENT ON COLUMN NAN_CLASS.RANK
IS
  'Item order' ;
  COMMENT ON COLUMN NAN_CLASS.ID_INST_OWNER
IS
  'Institution owner of the concept. 0 - ALERT' ;
  COMMENT ON COLUMN NAN_CLASS.ID_CONCEPT_VERSION
IS
  'Concept version ID' ;
  COMMENT ON COLUMN NAN_CLASS.ID_CONCEPT_TERM
IS
  'Concept term ID' ;
  COMMENT ON COLUMN NAN_CLASS.ID_NAN_DOMAIN
IS
  'Domain ID (surrogate key)' ;
  COMMENT ON COLUMN NAN_CLASS.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NAN_CLASS.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NAN_CLASS.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NAN_CLASS.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NAN_CLASS.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NAN_CLASS.UPDATE_INSTITUTION
IS
  'Update Institution' ;
