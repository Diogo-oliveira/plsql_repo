

  CREATE TABLE PAT_PREGNANCY_RISK
   (	ID_PAT_PREGNANCY_RISK NUMBER(24,0) NOT NULL ENABLE,
	DT_PAT_PREGNANCY_RISK DATE NOT NULL ENABLE,
	ID_PATIENT NUMBER(24,0) NOT NULL ENABLE,
	ID_PREGNANCY_RISK_EVAL NUMBER(12,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	POINTS NUMBER(2,0) NOT NULL ENABLE
   );


COMMENT ON TABLE pat_pregnancy_risk IS 'Avaliação do grau de risco de gravidez da utente, na consulta de planeamento familiar' ;

COMMENT ON COLUMN pat_pregnancy_risk.dt_pat_pregnancy_risk IS 'Data de avaliação / registo' ;
COMMENT ON COLUMN pat_pregnancy_risk.points IS 'Pontuação fianl' ;

