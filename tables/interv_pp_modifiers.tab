CREATE TABLE INTERV_PP_MODIFIERS
 (ID_INTERV_PP_MODIFIERS NUMBER(24) NOT NULL
 ,ID_INTERV_PRESC_PLAN NUMBER(24) NOT NULL
 ,ID_MODIFIER NUMBER(24) NOT NULL
 ,ID_INST_OWNER NUMBER(24) NOT NULL
 ,CREATE_USER VARCHAR2(24)
 ,CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION NUMBER(24)
 ,UPDATE_USER VARCHAR2(24)
 ,UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
 );

COMMENT ON TABLE INTERV_PP_MODIFIERS IS 'Procedures modifying factors for an execution plan';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.ID_INTERV_PP_MODIFIERS IS 'Primary key';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.ID_INTERV_PRESC_PLAN IS 'Procedure execution ID';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.ID_MODIFIER IS 'Modifying factor ID (terminology server ID)';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.ID_INST_OWNER IS 'Institution owner ID (terminology server ID)';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN INTERV_PP_MODIFIERS.UPDATE_INSTITUTION IS 'Update Institution';
