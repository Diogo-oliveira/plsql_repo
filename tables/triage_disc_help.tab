

  CREATE TABLE TRIAGE_DISC_HELP
   (	ID_TRIAGE_DISC_HELP NUMBER(12,0) NOT NULL ENABLE,
	CODE_TRIAGE_DISC_HELP VARCHAR2(200) NOT NULL ENABLE,
	CODE_TITLE_TRIAGE_DISC_HELP VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE
   );


COMMENT ON TABLE triage_disc_help IS 'Textos de ajuda dos discriminadores, de acordo com o manual de triagem de Manchester' ;

COMMENT ON COLUMN triage_disc_help.code_triage_disc_help IS 'Código para tradução da ajuda' ;
COMMENT ON COLUMN triage_disc_help.code_title_triage_disc_help IS 'Código para tradução do título da ajuda' ;
COMMENT ON COLUMN triage_disc_help.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN triage_disc_help.id_triage_disc_help IS 'Chave primária' ;


