-- CHANGED BY: Pedro Albuquerque
-- CHANGED DATE: 2009-ABR-02
-- CHANGING REASON: create table
-- Create table
create table IRREGULAR_FREQUENCY
(
  ID_IRREGULAR_FREQUENCY   	NUMBER(12),
  INTERNAL_NAME            	VARCHAR2(50),
  CODE_IRREGULAR_FREQUENCY  VARCHAR2(50),
  RANK						NUMBER(6) default null
)
tablespace TABLE_S
  pctfree 10
  pctused 40
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    minextents 1
    maxextents unlimited
  );

alter table IRREGULAR_FREQUENCY
  add constraint IF_PK primary key (ID_IRREGULAR_FREQUENCY)
  using index 
  tablespace TABLE_S;
  
comment on table IRREGULAR_FREQUENCY is 'Tabela que contém a lista das frequências para as posologias irregulares';
comment on column IRREGULAR_FREQUENCY.ID_IRREGULAR_FREQUENCY is 'ID das frequências';
comment on column IRREGULAR_FREQUENCY.INTERNAL_NAME is 'Descritivo das frequências';
comment on column IRREGULAR_FREQUENCY.CODE_IRREGULAR_FREQUENCY is 'Código da tabela translation para as frequências';
comment on column irregular_frequency.rank is 'Order values' ;
-- CHANGE END Pedro Albuquerque
