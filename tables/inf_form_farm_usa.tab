
  CREATE TABLE INF_FORM_FARM_USA
   (	FORM_FARM_ID NUMBER(4,0) NOT NULL ENABLE,
	DESCR VARCHAR2(80) NOT NULL ENABLE
   );


COMMENT ON TABLE inf_form_farm_usa IS 'Formas farmacêuticas.' ;

COMMENT ON COLUMN inf_form_farm_usa.form_farm_id IS 'Código identificador da forma farmacêutica do medicamento.' ;
COMMENT ON COLUMN inf_form_farm_usa.descr IS 'Descrição da forma farmacêutica do medicamento, de acordo com a lista "Standard Terms" (PharmEuropa).' ;

