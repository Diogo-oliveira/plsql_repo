

  CREATE TABLE PENDING_ISSUE_TITLE
   (	ID_PENDING_ISSUE_TITLE NUMBER(24,0),
	DESC_TITLE VARCHAR2(200),
	CREATE_USER VARCHAR2(24),
	CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	CREATE_INSTITUTION NUMBER(24,0),
	UPDATE_USER VARCHAR2(24),
	UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	UPDATE_INSTITUTION NUMBER(24,0)
   ) ;


   COMMENT ON COLUMN PENDING_ISSUE_TITLE.ID_PENDING_ISSUE_TITLE IS 'Pending issue title ID';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.DESC_TITLE IS 'Pending issue title desciption';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.CREATE_USER IS 'Creation User';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.CREATE_TIME IS 'Creation Time';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.CREATE_INSTITUTION IS 'Creation Institution';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.UPDATE_USER IS 'Update User';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.UPDATE_TIME IS 'Update Time';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE.UPDATE_INSTITUTION IS 'Update Institution';



   COMMENT ON TABLE PENDING_ISSUE_TITLE  IS 'Pending issue title';

   comment on column PENDING_ISSUE_TITLE.ID_PENDING_ISSUE_TITLE  is 'Pending issue title identification';

   comment on column PENDING_ISSUE_TITLE.DESC_TITLE  is 'Pending issue title description';

