

  CREATE TABLE ROTATION_INTERVAL
   (	ID_ROTATION_INTERVAL NUMBER(24,0) NOT NULL ENABLE,
	INTERVAL VARCHAR2(60) NOT NULL ENABLE,
	INTERNAL_NAME VARCHAR2(200),
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );



