create table JUSTIFY_REA_AREA
(
  ID_JUSTIFY_REA_AREA NUMBER(24) not null,
  INTERN_NAME         VARCHAR2(200 CHAR) not null,
  CREATE_USER         VARCHAR2(24),
  CREATE_TIME         TIMESTAMP(6) WITH LOCAL TIME ZONE,
  CREATE_INSTITUTION  NUMBER(24),
  UPDATE_USER         VARCHAR2(24 CHAR),
  UPDATE_TIME         TIMESTAMP(6) WITH LOCAL TIME ZONE,
  UPDATE_INSTITUTION  NUMBER(24)
)


-- Add comments to the table 
comment on table JUSTIFY_REA_AREA
  is 'Contains the different areas in ALERT for justify reasons.';
-- Add comments to the columns 
comment on column JUSTIFY_REA_AREA.ID_JUSTIFY_REA_AREA
  is 'Justify reason area primary key';
comment on column JUSTIFY_REA_AREA.INTERN_NAME
  is 'Justify reason internal name';
  
  
  
  
