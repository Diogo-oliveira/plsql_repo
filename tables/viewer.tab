

  CREATE TABLE VIEWER
   (	ID_VIEWER NUMBER(24,0) NOT NULL ENABLE,
	INTERNAL_NAME VARCHAR2(200) NOT NULL ENABLE,
	MC_NAME VARCHAR2(50)
   );


COMMENT ON TABLE viewer IS 'Identificação de todos os visualizadores existentes na aplicação. Exemplos: Viewer do PC, Cabeçalho.' ;

COMMENT ON COLUMN viewer.id_viewer IS 'Identificador.' ;
COMMENT ON COLUMN viewer.internal_name IS 'Nome interno.' ;
COMMENT ON COLUMN viewer.mc_name IS 'Nome do movie clip flash' ;


