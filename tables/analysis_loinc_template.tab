

  CREATE TABLE ANALYSIS_LOINC_TEMPLATE
   (	ID_ANALYSIS_LOINC_TEMPLATE NUMBER(24,0) NOT NULL ENABLE,
	ID_ANALYSIS NUMBER(12,0) NOT NULL ENABLE,
	LOINC_CODE VARCHAR2(200) NOT NULL ENABLE
   );


COMMENT ON TABLE analysis_loinc_template IS 'Definição dos códigos LOINC associáveis às análises. Aqui ficam arquivadas todas as possibilidades estudadas para a associação de análises a códigos LOINC, ainda que o código escolhido por cada instituição fique associado à análise em ANALYSIS_DEP_CLIN_SERV.' ;

COMMENT ON COLUMN analysis_loinc_template.loinc_code IS 'Código LOINC' ;


