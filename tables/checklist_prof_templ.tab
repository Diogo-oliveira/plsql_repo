/** Tables */
CREATE TABLE CHECKLIST_PROF_TEMPL(
    FLG_CONTENT_CREATOR VARCHAR2(1 CHAR) NOT NULL,
INTERNAL_NAME VARCHAR2(1000 CHAR) NOT NULL,
VERSION NUMBER(6) NOT NULL,
ID_CHECKLIST_VERSION NUMBER(24) NOT NULL,
ID_PROFILE_TEMPLATE NUMBER(24) NOT NULL,
FLG_WRITE VARCHAR2(1 CHAR) NOT NULL,
FLG_DEFAULT VARCHAR2(1 CHAR) NOT NULL,
CREATE_USER VARCHAR2(24 CHAR),
    CREATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
CREATE_INSTITUTION NUMBER(24),
UPDATE_USER VARCHAR2(24 CHAR),
UPDATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
UPDATE_INSTITUTION NUMBER(24)
);

COMMENT ON TABLE CHECKLIST_PROF_TEMPL IS 'Authorized profiles for checklist';

COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.FLG_CONTENT_CREATOR IS 'Content creator: Checklist created by (A)LERT, Checklist created by (I)nstitution';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.INTERNAL_NAME IS 'Checklist''s internal name (original name)';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.VERSION IS 'An integer to represent the version number of the checklist template';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.ID_CHECKLIST_VERSION IS 'Referred checklist (specific version)';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.ID_PROFILE_TEMPLATE IS 'Authorized profile to use checklist';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.FLG_WRITE IS 'This profile, in addition to visualize the checklist, can make records: (Y)es, (N)o';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.FLG_DEFAULT IS 'Checklist is considered as default for this profile: (Y)es, (N)o';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN CHECKLIST_PROF_TEMPL.UPDATE_INSTITUTION IS 'Update Institution';
-- CHANGE END: Ariel Machado