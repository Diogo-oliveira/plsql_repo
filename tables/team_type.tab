-- Creating Table 'TEAM_TYPE'
CREATE TABLE TEAM_TYPE
 (ID_TEAM_TYPE NUMBER(24) NOT NULL
 ,CODE_TEAM_TYPE VARCHAR2(200) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1)
 ,RANK NUMBER(24)
 ,CREATE_USER             VARCHAR2(24)
 ,CREATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION      NUMBER(24)
 ,UPDATE_USER             VARCHAR2(24)
 ,UPDATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION      NUMBER(24)

 );

COMMENT ON COLUMN TEAM_TYPE.ID_TEAM_TYPE IS 'ID';

COMMENT ON COLUMN TEAM_TYPE.CODE_TEAM_TYPE IS 'Code for translation';

COMMENT ON COLUMN TEAM_TYPE.FLG_AVAILABLE IS 'it is available';

COMMENT ON COLUMN TEAM_TYPE.RANK IS 'order of apperance';

COMMENT ON COLUMN TEAM_TYPE.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN TEAM_TYPE.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN TEAM_TYPE.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN TEAM_TYPE.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN TEAM_TYPE.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN TEAM_TYPE.UPDATE_INSTITUTION IS 'Update Institution';
-- CHANGE END: Rita Lopes