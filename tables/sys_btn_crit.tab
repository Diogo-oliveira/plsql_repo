

  CREATE TABLE SYS_BTN_CRIT
   (	ID_SYS_BTN_CRIT NUMBER NOT NULL ENABLE,
	ID_SYS_BUTTON NUMBER NOT NULL ENABLE,
	ID_CRITERIA NUMBER,
	FLG_AVAILABLE VARCHAR2(1),
	RANK NUMBER(3,0),
	FLG_MANDATORY VARCHAR2(1)
   );


COMMENT ON COLUMN sys_btn_crit.id_sys_btn_crit IS 'PK' ;
COMMENT ON COLUMN sys_btn_crit.id_sys_button IS 'ID do botão' ;
COMMENT ON COLUMN sys_btn_crit.id_criteria IS 'ID do critério' ;
COMMENT ON COLUMN sys_btn_crit.flg_available IS 'Disponível Y/N' ;
COMMENT ON COLUMN sys_btn_crit.rank IS 'Ordem de ordenação' ;
COMMENT ON COLUMN sys_btn_crit.flg_mandatory IS 'Prenchimento obrigatório Y/N' ;
