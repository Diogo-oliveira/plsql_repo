

  CREATE TABLE DIMENSION
   (	ID_DIMENSION NUMBER NOT NULL ENABLE,
	WIDTH NUMBER,
	HEIGHT NUMBER,
	HEIGHT_OPEN NUMBER,
	X_POSITION NUMBER,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	INTERNAL_NAME VARCHAR2(200)
   );


COMMENT ON COLUMN dimension.id_dimension IS 'Chave primária' ;
COMMENT ON COLUMN dimension.width IS 'Largura' ;
COMMENT ON COLUMN dimension.height IS 'Altura' ;
COMMENT ON COLUMN dimension.height_open IS 'Altura quando aberto' ;
COMMENT ON COLUMN dimension.x_position IS 'Posição X' ;
COMMENT ON COLUMN dimension.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN dimension.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;


