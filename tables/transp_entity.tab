

  CREATE TABLE TRANSP_ENTITY
   (	ID_TRANSP_ENTITY NUMBER(24,0) NOT NULL ENABLE,
	CODE_TRANSP_ENTITY VARCHAR2(200) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE,
	FLG_TRANSP VARCHAR2(1)
   );


COMMENT ON TABLE transp_entity IS 'Entidade responsável pelo transporte' ;

COMMENT ON COLUMN transp_entity.flg_type IS 'transporte escolhido por: D - médico, A - administrativo' ;
COMMENT ON COLUMN transp_entity.flg_transp IS 'Tipo de Transporte: A - Chegada;D-Partida' ;

ALTER TABLE TRANSP_ENTITY 
 ADD (FLG_AVAILABLE  VARCHAR2(1) DEFAULT 'Y' NOT NULL
 );

COMMENT ON COLUMN TRANSP_ENTITY.FLG_AVAILABLE IS 'Disponibilidade do registo na instituição onde está instalada a BD';

ALTER TABLE TRANSP_ENTITY MODIFY FLG_AVAILABLE default null;

ALTER TABLE transp_entity ADD (ID_CONTENT VARCHAR2(200));

