

  CREATE TABLE PARAMETER_ANALYSIS
   (	ID_PARAMETER_ANALYSIS NUMBER(12,0) NOT NULL ENABLE,
	CODE_PARAMETER_ANALYSIS VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE parameter_analysis IS 'Parâmetros das análises' ;

COMMENT ON COLUMN parameter_analysis.code_parameter_analysis IS 'Código para nome' ;
COMMENT ON COLUMN parameter_analysis.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN parameter_analysis.rank IS 'Ordem de apresentação' ;
