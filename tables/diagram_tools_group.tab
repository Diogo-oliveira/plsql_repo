

  CREATE TABLE DIAGRAM_TOOLS_GROUP
   (	ID_DIAGRAM_TOOLS_GROUP NUMBER(24,0) NOT NULL ENABLE,
	CODE_DIAGRAM_TOOLS_GROUP VARCHAR2(240) NOT NULL ENABLE,
	INTERNAL_NAME VARCHAR2(200) NOT NULL ENABLE,
	CODE_ACRONYM_GROUP VARCHAR2(200) NOT NULL ENABLE,
	RANK NUMBER(24,0) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE diagram_tools_group IS 'Definição dos diferentes grupos de diagramas' ;

COMMENT ON COLUMN diagram_tools_group.id_diagram_tools_group IS 'Chave primária' ;
COMMENT ON COLUMN diagram_tools_group.code_diagram_tools_group IS 'Código para obter os grupos de diagramas por língua' ;
COMMENT ON COLUMN diagram_tools_group.internal_name IS 'Nome interno' ;
COMMENT ON COLUMN diagram_tools_group.code_acronym_group IS 'Sigla associada a cada grupo por língua' ;
COMMENT ON COLUMN diagram_tools_group.rank IS 'Ordenação' ;
COMMENT ON COLUMN diagram_tools_group.flg_available IS 'Disponibilidade do registo' ;
COMMENT ON COLUMN diagram_tools_group.adw_last_update IS 'Data da última alteração' ;


