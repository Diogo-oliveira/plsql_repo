

  CREATE TABLE PENDING_ISSUE_TITLE_DEPT_HIST
   (	ID_PENDING_ISSUE_TITLE_HIST NUMBER(24,0),
	ID_DEPT NUMBER(24,0),
	CREATE_USER VARCHAR2(24),
	CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	CREATE_INSTITUTION NUMBER(24,0),
	UPDATE_USER VARCHAR2(24),
	UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	UPDATE_INSTITUTION NUMBER(24,0)
   ) ;


   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.ID_PENDING_ISSUE_TITLE_HIST IS 'Pending issue title history registry ID';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.ID_DEPT IS 'Dept ID';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.CREATE_USER IS 'Creation User';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.CREATE_TIME IS 'Creation Time';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.CREATE_INSTITUTION IS 'Creation Institution';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.UPDATE_USER IS 'Update User';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.UPDATE_TIME IS 'Update Time';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT_HIST.UPDATE_INSTITUTION IS 'Update Institution';



   COMMENT ON TABLE PENDING_ISSUE_TITLE_DEPT_HIST  IS 'Relation between pending issue title and dept history';

   comment on column PENDING_ISSUE_TITLE_DEPT_HIST.ID_PENDING_ISSUE_TITLE_HIST  is 'Pending issue title history registry identification';
   comment on column PENDING_ISSUE_TITLE_DEPT_HIST.ID_DEPT  is 'Department identification';

