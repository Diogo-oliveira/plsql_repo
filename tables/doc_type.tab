

  CREATE TABLE DOC_TYPE
   (	ID_DOC_TYPE NUMBER(12,0) NOT NULL ENABLE,
	CODE_DOC_TYPE VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	ID_DOC_ORI_TYPE NUMBER(6,0)
   );


COMMENT ON TABLE doc_type IS 'Especificação do tipo de documento' ;

COMMENT ON COLUMN doc_type.code_doc_type IS 'Código para tradução do tipo de documento' ;
COMMENT ON COLUMN doc_type.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;


ALTER TABLE doc_type add FLG_DUPLICATE varchar2(1) DEFAULT 'Y' NOT NULL;
comment on column doc_type.flg_duplicate is 'Y se o documento pode existir em duplicado, N caso contrário';


