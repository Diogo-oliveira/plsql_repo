

  CREATE TABLE MDM_EVALUATION
   (	ID_MDM_EVALUATION NUMBER(24,0) NOT NULL ENABLE,
	FLG_ORIGEM VARCHAR2(1) NOT NULL ENABLE,
	FLG_CODING VARCHAR2(60) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	CMS_AREA VARCHAR2(5)
   );

comment on column MDM_EVALUATION.ID_MDM_EVALUATION
  is 'Chave primária';
comment on column MDM_EVALUATION.FLG_ORIGEM
  is 'Tipo de critério: I-Imaging, L-Lab;D-Diagnosis,M-Medication,P-Procedures,F-Fluids,RR-Records review';
comment on column MDM_EVALUATION.FLG_CODING
  is 'Tipo de codifiação para cada critério';
comment on column MDM_EVALUATION.FLG_AVAILABLE
  is 'Disponibilidade do registo';
comment on column MDM_EVALUATION.ADW_LAST_UPDATE
  is 'Data da última alteração';
comment on column MDM_EVALUATION.CMS_AREA
  is 'Area de registo para a MDM_PROF_CODING';

