

  CREATE TABLE SR_EVAL_NOTES
   (	ID_SR_EVAL_NOTES NUMBER(38,0) NOT NULL ENABLE,
	ID_SR_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_REG DATE NOT NULL ENABLE,
	NOTES VARCHAR2(4000)
   );


COMMENT ON TABLE sr_eval_notes IS 'Tabela de notas as avaliações pré, intra e pós-operatórias' ;

COMMENT ON COLUMN sr_eval_notes.id_sr_eval_notes IS 'ID único' ;
COMMENT ON COLUMN sr_eval_notes.id_professional IS 'ID do profissional que registou as notas' ;
COMMENT ON COLUMN sr_eval_notes.dt_reg IS 'Data do registo das notas' ;
COMMENT ON COLUMN sr_eval_notes.notes IS 'Notas da avaliação' ;
