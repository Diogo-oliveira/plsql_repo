

  CREATE TABLE BODY_PART_IMAGE
   (	ID_BODY_PART_IMAGE NUMBER(24,0) NOT NULL ENABLE,
	INTERN_NAME_BP VARCHAR2(200) NOT NULL ENABLE,
	IMAGE_NAME VARCHAR2(200) NOT NULL ENABLE
   );


COMMENT ON TABLE body_part_image IS 'Imagens utilizadas no registo de topografia' ;

COMMENT ON COLUMN body_part_image.intern_name_bp IS 'Nome da parte do corpo, p/ uso interno' ;
COMMENT ON COLUMN body_part_image.image_name IS 'Nome da imagem' ;
