

  CREATE TABLE ANALYSIS_REQ
   (	ID_ANALYSIS_REQ NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	ID_PROF_WRITES NUMBER(24,0) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(12,0),
	ID_SCHEDULE NUMBER(24,0),
	ID_SCHED_CONSULT NUMBER(24,0),
	DT_REQ DATE NOT NULL ENABLE,
	NOTES VARCHAR2(4000),
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(4000),

	ID_PROF_APPROVE NUMBER(24,0),
	ID_PROF_AUTHORIZE NUMBER(24,0),
	FLG_TIME VARCHAR2(1) NOT NULL ENABLE,
	DT_BEGIN DATE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	ID_EPISODE_ORIGIN NUMBER(24,0),
	ID_EPISODE_DESTINATION NUMBER(24,0),
	DT_PEND_REQ DATE,
	ID_PROTOCOLS NUMBER(24,0),
	ID_PREV_EPISODE NUMBER(24,0),
	NOTES_TECH VARCHAR2(240)
   );


COMMENT ON TABLE analysis_req IS 'Cabeçalhos de requisição de análises. Permite associar os agendamentos das análises com a consulta (ou outro evento) para a qual são necessários' ;

COMMENT ON COLUMN analysis_req.id_episode IS 'Chave primaria' ;
COMMENT ON COLUMN analysis_req.id_prof_approve IS 'Profissional que aprova o pedido de exame ao exterior (Dir. Serviço)' ;
COMMENT ON COLUMN analysis_req.id_prof_authorize IS 'Profissional que autoriza o pedido de exame ao exterior (Dir. Clínico)' ;
COMMENT ON COLUMN analysis_req.flg_time IS 'Realização: E - neste episódio; N - próximo episódio; B - entre episódios' ;
COMMENT ON COLUMN analysis_req.dt_begin IS 'Data pretendida para início da execução do exame (ie, ñ imediata)' ;
COMMENT ON COLUMN analysis_req.flg_status IS 'F - c/ resultado; R - requisitado; C - cancelado; E - em execução; D - pendente; P - parcial; T - em ransporte; H - colhido' ;
COMMENT ON COLUMN analysis_req.id_episode_origin IS 'ID do episódio em que foi registada a req. inicialmente (ie, se FLG_TIME = ''N'')' ;
COMMENT ON COLUMN analysis_req.dt_pend_req IS 'ID do episódio em que foi registada a req. destino (se FLG_TIME = ''N'')' ;
COMMENT ON COLUMN analysis_req.id_prev_episode IS 'ID do protocolo, caso tenha sido requesitado através de um protocolo.' ;
COMMENT ON COLUMN analysis_req.notes_tech IS 'ID do episódio em que foi registada a req. inicialmente' ;

comment on column ANALYSIS_REQ.ID_EPISODE_DESTINATION
  is 'ID do episódio em que foi registada a req. destino (se FLG_TIME = ''N'')';
comment on column ANALYSIS_REQ.ID_PROTOCOLS
  is 'ID do protocolo, caso tenha sido requisitado através de um protocolo.';
comment on column ANALYSIS_REQ.ID_PREV_EPISODE
  is 'ID do episódio em que foi registada a req. inicialmente';
comment on column ANALYSIS_REQ.NOTES_TECH
  is '';

  
alter table analysis_req add (dt_req_tstz timestamp with local time zone);
alter table analysis_req add (dt_cancel_tstz timestamp with local time zone);
alter table analysis_req add (dt_begin_tstz timestamp with local time zone);
alter table analysis_req add (dt_pend_req_tstz timestamp with local time zone);

COMMENT ON COLUMN analysis_req.dt_begin_tstz IS 'Data pretendida para início da execução do exame (ie, ñ imediata)' ;
COMMENT ON COLUMN analysis_req.dt_pend_req_tstz IS 'ID do episódio em que foi registada a req. destino (se FLG_TIME = ''N'')' ;

