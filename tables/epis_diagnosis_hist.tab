

  CREATE TABLE EPIS_DIAGNOSIS_HIST
   (	ID_EPIS_DIAGNOSIS_HIST NUMBER(24,0) NOT NULL ENABLE,
	ID_EPIS_DIAGNOSIS NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_CREATION DATE NOT NULL ENABLE,
	FLG_STATUS VARCHAR2(2) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(2) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	NOTES VARCHAR2(200),
	FLG_FINAL_TYPE VARCHAR2(240),
	ID_EPIS_DIAGNOSIS_NOTES NUMBER
   );



COMMENT ON TABLE epis_diagnosis_hist IS 'Histórico dos episódios de diagnósticos' ;

COMMENT ON COLUMN epis_diagnosis_hist.id_epis_diagnosis_hist IS 'Chave primária' ;
COMMENT ON COLUMN epis_diagnosis_hist.id_epis_diagnosis IS 'Episódio do diagnóstico' ;
COMMENT ON COLUMN epis_diagnosis_hist.id_professional IS 'Profissional' ;
COMMENT ON COLUMN epis_diagnosis_hist.dt_creation IS 'Data de registo' ;
COMMENT ON COLUMN epis_diagnosis_hist.flg_status IS 'Estado do episódio:A -Activo; C - Cancelar; D - Despiste; F - Confirmado; R - Declinar;B - Base' ;
COMMENT ON COLUMN epis_diagnosis_hist.flg_type IS 'Tipo de diagnóstico. P - Provisório;D - Definitivo.Novo tipo de diagnóstico: B - Diagnóstico base, existe apenas para o ORIS.' ;
COMMENT ON COLUMN epis_diagnosis_hist.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN epis_diagnosis_hist.flg_final_type IS 'Tipo de diagnóstico final: P - Primary;S - Secondary' ;


