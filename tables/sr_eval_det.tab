

  CREATE TABLE SR_EVAL_DET
   (	ID_SR_EVAL_DET NUMBER(38,0) NOT NULL ENABLE,
	ID_SR_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_EVALUATION NUMBER(38,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_REG DATE NOT NULL ENABLE,
	VAL_DOMAIN VARCHAR2(200),
	VAL_TEXT VARCHAR2(4000),
	VAL_NUMBER NUMBER(38,4),
	VAL_DATE DATE
   );


COMMENT ON COLUMN sr_eval_det.id_sr_eval_det IS 'ID único' ;
COMMENT ON COLUMN sr_eval_det.id_sr_evaluation IS 'ID do item da avaliação' ;
COMMENT ON COLUMN sr_eval_det.id_professional IS 'ID do profissional que efectuou o último registo do item' ;
COMMENT ON COLUMN sr_eval_det.dt_reg IS 'Data do último registo do item' ;
COMMENT ON COLUMN sr_eval_det.val_domain IS 'Valor seleccionado do domínio (pode ser sim/não, boa/má, etc.)' ;
COMMENT ON COLUMN sr_eval_det.val_text IS 'Valor em formato texto' ;
COMMENT ON COLUMN sr_eval_det.val_number IS 'Valor em formato numérico' ;
COMMENT ON COLUMN sr_eval_det.val_date IS 'Valor em formato data' ;


