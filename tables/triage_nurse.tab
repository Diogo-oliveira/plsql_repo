

  CREATE TABLE TRIAGE_NURSE
   (	ID_TRIAGE_NURSE NUMBER NOT NULL ENABLE,
	CODE_TRIAGE_NURSE VARCHAR2(240) NOT NULL ENABLE,
	ID_TRIAGE_TYPE NUMBER NOT NULL ENABLE,
	ID_TRIAGE_COLOR NUMBER(12,0) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	RANK NUMBER NOT NULL ENABLE
   );


COMMENT ON COLUMN triage_nurse.id_triage_nurse IS 'Chave primária' ;
COMMENT ON COLUMN triage_nurse.code_triage_nurse IS 'Descrição do triage nurse' ;
COMMENT ON COLUMN triage_nurse.id_triage_type IS 'Tipo de triagem' ;
COMMENT ON COLUMN triage_nurse.id_triage_color IS 'Cor' ;
COMMENT ON COLUMN triage_nurse.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN triage_nurse.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN triage_nurse.rank IS 'Ordem de apresentação' ;


