CREATE TABLE request_approval_hist (
  id_request_approval_hist NUMBER(24,0)                      NOT NULL,
  id_consult_req_hist      NUMBER(24,0)                      NOT NULL,
  id_professional          NUMBER(24,0)                      NOT NULL,
  create_user              VARCHAR2(24 char)                      NULL,
  create_time              TIMESTAMP(6) WITH LOCAL TIME ZONE NULL,
  create_institution       NUMBER(24,0)                      NULL,
  update_user              VARCHAR2(24 char)                      NULL,
  update_time              TIMESTAMP(6) WITH LOCAL TIME ZONE NULL,
  update_institution       NUMBER(24,0)                      NULL
);

COMMENT ON COLUMN request_approval_hist.id_request_approval_hist IS 'request approval identifier';
COMMENT ON COLUMN request_approval_hist.id_consult_req_hist IS 'consult_req_hist identifier';
COMMENT ON COLUMN request_approval_hist.id_professional IS 'professional identifier';
COMMENT ON COLUMN request_approval_hist.create_user IS 'Creation User';
COMMENT ON COLUMN request_approval_hist.create_time IS 'Creation Time';
COMMENT ON COLUMN request_approval_hist.create_institution IS 'Creation Institution';
COMMENT ON COLUMN request_approval_hist.update_user IS 'Update User';
COMMENT ON COLUMN request_approval_hist.update_time IS 'Update Time';
COMMENT ON COLUMN request_approval_hist.update_institution IS 'Update Institution';
-- CHANGE END: Sérgio Santos

