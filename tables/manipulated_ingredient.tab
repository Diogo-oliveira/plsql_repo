

  CREATE TABLE MANIPULATED_INGREDIENT
   (	ID_MANIPULATED_INGREDIENT NUMBER(24,0) NOT NULL ENABLE,
	ID_INGREDIENT NUMBER(24,0) NOT NULL ENABLE,
	ID_MANIPULATED NUMBER(24,0) NOT NULL ENABLE,
	PERCENT NUMBER(3,0) NOT NULL ENABLE
   );


COMMENT ON TABLE manipulated_ingredient IS 'Constituição dos manipulados.' ;

COMMENT ON COLUMN manipulated_ingredient.id_manipulated_ingredient IS 'Chave primária.' ;
COMMENT ON COLUMN manipulated_ingredient.id_ingredient IS 'ID do constituinte.' ;
COMMENT ON COLUMN manipulated_ingredient.id_manipulated IS 'ID do manipulado.' ;
COMMENT ON COLUMN manipulated_ingredient.percent IS 'Percentagem do constituinte.' ;
