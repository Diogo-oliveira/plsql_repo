

  CREATE TABLE PRESCRIPTION_PHARM
   (	ID_PRESCRIPTION_PHARM NUMBER(24,0) NOT NULL ENABLE,
	ID_PRESCRIPTION NUMBER(24,0) NOT NULL ENABLE,
	EMB_ID NUMBER(10,0),
	ID_DRUG NUMBER(12,0),
	QTY NUMBER(2,0) NOT NULL ENABLE,
	GENERICO VARCHAR2(1),
	DOSAGE VARCHAR2(1000),
	JUSTIF VARCHAR2(2000),
	VIAS_ADMIN_ID NUMBER(3,0),
	FLG_CI VARCHAR2(1) NOT NULL ENABLE,
	FLG_CHEAPER VARCHAR2(1) NOT NULL ENABLE,

	FLG_JUSTIF VARCHAR2(1) NOT NULL ENABLE,
	DIPLOMA_ID NUMBER(2,0),
	FLG_STATUS VARCHAR2(1),
	DT_PRESCRIPTION_PHARM DATE NOT NULL ENABLE,
	DESC_MANIP VARCHAR2(1000),
	STOCK VARCHAR2(1),
	NOTES VARCHAR2(173),
	FLG_ATTENTION VARCHAR2(2),
	FLG_ATTENTION_PRINT VARCHAR2(1),
	ID_DIETARY_DRUG NUMBER(24,0),
	ID_MANIPULATED NUMBER(24,0),
	DESC_DIETARY VARCHAR2(1000),
	QTY_MANIP NUMBER(6,2),

	UNIT_MANIP VARCHAR2(4),
	PVP NUMBER(8,2),
	P_REF NUMBER(8,2),
	P_REF_PEN NUMBER(8,2)
   );


COMMENT ON TABLE prescription_pharm IS 'Prescrições para a farmácia enterna e farmácia do hospital.' ;

COMMENT ON COLUMN prescription_pharm.id_prescription_pharm IS 'Chave primária.' ;
COMMENT ON COLUMN prescription_pharm.id_prescription IS 'ID da prescrição.' ;
COMMENT ON COLUMN prescription_pharm.emb_id IS 'ID da embalagem.' ;
COMMENT ON COLUMN prescription_pharm.id_drug IS 'ID do medicamento. Utilizado na prescrição interna; caso contrário preenche-se EMB_ID.' ;
COMMENT ON COLUMN prescription_pharm.qty IS 'Nº de embalagens.' ;
COMMENT ON COLUMN prescription_pharm.generico IS 'Autorização de genérico.' ;
COMMENT ON COLUMN prescription_pharm.dosage IS 'Posologia.' ;
COMMENT ON COLUMN prescription_pharm.justif IS 'Justificação da prescrição.' ;
COMMENT ON COLUMN prescription_pharm.vias_admin_id IS 'ID da via de administração.' ;
COMMENT ON COLUMN prescription_pharm.flg_ci IS 'Chamada de atenção de contra-indicações: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN prescription_pharm.flg_cheaper IS 'Chamada de atenção de medicamentos mais baratos: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN prescription_pharm.flg_justif IS 'Chamada de atenção de justificação de prescrição: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN prescription_pharm.diploma_id IS 'ID do diploma.' ;

COMMENT ON COLUMN prescription_pharm.flg_status IS 'Estado da requisição: D - pendente, R - requisitado, E - em curso, F - concluído, C - anulado, I - interrompido' ;
COMMENT ON COLUMN prescription_pharm.dt_prescription_pharm IS 'Data em que o medicamento foi prescrito.' ;
COMMENT ON COLUMN prescription_pharm.desc_manip IS 'Descritivo do manipulado.' ;
COMMENT ON COLUMN prescription_pharm.stock IS 'Indicação se o medicamento prescrito (Administrar neste episódio) existe em stock (informação dada pela enfermeira).' ;
COMMENT ON COLUMN prescription_pharm.notes IS 'Notas da requisição. (O nº limite de carateres é 173 pois é o máximo permitido na receita impressa)' ;
COMMENT ON COLUMN prescription_pharm.flg_attention IS 'Flag que indica a chamada de atenção prioritária: CI - contra-indicações; B - medicamentos mais baratos; J - justificação de prescrição; IM - interacções medicamentosas; IA - interacções entre medicamentos e alergias' ;
COMMENT ON COLUMN prescription_pharm.flg_attention_print IS 'Flag que indica se o profissional prosseguiu (Y) ou não (N) com a impressão apesar das chamadas de atenção.' ;
COMMENT ON COLUMN prescription_pharm.id_dietary_drug IS 'ID do dietético.' ;
COMMENT ON COLUMN prescription_pharm.id_manipulated IS 'ID do manipulado (se a prescrição for feita através dos mais frequentes).' ;
COMMENT ON COLUMN prescription_pharm.desc_dietary IS 'Descritivo do dietético.' ;
COMMENT ON COLUMN prescription_pharm.qty_manip IS 'Quantidade a preparar (só existe nos manipulados).' ;
COMMENT ON COLUMN prescription_pharm.unit_manip IS 'Unidade da quantidade a preparar, QTY_MANIP (só existe para manipulados)' ;
COMMENT ON COLUMN prescription_pharm.pvp IS 'PVP do medicamento no momento em que foi prescrito (não é utilizado; poderá servir apenas para auditoria).' ;

COMMENT ON COLUMN prescription_pharm.p_ref IS 'Preço de referência do medicamento no momento em que foi prescrito (não é utilizado; poderá servir apenas para auditoria).' ;
COMMENT ON COLUMN prescription_pharm.p_ref_pen IS 'Preço de referência para pensionistas do medicamento no momento em que foi prescrito (não é utilizado; poderá servir apenas para auditoria).' ;

