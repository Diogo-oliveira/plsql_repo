

  CREATE TABLE DISCHARGE
   (	ID_DISCHARGE NUMBER(24,0) NOT NULL ENABLE,
	ID_DISCH_REAS_DEST NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(4000),
	ID_PROF_MED NUMBER(24,0),
	DT_MED DATE,
	NOTES_MED VARCHAR2(4000),
	ID_PROF_ADMIN NUMBER(24,0),
	DT_ADMIN DATE,

	NOTES_ADMIN VARCHAR2(4000),
	FLG_STATUS VARCHAR2(1),
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	ID_TRANSP_ENT_ADM NUMBER(24,0),
	ID_TRANSP_ENT_MED NUMBER(24,0),
	NOTES_JUSTIFY VARCHAR2(4000),
	PRICE NUMBER(12,4),
	CURRENCY VARCHAR2(10),
	FLG_PAYMENT VARCHAR2(1)
   );


COMMENT ON TABLE discharge IS 'Alta administrativa e médica. Marca o fim do episódio.Mesmo que o utente não responda à chamada e o episódio não contenha registo de informação clínica, há sempre uma alta administrativa, onde se regista a falta de comparência. A alta clínica é dada pelo médico. Pode haver cancelamento de qualquer uma das altas.Para cada epis. existe um único registo não cancelado nesta tabela.' ;

COMMENT ON COLUMN discharge.id_episode IS 'Chave primaria' ;
COMMENT ON COLUMN discharge.id_prof_cancel IS 'Prof responsável palo cancelamento da alta médica' ;
COMMENT ON COLUMN discharge.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN discharge.id_prof_med IS 'Prof. responsável pela alta médica' ;
COMMENT ON COLUMN discharge.dt_med IS 'Data de alta médica' ;
COMMENT ON COLUMN discharge.notes_med IS 'Notas de alta médica' ;
COMMENT ON COLUMN discharge.id_prof_admin IS 'Prof. responsável pela alta administrativa' ;
COMMENT ON COLUMN discharge.dt_admin IS 'Data de alta administrativa' ;
COMMENT ON COLUMN discharge.notes_admin IS 'Notas de alta administrativa' ;
COMMENT ON COLUMN discharge.flg_status IS 'A - activo, C - cancelado' ;
COMMENT ON COLUMN discharge.flg_type IS 'Tipo: F - fim de episódio; D - alta (fim de seguimento do doente); P - pendente' ;
COMMENT ON COLUMN discharge.id_transp_ent_adm IS 'Transporte indicado pelo administrativo' ;
COMMENT ON COLUMN discharge.id_transp_ent_med IS 'Transporte indicado pelo médico' ;

COMMENT ON COLUMN discharge.price IS 'Appointment price' ;
COMMENT ON COLUMN discharge.currency IS 'Appointment price currency' ;
COMMENT ON COLUMN discharge.flg_payment IS 'Y when appointment is payed, N when appointment is not payed. Null if not applicable.' ;

comment on column DISCHARGE.FLG_STATUS
  is 'A - activo, C - cancelado, R - reaberto';
comment on column DISCHARGE.FLG_TYPE
  is 'Tipo: F - fim de episódio; D - alta (fim de seguimento do doente)';

