DECLARE
	e_object_exists EXCEPTION;
    PRAGMA EXCEPTION_INIT(e_object_exists, -00955);
BEGIN
	pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'SCHEDULE_ANALYSIS_HIST', i_obj_type => 'TABLE',i_flg_category => 'HST', i_flg_nzd => 'N');

	EXECUTE IMMEDIATE 'CREATE TABLE SCHEDULE_ANALYSIS_HIST
						 (ID_SCHEDULE NUMBER(24) NOT NULL
						 ,CREATE_USER VARCHAR2(24 CHAR)
						 ,CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
						 ,CREATE_INSTITUTION NUMBER(24)
						 ,UPDATE_USER VARCHAR2(24 CHAR)
						 ,UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
						 ,UPDATE_INSTITUTION NUMBER(24)
						 ,ID_HARVEST NUMBER(24) NOT NULL
						 ,DT_UPDATE TIMESTAMP WITH LOCAL TIME ZONE DEFAULT current_timestamp
						 )
					   tablespace TABLE_M';
EXCEPTION
	WHEN e_object_exists THEN dbms_output.put_line('Tentativa de criação da tabela SCHEDULE_ANALYSIS_HIST falhou porque já existe');
END;
/

COMMENT ON TABLE SCHEDULE_ANALYSIS_HIST IS 'History table for updates support';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.ID_SCHEDULE IS 'schedule identifier';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.CREATE_USER IS 'Creation user';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.CREATE_TIME IS 'Create time';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.CREATE_INSTITUTION IS 'Creation institution';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.UPDATE_USER IS 'Update user';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.UPDATE_TIME IS 'Update time';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.UPDATE_INSTITUTION IS 'Update institution';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.ID_HARVEST IS 'harvest id. A Lab appointment can have 0 (no requisition) or 1 or more harvests (with requisition)';
COMMENT ON COLUMN SCHEDULE_ANALYSIS_HIST.DT_UPDATE IS 'date of update occurrence';

