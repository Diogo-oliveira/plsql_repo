COMMENT ON COLUMN ANALYSIS_BODY_STRUCTURE.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN ANALYSIS_BODY_STRUCTURE.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN ANALYSIS_BODY_STRUCTURE.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN ANALYSIS_BODY_STRUCTURE.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN ANALYSIS_BODY_STRUCTURE.UPDATE_INSTITUTION IS 'Update Institution';
