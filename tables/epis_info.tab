

  CREATE TABLE EPIS_INFO
   (	ID_EPIS_INFO NUMBER(24,0) NOT NULL ENABLE,
	ID_BED NUMBER(24,0),
	ID_ROOM NUMBER(24,0),
	ID_PROFESSIONAL NUMBER(24,0),
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	NORTON NUMBER(3,0),
	FLG_HYDRIC VARCHAR2(1),
	FLG_WOUND VARCHAR2(1),
	COMPANION VARCHAR2(200),
	FLG_UNKNOWN VARCHAR2(1),
	DESC_INFO VARCHAR2(4000),

	ID_SCHEDULE NUMBER(24,0),
	ID_FIRST_NURSE_RESP NUMBER(24,0),
	FLG_STATUS VARCHAR2(1),
	ID_DEP_CLIN_SERV NUMBER(24,0),
	ID_FIRST_DEP_CLIN_SERV NUMBER(24,0)
   );


COMMENT ON TABLE epis_info IS 'Esta tabela contém informação variada sobre o episódio. Serve para facilitar o acesso aos dados mais requisitados do episódio, uma vez que assim ficam centralizados.Não contém histórico, mas sim o estado mais actualizado do doente.' ;

COMMENT ON COLUMN epis_info.id_epis_info IS 'Chave primária' ;
COMMENT ON COLUMN epis_info.norton IS 'Escala de Norton' ;
COMMENT ON COLUMN epis_info.flg_hydric IS 'Registos hídricos' ;
COMMENT ON COLUMN epis_info.flg_wound IS 'Avaliação de feridas' ;
COMMENT ON COLUMN epis_info.flg_unknown IS 'Pacientes temporários' ;
COMMENT ON COLUMN epis_info.desc_info IS 'Para conter a queixa, diagnóstico, etc' ;
COMMENT ON COLUMN epis_info.flg_status IS 'Estado: E - efectivado (epis. iniciado), D - alta médica, M - alta administrativa, A - alta automática, C - espera corredor, N - atendimento enfermagem pré-consulta, P - atendimento enfermagem pós-consulta, T - consulta' ;
COMMENT ON COLUMN epis_info.id_first_dep_clin_serv IS 'Primeiro serviço clínico para o departamento' ;

comment on column EPIS_INFO.ID_DEP_CLIN_SERV
  is 'Serviço clinico do departamento';
comment on column EPIS_INFO.ID_FIRST_DEP_CLIN_SERV
  is 'Primeiro serviço clinico do departamento';


