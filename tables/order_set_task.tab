CREATE TABLE ORDER_SET_TASK
 (ID_ORDER_SET_TASK NUMBER(24) NOT NULL
 ,ID_ORDER_SET NUMBER(24) NOT NULL
 ,ID_TASK_TYPE NUMBER(24) NOT NULL
 );
 
COMMENT ON TABLE ORDER_SET_TASK IS 'Tasks associated with order sets';
COMMENT ON COLUMN ORDER_SET_TASK.ID_ORDER_SET_TASK IS 'Order set task ID';
COMMENT ON COLUMN ORDER_SET_TASK.ID_ORDER_SET IS 'Order set ID';
COMMENT ON COLUMN ORDER_SET_TASK.ID_TASK_TYPE IS 'Task type ID';