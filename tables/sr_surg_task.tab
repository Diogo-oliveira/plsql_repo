

  CREATE TABLE SR_SURG_TASK
   (	ID_SR_SURG_TASK NUMBER(24,0) NOT NULL ENABLE,
	CODE_SR_SURG_TASK VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	DESC_SURG_TASK VARCHAR2(4000)
   );


COMMENT ON COLUMN sr_surg_task.id_sr_surg_task IS 'Id da terefa' ;
COMMENT ON COLUMN sr_surg_task.code_sr_surg_task IS 'Código para obtenção da descrição' ;
COMMENT ON COLUMN sr_surg_task.flg_available IS 'Indica se a tarefa está disponível' ;
COMMENT ON COLUMN sr_surg_task.desc_surg_task IS 'Descrição da tarefa' ;
