

  CREATE TABLE ROOM_EXT_SYS
   (	ID_ROOM_EXT_SYS NUMBER(24,0) NOT NULL ENABLE,
	ID_EXTERNAL_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_ROOM NUMBER(24,0) NOT NULL ENABLE,
	VALUE VARCHAR2(30) NOT NULL ENABLE
   );


COMMENT ON TABLE room_ext_sys IS 'Correspondência dos identificadores únicos das salas, no Alert e nos sistemas externos' ;

COMMENT ON COLUMN room_ext_sys.value IS 'Identificador único da sala, no sistema externo indicado' ;
