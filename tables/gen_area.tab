CREATE TABLE GEN_AREA
 (CODE VARCHAR2(30) NOT NULL
 ,DESCRIPTION VARCHAR2(200)
 )
TABLESPACE TABLE_S;


COMMENT ON TABLE GEN_AREA IS 'Generic table with the severall areas that use this process' ;
COMMENT ON COLUMN GEN_AREA.CODE IS 'Code that identifies each area. Ex. EXM, LAB, MED.' ;
COMMENT ON COLUMN GEN_AREA.DESCRIPTION IS 'Description of the area' ;
