	ID_INSTITUTION NUMBER(12,0) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	ID_PROF NUMBER(24,0),
	ID_PROF_CLIN_SERV NUMBER(12,0),
	ID_CLIN_SERV NUMBER(12,0),
	ID_SCH_SERVICE NUMBER(24,0) NOT NULL ENABLE,
	DT_BEGIN DATE NOT NULL ENABLE,
	MAX_VACANCIES NUMBER(6,0),
	USED_VACANCIES NUMBER(6,0),

	DT_EXPIRE DATE,
	DT_END DATE,
	ID_DEP_CLIN_SERV NUMBER(24,0),
	ID_ROOM NUMBER(24,0),
	ID_SCH_EVENT NUMBER(24,0),
	DURATION NUMBER(8,0)
   );


