

  CREATE TABLE INF_PATOL_ESP
   (	PATOL_ESP_ID NUMBER(2,0) NOT NULL ENABLE,
	DESCR VARCHAR2(100) NOT NULL ENABLE,
	DISP VARCHAR2(30) NOT NULL ENABLE
   );


COMMENT ON TABLE inf_patol_esp IS 'Patologias especiais.' ;

COMMENT ON COLUMN inf_patol_esp.patol_esp_id IS 'Código identificador da patologia especial.' ;
COMMENT ON COLUMN inf_patol_esp.descr IS 'Descrição da patologia especial que beneficia de taxa de comparticipação especial.' ;
COMMENT ON COLUMN inf_patol_esp.disp IS 'Descrição do tipo de dispensa do medicamento (Farmácia de Oficina ou Farmácia Hospitalar).' ;


