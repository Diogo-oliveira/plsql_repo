

  CREATE TABLE VACCINE_PRESC_PLAN
   (	ID_VACCINE_PRESC_PLAN NUMBER(24,0) NOT NULL ENABLE,
	ID_VACCINE_PRESC_DET NUMBER(24,0) NOT NULL ENABLE,
	DT_PLAN DATE,
	NOTES VARCHAR2(4000),
	ID_PROF_CANCEL NUMBER(24,0),
	DT_CANCEL DATE,
	NOTES_CANCEL VARCHAR2(4000),
	ID_SCHEDULE NUMBER(24,0)
   );


COMMENT ON COLUMN vaccine_presc_plan.dt_plan IS 'Data planeada' ;
COMMENT ON COLUMN vaccine_presc_plan.notes IS 'Notas' ;
COMMENT ON COLUMN vaccine_presc_plan.dt_cancel IS 'Data de cancelamento' ;
COMMENT ON COLUMN vaccine_presc_plan.notes_cancel IS 'Notas de cancelamento' ;




ALTER TABLE VACCINE_PRESC_PLAN ADD DT_CANCEL_TSTZ TIMESTAMP WITH LOCAL TIME ZONE; 
COMMENT ON COLUMN VACCINE_PRESC_PLAN.DT_CANCEL_TSTZ IS 'Data de cancelamento'; 




ALTER TABLE VACCINE_PRESC_PLAN ADD DT_PLAN_TSTZ TIMESTAMP WITH LOCAL TIME ZONE; 
COMMENT ON COLUMN VACCINE_PRESC_PLAN.DT_PLAN_TSTZ IS 'Data planeada'; 


