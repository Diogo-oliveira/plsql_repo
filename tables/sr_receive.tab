

  CREATE TABLE SR_RECEIVE
   (	ID_SR_RECEIVE NUMBER(24,0) NOT NULL ENABLE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	FLG_MANUAL VARCHAR2(1) NOT NULL ENABLE,
	DT_RECEIVE DATE NOT NULL ENABLE,
	ID_PROF NUMBER(24,0),
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE sr_receive IS 'Informação acerca do estado de admissão para cirurgia de um episódio' ;

COMMENT ON COLUMN sr_receive.id_sr_receive IS 'Id da admissão' ;
COMMENT ON COLUMN sr_receive.flg_status IS 'Estado da admissão. Y- Admitido; N- Não admitido' ;
COMMENT ON COLUMN sr_receive.flg_manual IS 'Indica se o estado de admissão foi definido manualmente. Y- Manual; N- Automático' ;
COMMENT ON COLUMN sr_receive.dt_receive IS 'Data de criação do registo' ;
COMMENT ON COLUMN sr_receive.id_prof IS 'Id do profissional no caso da admissão ser manual.' ;
COMMENT ON COLUMN sr_receive.id_episode IS 'Id do episódio' ;


