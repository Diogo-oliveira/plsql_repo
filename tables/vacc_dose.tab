        EXECUTE IMMEDIATE 'alter table vacc_dose add FLG_BOOSTER VARCHAR2(1 CHAR) default ''N'' not null';
  EXECUTE IMMEDIATE 'comment on column VACC_DOSE.flg_booster  is ''Indicação se é dose de reforço (Y - Sim, N - Não)''';
    EXCEPTION
        WHEN e_name_already_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;

   
END;
