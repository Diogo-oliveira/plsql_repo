

  CREATE TABLE FLOORS_DEP_POSITION
   (	ID_FLOORS_DEP_POSITION NUMBER(24,0) NOT NULL ENABLE,
	ID_FLOORS_DEPARTMENT NUMBER(24,0) NOT NULL ENABLE,
	POSITION_X NUMBER(24,2) NOT NULL ENABLE,
	POSITION_Y NUMBER(24,2) NOT NULL ENABLE,
	RANK NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE floors_dep_position IS 'Posições dos andares dentro de cada departamento' ;

COMMENT ON COLUMN floors_dep_position.id_floors_dep_position IS 'Chave primária' ;
COMMENT ON COLUMN floors_dep_position.id_floors_department IS 'Relação departamento/andar' ;
COMMENT ON COLUMN floors_dep_position.position_x IS 'Posição X' ;
COMMENT ON COLUMN floors_dep_position.position_y IS 'Posição Y' ;
COMMENT ON COLUMN floors_dep_position.rank IS 'Ordenação' ;


