create table DOCUMENTATION_ACCESS
(
  ID_DOCUMENTATION_ACCESS NUMBER(24) not null,
  ID_DOC_AREA     NUMBER(24) not null,
  ID_PROFILE_TEMPL_ACCESS  NUMBER(24) not null,
  ADW_LAST_UPDATE DATE not null
);



COMMENT ON COLUMN documentation_access.id_documentation_access IS 'Chave primária' ;
COMMENT ON COLUMN documentation_access.id_doc_area IS 'ID de doc_area' ;
COMMENT ON COLUMN documentation_access.id_profile_templ_access IS 'ID de profile_templ_access' ;
COMMENT ON COLUMN documentation_access.adw_last_update IS 'Data da última alteração' ;

