	UPDATE_INSTITUTION NUMBER(24,0)
   );
 
 
   COMMENT ON COLUMN PAT_CIT.ID_PAT_CIT IS 'Id único de identificação de registo (por sequência)';
   COMMENT ON COLUMN PAT_CIT.ID_PATIENT IS 'Id do paciente';
   COMMENT ON COLUMN PAT_CIT.ID_EPISODE IS 'Id do episódio';
   COMMENT ON COLUMN PAT_CIT.FLG_PAT_DISEASE_STATE IS 'Estado de doença do paciente: ''I''-incapacitante para actividade profissional, ''E''-exige cuidados inadiáveis';
