	HOME_AUTHORIZATION VARCHAR2(4000), 
	FLG_STATUS VARCHAR2(1), 
	FLG_TYPE VARCHAR2(1), 
	ID_PROF_WRITES NUMBER(24,0), 
	ID_PROF_CANCEL NUMBER(24,0), 
