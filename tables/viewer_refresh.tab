

  CREATE TABLE VIEWER_REFRESH
   (	ID_VIEWER_REFRESH NUMBER(24,0) NOT NULL ENABLE,
	ID_VIEWER NUMBER(24,0) NOT NULL ENABLE,
	VIEWER_SCREEN VARCHAR2(200) NOT NULL ENABLE,
	SERVICE VARCHAR2(2000) NOT NULL ENABLE
   );


COMMENT ON TABLE viewer_refresh IS 'Parâmetros específicos a considerar no sincronismo de ecrãs. Pode ser usado para activar por defeito um filtro existente num ecrã.' ;

COMMENT ON COLUMN viewer_refresh.id_viewer_refresh IS 'Identificador.' ;
COMMENT ON COLUMN viewer_refresh.id_viewer IS 'Identificador do viewer.' ;
COMMENT ON COLUMN viewer_refresh.viewer_screen IS 'Ecrã do Viewer. Vazio ou "" significa todos os ecrãs.' ;
COMMENT ON COLUMN viewer_refresh.service IS 'Nome completo do serviço a invocado.' ;


