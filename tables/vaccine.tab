

  CREATE TABLE VACCINE
   (	ID_VACCINE NUMBER(12,0) NOT NULL ENABLE,
	CODE_VACCINE VARCHAR2(200) NOT NULL ENABLE,
	COST NUMBER(18,4),
	PRICE NUMBER(18,4),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	RANK NUMBER(5,0) NOT NULL ENABLE
   );


COMMENT ON COLUMN vaccine.cost IS 'Custo monetário' ;
COMMENT ON COLUMN vaccine.price IS 'Preço de venda' ;
COMMENT ON COLUMN vaccine.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN vaccine.rank IS 'Rank provisório para o Algarve. Futuramente deverá passar para uma tabela com ID_INSTITUTION e ID_SOFTWARE' ;
