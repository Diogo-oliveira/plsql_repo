

  CREATE TABLE DOC_ELEMENT_QUALIF
   (	ID_DOC_ELEMENT_QUALIF NUMBER(24,0) NOT NULL ENABLE,
	ID_DOC_ELEMENT_CRIT NUMBER(24,0) NOT NULL ENABLE,
	ID_DOC_QUALIFICATION NUMBER NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN doc_element_qualif.id_doc_element_qualif IS 'Chave primária' ;
COMMENT ON COLUMN doc_element_qualif.id_doc_element_crit IS 'Critério do elemento' ;
COMMENT ON COLUMN doc_element_qualif.id_doc_qualification IS 'Qualificação' ;
COMMENT ON COLUMN doc_element_qualif.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN doc_element_qualif.adw_last_update IS 'Data da última alteração' ;


