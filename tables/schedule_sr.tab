

  CREATE TABLE SCHEDULE_SR
   (	ID_SCHEDULE_SR NUMBER(24,0) NOT NULL ENABLE,
	ID_SCHED_SR_PARENT NUMBER(24,0),
	ID_SCHEDULE NUMBER(24,0),
	ID_EPISODE NUMBER(24,0),
	DT_TARGET DATE,
	DT_INTERV_PREVIEW DATE,
	ID_PATIENT NUMBER(24,0) NOT NULL ENABLE,
	DURATION NUMBER(6,0),
	ID_DIAGNOSIS NUMBER(12,0),
	ID_SPECIALITY NUMBER(12,0),
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,

	FLG_SCHED VARCHAR2(1) NOT NULL ENABLE,
	ID_DEPT_DEST NUMBER(24,0),
	PREV_RECOVERY_TIME NUMBER(6,0),
	ID_SR_CANCEL_REASON NUMBER(24,0),
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(4000),
	ID_PROF_REG NUMBER(24,0),
	ID_INSTITUTION NUMBER(12,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE schedule_sr IS 'Agendamento do Bloco Operatório' ;

COMMENT ON COLUMN schedule_sr.id_schedule_sr IS 'Id único' ;
COMMENT ON COLUMN schedule_sr.id_sched_sr_parent IS 'Id do agendamento "pai". Preenchida apenas em reagendamentos' ;
COMMENT ON COLUMN schedule_sr.id_schedule IS 'ID do agendamento. Pode não estar preenchido, nos casos em que o agendamento ainda não tenha sido efetuado (episódios temporários).' ;
COMMENT ON COLUMN schedule_sr.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN schedule_sr.dt_target IS 'Data de agendamento da cirurgia' ;
COMMENT ON COLUMN schedule_sr.dt_interv_preview IS 'Data prevista de realização da intervenção. Pode não estar preenchido, nos casos em que o agendamento ainda não tenha sido efetuado (episódios temporários).' ;
COMMENT ON COLUMN schedule_sr.duration IS 'Duração da cirurgia (em minutos)' ;
COMMENT ON COLUMN schedule_sr.id_diagnosis IS 'ID do diagnóstico' ;
COMMENT ON COLUMN schedule_sr.id_speciality IS 'ID da especialidade' ;
COMMENT ON COLUMN schedule_sr.flg_status IS 'Estado do agendamento. Valores possíveis: A- Activo, I- Inactivo (alta), C- Cancelado' ;
COMMENT ON COLUMN schedule_sr.flg_sched IS 'Estado do agendamento. Valores possíveis: A- agendada, N- não agendada' ;
COMMENT ON COLUMN schedule_sr.id_dept_dest IS 'ID do departamento de destino previsto. Não é obrigatório preencher.' ;
COMMENT ON COLUMN schedule_sr.prev_recovery_time IS 'Tempo previsto de permanência no recobro' ;

COMMENT ON COLUMN schedule_sr.id_sr_cancel_reason IS 'ID do motivo de cancelamento da cirurgia' ;
COMMENT ON COLUMN schedule_sr.dt_cancel IS 'Data de cancelamento' ;
COMMENT ON COLUMN schedule_sr.id_prof_cancel IS 'ID do profissional que cancelou a intervenção' ;
COMMENT ON COLUMN schedule_sr.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN schedule_sr.id_prof_reg IS 'ID do profissional que iniciou o registo. Deve ser utilizado para Cirurgias ainda não agendadas' ;
COMMENT ON COLUMN schedule_sr.id_institution IS 'ID da instituição onde será realizada a cirurgia' ;
COMMENT ON COLUMN schedule_sr.adw_last_update IS 'Data da última alteração' ;

