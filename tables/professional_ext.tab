

  CREATE TABLE PROFESSIONAL_EXT
   (	ID_PROFESSIONAL_EXT NUMBER(24,0),
	TITLE VARCHAR2(6),
	FIRST_NAME VARCHAR2(50),
	LAST_NAME VARCHAR2(50),
	GENDER VARCHAR2(1),
	ID_SPECIALITY NUMBER(12,0),
	ID_CATEGORY NUMBER(12,0),
	ID_CATEGORY_SUB NUMBER(12,0),
	ID_LANGUAGE NUMBER(6,0),
	ADDRESS VARCHAR2(200),
	CITY VARCHAR2(200),
	DISTRICT VARCHAR2(200),
	ZIP_CODE VARCHAR2(200),
	ID_COUNTRY NUMBER(12,0),
	WORK_PHONE VARCHAR2(30),
	FAX VARCHAR2(30),
	WEBSITE VARCHAR2(200),
	EMAIL VARCHAR2(100),
	CELL_PHONE VARCHAR2(30),
	ID_INSTITUTION NUMBER(24,0),
	FLG_AVAILABLE VARCHAR2(1),
	DT_BIRTH DATE,
	NUM_CONTACT VARCHAR2(30),
	MARITAL_STATUS VARCHAR2(1),
	INITIALS VARCHAR2(30),
	MIDDLE_NAME VARCHAR2(100),
	NAME VARCHAR2(200)
   ) ;


   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_PROFESSIONAL_EXT IS 'External professional ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.TITLE IS 'External professional title';



   COMMENT ON COLUMN PROFESSIONAL_EXT.FIRST_NAME IS 'First name';



   COMMENT ON COLUMN PROFESSIONAL_EXT.LAST_NAME IS 'Last name';



   COMMENT ON COLUMN PROFESSIONAL_EXT.GENDER IS 'Gender: M - Male; F - Female; I - Indefinite';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_SPECIALITY IS 'Speciality ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_CATEGORY IS 'Category ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_CATEGORY_SUB IS 'Surgery category ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_LANGUAGE IS 'Prefered language ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ADDRESS IS 'Address';



   COMMENT ON COLUMN PROFESSIONAL_EXT.CITY IS 'City';



   COMMENT ON COLUMN PROFESSIONAL_EXT.DISTRICT IS 'County';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ZIP_CODE IS 'Zip code';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_COUNTRY IS 'Country ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.WORK_PHONE IS 'Work phone number';



   COMMENT ON COLUMN PROFESSIONAL_EXT.FAX IS 'Fax number';



   COMMENT ON COLUMN PROFESSIONAL_EXT.WEBSITE IS 'Website address';



   COMMENT ON COLUMN PROFESSIONAL_EXT.EMAIL IS 'Professional e-mail';



   COMMENT ON COLUMN PROFESSIONAL_EXT.CELL_PHONE IS 'Cell phone number';



   COMMENT ON COLUMN PROFESSIONAL_EXT.ID_INSTITUTION IS 'Institution ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT.FLG_AVAILABLE IS 'External professional status: Y - Yes; N - No';



   COMMENT ON COLUMN PROFESSIONAL_EXT.DT_BIRTH IS 'Birth Date';



   COMMENT ON COLUMN PROFESSIONAL_EXT.NUM_CONTACT IS 'Home phone number';



   COMMENT ON COLUMN PROFESSIONAL_EXT.MARITAL_STATUS IS 'Marital status: D - Divorced; F - Separated; M - Married; S - Single; U - Living with partner; W - Widow(er)';



   COMMENT ON COLUMN PROFESSIONAL_EXT.INITIALS IS 'Name initials';



   COMMENT ON COLUMN PROFESSIONAL_EXT.MIDDLE_NAME IS 'Middle name';



   COMMENT ON COLUMN PROFESSIONAL_EXT.NAME IS 'Full name';



   COMMENT ON TABLE PROFESSIONAL_EXT  IS 'External professionals';


