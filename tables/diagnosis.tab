

  CREATE TABLE DIAGNOSIS
   (	ID_DIAGNOSIS NUMBER(12,0) NOT NULL ENABLE,
	ID_DIAGNOSIS_PARENT NUMBER(12,0),
	CODE_DIAGNOSIS VARCHAR2(200) NOT NULL ENABLE,
	CODE_ICD VARCHAR2(30) NOT NULL ENABLE,
	FLG_SELECT VARCHAR2(1) NOT NULL ENABLE,
	FLG_FAMILY VARCHAR2(1),
	FLG_POS_BIRTH VARCHAR2(1),
	FLG_JOB VARCHAR2(1),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,

	FLG_OTHER VARCHAR2(1),
	GENDER VARCHAR2(1),
	AGE_MIN NUMBER(3,0),
	AGE_MAX NUMBER(3,0),
	MDM_CODING NUMBER(12,0),
	FLG_SUBTYPE VARCHAR2(1)
   );


COMMENT ON TABLE diagnosis IS 'Tabela que contém os diagnósticos atribuíveis aos pacientes.' ;

COMMENT ON COLUMN diagnosis.id_diagnosis IS 'Chave primária' ;
COMMENT ON COLUMN diagnosis.code_diagnosis IS 'Código para diagnóstico' ;
COMMENT ON COLUMN diagnosis.code_icd IS 'Código internacional do diagnóstico' ;
COMMENT ON COLUMN diagnosis.flg_select IS 'Diagnóstico seleccionável pelo utilizador. Nem todos os diag. válidos são seleccionáveis (por ex, os níveis superiores da hierarquia de diag.)' ;
COMMENT ON COLUMN diagnosis.flg_family IS 'Diagnóstico dos Antecedentes Familiares' ;
COMMENT ON COLUMN diagnosis.flg_pos_birth IS 'Doença habitual no período neo natal' ;
COMMENT ON COLUMN diagnosis.flg_job IS 'Doença de trabalho' ;
COMMENT ON COLUMN diagnosis.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN diagnosis.flg_type IS 'Tipo: D - ICD9, P-  ICPC2, C - ICD9 CM' ;
COMMENT ON COLUMN diagnosis.flg_other IS 'Indicação de "outro diagnóstico" (permite seleccionar um diag. registado manualmente)' ;
COMMENT ON COLUMN diagnosis.gender IS 'Género ao qual se aplica' ;
COMMENT ON COLUMN diagnosis.age_min IS 'Idade mínima à qual se aplica' ;
COMMENT ON COLUMN diagnosis.age_max IS 'Idade máxima à qual se aplica' ;

COMMENT ON COLUMN diagnosis.mdm_coding IS 'Codificação de  MDM "Medical Decision Making"' ;
COMMENT ON COLUMN diagnosis.flg_subtype IS 'Subtipo ICPC2: S - Sinais/Sintomas; I - Infecções; N - Neoplasias; T - Traumatismos; A - Anomalias Congénitas; O - Outros diagnósticos' ;

COMMENT ON COLUMN diagnosis.flg_type IS 'Tipo: D - ICD9, P-  ICPC2, C - ICD9 CM, B - ICD10 (Brazilian market)';

