--Creating Table 'TAB_BUTTON_PROF_INST'
CREATE TABLE TAB_BUTTON_PROF_INST
 (ID_TAB_BUTTON_PROF_INST NUMBER(24) NOT NULL,
 ID_TAB_BUTTON NUMBER(24) NOT NULL,
 ID_INSTITUTION NUMBER(24) NOT NULL,
 ID_PROFILE_TEMPLATE NUMBER(12) NOT NULL,
 RANK NUMBER(6),
 FLG_DEFAULT VARCHAR2(1 CHAR) NOT NULL,
 FLG_AVAILABLE VARCHAR2(1 CHAR) DEFAULT 'Y' NOT NULL
 );


comment on column TAB_BUTTON_PROF_INST.ID_TAB_BUTTON_PROF_INST
  is 'ID unique';
comment on column TAB_BUTTON_PROF_INST.ID_TAB_BUTTON
  is 'ID Tab button';
comment on column TAB_BUTTON_PROF_INST.ID_INSTITUTION
  is 'Id institution';
comment on column TAB_BUTTON_PROF_INST.ID_PROFILE_TEMPLATE
  is 'Id profile';
comment on column TAB_BUTTON_PROF_INST.RANK
  is 'Order of apperance';
comment on column TAB_BUTTON_PROF_INST.FLG_DEFAULT
  is 'If it is the default one';
comment on column TAB_BUTTON_PROF_INST.FLG_AVAILABLE
  is 'If it is available';
-- CHANGE END: Elisabete Bugalho

