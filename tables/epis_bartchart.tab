

  CREATE TABLE EPIS_BARTCHART
   (	ID_EPIS_BARTCHART NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_CREATION DATE NOT NULL ENABLE,
	ID_PROF_LAST_UPDATE NUMBER(24,0),
	DT_LAST_UPDATE DATE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	ID_EPIS_COMPLAINT NUMBER(24,0)
   );


COMMENT ON COLUMN epis_bartchart.id_epis_bartchart IS 'Chave primária' ;
COMMENT ON COLUMN epis_bartchart.dt_creation IS 'Data de criação' ;
COMMENT ON COLUMN epis_bartchart.id_prof_last_update IS 'Profissional que efectou a última alteração' ;
COMMENT ON COLUMN epis_bartchart.dt_last_update IS 'Data em que se efectuou a última alteração' ;
COMMENT ON COLUMN epis_bartchart.flg_status IS 'Estado: A - Activo; I - Inactivo' ;
COMMENT ON COLUMN epis_bartchart.adw_last_update IS 'Data da última alteração BD' ;
COMMENT ON COLUMN epis_bartchart.id_epis_complaint IS 'Chave primária' ;
