

  CREATE TABLE MANIPULATED_GROUP
   (	ID_MANIPULATED_GROUP NUMBER(24,0) NOT NULL ENABLE,
	CODE_MANIPULATED_GROUP VARCHAR2(200) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE manipulated_group IS 'Grupos de manipulados.' ;

COMMENT ON COLUMN manipulated_group.id_manipulated_group IS 'Chave primária.' ;
COMMENT ON COLUMN manipulated_group.code_manipulated_group IS 'Nome do grupo.' ;
COMMENT ON COLUMN manipulated_group.adw_last_update IS 'Data da última alteração.' ;
