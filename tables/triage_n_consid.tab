

  CREATE TABLE TRIAGE_N_CONSID
   (	ID_TRIAGE_N_CONSID NUMBER(24,0) NOT NULL ENABLE,
	ID_TRIAGE_BOARD NUMBER(12,0) NOT NULL ENABLE,
	ID_TRIAGE_CONSIDERATIONS NUMBER(24,0) NOT NULL ENABLE,
	ID_TRIAGE_DISCRIMINATOR NUMBER(12,0) NOT NULL ENABLE,
	ID_TRIAGE_COLOR NUMBER(12,0) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN triage_n_consid.id_triage_n_consid IS 'Chave primária' ;
COMMENT ON COLUMN triage_n_consid.id_triage_board IS 'Chave primária' ;
COMMENT ON COLUMN triage_n_consid.id_triage_considerations IS 'Chave primária' ;
COMMENT ON COLUMN triage_n_consid.id_triage_discriminator IS 'Chave primária' ;
COMMENT ON COLUMN triage_n_consid.id_triage_color IS 'Chave primária' ;
COMMENT ON COLUMN triage_n_consid.flg_available IS 'Disponibilidade do registo' ;
COMMENT ON COLUMN triage_n_consid.adw_last_update IS 'Data da última alteração' ;


