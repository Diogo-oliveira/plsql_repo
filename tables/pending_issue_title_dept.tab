

  CREATE TABLE PENDING_ISSUE_TITLE_DEPT
   (	ID_PENDING_ISSUE_TITLE NUMBER(24,0),
	ID_DEPT NUMBER(24,0),
	CREATE_USER VARCHAR2(24),
	CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	CREATE_INSTITUTION NUMBER(24,0),
	UPDATE_USER VARCHAR2(24),
	UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	UPDATE_INSTITUTION NUMBER(24,0)
   ) ;


   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.ID_PENDING_ISSUE_TITLE IS 'Pending issue title ID';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.ID_DEPT IS 'Dept ID';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.CREATE_USER IS 'Creation User';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.CREATE_TIME IS 'Creation Time';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.CREATE_INSTITUTION IS 'Creation Institution';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.UPDATE_USER IS 'Update User';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.UPDATE_TIME IS 'Update Time';



   COMMENT ON COLUMN PENDING_ISSUE_TITLE_DEPT.UPDATE_INSTITUTION IS 'Update Institution';



   COMMENT ON TABLE PENDING_ISSUE_TITLE_DEPT  IS 'Relation between pending issue title and dept';

   comment on column PENDING_ISSUE_TITLE_DEPT.ID_PENDING_ISSUE_TITLE  is 'Pending issue title identification';
   comment on column PENDING_ISSUE_TITLE_DEPT.ID_DEPT  is 'Department identification';

