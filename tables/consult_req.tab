

  CREATE TABLE CONSULT_REQ
   (	ID_CONSULT_REQ NUMBER(24,0) NOT NULL ENABLE,
	DT_CONSULT_REQ DATE NOT NULL ENABLE,
	CONSULT_TYPE VARCHAR2(200),
	ID_CLINICAL_SERVICE NUMBER(12,0),
	ID_PATIENT NUMBER(24,0) NOT NULL ENABLE,
	ID_INSTIT_REQUESTS NUMBER(12,0) NOT NULL ENABLE,
	ID_INST_REQUESTED NUMBER(12,0),
	ID_EPISODE NUMBER(24,0),
	ID_PROF_REQ NUMBER(24,0) NOT NULL ENABLE,
	ID_PROF_AUTH NUMBER(24,0),
	ID_PROF_APPR NUMBER(24,0),

	ID_PROF_PROC NUMBER(24,0),
	DT_SCHEDULED DATE,
	NOTES VARCHAR2(4000),
	ID_PROF_CANCEL NUMBER(24,0),
	DT_CANCEL DATE,
	NOTES_CANCEL VARCHAR2(4000),
	ID_DEP_CLIN_SERV NUMBER(24,0),
	ID_PROF_REQUESTED NUMBER(24,0),
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	NOTES_ADMIN VARCHAR2(4000)
   );


COMMENT ON TABLE consult_req IS 'Requisição de consultas ao exterior  e internas' ;

COMMENT ON COLUMN consult_req.dt_consult_req IS 'Data da requisição' ;
COMMENT ON COLUMN consult_req.consult_type IS 'Tipo de exame / consulta requisitada. Se requisição é externa, preenche-se ID_CLINICAL_SERVICE (se o tipo de serviço pretendido está registado na BD da instituição requisitante) ou CONSULT_TYPE (campo de texto livre).Se requisição é interna, selecciona-se não só o tipo de serviço, mas tb o departamento (DEP_CLIN_SERV).' ;
COMMENT ON COLUMN consult_req.id_clinical_service IS 'Tipo de exame / consulta requisitada. Se requisição é externa, preenche-se ID_CLINICAL_SERVICE (se o tipo de serviço pretendido está registado na BD da instituição requisitante) ou CONSULT_TYPE (campo de texto livre).Se requisição é interna, selecciona-se não só o tipo de serviço, mas tb o departamento (DEP_CLIN_SERV).' ;
COMMENT ON COLUMN consult_req.id_episode IS 'Episódio em q foi requisitada a consulta' ;
COMMENT ON COLUMN consult_req.dt_scheduled IS 'Data / hora requisitada' ;
COMMENT ON COLUMN consult_req.notes IS 'Notas ao médico requisitado' ;
COMMENT ON COLUMN consult_req.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN consult_req.id_dep_clin_serv IS 'Tipo de exame / consulta requisitada. Se requisição é externa, preenche-se ID_CLINICAL_SERVICE (se o tipo de serviço pretendido está registado na BD da instituição requisitante) ou CONSULT_TYPE (campo de texto livre).Se requisição é interna, selecciona-se não só o tipo de serviço, mas tb o departamento (DEP_CLIN_SERV).' ;
COMMENT ON COLUMN consult_req.id_prof_requested IS 'Profissional requisitado, se é uma requisição interna.' ;
COMMENT ON COLUMN consult_req.flg_status IS 'Estado: R - requisitado, F - pedido lido, P - respondido, A - resposta lida, C - cancelado, T - autorizado, V - aprovado, S - processado' ;
COMMENT ON COLUMN consult_req.notes_admin IS 'Notas ao administrativo' ;

ALTER TABLE consult_req ADD id_schedule NUMBER(24);
COMMENT ON COLUMN consult_req.id_schedule IS 'Schedule identifier';

