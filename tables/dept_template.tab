

  CREATE TABLE DEPT_TEMPLATE
   (	ID_DEPT_TEMPLATE NUMBER NOT NULL ENABLE,
	ID_DOC_TEMPLATE NUMBER(24,0) NOT NULL ENABLE,
	ID_DEPARTMENT NUMBER(24,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	FLG_GENDER VARCHAR2(1),
	AGE_MAX NUMBER,
	AGE_MIN NUMBER
   );

comment on column DEPT_TEMPLATE.ID_DEPT_TEMPLATE
  is 'Chave primária';
comment on column DEPT_TEMPLATE.ADW_LAST_UPDATE
  is 'Data da última alteração';
comment on column DEPT_TEMPLATE.FLG_AVAILABLE
  is 'Disponibilidade do registo na instituição onde está instalada a BD';
comment on column DEPT_TEMPLATE.FLG_GENDER
  is 'Sexo:F-feminino,M-masculino,G-geral';
comment on column DEPT_TEMPLATE.AGE_MAX
  is 'Idade máxima';
comment on column DEPT_TEMPLATE.AGE_MIN
  is 'Idade minima';

