

  CREATE TABLE DOC_QUANTIFICATION
   (	ID_DOC_QUANTIFICATION NUMBER(24,0) NOT NULL ENABLE,
	CODE_DOC_QUANTIFICATION VARCHAR2(240) NOT NULL ENABLE,
	LEVEL_QUANT VARCHAR2(10),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN doc_quantification.id_doc_quantification IS 'Chave primária' ;
COMMENT ON COLUMN doc_quantification.code_doc_quantification IS 'Descrição do quantificador' ;
COMMENT ON COLUMN doc_quantification.level_quant IS 'Grau de quantificação' ;
COMMENT ON COLUMN doc_quantification.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN doc_quantification.adw_last_update IS 'Data da última alteração' ;

