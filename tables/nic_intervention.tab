BEGIN
  pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'NIC_INTERVENTION', i_obj_type => 'TABLE', i_flg_category => 'CNT', i_flg_alert_default => 'N', i_delete_script => '', i_flg_default_child => 'N', i_flg_nzd => 'N', i_responsible_team => 'NURSING');
END;
/
CREATE TABLE NIC_INTERVENTION
  (
    ID_NIC_INTERVENTION    NUMBER (24) NOT NULL ,
    ID_TERMINOLOGY_VERSION NUMBER (24) NOT NULL ,
    INTERVENTION_CODE      NUMBER (24) NOT NULL ,
    CODE_NAME              VARCHAR2 (200 CHAR) AS ( 'NIC_INTERVENTION.CODE_NAME.'
    ||TO_CHAR(ID_NIC_INTERVENTION) ) VIRTUAL NOT NULL ,
    CODE_DEFINITION VARCHAR2 (200 CHAR) AS ( 'NIC_INTERVENTION.CODE_DEFINITION.'
    ||TO_CHAR(ID_NIC_INTERVENTION) ) VIRTUAL NOT NULL ,
    REFERENCES CLOB ,
    ID_INST_OWNER      NUMBER (24) ,
    ID_CONCEPT_VERSION NUMBER (24) ,
    ID_CONCEPT_TERM    NUMBER (24) ,
    CREATE_USER        VARCHAR2 (30 CHAR) ,
    CREATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION NUMBER (24) ,
    UPDATE_USER        VARCHAR2 (30 CHAR) ,
    UPDATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION NUMBER (24)
  )
  TABLESPACE ALERT_DATA LOB
  (
    REFERENCES
  )
  STORE AS
  (
    TABLESPACE ALERT_LOB PCTVERSION 10 ENABLE STORAGE IN ROW NOCACHE
  ) ;
COMMENT ON TABLE NIC_INTERVENTION
IS
  'NIC - Nursing Intervention' ;
  COMMENT ON COLUMN NIC_INTERVENTION.ID_NIC_INTERVENTION
IS
  'Intervention ID (surrogate key)' ;
  COMMENT ON COLUMN NIC_INTERVENTION.ID_TERMINOLOGY_VERSION
IS
  'NIC Version ID' ;
  COMMENT ON COLUMN NIC_INTERVENTION.INTERVENTION_CODE
IS
  'NIC Intervention code in the standardized classification of nursing interventions.' ;
  COMMENT ON COLUMN NIC_INTERVENTION.CODE_NAME
IS
  'Translation code for NIC Intervention label' ;
  COMMENT ON COLUMN NIC_INTERVENTION.CODE_DEFINITION
IS
  'Translation code for NIC Intervention definition' ;
  COMMENT ON COLUMN NIC_INTERVENTION.REFERENCES
IS
  'Citation References related with the NIC Intervention' ;
  COMMENT ON COLUMN NIC_INTERVENTION.ID_INST_OWNER
IS
  'Institution owner of the concept. 0 - ALERT' ;
  COMMENT ON COLUMN NIC_INTERVENTION.ID_CONCEPT_VERSION
IS
  'Concept version ID' ;
  COMMENT ON COLUMN NIC_INTERVENTION.ID_CONCEPT_TERM
IS
  'Concept term ID' ;
  COMMENT ON COLUMN NIC_INTERVENTION.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NIC_INTERVENTION.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NIC_INTERVENTION.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NIC_INTERVENTION.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NIC_INTERVENTION.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NIC_INTERVENTION.UPDATE_INSTITUTION
IS
  'Update Institution' ;
