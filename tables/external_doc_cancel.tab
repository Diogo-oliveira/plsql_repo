create table EXTERNAL_DOC_CANCEL
(
  ID_EXTERNAL_DOC_CANCEL NUMBER NOT NULL,
  ID_EXTERNAL_DOC        NUMBER not null,
  ID_PROFESSIONAL_CANCEL NUMBER not null,
  ID_EPISODE             NUMBER not null,
  DT_CANCEL              TIMESTAMP(6) WITH LOCAL TIME ZONE
);

comment on table EXTERNAL_DOC_CANCEL
  is 'Tabela de dados externos cancelados para um episodio';
	
comment on column EXTERNAL_DOC_CANCEL.ID_EXTERNAL_DOC_CANCEL
  is 'Chave primaria';