COMMENT ON COLUMN REHAB_PLAN_CATEGORY_TEAM.CREATE_INSTITUTION IS 'Creation Institution';

COMMENT ON COLUMN REHAB_PLAN_CATEGORY_TEAM.UPDATE_USER IS 'Update User';

COMMENT ON COLUMN REHAB_PLAN_CATEGORY_TEAM.UPDATE_TIME IS 'Update Time';

COMMENT ON COLUMN REHAB_PLAN_CATEGORY_TEAM.UPDATE_INSTITUTION IS 'Update Institution';
