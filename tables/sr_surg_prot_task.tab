

  CREATE TABLE SR_SURG_PROT_TASK
   (	ID_SR_SURG_PROT_TASK NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_SURG_PROTOCOL NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_SURG_TASK NUMBER(24,0) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN sr_surg_prot_task.id_sr_surg_prot_task IS 'Id único' ;
COMMENT ON COLUMN sr_surg_prot_task.id_sr_surg_protocol IS 'ID do protocolo cirúrgico' ;
COMMENT ON COLUMN sr_surg_prot_task.id_sr_surg_task IS 'Id da terefa' ;
COMMENT ON COLUMN sr_surg_prot_task.flg_available IS 'Indica se a relação está activa' ;
COMMENT ON COLUMN sr_surg_prot_task.adw_last_update IS 'Data da última alteração' ;
