

  CREATE TABLE MATERIAL_PROTOCOLS
   (	ID_MATERIAL_PROTOCOLS NUMBER(24,0) NOT NULL ENABLE,
	ID_PROTOCOLS NUMBER(24,0) NOT NULL ENABLE,
	ID_MATERIAL NUMBER(24,0) NOT NULL ENABLE,
	QTY_REQ NUMBER(3,0),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE
   );


COMMENT ON COLUMN material_protocols.id_material_protocols IS 'ID único' ;
COMMENT ON COLUMN material_protocols.id_protocols IS 'ID do protocolo' ;
COMMENT ON COLUMN material_protocols.id_material IS 'ID do material' ;
COMMENT ON COLUMN material_protocols.qty_req IS 'Quantidade' ;
COMMENT ON COLUMN material_protocols.flg_available IS 'Indica se está disponível' ;
COMMENT ON COLUMN material_protocols.rank IS 'Ordem de visualização' ;
COMMENT ON COLUMN material_protocols.adw_last_update IS 'Data da última alteração' ;
