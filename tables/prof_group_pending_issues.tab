create table PROF_GROUP_PENDING_ISSUES
(
  ID_GROUP        NUMBER(24) not null,
  ID_PROFESSIONAL NUMBER(24) not null,
  STATE           VARCHAR2(30) not null
);
-- Add comments to the table 
comment on table PROF_GROUP_PENDING_ISSUES
  is 'Professional in the group';
-- Add comments to the columns 
comment on column PROF_GROUP_PENDING_ISSUES.ID_GROUP
  is 'group identification';
comment on column PROF_GROUP_PENDING_ISSUES.ID_PROFESSIONAL
  is 'professional identification';
comment on column PROF_GROUP_PENDING_ISSUES.STATE
  is 'group state A- Active / I - Inactive';
