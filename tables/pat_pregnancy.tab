	FIRST_FETAL_CARDIAC NUMBER(2,0),

	WEIGHT_BEFORE_PREGN NUMBER(4,1),
	FLG_MULTIPLE VARCHAR2(1),
	FLG_STATUS VARCHAR2(2),
