CREATE TABLE ORDER_SET_FREQUENT
 (ID_ORDER_SET NUMBER(24) NOT NULL
 ,RANK NUMBER(6)
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 );

COMMENT ON TABLE ORDER_SET_FREQUENT IS 'Most frequent Order Sets by institution and software';
COMMENT ON COLUMN ORDER_SET_FREQUENT.ID_ORDER_SET IS 'Order Set ID';
COMMENT ON COLUMN ORDER_SET_FREQUENT.RANK IS 'Rank of frequent order set';
COMMENT ON COLUMN ORDER_SET_FREQUENT.ID_INSTITUTION IS 'Institution where the order set is frequent';
COMMENT ON COLUMN ORDER_SET_FREQUENT.ID_SOFTWARE IS 'Software where the order set is frequent';