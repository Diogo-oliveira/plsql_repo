

  CREATE TABLE SR_SURG_PROTOCOL
   (	ID_SR_SURG_PROTOCOL NUMBER(24,0) NOT NULL ENABLE,
	CODE_SR_SURG_PROTOCOL VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	DESC_PROTOCOL VARCHAR2(4000) NOT NULL ENABLE
   );


COMMENT ON COLUMN sr_surg_protocol.id_sr_surg_protocol IS 'ID do protocolo cirúrgico' ;
COMMENT ON COLUMN sr_surg_protocol.code_sr_surg_protocol IS 'Código para obtenção da descrição' ;
COMMENT ON COLUMN sr_surg_protocol.flg_available IS 'Indica se o protolo está disponível' ;
COMMENT ON COLUMN sr_surg_protocol.desc_protocol IS 'Descrição do protocolo' ;
