

  CREATE TABLE SOFTWARE_DEPT
   (	ID_SOFTWARE_DEPT NUMBER(24,0) NOT NULL ENABLE,
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE,
	ID_DEPT NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE software_dept IS 'Associação entre departamento e software.' ;

COMMENT ON COLUMN software_dept.id_software_dept IS 'Chave primária' ;
COMMENT ON COLUMN software_dept.id_software IS 'ID do software' ;
COMMENT ON COLUMN software_dept.id_dept IS 'ID do departamento' ;
