
  CREATE TABLE INF_PRECO_IT
   (	EMB_ID NUMBER(10,0) NOT NULL ENABLE,
	TIPO_PRECO_ID NUMBER(3,0) NOT NULL ENABLE,
	PRECO NUMBER(8,2) NOT NULL ENABLE,
	DATA_PRECO DATE NOT NULL ENABLE
   );


COMMENT ON TABLE inf_preco_it IS 'Preço de cada embalagem.' ;

COMMENT ON COLUMN inf_preco_it.emb_id IS 'Código identificador da embalagem.' ;
COMMENT ON COLUMN inf_preco_it.tipo_preco_id IS 'Código identificador do tipo de preço/código de geração da embalagem.' ;
COMMENT ON COLUMN inf_preco_it.preco IS 'Preço da embalagem do medicamento em euros.' ;
COMMENT ON COLUMN inf_preco_it.data_preco IS 'Data de atribuição do preço.' ;

