

  CREATE TABLE SAMPLE_TEXT_TYPE
   (	ID_SAMPLE_TEXT_TYPE NUMBER(12,0) NOT NULL ENABLE,
	INTERN_NAME_SAMPLE_TEXT_TYPE VARCHAR2(200) NOT NULL ENABLE,
	DESC_SAMPLE_TEXT_TYPE VARCHAR2(200),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	CODE_SAMPLE_TEXT_TYPE VARCHAR2(200) NOT NULL ENABLE,
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE sample_text_type IS 'Tipos de texto. Ajuda a indicar o sítio onde são utilizados' ;

COMMENT ON COLUMN sample_text_type.intern_name_sample_text_type IS 'Nome de uso interno, p/ identificação do registo pretendido' ;
COMMENT ON COLUMN sample_text_type.desc_sample_text_type IS 'Descritivo p/ facilitar a percepção do objectivo do registo' ;
COMMENT ON COLUMN sample_text_type.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;




ALTER TABLE SAMPLE_TEXT_TYPE ADD (ID_CONTENT VARCHAR2(200));
COMMENT ON COLUMN SAMPLE_TEXT_TYPE.ID_CONTENT  IS 'Identificador único de conteúdo';

