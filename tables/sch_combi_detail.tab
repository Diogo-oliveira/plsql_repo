CREATE TABLE SCH_COMBI_DETAIL
 (ID_SCH_COMBI NUMBER(24) NOT NULL
 ,ID_CODE NUMBER(24) NOT NULL
 ,ID_SCH_COMBI_DETAIL NUMBER(24) NOT NULL
 ,ID_SCH_EVENT NUMBER(24) NOT NULL
 ,ID_DEP_CLIN_SERV NUMBER(24) NOT NULL
 ,ID_EXAM NUMBER(24)
 ,DEP_TYPE VARCHAR2(2)
 ,ID_CODE_PARENT NUMBER(24)
 ,MIN_TIME_AFTER NUMBER(6)
 ,MAX_TIME_AFTER NUMBER(6)
 ,FLG_OPTIONAL VARCHAR2(1)
 ,CREATE_USER VARCHAR2(24)
 ,CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION NUMBER
 ,UPDATE_USER VARCHAR2(24)
 ,UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
 );

COMMENT ON TABLE SCH_COMBI_DETAIL IS 'list of eventual appointments that will make up entire single visit'; 
COMMENT ON COLUMN SCH_COMBI_DETAIL.ID_SCH_COMBI IS 'master key';
COMMENT ON COLUMN SCH_COMBI_DETAIL.ID_CODE IS 'sequence number set by user';
COMMENT ON COLUMN SCH_COMBI_DETAIL.ID_SCH_COMBI_DETAIL IS 'synthetic key needed for easy referencing inside schedule table';
COMMENT ON COLUMN SCH_COMBI_DETAIL.ID_SCH_EVENT IS 'event for this particular appointment';
COMMENT ON COLUMN SCH_COMBI_DETAIL.ID_DEP_CLIN_SERV IS 'dcs for this particular appointment';
COMMENT ON COLUMN SCH_COMBI_DETAIL.DEP_TYPE IS 'dep type for this particular appointment';
COMMENT ON COLUMN SCH_COMBI_DETAIL.ID_CODE_PARENT IS 'this parent appointment, if any, must  be perfomed before this one';
COMMENT ON COLUMN SCH_COMBI_DETAIL.MIN_TIME_AFTER IS 'this amount of minutes must be respected before  the next  event takes place';
COMMENT ON COLUMN SCH_COMBI_DETAIL.MAX_TIME_AFTER IS 'the next event must not be scheduled after this amount of minutes';
COMMENT ON COLUMN SCH_COMBI_DETAIL.FLG_OPTIONAL IS 'Y = this event may not be scheduled';
COMMENT ON COLUMN SCH_COMBI_DETAIL.CREATE_USER IS 'Creation user';
COMMENT ON COLUMN SCH_COMBI_DETAIL.CREATE_TIME IS 'creation time';
COMMENT ON COLUMN SCH_COMBI_DETAIL.CREATE_INSTITUTION IS 'creation institution';
COMMENT ON COLUMN SCH_COMBI_DETAIL.UPDATE_USER IS 'update user';
COMMENT ON COLUMN SCH_COMBI_DETAIL.UPDATE_TIME IS 'update time';
COMMENT ON COLUMN SCH_COMBI_DETAIL.UPDATE_INSTITUTION IS 'update institution';
-- CHANGE END: Telmo Castro

