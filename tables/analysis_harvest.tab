	ID_ANALYSIS_REQ_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_HARVEST NUMBER(24,0) NOT NULL ENABLE,
	ID_ANALYSIS_REQ_PAR NUMBER(24,0),
	ID_SAMPLE_RECIPIENT NUMBER(24,0)
