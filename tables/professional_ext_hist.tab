

  CREATE TABLE PROFESSIONAL_EXT_HIST
   (	ID_PROFESSIONAL_EXT_HIST NUMBER(24,0),
	ID_PROFESSIONAL_EXT NUMBER(24,0),
	TITLE VARCHAR2(6),
	FIRST_NAME VARCHAR2(50),
	LAST_NAME VARCHAR2(50),
	GENDER VARCHAR2(1),
	ID_SPECIALITY NUMBER(12,0),
	ID_CATEGORY NUMBER(12,0),
	ID_CATEGORY_SUB NUMBER(12,0),
	ID_LANGUAGE NUMBER(6,0),
	ADDRESS VARCHAR2(200),
	CITY VARCHAR2(200),
	DISTRICT VARCHAR2(200),
	ZIP_CODE VARCHAR2(200),
	ID_COUNTRY NUMBER(12,0),
	WORK_PHONE VARCHAR2(30),
	FAX VARCHAR2(30),
	WEBSITE VARCHAR2(200),
	EMAIL VARCHAR2(100),
	CELL_PHONE VARCHAR2(30),
	ID_INSTITUTION NUMBER(24,0),
	FLG_AVAILABLE VARCHAR2(1),
	DT_BIRTH DATE,
	NUM_CONTACT VARCHAR2(30),
	MARITAL_STATUS VARCHAR2(1),
	INITIALS VARCHAR2(30),
	MIDDLE_NAME VARCHAR2(100),
	NAME VARCHAR2(200),
	BEGIN_DATE TIMESTAMP (6) WITH LOCAL TIME ZONE,
	END_DATE TIMESTAMP (6) WITH LOCAL TIME ZONE,
	ID_PROFESSIONAL NUMBER(24,0)
   ) ;


   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_PROFESSIONAL_EXT IS 'External professional ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.TITLE IS 'External professional title';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.FIRST_NAME IS 'First name';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.LAST_NAME IS 'Last name';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.GENDER IS 'Gender: M - Male; F - Female';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_SPECIALITY IS 'Speciality ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_CATEGORY IS 'Category ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_CATEGORY_SUB IS 'Surgery category ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_LANGUAGE IS 'Prefered language ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ADDRESS IS 'Address';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.CITY IS 'City';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.DISTRICT IS 'County';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ZIP_CODE IS 'Zip code';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_COUNTRY IS 'Country ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.WORK_PHONE IS 'Work phone number';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.FAX IS 'Fax number';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.WEBSITE IS 'Website address';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.EMAIL IS 'Professional e-mail';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.CELL_PHONE IS 'Cell phone number';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_INSTITUTION IS 'Institution ID';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.FLG_AVAILABLE IS 'External professional status: Y - Yes; N - No';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.DT_BIRTH IS 'Birth Date';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.NUM_CONTACT IS 'Home phone number';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.MARITAL_STATUS IS 'Marital status: D - Divorced; F - Separated; M - Married; S - Single; U - Living with partner; W - Widow(er)';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.INITIALS IS 'Name initials';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.MIDDLE_NAME IS 'Middle name';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.NAME IS 'Full name';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.BEGIN_DATE IS 'Registry creation date';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.END_DATE IS 'Registry outdated date';



   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.ID_PROFESSIONAL IS 'Professional that created/updated the information';



   COMMENT ON TABLE PROFESSIONAL_EXT_HIST  IS 'External professionals history';


   alter table professional_ext_hist add(office_name varchar2(200));

   COMMENT ON COLUMN PROFESSIONAL_EXT_HIST.OFFICE_NAME IS 'Office''s name';

