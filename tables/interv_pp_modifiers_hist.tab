CREATE TABLE INTERV_PP_MODIFIERS_HIST
(
  dt_interv_pp_modifiers_hist TIMESTAMP(6) WITH LOCAL TIME ZONE NOT NULL,
id_interv_presc_plan_hist   NUMBER(24) NOT NULL,
  id_modifier                 NUMBER(24) NOT NULL,
  id_inst_owner               NUMBER(24) NOT NULL,
  id_prof_last_update         NUMBER(24),
  dt_last_update_tstz         TIMESTAMP(6) WITH LOCAL TIME ZONE,
  create_user                 VARCHAR2(24),
  create_time                 TIMESTAMP(6) WITH LOCAL TIME ZONE,
  create_institution          NUMBER(24),
  update_user                 VARCHAR2(24),
  update_time                 TIMESTAMP(6) WITH LOCAL TIME ZONE,
  update_institution          NUMBER(24)
);


COMMENT ON TABLE INTERV_PP_MODIFIERS_HIST IS 'Procedures modifying factors for an execution plan history';

COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.DT_INTERV_PP_MODIFIERS_HIST IS 'Date when the register was made';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.ID_INTERV_PRESC_PLAN_HIST IS 'Procedure execution history ID';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.ID_MODIFIER IS 'Modifying factor ID (terminology server ID)';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.ID_INST_OWNER IS 'Institution owner ID (terminology server ID)';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.ID_PROF_LAST_UPDATE IS 'Last update professional ID';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.DT_LAST_UPDATE_TSTZ IS 'Last update date';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN INTERV_PP_MODIFIERS_HIST.UPDATE_INSTITUTION IS 'Update Institution';
