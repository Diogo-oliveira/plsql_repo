

  CREATE TABLE ANALYSIS_EXT_SYS_DELETE
   (	ID_ANALYSIS_EXT_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_EXTERNAL_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_ANALYSIS NUMBER(24,0) NOT NULL ENABLE,
	VALUE VARCHAR2(30) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE analysis_ext_sys_delete IS 'Correspondência dos identificadores únicos das análises, no Alert e nos sistemas externos' ;

COMMENT ON COLUMN analysis_ext_sys_delete.value IS 'Identificador único da análise, no sistema externo indicado' ;
