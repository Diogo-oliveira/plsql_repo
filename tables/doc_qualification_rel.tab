CREATE TABLE DOC_QUALIFICATION_REL
( ID_DOC_QUALIFICATION_REL NUMBER(24,0) NOT NULL ENABLE,
  ID_DOC_ELEMENT_QUALIF NUMBER(24,0) NOT NULL ENABLE,
  ID_GROUP NUMBER(24,0) NOT NULL ENABLE,
  ID_DOC_QUALIF_REL_PARENT NUMBER(24,0),
  FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
  FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
  ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );
  
COMMENT ON COLUMN DOC_QUALIFICATION_REL.id_doc_qualification_rel IS 'Chave primária' ;
COMMENT ON COLUMN DOC_QUALIFICATION_REL.id_doc_element_qualif IS 'Identificador da associação de elementos com a qualificação' ;
COMMENT ON COLUMN DOC_QUALIFICATION_REL.id_group IS 'Grupo' ;
COMMENT ON COLUMN DOC_QUALIFICATION_REL.id_doc_qualif_rel_parent IS 'Código do elemento pai' ;
COMMENT ON COLUMN DOC_QUALIFICATION_REL.flg_type IS 'Tipo de relação. E - Exclusividade; U - Única; R - Relacional' ;
COMMENT ON COLUMN DOC_QUALIFICATION_REL.flg_available IS 'Disponibilidade do registo' ;
COMMENT ON COLUMN DOC_QUALIFICATION_REL.adw_last_update IS 'Data da última alteração' ;

