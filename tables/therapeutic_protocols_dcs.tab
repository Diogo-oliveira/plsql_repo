create table THERAPEUTIC_PROTOCOLS_DCS
(
  ID_THERAPEUTIC_PROTOCOLS_DCS NUMBER(24) not null,
  ID_THERAPEUTIC_PROTOCOLS     NUMBER(24),
  ID_DEP_CLIN_SERV             NUMBER(24),
  RANK                         NUMBER(6) not null,
  FLG_TYPE                     VARCHAR2(1) not null,
  ID_INSTITUTION               NUMBER(24),
  ID_PROFESSIONAL              NUMBER(24),
  ID_SOFTWARE                  NUMBER(24)
);

comment on table THERAPEUTIC_PROTOCOLS_DCS is 'Protocolos Terapêuticos mais frequentemente escolhidos pelos departamentos e sev. clínicos';

comment on column THERAPEUTIC_PROTOCOLS_DCS.ID_THERAPEUTIC_PROTOCOLS is 'Identificação do Protocolo Terapêutico';
comment on column THERAPEUTIC_PROTOCOLS_DCS.ID_DEP_CLIN_SERV is 'Departamento + serv. clínico que executa o medicamento, ou para o qual o medicamento é mais frequente';
comment on column THERAPEUTIC_PROTOCOLS_DCS.FLG_TYPE is 'Tipo de acção do departamento e serv. clínico: P - pode requisitar, M - mais frequentes';
comment on column THERAPEUTIC_PROTOCOLS_DCS.ID_INSTITUTION is 'Identificação da instituição';
comment on column THERAPEUTIC_PROTOCOLS_DCS.ID_PROFESSIONAL is 'Identificação do profissional';
comment on column THERAPEUTIC_PROTOCOLS_DCS.ID_SOFTWARE is 'Identificação da aplicação';