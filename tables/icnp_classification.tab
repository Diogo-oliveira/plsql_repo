

  CREATE TABLE ICNP_CLASSIFICATION
   (	ID_CLASSIFICATION NUMBER(12,0) NOT NULL ENABLE,
	CODE_CLASSIFICATION VARCHAR2(200) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	CODE_HELP_CLASSIFICATION VARCHAR2(200),
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE icnp_classification IS 'Classifcação: Diagnóstico / Acção' ;

COMMENT ON COLUMN icnp_classification.code_classification IS 'Código para tradução da classificação' ;
COMMENT ON COLUMN icnp_classification.flg_type IS 'P - Phenomenon; A - Action' ;
COMMENT ON COLUMN icnp_classification.code_help_classification IS 'Código para tradução da ajuda de contexto da classificação' ;


