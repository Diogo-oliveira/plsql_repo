

  CREATE TABLE CONTRACEPTIVE
   (	ID_CONTRACEPTIVE NUMBER(12,0) NOT NULL ENABLE,
	CODE_CONTRACEPTIVE VARCHAR2(200) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE contraceptive IS 'Métodos contraceptivos' ;

COMMENT ON COLUMN contraceptive.code_contraceptive IS 'Código para tradução' ;
COMMENT ON COLUMN contraceptive.flg_type IS 'Método contraceptivo: H - hormonal, B - barreira, D - DIU, E - esterilização, N - natural' ;
COMMENT ON COLUMN contraceptive.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
