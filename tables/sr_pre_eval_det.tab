

  CREATE TABLE SR_PRE_EVAL_DET
   (	ID_SR_PRE_EVAL_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_PRE_EVAL NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	VALUE1 VARCHAR2(200),
	VALUE2 VARCHAR2(200),
	ID_PROFESSIONAL NUMBER(24,0),
	DT_REG DATE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	ID_SR_PRE_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE sr_pre_eval_det IS 'Detalhe das visitas pré-operatórias e pré-anestésicas' ;

COMMENT ON COLUMN sr_pre_eval_det.id_sr_pre_eval_det IS 'ID único' ;
COMMENT ON COLUMN sr_pre_eval_det.id_sr_pre_eval IS 'ID do item da avaliação pré-operatória' ;
COMMENT ON COLUMN sr_pre_eval_det.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_pre_eval_det.value1 IS 'Valor do primeiro campo da resposta' ;
COMMENT ON COLUMN sr_pre_eval_det.value2 IS 'Valor do segundo campo da resposta' ;
COMMENT ON COLUMN sr_pre_eval_det.id_professional IS 'ID do profissional' ;
COMMENT ON COLUMN sr_pre_eval_det.dt_reg IS 'Data do registo' ;
COMMENT ON COLUMN sr_pre_eval_det.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN sr_pre_eval_det.id_sr_pre_eval_visit IS 'ID da visita pré-operatória ou pré-anestésica' ;
