

  CREATE TABLE REP_DESTINATION
   (	ID_REP_DESTINATION NUMBER(3,0) NOT NULL ENABLE,
	CODE_REP_DESTINATION VARCHAR2(200) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1),
	DEFAULT_VALUE VARCHAR2(200),
	RANK NUMBER(6,0)
   );


COMMENT ON COLUMN rep_destination.id_rep_destination IS 'Chave primária' ;
COMMENT ON COLUMN rep_destination.code_rep_destination IS 'Código para obtenção da descrição' ;
COMMENT ON COLUMN rep_destination.flg_type IS 'Tipo de valor de preenchimento do valor de destino. N- Keypad numérico (nº telefone), E- Email (texto), P- Path (caminho de disco)' ;
COMMENT ON COLUMN rep_destination.default_value IS 'Valor por defeito' ;
COMMENT ON COLUMN rep_destination.rank IS 'Ordem de visualização' ;


