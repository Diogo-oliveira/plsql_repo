

  CREATE TABLE FLOORS
   (	ID_FLOORS NUMBER(24,0) NOT NULL ENABLE,
	CODE_FLOORS VARCHAR2(200) NOT NULL ENABLE,
	IMAGE_PLANT VARCHAR2(200),
	RANK NUMBER(24,0) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE floors IS 'Parametrização dos andares' ;

COMMENT ON COLUMN floors.id_floors IS 'Chave primária' ;
COMMENT ON COLUMN floors.code_floors IS 'Código para obter os andares por língua' ;
COMMENT ON COLUMN floors.image_plant IS 'Imagem' ;
COMMENT ON COLUMN floors.rank IS 'Ordenação' ;
COMMENT ON COLUMN floors.flg_available IS 'Disponibilidade do registo' ;
COMMENT ON COLUMN floors.adw_last_update IS 'Data da última alteração' ;


