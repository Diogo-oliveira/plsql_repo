--Rui Batista 2008/01/10
--Tabela de registo dos parâmetros utilizados pelo interface de criação de episódios de bloco operatório
create table sr_interface_param 
( id_sr_interface_param number(24),
 dt_reg            timestamp with local time zone,
 id_lang           number(6),
 id_prof           number(24),
 id_institution    number(24),
 id_software       number(24),
 id_patient        number(24),
 id_episode_ext    VARCHAR2(50),
 id_episode        number(24),
 id_visit          number(24));

COMMENT ON COLUMN sr_interface_param.id_sr_interface_param IS 'ID único';
COMMENT ON COLUMN sr_interface_param.dt_reg IS 'Data de registo';
COMMENT ON COLUMN sr_interface_param.id_lang IS 'ID do idioma passado por parâmetro';
COMMENT ON COLUMN sr_interface_param.id_prof IS 'ID do profissional passado por parâmetro';
COMMENT ON COLUMN sr_interface_param.id_institution IS 'ID da instituição passado por parâmetro';
COMMENT ON COLUMN sr_interface_param.id_software IS 'ID do software passado por parâmetro';
COMMENT ON COLUMN sr_interface_param.id_patient IS 'ID do paciente passado por parâmetro';
COMMENT ON COLUMN sr_interface_param.id_episode_ext IS 'ID do episódio externo (SONHO) passado por parâmetro';
COMMENT ON COLUMN sr_interface_param.id_episode IS 'ID do episódio criado pelo Alert';
COMMENT ON COLUMN sr_interface_param.id_visit IS 'ID da visita criada pelo Alert';

