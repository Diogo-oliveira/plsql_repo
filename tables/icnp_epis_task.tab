
CREATE TABLE ICNP_EPIS_TASK
(ID_ICNP_EPIS_TASK NUMBER(24,0) NOT NULL ENABLE,
 ID_ICNP_EPIS_INTERV NUMBER(24,0) NOT NULL ENABLE,
 ID_TASK NUMBER(24,0) NOT NULL ENABLE
);


COMMENT ON TABLE ICNP_EPIS_TASK IS 'Tabela onde estão os registos de outra funcionalidade associados a uma intervenção' ;

COMMENT ON COLUMN ICNP_EPIS_TASK.ID_ICNP_EPIS_TASK IS 'Chave primária' ;
COMMENT ON COLUMN ICNP_EPIS_TASK.ID_ICNP_EPIS_INTERV IS 'ID da intervenção a que está associado o registo' ;
COMMENT ON COLUMN ICNP_EPIS_TASK.ID_TASK IS 'ID do registo de outra funcionalidade' ;


ALTER TABLE ICNP_EPIS_TASK
ADD (ID_ICNP_INTERV_PLAN NUMBER(24));

COMMENT ON COLUMN ICNP_EPIS_TASK.ID_ICNP_INTERV_PLAN IS 'ID do plano da intervenção a que pode estar associado o registo';


