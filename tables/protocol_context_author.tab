
CREATE TABLE PROTOCOL_CONTEXT_AUTHOR
 (ID_PROTOCOL_CONTEXT_AUTHOR NUMBER(24) NOT NULL
 ,ID_PROTOCOL NUMBER(24) NOT NULL
 ,FIRST_NAME VARCHAR2(200)
 ,LAST_NAME VARCHAR2(200)
 ,TITLE VARCHAR2(200)
 );
 
COMMENT ON COLUMN PROTOCOL_CONTEXT_AUTHOR.ID_PROTOCOL_CONTEXT_AUTHOR IS 'ID of protocol context author';
COMMENT ON COLUMN PROTOCOL_CONTEXT_AUTHOR.ID_PROTOCOL IS 'ID of protocol';
COMMENT ON COLUMN PROTOCOL_CONTEXT_AUTHOR.FIRST_NAME IS 'Author first name';
COMMENT ON COLUMN PROTOCOL_CONTEXT_AUTHOR.LAST_NAME IS 'Author last name';
COMMENT ON COLUMN PROTOCOL_CONTEXT_AUTHOR.TITLE IS 'Author title';

