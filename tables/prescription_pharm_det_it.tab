
  CREATE TABLE PRESCRIPTION_PHARM_DET_IT
   (	ID_PRESCRIPTION_PHARM_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_PRESCRIPTION_PHARM NUMBER(24,0) NOT NULL ENABLE,
	ID_INGREDIENT NUMBER(24,0) NOT NULL ENABLE,
	QTY NUMBER(2,0) NOT NULL ENABLE
   );


COMMENT ON TABLE prescription_pharm_det_it IS 'Constituição das prescrições de manipulados.' ;

COMMENT ON COLUMN prescription_pharm_det_it.id_ingredient IS 'ID do constituinte.' ;
COMMENT ON COLUMN prescription_pharm_det_it.qty IS 'Quantidade prescrita.' ;
COMMENT ON COLUMN prescription_pharm_det_it.id_prescription_pharm_det IS 'Chave primária.' ;
COMMENT ON COLUMN prescription_pharm_det_it.id_prescription_pharm IS 'ID da prescrição.' ;

