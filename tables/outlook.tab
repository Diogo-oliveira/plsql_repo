

  CREATE TABLE OUTLOOK
   (	ID_OUTLOOK NUMBER,
	DT_BEGIN DATE NOT NULL ENABLE,
	DT_END DATE NOT NULL ENABLE,
	SUBJECT VARCHAR2(1000),
	LOCALIZATION VARCHAR2(1000),
	PROF_NAME VARCHAR2(1000),
	PAT_NAME VARCHAR2(1000),
	APPOINTMENT_TYPE VARCHAR2(50),
	APPOINTMENT_SPECIALITY VARCHAR2(50),
	FLG_UPDT VARCHAR2(50),
	DT_UPDT DATE

   );

