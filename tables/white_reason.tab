

  CREATE TABLE WHITE_REASON
   (	ID_WHITE_REASON NUMBER(24,0) NOT NULL ENABLE,
	CODE_WHITE_REASON VARCHAR2(200) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE white_reason IS 'Razões da passagem no SU, para doentes não triados ("brancos")' ;

COMMENT ON COLUMN white_reason.code_white_reason IS 'Código para tradução das razões da passagem no SU, para doentes não triados (brancos)' ;
COMMENT ON COLUMN white_reason.rank IS 'Ordem de visualização' ;
