

  CREATE TABLE DIETARY_DRUG
   (	ID_DIETARY_DRUG NUMBER(24,0) NOT NULL ENABLE,
	CODE_DIETARY_DRUG VARCHAR2(200) NOT NULL ENABLE,
	QTY NUMBER(4,0),
	MEASURE_UNIT VARCHAR2(100),
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE dietary_drug IS 'Dietéticos' ;

COMMENT ON COLUMN dietary_drug.id_dietary_drug IS 'Chave primária.' ;
COMMENT ON COLUMN dietary_drug.code_dietary_drug IS 'Nome do dietético.' ;
COMMENT ON COLUMN dietary_drug.qty IS 'Dosagem.' ;
COMMENT ON COLUMN dietary_drug.measure_unit IS 'Unidade de medida da dosagem.' ;
COMMENT ON COLUMN dietary_drug.flg_type IS 'Dietético para a farmácia exterior (E) ou para a farmácia da instituição (I).' ;
COMMENT ON COLUMN dietary_drug.adw_last_update IS 'Data da última alteração.' ;
