-- Creating Table 'INTERV_CONDITION_ICF'
CREATE TABLE INTERV_CONDITION_ICF
 (ID_INTERV_CONDITION NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 ,ID_PROFESSIONAL NUMBER(24) NOT NULL
 ,ID_ICF NUMBER(24) NOT NULL
 );
COMMENT ON TABLE INTERV_CONDITION_ICF
  is 'Parametrization of intervention conditions';
COMMENT ON COLUMN INTERV_CONDITION_ICF.ID_INTERV_CONDITION IS 'ID INTERV CONDITION';
COMMENT ON COLUMN INTERV_CONDITION_ICF.ID_INSTITUTION IS 'ID institution';
COMMENT ON COLUMN INTERV_CONDITION_ICF.ID_SOFTWARE IS 'ID software';
COMMENT ON COLUMN INTERV_CONDITION_ICF.ID_PROFESSIONAL IS 'id_professional';
COMMENT ON COLUMN INTERV_CONDITION_ICF.ID_ICF IS 'ID ICF';

