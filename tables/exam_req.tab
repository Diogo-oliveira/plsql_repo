

  CREATE TABLE EXAM_REQ
   (	ID_EXAM_REQ NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	ID_PROF_REQ NUMBER(24,0) NOT NULL ENABLE,
	DT_REQ DATE NOT NULL ENABLE,
	NOTES VARCHAR2(4000),
	ID_INSTITUTION NUMBER(12,0),
	ID_SCHEDULE NUMBER(24,0),
	ID_SCHED_CONSULT NUMBER(24,0),
	ID_PROF_CANCEL NUMBER(24,0),
	DT_CANCEL DATE,
	NOTES_CANCEL VARCHAR2(4000),

	ID_PROF_APPROVE NUMBER(24,0),
	ID_PROF_AUTHORIZE NUMBER(24,0),
	FLG_TIME VARCHAR2(1) NOT NULL ENABLE,
	DT_BEGIN DATE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	ID_EPISODE_ORIGIN NUMBER(24,0),
	ID_EPISODE_DESTINATION NUMBER(24,0),
	DT_PEND_REQ DATE,
	ID_PROF_RESCHEDULE NUMBER(24,0),
	DT_RESCHEDULE DATE,
	DT_BEGIN_OLD DATE,
	ID_PROTOCOLS NUMBER(24,0),
	ID_PREV_EPISODE NUMBER(24,0),

	NOTES_TECH VARCHAR2(240)
   );


COMMENT ON TABLE exam_req IS 'Cabeçalhos de requisição de exames. Permite associar os agendamentos dos exames com a consulta (ou outro evento) para a qual são necessários.Um cabeçalho de requisição por serv. clínico destino.' ;

COMMENT ON COLUMN exam_req.id_prof_approve IS 'Profissional que aprova o pedido de exame ao exterior (Dir. Serviço)' ;
COMMENT ON COLUMN exam_req.id_prof_authorize IS 'Profissional que autoriza o pedido de exame ao exterior (Dir. Clínico)' ;
COMMENT ON COLUMN exam_req.flg_time IS 'Realização: E - neste episódio; N - próximo episódio; B - entre episódios' ;
COMMENT ON COLUMN exam_req.dt_begin IS 'Data pretendida para início da execução do exame (ie, ñ imediata)' ;
COMMENT ON COLUMN exam_req.flg_status IS 'F - c/ resultado; R - requisitado; C - cancelado; E - em execução; D - pendente; P - parcial' ;
COMMENT ON COLUMN exam_req.id_episode_origin IS 'ID do episódio em que foi registada a req. inicialmente (se FLG_TIME = ''N'')' ;
COMMENT ON COLUMN exam_req.id_episode_destination IS 'ID do episódio em que foi registada a req. destino (se FLG_TIME = ''N'')' ;
COMMENT ON COLUMN exam_req.dt_pend_req IS 'Data em que passa de pendente a requisitado.' ;
COMMENT ON COLUMN exam_req.id_prof_reschedule IS 'Profissional responsável pelo reagendamento' ;
COMMENT ON COLUMN exam_req.dt_reschedule IS 'Data de reagendamento' ;
COMMENT ON COLUMN exam_req.dt_begin_old IS 'Data de realização de exame, antes do reagendamento' ;
COMMENT ON COLUMN exam_req.id_protocols IS 'ID do protocolo, caso tenha sido requesitado através de um protocolo.' ;
COMMENT ON COLUMN exam_req.id_prev_episode IS 'ID do episódio em que foi registada a req. inicialmente' ;

COMMENT ON COLUMN exam_req.id_episode IS 'Chave primaria' ;
COMMENT ON COLUMN exam_req.dt_req IS 'Data / hora da requisição' ;

