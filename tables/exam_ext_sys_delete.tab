

  CREATE TABLE EXAM_EXT_SYS_DELETE
   (	ID_EXAM_EXT_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_EXAM NUMBER(12,0) NOT NULL ENABLE,
	ID_EXTERNAL_SYS NUMBER(12,0) NOT NULL ENABLE,
	VALUE VARCHAR2(30) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE exam_ext_sys_delete IS 'Correspondência dos identificadores únicos dos exames, no Alert e nos sistemas externos' ;

COMMENT ON COLUMN exam_ext_sys_delete.value IS 'Identificador único do exame, no sistema externo indicado' ;
