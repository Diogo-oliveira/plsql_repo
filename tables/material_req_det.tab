

  CREATE TABLE MATERIAL_REQ_DET
   (	ID_MATERIAL_REQ_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_MATERIAL_REQ NUMBER(24,0) NOT NULL ENABLE,
	ID_MATERIAL NUMBER(24,0) NOT NULL ENABLE,
	NUM_REQUEST NUMBER(3,0) NOT NULL ENABLE,
	NOTES VARCHAR2(4000),
	ID_ROOM NUMBER(24,0),
	NUM_SUPPLIED NUMBER(3,0),
	FLG_STATE VARCHAR2(1) NOT NULL ENABLE,
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(4000)

   );


COMMENT ON TABLE material_req_det IS 'Material clínico requisitado no cabe' ;

COMMENT ON COLUMN material_req_det.id_material_req_det IS 'ID único do detalhe da requisição' ;
COMMENT ON COLUMN material_req_det.id_material_req IS 'Id da requisição de materiais' ;
COMMENT ON COLUMN material_req_det.id_material IS 'ID do material requisitado' ;
COMMENT ON COLUMN material_req_det.num_request IS 'Número necessário do material requisitado' ;
COMMENT ON COLUMN material_req_det.notes IS 'Notas' ;
COMMENT ON COLUMN material_req_det.id_room IS 'ID da sala para onde o material deverá ser transportado' ;
COMMENT ON COLUMN material_req_det.num_supplied IS 'Número fornecido' ;
COMMENT ON COLUMN material_req_det.flg_state IS 'Estado do detalhe da requisição. Valores possíveis: R - Requisitado, T - Em transporte, P - Parcial, F - Terminado, C - Cancelado' ;
COMMENT ON COLUMN material_req_det.dt_cancel IS 'Data de cancelamento' ;
COMMENT ON COLUMN material_req_det.id_prof_cancel IS 'Profissional que cancelou o detalhe' ;
COMMENT ON COLUMN material_req_det.notes_cancel IS 'Notas de cancelamento' ;
