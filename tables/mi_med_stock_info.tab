 CREATE TABLE ALERT.MI_MED_STOCK_INFO
   (ID_MI_MED_STOCK_INFO NUMBER(24,0),
    ID_DRUG VARCHAR2(255),
      VERSION VARCHAR2(255),
      LOT_NUMBER VARCHAR2(200 CHAR),
      BAR_CODE VARCHAR2(30 CHAR),
      DT_EXPIRE DATE,
      QTY_STOCK NUMBER(24,0),
      ID_UNIT_DISP NUMBER(24,0),
      AVERAGE_PRICE NUMBER(24,4),
      ID_CURRENCY NUMBER(6,0),
      ID_INSTITUTION NUMBER(24,0),
      DT_LAST_UPDATE TIMESTAMP (6) WITH LOCAL TIME ZONE,
      FLG_AVAILABLE VARCHAR2(1),
      CREATE_USER VARCHAR2(24),
      CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
      CREATE_INSTITUTION NUMBER(24,0),
      UPDATE_USER VARCHAR2(24),
      UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
      UPDATE_INSTITUTION NUMBER(24,0)
   ) ;

   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.ID_MI_MED_STOCK_INFO IS 'Mi_med_stock_info identification';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.ID_DRUG IS 'Drug identification';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.VERSION IS 'Version';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.LOT_NUMBER IS 'Lot number';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.BAR_CODE IS 'Bar code';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.DT_EXPIRE IS 'Expiration date';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.QTY_STOCK IS 'Stock quantity';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.ID_UNIT_DISP IS 'Dispense unit identification';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.AVERAGE_PRICE IS 'Average price';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.ID_CURRENCY IS 'Currency identification';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.ID_INSTITUTION IS 'Institution identification';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.DT_LAST_UPDATE IS 'Last update date';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.FLG_AVAILABLE IS 'Registry availability: (Y)es | (N)o';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.CREATE_USER IS 'Creation User';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.CREATE_TIME IS 'Creation Time';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.CREATE_INSTITUTION IS 'Creation Institution';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.UPDATE_USER IS 'Update User';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.UPDATE_TIME IS 'Update Time';
   COMMENT ON COLUMN ALERT.MI_MED_STOCK_INFO.UPDATE_INSTITUTION IS 'Update Institution';
   COMMENT ON TABLE ALERT.MI_MED_STOCK_INFO  IS 'Table of drug packages';
