

  CREATE TABLE RELIGION
   (	ID_RELIGION NUMBER(12,0) NOT NULL ENABLE,
	CODE_RELIGION VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(12,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE religion IS 'Religiões' ;

COMMENT ON COLUMN religion.code_religion IS 'Código para tradução' ;
COMMENT ON COLUMN religion.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
