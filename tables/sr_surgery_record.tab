

  CREATE TABLE SR_SURGERY_RECORD
   (	ID_SURGERY_RECORD NUMBER(24,0) NOT NULL ENABLE,
	ID_SCHEDULE_SR NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_INTERVENTION NUMBER(24,0),
	ID_PROF_TEAM NUMBER(24,0),
	ID_PATIENT NUMBER(24,0) NOT NULL ENABLE,
	FLG_PAT_STATUS VARCHAR2(1),
	FLG_STATE VARCHAR2(1) NOT NULL ENABLE,
	FLG_SURG_NAT VARCHAR2(1),
	FLG_SURG_TYPE VARCHAR2(1),
	FLG_URGENCY VARCHAR2(1),
	ID_ANESTHESIA_TYPE NUMBER(24,0),

	ID_CLINICAL_SERVICE NUMBER(12,0),
	NOTES VARCHAR2(2000),
	DT_ANEST_START DATE,
	DT_ANEST_END DATE,
	DT_SR_ENTRY DATE,
	DT_SR_EXIT DATE,
	DT_ROOM_ENTRY DATE,
	DT_ROOM_EXIT DATE,
	DT_RCV_ENTRY DATE,
	DT_RCV_EXIT DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	DT_CANCEL DATE,
	NOTES_CANCEL VARCHAR2(2000),

	ID_INSTITUTION NUMBER(12,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN sr_surgery_record.id_surgery_record IS 'ID único' ;
COMMENT ON COLUMN sr_surgery_record.id_schedule_sr IS 'ID do agendamento' ;
COMMENT ON COLUMN sr_surgery_record.id_prof_team IS 'ID da equipa de profissionais' ;
COMMENT ON COLUMN sr_surgery_record.id_patient IS 'ID do paciente' ;
COMMENT ON COLUMN sr_surgery_record.flg_pat_status IS 'Estado do paciente. Valores possíveis: A-Ausente, W- Em espera, L- Pedido de transporte para o bloco, T- Em transporte para o bloco, V- Acolhido no bloco, P- Em preparação, R- Preparado para a cirurgia, S- Em cirurgia, F- Terminou a cirurgia, Y- No recobro, D- Alta do Recobro, O- Em transporte para outro local no hospital ou noutra instituição' ;
COMMENT ON COLUMN sr_surgery_record.flg_state IS 'Estado da cirurgia. Estados possíveis: T- Por Agendar A- Agendado V- Avaliação pré-operatória, P- Preparação cirúrgica, S- Em Cirurgia, O- Avaliação Pós-operatória - R- Recobro, F- Terminado, C - Cancelado' ;
COMMENT ON COLUMN sr_surgery_record.flg_surg_nat IS 'Natureza da cirurgia. Valores possíveis: C- Convencional, A- Ambulatória' ;
COMMENT ON COLUMN sr_surgery_record.flg_surg_type IS 'Tipo de cirurgia. Valores possíveis: I- Limpa, L- Limpa-contaminada, C- Contaminada, P- Conspurcada' ;
COMMENT ON COLUMN sr_surgery_record.flg_urgency IS 'Indica tipo de urgência: U- Urgente, P- Programada' ;
COMMENT ON COLUMN sr_surgery_record.id_anesthesia_type IS 'ID do tipo de anestesia' ;
COMMENT ON COLUMN sr_surgery_record.id_clinical_service IS 'ID do serviço clínico' ;
COMMENT ON COLUMN sr_surgery_record.notes IS 'Notas da cirurgia' ;
COMMENT ON COLUMN sr_surgery_record.dt_anest_start IS 'Data / hora de início da anestesia' ;

COMMENT ON COLUMN sr_surgery_record.dt_anest_end IS 'Data / hora do fim da anestesia' ;
COMMENT ON COLUMN sr_surgery_record.dt_sr_entry IS 'Data / hora de entrada do paciente no Bloco Operatório' ;
COMMENT ON COLUMN sr_surgery_record.dt_sr_exit IS 'Data / hora de saída do paciente do Bloco Operatório' ;
COMMENT ON COLUMN sr_surgery_record.dt_room_entry IS 'Data / hora de entrada do paciente na sala operatória' ;
COMMENT ON COLUMN sr_surgery_record.dt_room_exit IS 'Data / hora de saída do paciente da sala operatória' ;
COMMENT ON COLUMN sr_surgery_record.dt_rcv_entry IS 'Data / hora de entrada do paciente na sala de recobro' ;
COMMENT ON COLUMN sr_surgery_record.dt_rcv_exit IS 'Data / hora de saída do paciente da sala de recobro' ;
COMMENT ON COLUMN sr_surgery_record.id_prof_cancel IS 'ID do profissional que efectuou o cancelamento' ;
COMMENT ON COLUMN sr_surgery_record.dt_cancel IS 'Data de cancelamento' ;
COMMENT ON COLUMN sr_surgery_record.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN sr_surgery_record.id_institution IS 'ID da instituição' ;
COMMENT ON COLUMN sr_surgery_record.adw_last_update IS 'Data da última alteração' ;


