

  CREATE TABLE PROF_GROUPS_HIST
   (	ID_GROUP_HIST NUMBER(24,0),
	ID_GROUP NUMBER(24,0),
	ID_PROFESSIONAL NUMBER(24,0),
	FLG_STATE VARCHAR2(1),
	NOTES VARCHAR2(200),
	CREATE_USER VARCHAR2(24),
	CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	CREATE_INSTITUTION NUMBER(24,0),
	UPDATE_USER VARCHAR2(24),
	UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
	UPDATE_INSTITUTION NUMBER(24,0)
   ) ;


   COMMENT ON COLUMN PROF_GROUPS_HIST.ID_GROUP_HIST IS 'History table identification';



   COMMENT ON COLUMN PROF_GROUPS_HIST.ID_GROUP IS 'Group identification';



   COMMENT ON COLUMN PROF_GROUPS_HIST.ID_PROFESSIONAL IS 'Professional identification';



   COMMENT ON COLUMN PROF_GROUPS_HIST.FLG_STATE IS 'Professional state A- Active / I - Inactive';



   COMMENT ON COLUMN PROF_GROUPS_HIST.NOTES IS 'Notes';



   COMMENT ON COLUMN PROF_GROUPS_HIST.CREATE_USER IS 'Creation User';



   COMMENT ON COLUMN PROF_GROUPS_HIST.CREATE_TIME IS 'Creation Time';



   COMMENT ON COLUMN PROF_GROUPS_HIST.CREATE_INSTITUTION IS 'Creation Institution';



   COMMENT ON COLUMN PROF_GROUPS_HIST.UPDATE_USER IS 'Update User';



   COMMENT ON COLUMN PROF_GROUPS_HIST.UPDATE_TIME IS 'Update Time';



   COMMENT ON COLUMN PROF_GROUPS_HIST.UPDATE_INSTITUTION IS 'Update Institution';



   COMMENT ON TABLE PROF_GROUPS_HIST  IS 'History Professionals in groups';

   alter table PROF_GROUPS_HIST modify NOTES VARCHAR2(4000);

