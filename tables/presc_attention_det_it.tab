
  CREATE TABLE PRESC_ATTENTION_DET_IT
   (	ID_PRESC_ATTENTION_DET NUMBER(24,0) NOT NULL ENABLE,
	DT_ATTENTION_READ DATE NOT NULL ENABLE,
	ID_PRESCRIPTION_PHARM NUMBER(24,0) NOT NULL ENABLE,
	FLG_ATTENTION VARCHAR2(2) NOT NULL ENABLE,
	ID_PROF_READ NUMBER(24,0) NOT NULL ENABLE,
	ID_INSTITUTION_READ NUMBER(12,0) NOT NULL ENABLE,
	ID_SOFTWARE_READ NUMBER(24,0) NOT NULL ENABLE,
	NOTES_READ VARCHAR2(4000)
   );


COMMENT ON TABLE presc_attention_det_it IS 'Detalhe da leitura das chamadas de atenção.' ;

COMMENT ON COLUMN presc_attention_det_it.id_presc_attention_det IS 'Chave primária' ;
COMMENT ON COLUMN presc_attention_det_it.dt_attention_read IS 'Data de leitura da chamada de atenção' ;
COMMENT ON COLUMN presc_attention_det_it.id_prescription_pharm IS 'ID da prescrição do fármaco em que foi gerada a chamada de atenção.' ;
COMMENT ON COLUMN presc_attention_det_it.flg_attention IS 'Flag que identifica a chamada de atenção: CI - contra-indicações; B - medicamentos mais baratos; J - justificação de prescrição; IM - interacções medicamentosas; IA - interacções entre medicamentos e alergias' ;
COMMENT ON COLUMN presc_attention_det_it.id_prof_read IS 'ID do profissional que leu a chamada de atenção.' ;
COMMENT ON COLUMN presc_attention_det_it.id_institution_read IS 'ID da instituição onde a chamada de atenção foi lida.' ;
COMMENT ON COLUMN presc_attention_det_it.id_software_read IS 'ID de software onde a chamada de atenção foi lida.' ;
COMMENT ON COLUMN presc_attention_det_it.notes_read IS 'Notas de leitura da chamada de atenção (não está a ser utilizado)' ;

--João Eiras, 03-09-2007
--Timezones changes
ALTER TABLE PRESC_ATTENTION_DET_IT ADD DT_ATTENTION_READ_TSTZ TIMESTAMP WITH LOCAL TIME ZONE; 
COMMENT ON COLUMN PRESC_ATTENTION_DET_IT.DT_ATTENTION_READ_TSTZ IS 'Data de leitura da chamada de aten‡Æo'; 
