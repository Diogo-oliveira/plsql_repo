

  CREATE TABLE SYS_SCREEN_TEMPLATE
   (	ID_SYS_SCREEN_TEMPLATE NUMBER(24,0) NOT NULL ENABLE,
	XML_TEMPLATE LONG NOT NULL ENABLE,
	INTERNAL_NAME VARCHAR2(200) NOT NULL ENABLE
   );


COMMENT ON TABLE sys_screen_template IS 'Contains all available screen templates defined in XML. Patient Id screen templates are an example.' ;

COMMENT ON COLUMN sys_screen_template.id_sys_screen_template IS 'Primary key' ;
COMMENT ON COLUMN sys_screen_template.xml_template IS 'Internal name' ;
COMMENT ON COLUMN sys_screen_template.internal_name IS 'XML with the screen template' ;
