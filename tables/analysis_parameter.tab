

  CREATE TABLE ANALYSIS_PARAMETER
   (	ID_ANALYSIS_PARAMETER NUMBER(24,0) NOT NULL ENABLE,
	CODE_ANALYSIS_PARAMETER VARCHAR2(200) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE analysis_parameter IS 'Tabela de definição de parâmetros de análises' ;

COMMENT ON COLUMN analysis_parameter.code_analysis_parameter IS 'Código para tradução gerado pelo trigger da tabela' ;

