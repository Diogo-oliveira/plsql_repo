

  CREATE TABLE ANALYSIS_RES_PAR_CALC(  
	ID_ANALYSIS_RES_CALC NUMBER(24,0),
  	ID_ANALYSIS_PARAMETER NUMBER(24,0),
  	ORDER_NUM NUMBER(6,0),
  	CREATE_USER VARCHAR2(24 CHAR),
  	CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
  	CREATE_INSTITUTION NUMBER(24,0),
  	UPDATE_USER VARCHAR2(24 CHAR),
  	UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
  	UPDATE_INSTITUTION NUMBER(24,0));

   COMMENT ON TABLE ANALYSIS_RES_PAR_CALC  IS 'Table where one can find the parameters necessary for a calculated result';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.ID_ANALYSIS_RES_CALC IS 'Id analysis result calculator';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.ID_ANALYSIS_PARAMETER IS 'Id analysis parameter from where comes the result';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.ORDER_NUM IS 'Order that the parameter is in the formula';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.CREATE_USER IS 'Creation User';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.CREATE_TIME IS 'Creation Time';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.CREATE_INSTITUTION IS 'Creation Institution';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.UPDATE_USER IS 'Update User';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.UPDATE_TIME IS 'Update Time';
   COMMENT ON COLUMN ANALYSIS_RES_PAR_CALC.UPDATE_INSTITUTION IS 'Update Institution';



