CREATE TABLE ANALYSIS_PARAM_FUNCIONALITY
 (ID_ANALYSIS_PARAM_FUNCIONALITY  NUMBER(24) NOT NULL
 ,ID_PARAMETER_ANALYSIS  NUMBER(12) NOT NULL
 ,FLG_TYPE  VARCHAR2(1) NOT NULL
 ,ID_SOFTWARE  NUMBER(24) NOT NULL
 ,ID_INSTITUTION  NUMBER(24) NOT NULL
 ,ADW_LAST_UPDATE  DATE NOT NULL
 );

COMMENT ON TABLE ANALYSIS_PARAM_FUNCIONALITY IS 'Tabela de definição de parâmetros de análises';

COMMENT ON COLUMN ANALYSIS_PARAM_FUNCIONALITY.ID_PARAMETER_ANALYSIS IS 'Id analysis parameter';

COMMENT ON COLUMN ANALYSIS_PARAM_FUNCIONALITY.FLG_TYPE IS 'Tipo de funcionalidade para a qual o parâmetro é parametrizado. S - Análises Seriadas; M - Saúde Materna';

COMMENT ON COLUMN ANALYSIS_PARAM_FUNCIONALITY.ID_SOFTWARE IS 'Aplicação Alert para a qual se verifica o registo';

COMMENT ON COLUMN ANALYSIS_PARAM_FUNCIONALITY.ID_INSTITUTION IS 'Instituição para a qual o parâmetro está disponível';



ALTER TABLE ANALYSIS_PARAM_FUNCIONALITY
ADD (ID_ANALYSIS NUMBER(12));

COMMENT ON COLUMN ANALYSIS_PARAM_FUNCIONALITY.ID_ANALYSIS IS 'ID da análise associada ao parâmetro';

