CREATE TABLE GROUP_ACCESS_PROF
 (ID_GROUP_ACCESS_PROF NUMBER(24) NOT NULL
 ,ID_GROUP_ACCESS NUMBER(24) NOT NULL
 ,ID_PROFESSIONAL NUMBER(24) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1 CHAR) DEFAULT 'Y' NOT NULL
 ,CREATE_USER VARCHAR2(24 CHAR)
 ,CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION NUMBER(24)
 ,UPDATE_USER VARCHAR2(24 CHAR)
 ,UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
 );
 
COMMENT ON TABLE GROUP_ACCESS_PROF IS 'Relation between group access and profissional';
COMMENT ON COLUMN GROUP_ACCESS_PROF.ID_GROUP_ACCESS_PROF IS 'Table ID';
COMMENT ON COLUMN GROUP_ACCESS_PROF.ID_GROUP_ACCESS IS 'Group access ID';
COMMENT ON COLUMN GROUP_ACCESS_PROF.ID_PROFESSIONAL IS 'Professional ID';
COMMENT ON COLUMN GROUP_ACCESS_PROF.FLG_AVAILABLE IS 'Indicates if the group access is available for the professional';
COMMENT ON COLUMN GROUP_ACCESS_PROF.CREATE_USER IS 'Create user';
COMMENT ON COLUMN GROUP_ACCESS_PROF.CREATE_TIME IS 'Create time';
COMMENT ON COLUMN GROUP_ACCESS_PROF.CREATE_INSTITUTION IS 'Create institution';
COMMENT ON COLUMN GROUP_ACCESS_PROF.UPDATE_USER IS 'Update user';
COMMENT ON COLUMN GROUP_ACCESS_PROF.UPDATE_TIME IS 'Update time';
COMMENT ON COLUMN GROUP_ACCESS_PROF.UPDATE_INSTITUTION IS 'Update institution';
