
  CREATE TABLE INF_CFT_LNK_IT
   (	MED_ID NUMBER(10,0) NOT NULL ENABLE,
	CODE VARCHAR2(14) NOT NULL ENABLE
   );


COMMENT ON TABLE inf_cft_lnk_it IS 'Classificação CFT de cada medicamento.' ;

COMMENT ON COLUMN inf_cft_lnk_it.med_id IS 'Código identificador do medicamento.' ;
COMMENT ON COLUMN inf_cft_lnk_it.code IS 'Código identificador da Classificação Farmacoterapêutica (CFT) do medicamento.' ;

