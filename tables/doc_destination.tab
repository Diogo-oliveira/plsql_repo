

  CREATE TABLE DOC_DESTINATION
   (	ID_DOC_DESTINATION NUMBER(6,0) NOT NULL ENABLE,
	CODE_DOC_DESTINATION VARCHAR2(200),
	FLG_AVAILABLE VARCHAR2(1),
	RANK NUMBER(6,0),
	ADW_LAST_UPDATE DATE
   );

-- cmf 14-01-2016
COMMENT ON TABLE alert.doc_destination 							IS 'Possible Destination/location of documents';
COMMENT ON COLUMN ALERT.DOC_DESTINATION.ID_DOC_DESTINATION 		IS 'Primary Key';
COMMENT ON COLUMN ALERT.DOC_DESTINATION.CODE_DOC_DESTINATION 	IS 'Code for translation purpose';
COMMENT ON COLUMN ALERT.DOC_DESTINATION.FLG_AVAILABLE 			IS 'Availability of code (Y/N)';
COMMENT ON COLUMN ALERT.DOC_DESTINATION.RANK 					IS 'Value for sorting purpose';
COMMENT ON COLUMN ALERT.DOC_DESTINATION.ADW_LAST_UPDATE 		IS 'DEPRECATED';
