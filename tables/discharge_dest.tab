

  CREATE TABLE DISCHARGE_DEST
   (	ID_DISCHARGE_DEST NUMBER(12,0) NOT NULL ENABLE,
	CODE_DISCHARGE_DEST VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(3)
   );


COMMENT ON TABLE discharge_dest IS 'Destinos exteriores à instituição' ;

COMMENT ON COLUMN discharge_dest.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN discharge_dest.flg_type IS 'Tipo: D - médica, A - administrativa, S - social, ou combinações destes valores' ;



ALTER TABLE DISCHARGE_DEST ADD (ID_CONTENT VARCHAR2(200));
COMMENT ON COLUMN DISCHARGE_DEST.ID_CONTENT  IS 'Identificador único de conteúdo';


