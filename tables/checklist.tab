/** Tables */
CREATE TABLE CHECKLIST
(
    FLG_CONTENT_CREATOR VARCHAR2(1 CHAR) NOT NULL,
    INTERNAL_NAME VARCHAR2(1000 CHAR) NOT NULL,
    ID_CHECKLIST NUMBER(24) NOT NULL,
    FLG_AVAILABLE VARCHAR2(1 CHAR) NOT NULL,
    FLG_STATUS VARCHAR2(1 CHAR) NOT NULL,
    ID_CONTENT VARCHAR2(200 CHAR),
    DT_CANCEL_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
    ID_PROF_CANCEL NUMBER(24),
    ID_CANCEL_REASON NUMBER(24),
    CANCEL_NOTES VARCHAR2(1000 CHAR),
CREATE_USER VARCHAR2(24 CHAR),
    CREATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
CREATE_INSTITUTION NUMBER(24),
UPDATE_USER VARCHAR2(24 CHAR),
UPDATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
UPDATE_INSTITUTION NUMBER(24)
);


COMMENT ON TABLE CHECKLIST IS 'Checklist';

COMMENT ON COLUMN CHECKLIST.FLG_CONTENT_CREATOR IS 'Content creator: Checklist created by (A)LERT, Checklist created by (I)nstitution';
COMMENT ON COLUMN CHECKLIST.INTERNAL_NAME IS 'Checklist''s internal name (original name)';
COMMENT ON COLUMN CHECKLIST.ID_CHECKLIST IS 'Alternate key';
COMMENT ON COLUMN CHECKLIST.FLG_AVAILABLE IS 'Checklist is available for use: (Y)es, (N)o';
COMMENT ON COLUMN CHECKLIST.FLG_STATUS IS 'Checklist state: (A)ctive, (C) ancelled';
COMMENT ON COLUMN CHECKLIST.ID_CONTENT IS 'Content ID';
COMMENT ON COLUMN CHECKLIST.DT_CANCEL_TIME IS 'Cancel time';
COMMENT ON COLUMN CHECKLIST.ID_PROF_CANCEL IS 'Profissional who did the cancellation of checklist from BackOffice';
COMMENT ON COLUMN CHECKLIST.ID_CANCEL_REASON IS 'Cancel reason ID';
COMMENT ON COLUMN CHECKLIST.CANCEL_NOTES IS 'Cancelation notes';
COMMENT ON COLUMN CHECKLIST.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN CHECKLIST.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN CHECKLIST.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN CHECKLIST.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN CHECKLIST.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN CHECKLIST.UPDATE_INSTITUTION IS 'Update Institution';
-- CHANGE END: Ariel Machado