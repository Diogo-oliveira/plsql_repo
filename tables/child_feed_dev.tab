--TABLE CHILD_FEED_DEV

  CREATE TABLE CHILD_FEED_DEV
   (	ID_CHILD_FEED_DEV NUMBER(12,0) NOT NULL ENABLE,
	CODE_CHILD_FEED_DEV VARCHAR2(200) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	FLG_AGE NUMBER(2,0),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE child_feed_dev IS 'Alimentos durante o 1º ano de vida OU parâmetros de análise do desenvolvimento psicomotor' ;

COMMENT ON COLUMN child_feed_dev.code_child_feed_dev IS 'Código para tradução' ;
COMMENT ON COLUMN child_feed_dev.flg_type IS 'A - alimento, P - parâmetro de análise do desenvolvimento' ;
COMMENT ON COLUMN child_feed_dev.flg_age IS 'Indicador da idade aproximada em que são analisados os parâmetros de desenvolvimento psicomotor: A-  antes do 1º ano de vida, B - 18 meses, C - 24 meses, D - 4/5 anos' ;
COMMENT ON COLUMN child_feed_dev.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;


