

  CREATE TABLE DRUG_REQ_DET
   (	ID_DRUG_REQ_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_DRUG_REQ NUMBER(24,0) NOT NULL ENABLE,
	ID_DRUG NUMBER(12,0) NOT NULL ENABLE,
	QTY_REQ NUMBER(6,0) NOT NULL ENABLE,
	QTY_SUPPLIED NUMBER(6,0),
	NOTES VARCHAR2(4000),
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(4000),
	ID_DEP_CLIN_SERV NUMBER(24,0),

	DOSAGE VARCHAR2(1000),
	NOTES_JUSTIF VARCHAR2(4000),
	FLG_CI VARCHAR2(1),
	FLG_CHEAPER VARCHAR2(1),
	FLG_JUSTIF VARCHAR2(1),
	FLG_ATTENTION VARCHAR2(2),
	FLG_ATTENTION_PRINT VARCHAR2(1),
	ID_DRUG_DESPACHOS NUMBER(24,0),
	ID_PROF_PENDING NUMBER(24,0),
	DT_PENDING DATE,
	NOTES_PENDING VARCHAR2(4000),
	PHARMACIST_VALIDATION VARCHAR2(1)
   );



COMMENT ON TABLE drug_req_det IS 'Medicamentos requisitados' ;

COMMENT ON COLUMN drug_req_det.qty_supplied IS 'Quantidade fornecida' ;
COMMENT ON COLUMN drug_req_det.flg_status IS 'T - temporária; D - pendente; R - requisitado; E - em execução; P - fornecimento parcial; F - terminado; C - cancelado; J - pedido rejeitado' ;
COMMENT ON COLUMN drug_req_det.id_dep_clin_serv IS 'Departamento e serv. clínico requisitado.No caso de estar parametrizado em DRUG_DEP_CLIN_SERV que o medicamento pode ser fornecido em mais do que um departamento e / ou serv. clínico, é necessário indicar na requisição qual o pretendido.' ;
COMMENT ON COLUMN drug_req_det.dosage IS 'Posologia.' ;
COMMENT ON COLUMN drug_req_det.notes_justif IS 'Justificação da prescrição.' ;
COMMENT ON COLUMN drug_req_det.flg_ci IS 'Chamada de atenção de contra-indicações: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN drug_req_det.flg_cheaper IS 'Chamada de atenção de medicamentos mais baratos: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN drug_req_det.flg_justif IS 'Chamada de atenção de justificação de prescrição: Y - sim; N - não; R - lida.' ;
COMMENT ON COLUMN drug_req_det.flg_attention IS 'Flag que indica a chamada de atenção prioritária: CI - contra-indicações; B - medicamentos mais baratos; J - justificação de prescrição; IM - interacções medicamentosas' ;
COMMENT ON COLUMN drug_req_det.flg_attention_print IS 'Flag que indica se o profissional prosseguiu (Y) ou não (N) com a impressão apesar das chamadas de atenção.' ;
COMMENT ON COLUMN drug_req_det.id_drug_despachos IS 'Despacho associado a esta requisição.' ;
COMMENT ON COLUMN drug_req_det.id_prof_pending IS 'ID do profissional que passou a requisição para pendente.' ;
COMMENT ON COLUMN drug_req_det.dt_pending IS 'Data em que a requisição passou para pendente.' ;

COMMENT ON COLUMN drug_req_det.notes_pending IS 'Notas de passagem para pendente.' ;
COMMENT ON COLUMN drug_req_det.pharmacist_validation IS 'Indicação se a receita foi validado pelo farmacêutico.' ;
COMMENT ON COLUMN drug_req_det.id_drug IS 'Medicamento requisitado' ;
COMMENT ON COLUMN drug_req_det.qty_req IS 'Quantidade requisitada' ;


