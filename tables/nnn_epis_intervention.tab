BEGIN
  pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'NNN_EPIS_INTERVENTION', i_obj_type => 'TABLE', i_flg_category => 'TRS', i_flg_alert_default => 'N', i_delete_script => '', i_flg_default_child => 'N', i_flg_nzd => 'Y', i_responsible_team => 'NURSING');
END;
/
CREATE TABLE NNN_EPIS_INTERVENTION
  (
    ID_NNN_EPIS_INTERVENTION NUMBER (24) NOT NULL ,
    ID_NIC_INTERVENTION      NUMBER (24) NOT NULL ,
    ID_PATIENT               NUMBER (24) NOT NULL ,
    ID_EPISODE               NUMBER (24) NOT NULL ,
    ID_VISIT                 NUMBER (24) NOT NULL ,
    ID_PROFESSIONAL          NUMBER (24) NOT NULL ,
    ID_CANCEL_REASON         NUMBER (24) ,
    CANCEL_NOTES             VARCHAR2 (1000 CHAR) ,
    NIC_CODE                 NUMBER (24) NOT NULL ,
    FLG_REQ_STATUS           VARCHAR2 (1 CHAR) NOT NULL ,
    DT_VAL_TIME_START        TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    DT_VAL_TIME_END          TIMESTAMP WITH LOCAL TIME ZONE ,
    DT_TRS_TIME_START        TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    DT_TRS_TIME_END          TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_USER              VARCHAR2 (30 CHAR) ,
    CREATE_TIME              TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION       NUMBER (24) ,
    UPDATE_USER              VARCHAR2 (30 CHAR) ,
    UPDATE_TIME              TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION       NUMBER (24)
  )
  TABLESPACE ALERT_DATA ;

COMMENT ON TABLE NNN_EPIS_INTERVENTION
IS
  'NIC Interventions related to a NANDA Diagnosis within a patient''s nursing care plan' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_NNN_EPIS_INTERVENTION
IS
  'Episode''s NIC Intervention ID (surrogate key)' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_NIC_INTERVENTION
IS
  'NIC Intervention ID' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_PATIENT
IS
  'Patient ID' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_EPISODE
IS
  'Episode ID' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_VISIT
IS
  'Visit ID' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_PROFESSIONAL
IS
  'Profissional ID who did the creation/edition of this entry' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.ID_CANCEL_REASON
IS
  'Cancel reason ID' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.CANCEL_NOTES
IS
  'Cancelation notes' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.NIC_CODE
IS
  'NIC Intervention code in the standardized classification of nursing interventions.' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.FLG_REQ_STATUS
IS
  'Request status:
C Cancelled
D Draft
E Expired
F Finished
I Ignored
O Ongoing
P Suspended
R Requested
S Suggested
T Discontinued' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.DT_VAL_TIME_START
IS
  'Valid time start' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.DT_VAL_TIME_END
IS
  'Valid time end' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.DT_TRS_TIME_START
IS
  'Transaction time start' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.DT_TRS_TIME_END
IS
  'Transaction time end' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NNN_EPIS_INTERVENTION.UPDATE_INSTITUTION
IS
  'Update Institution' ;
