

  CREATE TABLE DISTRICT
   (	ID_DISTRICT NUMBER(12,0) NOT NULL ENABLE,
	CODE_DISTRICT VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE district IS 'Distritos. Para diferentes países há uma lista de distritos diferente.' ;

COMMENT ON COLUMN district.id_district IS 'Chave primária' ;
COMMENT ON COLUMN district.code_district IS 'Chave para tradução. DISTRICT.CODE_DISTRICT.<id_district>. Preenchido por trigger.' ;
COMMENT ON COLUMN district.flg_available IS 'Y - Disponível. N-Não disponível.' ;
COMMENT ON COLUMN district.adw_last_update IS 'Data da última alteração. Preenchido pelo trigger.' ;

