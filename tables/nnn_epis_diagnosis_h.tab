BEGIN
  pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'NNN_EPIS_DIAGNOSIS_H', i_obj_type => 'TABLE', i_flg_category => 'HST', i_flg_alert_default => 'N', i_delete_script => '', i_flg_default_child => 'N', i_flg_nzd => 'Y', i_responsible_team => 'NURSING');
END;
/
CREATE TABLE NNN_EPIS_DIAGNOSIS_H
  (
    ID_NNN_EPIS_DIAGNOSIS_H NUMBER (24) NOT NULL ,
    ID_NNN_EPIS_DIAGNOSIS   NUMBER (24) NOT NULL ,
    ID_NAN_DIAGNOSIS        NUMBER (24) NOT NULL ,
    ID_PATIENT              NUMBER (24) NOT NULL ,
    ID_EPISODE              NUMBER (24) NOT NULL ,
    ID_VISIT                NUMBER (24) NOT NULL ,
    ID_PROFESSIONAL         NUMBER (24) NOT NULL ,
    ID_CANCEL_REASON        NUMBER (24) ,
    CANCEL_NOTES            VARCHAR2 (1000 CHAR) ,
    NANDA_CODE              NUMBER (24) NOT NULL ,
    EDITED_DIAGNOSIS_NAME   VARCHAR2 (1000 CHAR) ,
    DT_DIAGNOSIS            TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    FLG_REQ_STATUS          VARCHAR2 (1 CHAR) NOT NULL ,
    DT_VAL_TIME_START       TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    DT_VAL_TIME_END         TIMESTAMP WITH LOCAL TIME ZONE ,
    DT_TRS_TIME_START       TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    DT_TRS_TIME_END         TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    CREATE_USER             VARCHAR2 (30 CHAR) ,
    CREATE_TIME             TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION      NUMBER (24) ,
    UPDATE_USER             VARCHAR2 (30 CHAR) ,
    UPDATE_TIME             TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION      NUMBER (24)
  )
  TABLESPACE ALERT_DATA ;
COMMENT ON TABLE NNN_EPIS_DIAGNOSIS_H
IS
  'History tracking for NANDA Nursing Diagnoses defined within a patient''s nursing care plan' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_NNN_EPIS_DIAGNOSIS_H
IS
  'Surrogate key' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_NNN_EPIS_DIAGNOSIS
IS
  'Episode''s NANDA Diagnosis ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_NAN_DIAGNOSIS
IS
  'NANDA Diagnosis ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_PATIENT
IS
  'Patient ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_EPISODE
IS
  'Episode ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_VISIT
IS
  'Visit ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_PROFESSIONAL
IS
  'Profissional ID who did the creation/edition of this entry' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.ID_CANCEL_REASON
IS
  'Cancel reason ID' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.CANCEL_NOTES
IS
  'Cancelation notes' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.NANDA_CODE
IS
  'NANDA Diagnosis code ' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.EDITED_DIAGNOSIS_NAME
IS
  'Edited diagnosis label that is required to duplicate an already existent in the care plan' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.DT_DIAGNOSIS
IS
  'Diagnosis date' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.FLG_REQ_STATUS
IS
  'Request status:
C Cancelled
D Draft
E Expired
F Finished
I Ignored
O Ongoing
P Suspended
R Requested
S Suggested
T Discontinued' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.DT_VAL_TIME_START
IS
  'Valid time start' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.DT_VAL_TIME_END
IS
  'Valid time end' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.DT_TRS_TIME_START
IS
  'Transaction time start' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.DT_TRS_TIME_END
IS
  'Transaction time end' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NNN_EPIS_DIAGNOSIS_H.UPDATE_INSTITUTION
IS
  'Update Institution' ;
