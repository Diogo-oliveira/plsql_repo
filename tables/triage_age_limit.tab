BEGIN
EXECUTE IMMEDIATE
'CREATE TABLE TRIAGE_AGE_LIMIT ( ' || --
'  ID_TRIAGE_AGE_LIMIT NUMBER(24) NOT NULL, ' || --
'ID_TRIAGE_BOARD_GROUP NUMBER(24), ' || --
'ID_TRIAGE_BOARD NUMBER(24), ' || --
'ID_TRIAGE_DISCRIMINATOR NUMBER(24), ' || --
'AGE_MIN NUMBER(6), ' || --
'AGE_MAX NUMBER(6), ' || --
'ID_INSTITUTION NUMBER(24) NOT NULL, ' || --
'FLG_AVAILABLE VARCHAR2(1 CHAR) NOT NULL, ' || --
'CREATE_USER VARCHAR2(24), ' || --
'  CREATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE, ' || --
'  CREATE_INSTITUTION NUMBER(24), ' || --
'  UPDATE_USER VARCHAR2(24), ' || --
'  UPDATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE, ' || --
'  UPDATE_INSTITUTION NUMBER(24) ' || --
') TABLESPACE TABLE_S';
END;
/

COMMENT ON TABLE TRIAGE_AGE_LIMIT IS 'Defines, by institution, the age limits applicable to triage board groups, triage flowcharts (boards) or triage discriminators.';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.ID_TRIAGE_AGE_LIMIT IS 'Age limit identifier';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.ID_TRIAGE_BOARD_GROUP IS 'Triage board group ID';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.ID_TRIAGE_BOARD IS 'Triage flowchart (board) ID';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.ID_TRIAGE_DISCRIMINATOR IS 'Triage discriminator ID';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.AGE_MIN IS 'Minimum age';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.AGE_MAX IS 'Maximum age';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.ID_INSTITUTION IS 'Institution ID';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.FLG_AVAILABLE IS 'Availability (Y) Yes (N) No';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.CREATE_USER IS 'Creation user';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.CREATE_TIME IS 'Creation date / time';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.CREATE_INSTITUTION IS 'Creation institution';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.UPDATE_USER IS 'Update user';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.UPDATE_TIME IS 'Update date / time';
COMMENT ON COLUMN TRIAGE_AGE_LIMIT.UPDATE_INSTITUTION IS 'Update institution';
-- CHANGE END: José Brito