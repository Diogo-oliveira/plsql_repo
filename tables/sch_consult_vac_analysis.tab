CREATE TABLE sch_consult_vac_analysis (
       id_sch_consult_vac_analysis NUMBER(24),
			 id_sch_consult_vacancy      NUMBER(24),
			 id_analysis                     NUMBER(24)
);


COMMENT ON TABLE sch_consult_vac_analysis IS 'Holds the analysis-specific data about vacancies.';
COMMENT ON COLUMN sch_consult_vac_analysis.id_sch_consult_vac_analysis IS 'Primary key';
COMMENT ON COLUMN sch_consult_vac_analysis.id_sch_consult_vacancy IS 'Vacancy identifier at the top table';
COMMENT ON COLUMN sch_consult_vac_analysis.id_analysis IS 'Analysis identifier';

COMMENT ON COLUMN SCH_CONSULT_VAC_ANALYSIS.ID_SCH_CONSULT_VAC_ANALYSIS IS 'Analysis vacancy identifier';

ALTER TABLE SCH_CONSULT_VAC_ANALYSIS MODIFY ID_ANALYSIS NUMBER(12);
