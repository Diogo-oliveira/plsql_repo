-- CHANGE REASON: [ALERT-290969] Nursing Care Plan: NANDA, NIC, NOC - Data Model
BEGIN
  pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'NAN_RELATED_FACTOR', i_obj_type => 'TABLE', i_flg_category => 'CNT', i_flg_alert_default => 'N', i_delete_script => '', i_flg_default_child => 'N', i_flg_nzd => 'N', i_responsible_team => 'NURSING');
END;
/
CREATE TABLE NAN_RELATED_FACTOR
  (
    ID_NAN_RELATED_FACTOR  NUMBER (24) NOT NULL ,
    ID_TERMINOLOGY_VERSION NUMBER (24) NOT NULL ,
    REL_FACTOR_CODE        VARCHAR2 (200 CHAR) NOT NULL ,
    CODE_DESCRIPTION       VARCHAR2 (200 CHAR) AS ( 'NAN_RELATED_FACTOR.CODE_DESCRIPTION.'
    ||TO_CHAR(ID_NAN_RELATED_FACTOR) ) VIRTUAL NOT NULL ,
    ID_INST_OWNER      NUMBER (24) ,
    ID_CONCEPT_VERSION NUMBER (24) ,
    ID_CONCEPT_TERM    NUMBER (24) ,
    ID_NAN_DIAGNOSIS   NUMBER (24) NOT NULL ,
    CREATE_USER        VARCHAR2 (30 CHAR) ,
    CREATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION NUMBER (24) ,
    UPDATE_USER        VARCHAR2 (30 CHAR) ,
    UPDATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION NUMBER (24)
  )
  TABLESPACE ALERT_DATA ;
COMMENT ON TABLE NAN_RELATED_FACTOR
IS
  'NANDA - Related factors for nursing diagnoses' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.ID_NAN_RELATED_FACTOR
IS
  'Related Factor ID (surrogate key)' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.ID_TERMINOLOGY_VERSION
IS
  'NANDA Version ID' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.REL_FACTOR_CODE
IS
  ' Related Factor code in the standardized classification of nursing diagnoses or an internal code used to uniquely identify the concept' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.CODE_DESCRIPTION
IS
  'Translation code for Related Factor description' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.ID_INST_OWNER
IS
  'Institution owner of the concept. 0 - ALERT' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.ID_CONCEPT_VERSION
IS
  'Concept version ID' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.ID_CONCEPT_TERM
IS
  'Concept term ID' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.ID_NAN_DIAGNOSIS
IS
  'Diagnosis ID (surrogate key)' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NAN_RELATED_FACTOR.UPDATE_INSTITUTION
IS
  'Update Institution' ;
  
