

  CREATE TABLE INF_SUBST_LNK
   (	MED_ID NUMBER(10,0) NOT NULL ENABLE,
	SUBST_ID NUMBER(10,0) NOT NULL ENABLE,
	QUANTIDADE VARCHAR2(80) NOT NULL ENABLE
   );


COMMENT ON TABLE inf_subst_lnk IS 'Substâncias activas nos medicamentos.' ;

COMMENT ON COLUMN inf_subst_lnk.med_id IS 'Código identificador do medicamento.' ;
COMMENT ON COLUMN inf_subst_lnk.subst_id IS 'Código identificador da substância activa do medicamento.' ;
COMMENT ON COLUMN inf_subst_lnk.quantidade IS 'Quantidade de substância activa no medicamento.' ;


