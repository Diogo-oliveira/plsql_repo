
  CREATE TABLE USA_EMB_DEP_CLIN_SERV
   (	ID_EMB_DEP_CLIN_SERV NUMBER(24,0) NOT NULL ENABLE,
	MEDID NUMBER(10,0),
	QTY NUMBER(2,0),
	DOSAGE VARCHAR2(100),
	GENERICO VARCHAR2(1),
	ID_DEP_CLIN_SERV NUMBER(24,0),
	ID_PROFESSIONAL NUMBER(24,0),
	RANK NUMBER(2,0) NOT NULL ENABLE,
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE,
	ID_DIETARY_DRUG NUMBER(24,0),
	ID_MANIPULATED NUMBER(24,0)
   );


COMMENT ON TABLE usa_emb_dep_clin_serv IS 'Medicamentos mais frequentes (para aviar na farmácia do exterior)' ;

COMMENT ON COLUMN usa_emb_dep_clin_serv.id_emb_dep_clin_serv IS 'Chave primária.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.medid IS 'ID do medicamento.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.qty IS 'Quantidades mais frequentes dos medicamentos mais frequentes.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.dosage IS 'Posologias mais frequentes dos medicamentos mais frequentes.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.generico IS 'Indicação se autoriza genérico (Y) ou não (N).' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.id_dep_clin_serv IS 'ID do departamento e serviço clínico para o qual a embalagem é mais frequente.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.rank IS 'Rank.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.id_software IS 'ID do software.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.id_dietary_drug IS 'ID do dietético.' ;
COMMENT ON COLUMN usa_emb_dep_clin_serv.id_manipulated IS 'ID do manipulado.' ;

