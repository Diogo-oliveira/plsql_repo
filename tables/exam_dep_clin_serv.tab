

  CREATE TABLE EXAM_DEP_CLIN_SERV
   (	ID_EXAM_DEP_CLIN_SERV NUMBER(24,0) NOT NULL ENABLE,
	ID_EXAM NUMBER(12,0),
	ID_DEP_CLIN_SERV NUMBER(24,0),
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	ID_EXAM_GROUP NUMBER(24,0),
	ID_INSTITUTION NUMBER(24,0),
	ID_PROFESSIONAL NUMBER(24,0),
	ID_SOFTWARE NUMBER(24,0) NOT NULL ENABLE,
	FLG_FIRST_RESULT VARCHAR2(6),

	FLG_MOV_PAT VARCHAR2(1),
	COST NUMBER(18,4),
	PRICE NUMBER(18,4)
   );


COMMENT ON TABLE exam_dep_clin_serv IS 'Exames mais frequentemente pedidos por um serv. clínico de um departamento' ;

COMMENT ON COLUMN exam_dep_clin_serv.id_dep_clin_serv IS 'Departamento + serv. clínico que executa o exame, ou para o qual o exame é mais frequente' ;
COMMENT ON COLUMN exam_dep_clin_serv.flg_type IS 'Tipo de acção do departamento e serv. clínico: R - realização do exame; P - pode pedir o exame, M - mais frequentes' ;
COMMENT ON COLUMN exam_dep_clin_serv.id_institution IS 'Instituição para a qual o exame é requisitável' ;
COMMENT ON COLUMN exam_dep_clin_serv.id_professional IS 'Profissional para o qual o exame é mais frequente' ;
COMMENT ON COLUMN exam_dep_clin_serv.id_software IS 'Aplicação Alert para a qual se verifica o registo' ;
COMMENT ON COLUMN exam_dep_clin_serv.flg_first_result IS 'Indicação de qual categoria de profissional deve registar resultados em 1º lugar: D - médico, T - técnico, N - enfermeiro, ou combinações (ex: DT - médico ou técnico). Esta flag é utilizada para impedir o registo de resultados pelo médico antes do técnico, por ex., para que não desapareça da grelha do técnico.' ;
COMMENT ON COLUMN exam_dep_clin_serv.flg_mov_pat IS 'Necessidade de movimentação do doente (só é preenchido se FLG_TYPE=''P'')' ;
COMMENT ON COLUMN exam_dep_clin_serv.cost IS 'Custo monetário (só é preenchido se FLG_TYPE=''P'')' ;
COMMENT ON COLUMN exam_dep_clin_serv.price IS 'Preço de de venda (só é preenchido se FLG_TYPE=''P'')' ;


ALTER TABLE EXAM_DEP_CLIN_SERV
ADD ID_EXTERNAL_SYS NUMBER;

COMMENT ON COLUMN EXAM_DEP_CLIN_SERV.ID_EXTERNAL_SYS
  IS 'Esta associação serve para associar exames a sistemas externos onde podemos receber resultados';  

