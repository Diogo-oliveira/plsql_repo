

  CREATE TABLE SR_SURG_PROT_TASK_DET
   (	ID_SR_SURG_PROT_TASK_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_SR_SURG_PROT_TASK NUMBER(24,0) NOT NULL ENABLE,
	ID_SCHEDULE NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	FLG_ACTIVE VARCHAR2(1) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_ACTIVE DATE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	ID_PROF_STATUS NUMBER(24,0) NOT NULL ENABLE,
	DT_STATUS DATE
   );



COMMENT ON COLUMN sr_surg_prot_task_det.id_sr_surg_prot_task_det IS 'ID único' ;
COMMENT ON COLUMN sr_surg_prot_task_det.id_sr_surg_prot_task IS 'Id da relação protocolo/tarefa' ;
COMMENT ON COLUMN sr_surg_prot_task_det.id_schedule IS 'ID do agendamento' ;
COMMENT ON COLUMN sr_surg_prot_task_det.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_surg_prot_task_det.flg_active IS 'Estado da tarefa (A- Activa, I- Inactiva, C- cancelada)' ;
COMMENT ON COLUMN sr_surg_prot_task_det.id_professional IS 'ID do profissional que Activou/Desactivou' ;
COMMENT ON COLUMN sr_surg_prot_task_det.dt_active IS 'Data da Activação/Inactivação' ;
COMMENT ON COLUMN sr_surg_prot_task_det.flg_status IS 'Estado da tarefa (D- Feito, W - Em espera)' ;
COMMENT ON COLUMN sr_surg_prot_task_det.id_prof_status IS 'ID do profissional que alterou o estado' ;
COMMENT ON COLUMN sr_surg_prot_task_det.dt_status IS 'Data do estado da tarefa' ;
