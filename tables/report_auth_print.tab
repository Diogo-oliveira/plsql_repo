--***********************************************************************

CREATE TABLE REPORT_AUTH_PRINT
(
  ID_REPORT_AUTH_PRINT     NUMBER(24)  NOT NULL,
  ID_REPORTS               NUMBER(24)  NOT NULL,
  ID_REP_PROFILE_TEMPLATE  NUMBER(12)  NOT NULL,
  ID_INSTITUTION           NUMBER(24)  NOT NULL,
  ID_SOFTWARE              NUMBER(24)  NOT NULL,
	PRINTER_NAME             VARCHAR2(30),
	FLG_AUTH                 VARCHAR2(1) NOT NULL
) 

TABLESPACE TABLE_S;

COMMENT ON TABLE REPORT_AUTH_PRINT IS 'Tabela que relaciona relatórios com os perfis dos profissionais REP_PROFILE_TEMPLATE. Permite atribuir o nome de uma impressora para cada perfil/relatório, assim como a autenticação antes de imprimir.';

COMMENT ON COLUMN REPORT_AUTH_PRINT.ID_REPORT_AUTH_PRINT IS 'Chave primária.';
COMMENT ON COLUMN REPORT_AUTH_PRINT.ID_REPORTS IS 'ID do relatório.';
COMMENT ON COLUMN REPORT_AUTH_PRINT.ID_REP_PROFILE_TEMPLATE IS 'ID do perfil do profissional.';
COMMENT ON COLUMN REPORT_AUTH_PRINT.ID_INSTITUTION IS 'ID da instituição.';
COMMENT ON COLUMN REPORT_AUTH_PRINT.ID_SOFTWARE IS 'ID do software.';
COMMENT ON COLUMN REPORT_AUTH_PRINT.PRINTER_NAME IS 'Contém o nome da impressora para a qual o report deve ser enviado. Se estiver a NULL significa que deve ser enviado para a impressora default.';
COMMENT ON COLUMN REPORT_AUTH_PRINT.FLG_AUTH IS 'Flag que indica se a autenticação é necessária para imprimir o relatório (‘Y’ ou ‘N’).';


