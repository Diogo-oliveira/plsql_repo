create table epis_supplies_det
(id_epis_supplies_det  number(24,0) not null enable,
id_epis_supplies  number(24,0) not null enable,
id_supplies number(24,0) not null enable,
qty  number(24), 
notes varchar2(2000), 		 
adw_last_update date not null enable );


COMMENT ON COLUMN epis_supplies_det.id_epis_supplies_det IS 'Chave primária' ;
COMMENT ON COLUMN epis_supplies_det.id_epis_supplies IS 'Chave primária';
COMMENT ON COLUMN epis_supplies_det.id_supplies IS 'Material' ;
COMMENT ON COLUMN epis_supplies_det.qty IS 'Quantidade' ;
COMMENT ON COLUMN epis_supplies_det.notes IS 'Notas' ;
COMMENT ON COLUMN epis_supplies_det.adw_last_update IS 'Data da última alteração' ;