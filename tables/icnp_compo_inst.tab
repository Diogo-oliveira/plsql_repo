

  CREATE TABLE ICNP_COMPO_INST
   (	ID_COMPO_INST NUMBER(24,0) NOT NULL ENABLE,
	ID_COMPOSITION NUMBER(12,0) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(12,0) NOT NULL ENABLE
   );


COMMENT ON TABLE icnp_compo_inst IS 'associates compositions to institutions' ;

COMMENT ON COLUMN icnp_compo_inst.id_compo_inst IS 'primary key' ;
COMMENT ON COLUMN icnp_compo_inst.id_composition IS 'a composition' ;
COMMENT ON COLUMN icnp_compo_inst.id_institution IS 'institution to which belongs the composition' ;


