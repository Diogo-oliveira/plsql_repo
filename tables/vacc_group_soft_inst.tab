-- Create table vacc_group_soft_inst
create table VACC_GROUP_SOFT_INST
(
  ID_VACC_GROUP_SOFT_INST NUMBER(12) not null,
  ID_VACC_GROUP           NUMBER(12) not null,
  ID_INSTITUTION          NUMBER(12) not null,
  ID_SOFTWARE             NUMBER(12) not null,
  ADW_LAST_UPDATE         DATE
)

-- Add comments to the table 
comment on table VACC_GROUP_SOFT_INST
  is 'Tabela de atribuição de grupos a instituições / softwares';
-- Add comments to the columns 
comment on column VACC_GROUP_SOFT_INST.ID_VACC_GROUP_SOFT_INST
  is 'Chave primária.';
comment on column VACC_GROUP_SOFT_INST.ID_VACC_GROUP
  is 'Atribuição de grupos a instituições';
comment on column VACC_GROUP_SOFT_INST.ID_INSTITUTION
  is 'Instituição';
comment on column VACC_GROUP_SOFT_INST.ID_SOFTWARE
  is 'Software';

