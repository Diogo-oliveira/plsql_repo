
  CREATE TABLE INF_ATC_LNK_IT
   (	MED_ID NUMBER(10,0) NOT NULL ENABLE,
	CODE VARCHAR2(10) NOT NULL ENABLE
   );


COMMENT ON TABLE inf_atc_lnk_it IS 'Classificação ATC de cada medicamento.' ;

COMMENT ON COLUMN inf_atc_lnk_it.med_id IS 'Código identificador do medicamento.' ;
COMMENT ON COLUMN inf_atc_lnk_it.code IS 'Código identificador da classificação Anatomical Therapeutical Chemical (ATC) do medicamento.' ;

