

  CREATE TABLE INTERV_PROTOCOLS
   (	ID_INTERV_PROTOCOLS NUMBER(24,0) NOT NULL ENABLE,
	ID_PROTOCOLS NUMBER(24,0) NOT NULL ENABLE,
	ID_INTERVENTION NUMBER(24,0),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_INTERV_TYPE VARCHAR2(1) NOT NULL ENABLE,
	NUM_TAKE NUMBER(3,0),
	INTERVAL NUMBER(7,0),
	DOSAGE NUMBER(19,3)
   );



COMMENT ON TABLE interv_protocols IS 'Protocolos parametrizados para as intervenções' ;

COMMENT ON COLUMN interv_protocols.id_protocols IS 'ID do protocolo' ;
COMMENT ON COLUMN interv_protocols.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN interv_protocols.flg_interv_type IS 'Tipo de intervenção:  N - normal, S - SOS, U - unitário, A - ad eternum, C - contínuo' ;
COMMENT ON COLUMN interv_protocols.num_take IS 'Número de tomas' ;
COMMENT ON COLUMN interv_protocols.interval IS 'Intervalo entre tomas' ;
COMMENT ON COLUMN interv_protocols.dosage IS 'Dosagem' ;


