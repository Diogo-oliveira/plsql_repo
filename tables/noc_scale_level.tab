BEGIN
  pk_frmw_objects.insert_into_frmw_objects(i_owner => 'ALERT', i_obj_name => 'NOC_SCALE_LEVEL', i_obj_type => 'TABLE', i_flg_category => 'CNT', i_flg_alert_default => 'N', i_delete_script => '', i_flg_default_child => 'N', i_flg_nzd => 'N', i_responsible_team => 'NURSING');
END;
/
CREATE TABLE NOC_SCALE_LEVEL
  (
    ID_NOC_SCALE_LEVEL     NUMBER (24) NOT NULL ,
    ID_TERMINOLOGY_VERSION NUMBER (24) NOT NULL ,
    SCALE_CODE             VARCHAR2 (200 CHAR) NOT NULL ,
    SCALE_LEVEL_VALUE      NUMBER (24) NOT NULL ,
    CODE_NOC_SCALE_LEVEL   VARCHAR2 (200 CHAR) AS ( 'NOC_SCALE_LEVEL.CODE_NOC_SCALE_LEVEL.'
    ||TO_CHAR(ID_NOC_SCALE_LEVEL) ) VIRTUAL NOT NULL ,
    RANK               NUMBER (6) NOT NULL ,
    ID_INST_OWNER      NUMBER (24) ,
    ID_CONCEPT_VERSION NUMBER (24) ,
    ID_CONCEPT_TERM    NUMBER (24) ,
    CREATE_USER        VARCHAR2 (30 CHAR) ,
    CREATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION NUMBER (24) ,
    UPDATE_USER        VARCHAR2 (30 CHAR) ,
    UPDATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION NUMBER (24)
  )
  TABLESPACE ALERT_DATA ;
COMMENT ON TABLE NOC_SCALE_LEVEL
IS
  'Five-point Likert scale(s) values' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.ID_NOC_SCALE_LEVEL
IS
  'NOC Scale level ID (surrogate key)' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.ID_TERMINOLOGY_VERSION
IS
  'NOC Version ID' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.SCALE_CODE
IS
  'Likert scale code' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.SCALE_LEVEL_VALUE
IS
  'Point Likert scale (1 to 5)' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.CODE_NOC_SCALE_LEVEL
IS
  'Translation code for scale level label' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.RANK
IS
  'Rank order' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.ID_INST_OWNER
IS
  'Institution owner of the concept. 0 - ALERT' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.ID_CONCEPT_VERSION
IS
  'Concept version ID' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.ID_CONCEPT_TERM
IS
  'Concept term ID' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NOC_SCALE_LEVEL.UPDATE_INSTITUTION
IS
  'Update Institution' ;
