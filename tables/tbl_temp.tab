--timestamps e mais colunas
DROP TABLE tbl_temp;

CREATE GLOBAL TEMPORARY TABLE tbl_temp (
  num_1  NUMBER,
  num_2  NUMBER,
  num_3  NUMBER,
  num_4  NUMBER,
  num_5  NUMBER,
  num_6  NUMBER,
  vc_1 VARCHAR2(4000),
  vc_2 VARCHAR2(4000),
  vc_3 VARCHAR2(4000),
  vc_4 VARCHAR2(4000),
  vc_5 VARCHAR2(4000),
  vc_6 VARCHAR2(4000),
  dt_1 DATE,
  dt_2 DATE,
  dt_3 DATE,
  dt_4 DATE,
  tstz_1 TIMESTAMP WITH LOCAL TIME ZONE NULL,
  tstz_2 TIMESTAMP WITH LOCAL TIME ZONE NULL,
  tstz_3 TIMESTAMP WITH LOCAL TIME ZONE NULL,
  tstz_4 TIMESTAMP WITH LOCAL TIME ZONE NULL
) ON COMMIT PRESERVE ROWS;

COMMENT ON TABLE tbl_temp IS 'Tabela auxiliar para utilizar guardar o resultado de  queries mais pesadas. Para inserir arrays com tamanhos diferentes usem a função insert_tbl_temp';

