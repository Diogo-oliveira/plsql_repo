/** Tables */
CREATE TABLE CHECKLIST_CLIN_SERV(
FLG_CONTENT_CREATOR VARCHAR2(1 CHAR) NOT NULL,
INTERNAL_NAME VARCHAR2(1000 CHAR) NOT NULL,
VERSION NUMBER(6) NOT NULL,
ID_CHECKLIST_VERSION NUMBER(24) NOT NULL,
    ID_CLINICAL_SERVICE NUMBER(24) NOT NULL,
CREATE_USER VARCHAR2(24 CHAR),
    CREATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
CREATE_INSTITUTION NUMBER(24),
UPDATE_USER VARCHAR2(24 CHAR),
UPDATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE,
UPDATE_INSTITUTION NUMBER(24)
);

COMMENT ON TABLE CHECKLIST_CLIN_SERV IS 'Specialties where checklist is applicable';

COMMENT ON COLUMN CHECKLIST_CLIN_SERV.FLG_CONTENT_CREATOR IS 'Content creator: Checklist created by (A)LERT, Checklist created by (I)nstitution';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.INTERNAL_NAME IS 'Checklist''s internal name (original name)';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.VERSION IS 'An integer to represent the version number of the checklist template';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.ID_CHECKLIST_VERSION IS 'Referred checklist (specific version)';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.ID_CLINICAL_SERVICE IS 'Clinical service ID';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN CHECKLIST_CLIN_SERV.UPDATE_INSTITUTION IS 'Update Institution';
-- CHANGE END: Ariel Machado