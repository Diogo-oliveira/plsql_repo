

  CREATE TABLE EPIS_INSTITUTION
   (	ID_EPIS_INSTITUTION NUMBER(24,0) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE epis_institution IS 'Histórico de instituições onde decorre um episódio.
A instituição actual do episódio é a que consta em VISIT.' ;



