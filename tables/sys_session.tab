

  CREATE TABLE SYS_SESSION
   (	ID_SYS_SESSION NUMBER(24,0) NOT NULL ENABLE,
	SESSION_SA VARCHAR2(200) NOT NULL ENABLE,
	IP VARCHAR2(200) NOT NULL ENABLE,
	DT_IN DATE NOT NULL ENABLE,
	DT_OUT DATE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE sys_session IS 'Sessões dos utilizadores.' ;

COMMENT ON COLUMN sys_session.session_sa IS 'Id de sessão no servidor' ;
COMMENT ON COLUMN sys_session.ip IS 'IP da máq onde foi iniciada a sessão' ;
COMMENT ON COLUMN sys_session.dt_in IS 'Data / hora de início de sessão' ;
COMMENT ON COLUMN sys_session.dt_out IS 'Data / hora de fim de sessão' ;
COMMENT ON COLUMN sys_session.id_professional IS 'Profissional que iniciou sessão' ;


DROP TABLE SYS_SESSION;
