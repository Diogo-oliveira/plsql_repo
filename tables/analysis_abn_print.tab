   COMMENT ON COLUMN "ALERT"."ANALYSIS_ABN_PRINT"."DT_ABN_PRINT_TSTZ" IS 'Date when the ABN record occurred';



   COMMENT ON COLUMN "ALERT"."ANALYSIS_ABN_PRINT"."FLG_STATUS" IS 'Status of the ABN record (''Y'' - Printed, ''N'' - Not Printed, ''X'' -Doesn''t apply)';



   COMMENT ON COLUMN "ALERT"."ANALYSIS_ABN_PRINT"."ID_PROF_ABN_PRINT" IS 'ID Professional that inserted the ABN record';



   COMMENT ON COLUMN "ALERT"."ANALYSIS_ABN_PRINT"."ID_ANALYSIS_REQ_DET" IS 'ID Analysis Req Detail';


