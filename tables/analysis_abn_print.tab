

  CREATE TABLE "ALERT"."ANALYSIS_ABN_PRINT"
   (	"DT_ABN_PRINT_TSTZ" TIMESTAMP (6) WITH LOCAL TIME ZONE DEFAULT current_timestamp,
	"FLG_STATUS" VARCHAR2(1) DEFAULT 'X',
	"ID_PROF_ABN_PRINT" NUMBER(24,0),
	"ID_ANALYSIS_REQ_DET" NUMBER(24,0)
   ) ;


