

  CREATE TABLE SR_RECEIVE_PROC
   (	ID_SR_RECEIVE_PROC NUMBER(24,0) NOT NULL ENABLE,
	CODE_RECEIVE_PROC VARCHAR2(200) NOT NULL ENABLE,
	FLG_MANDATORY_YN VARCHAR2(1) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1),
	FLG_ADMISSION VARCHAR2(1),
	FLG_NOT_APPL_YN VARCHAR2(1),
	SQL_CURSOR VARCHAR2(4000),
	CODE_LABEL VARCHAR2(200),
	FLG_COLOR_Y VARCHAR2(1),
	FLG_COLOR_N VARCHAR2(1),
	FLG_COLOR_NA VARCHAR2(1),

	FLG_MANDATORY_Y NUMBER(2,0),
	FLG_MANDATORY_N NUMBER(2,0),
	FLG_MANDATORY_NA NUMBER(2,0),
	RANK NUMBER(6,0),
	ADW_LAST_UPDATE DATE
   );


COMMENT ON COLUMN sr_receive_proc.id_sr_receive_proc IS 'ID único' ;
COMMENT ON COLUMN sr_receive_proc.code_receive_proc IS 'Código para obter descrição' ;
COMMENT ON COLUMN sr_receive_proc.flg_mandatory_yn IS 'Flag que indica se o ponto é vital para a admissão na cirurgia' ;
COMMENT ON COLUMN sr_receive_proc.flg_available IS 'Indica se está ou não disponível' ;
COMMENT ON COLUMN sr_receive_proc.flg_admission IS 'Identifica o ponto que determina se o paciente está pronto para ser admitido para cirurgia. Só pode haver um registo com valor = Y' ;
COMMENT ON COLUMN sr_receive_proc.flg_not_appl_yn IS 'Indica se a opção "Não Aplicável" está disponível' ;
COMMENT ON COLUMN sr_receive_proc.sql_cursor IS 'Código SQL para obtenção da descrição do item' ;
COMMENT ON COLUMN sr_receive_proc.code_label IS 'Código para obtenção da label da descrição na tabela Translation' ;
COMMENT ON COLUMN sr_receive_proc.flg_color_y IS 'Indica a cor a aplicar ao botão "Sim". Valores possíveis: R-Vermelho, G-Cinzento, X-Nenhuma' ;
COMMENT ON COLUMN sr_receive_proc.flg_color_n IS 'Indica a cor a aplicar ao botão "Não". Valores possíveis: R-Vermelho, G-Cinzento, X-Nenhuma' ;
COMMENT ON COLUMN sr_receive_proc.flg_color_na IS 'Indica a cor a aplicar ao botão "Não aplicável". Valores possíveis: R-Vermelho, G-Cinzento, X-Nenhuma' ;
COMMENT ON COLUMN sr_receive_proc.flg_mandatory_y IS 'Indica se o valor "Sim" do item é impeditivo do OK para Acolhimento. Valores possíveis: 1- É impeditivo, 0- Não é impeditivo' ;
COMMENT ON COLUMN sr_receive_proc.flg_mandatory_n IS 'Indica se o valor "Não" do item é impeditivo do OK para Acolhimento. Valores possíveis: 1- É impeditivo, 0- Não é impeditivo' ;

COMMENT ON COLUMN sr_receive_proc.flg_mandatory_na IS 'Indica se o valor "Não aplicável" do item é impeditivo do OK para Acolhimento. Valores possíveis: 1- É impeditivo, 0- Não é impeditivo' ;
COMMENT ON COLUMN sr_receive_proc.rank IS 'Ordem de visualização dos items' ;


