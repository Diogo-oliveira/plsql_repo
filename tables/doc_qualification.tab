

  CREATE TABLE DOC_QUALIFICATION
   (	ID_DOC_QUALIFICATION NUMBER(24,0) NOT NULL ENABLE,
	CODE_DOC_QUALIFICATION VARCHAR2(240) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN doc_qualification.id_doc_qualification IS 'Chave primária' ;
COMMENT ON COLUMN doc_qualification.code_doc_qualification IS 'Descrição do qualificador' ;
COMMENT ON COLUMN doc_qualification.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN doc_qualification.adw_last_update IS 'Data da última alteração' ;

