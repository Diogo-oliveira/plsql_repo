COMMENT ON COLUMN CARE_PLAN_TASK_REQ.DT_NEXT_TASK IS 'Data do próximo planeamento';
COMMENT ON COLUMN CARE_PLAN_TASK_REQ.ID_REQ IS 'Id da requisição';
COMMENT ON COLUMN CARE_PLAN_TASK_REQ.FLG_STATUS IS 'Flag que indica o estado  da  instância planeada';
COMMENT ON COLUMN CARE_PLAN_TASK_REQ.ID_TASK_TYPE IS 'Id do tipo de tarefa';



