
  CREATE TABLE DEP_CLIN_SERV_PERM
   (	ID_DEP_CLIN_SERV_PERM NUMBER(24,0) NOT NULL ENABLE,
	ID_DEP_CLIN_SERV NUMBER(24,0),
	ADW_LAST_UPDATE DATE,
	ID_EPIS_TYPE NUMBER(24,0),
	ID_CLIN_SERV NUMBER(24,0)
   );


COMMENT ON TABLE dep_clin_serv_perm IS 'Permissões de um DepClinServ para aceder a episódios de determinados tipos.' ;

COMMENT ON COLUMN dep_clin_serv_perm.id_dep_clin_serv_perm IS 'Primary Key' ;
COMMENT ON COLUMN dep_clin_serv_perm.id_dep_clin_serv IS 'ID Dep Clin Serv' ;
COMMENT ON COLUMN dep_clin_serv_perm.id_epis_type IS 'Episode Type' ;
COMMENT ON COLUMN dep_clin_serv_perm.id_clin_serv IS 'ID Clinical Service' ;

