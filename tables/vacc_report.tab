CREATE TABLE VACC_REPORT
 (ID_VACC_REPORT NUMBER(24) NOT NULL
 ,CODE_VACC_REPORT VARCHAR2(200) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1) NOT NULL
 ,RANK NUMBER(24) NOT NULL
 ,ID_CONTENT       VARCHAR2(200)
 ,CREATE_USER             VARCHAR2(24)
 ,CREATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION      NUMBER(24)
 ,UPDATE_USER             VARCHAR2(24)
 ,UPDATE_TIME             TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION      NUMBER(24) );
 
COMMENT ON TABLE VACC_REPORT IS 'Type of reported vaccines';

COMMENT ON COLUMN VACC_REPORT.ID_VACC_REPORT IS 'Record_ID';

COMMENT ON COLUMN VACC_REPORT.CODE_VACC_REPORT IS 'Code for diet type translation';

COMMENT ON COLUMN VACC_REPORT.FLG_AVAILABLE IS 'Indicates if it is available';

COMMENT ON COLUMN VACC_REPORT.RANK IS 'Order of appearance';

COMMENT ON COLUMN VACC_REPORT.ID_CONTENT IS 'Content parametrization';

COMMENT ON COLUMN VACC_REPORT.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN VACC_REPORT.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN VACC_REPORT.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN VACC_REPORT.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN VACC_REPORT.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN VACC_REPORT.UPDATE_INSTITUTION IS 'Update Institution';
