
CREATE TABLE PROTOCOL
 (ID_PROTOCOL NUMBER(24) NOT NULL
 ,ID_PROTOCOL_PREVIOUS_VERSION NUMBER(24)
 ,PROTOCOL_DESC VARCHAR2(200)
 ,DT_PROTOCOL TIMESTAMP WITH LOCAL TIME ZONE
 ,FLG_STATUS VARCHAR2(1)
 ,ID_EBM NUMBER(24)
 ,CONTEXT_DESC VARCHAR2(4000)
 ,CONTEXT_TITLE VARCHAR2(200)
 ,CONTEXT_ADAPTATION VARCHAR2(200)
 ,CONTEXT_TYPE_MEDIA NUMBER(24)
 ,CONTEXT_EDITOR VARCHAR2(200)
 ,CONTEXT_EDITION_SITE VARCHAR2(200)
 ,CONTEXT_EDITION VARCHAR2(200)
 ,DT_CONTEXT_EDITION DATE
 ,CONTEXT_ACCESS VARCHAR2(200)
 ,ID_CONTEXT_LANGUAGE NUMBER(6)
 ,FLG_CONTEXT_IMAGE VARCHAR2(1)
 ,CONTEXT_SUBTITLE VARCHAR2(200)
 ,ID_CONTEXT_ASSOCIATED_LANGUAGE NUMBER(6)
 ,ID_PROFESSIONAL NUMBER(24)
 ,ID_INSTITUTION NUMBER(12) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 ,ID_PROF_CANCEL NUMBER(24)
 ,DT_CANCEL TIMESTAMP WITH LOCAL TIME ZONE
 ,FLG_TYPE_RECOMMENDATION VARCHAR2(1)
 ,ADW_LAST_UPDATE DATE
 );

COMMENT ON COLUMN PROTOCOL.ID_PROTOCOL IS 'Protocol ID';
COMMENT ON COLUMN PROTOCOL.ID_PROTOCOL_PREVIOUS_VERSION IS 'Previous version of protocol in case we are still editing it';
COMMENT ON COLUMN PROTOCOL.PROTOCOL_DESC IS 'Protocol Title';
COMMENT ON COLUMN PROTOCOL.DT_PROTOCOL IS 'Creation date of protocol';
COMMENT ON COLUMN PROTOCOL.FLG_STATUS IS 'Status of protocol: Inactive/ Active';
COMMENT ON COLUMN PROTOCOL.ID_EBM IS 'EBM ID';
COMMENT ON COLUMN PROTOCOL.CONTEXT_DESC IS 'Description of other information';
COMMENT ON COLUMN PROTOCOL.CONTEXT_TITLE IS 'Other information context title';
COMMENT ON COLUMN PROTOCOL.CONTEXT_ADAPTATION IS 'Adaptation of context';
COMMENT ON COLUMN PROTOCOL.CONTEXT_TYPE_MEDIA IS 'Context media type';
COMMENT ON COLUMN PROTOCOL.CONTEXT_EDITOR IS 'Editor';
COMMENT ON COLUMN PROTOCOL.CONTEXT_EDITION_SITE IS 'Edition site';
COMMENT ON COLUMN PROTOCOL.CONTEXT_EDITION IS 'Edition';
COMMENT ON COLUMN PROTOCOL.DT_CONTEXT_EDITION IS 'Date of context edition';
COMMENT ON COLUMN PROTOCOL.CONTEXT_ACCESS IS 'Access';
COMMENT ON COLUMN PROTOCOL.ID_CONTEXT_LANGUAGE IS 'Language for context';
COMMENT ON COLUMN PROTOCOL.FLG_CONTEXT_IMAGE IS 'Flag for image associated to context';
COMMENT ON COLUMN PROTOCOL.CONTEXT_SUBTITLE IS 'Author last name';
COMMENT ON COLUMN PROTOCOL.ID_CONTEXT_ASSOCIATED_LANGUAGE IS 'Associated language to image';
COMMENT ON COLUMN PROTOCOL.ID_PROFESSIONAL IS 'Professionals associated to protocol';
COMMENT ON COLUMN PROTOCOL.ID_INSTITUTION IS 'Institution associated to protocol';
COMMENT ON COLUMN PROTOCOL.ID_SOFTWARE IS 'Software associated to protocol';
COMMENT ON COLUMN PROTOCOL.ID_PROF_CANCEL IS 'Professional ID which cancel or deprecate guideline';
COMMENT ON COLUMN PROTOCOL.DT_CANCEL IS 'Date when protocol was canceled or deprecated';
COMMENT ON COLUMN PROTOCOL.ADW_LAST_UPDATE IS 'ADW column';


