CREATE TABLE SCH_CONSULT_VAC_MFR
 (ID_SCH_CONSULT_VACANCY NUMBER(24) NOT NULL
 ,ID_PHYSIATRY_AREA NUMBER(24) NOT NULL
 );

COMMENT ON TABLE SCH_CONSULT_VAC_MFR IS 'MFR specific vacancy data. These are permanent attributes that could not be guaranteed by table sch_consult_vac_mfr_slot';
COMMENT ON COLUMN SCH_CONSULT_VAC_MFR.ID_SCH_CONSULT_VACANCY IS 'vacancy id (primary key)';
COMMENT ON COLUMN SCH_CONSULT_VAC_MFR.ID_PHYSIATRY_AREA IS 'physiatry area to which this vacancy applies';

