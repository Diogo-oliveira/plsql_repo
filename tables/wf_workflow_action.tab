CREATE TABLE WF_WORKFLOW_ACTION
 (ID_WORKFLOW_ACTION NUMBER(24) NOT NULL
 ,INTERNAL_NAME VARCHAR2(200)
 ,DESCRIPTION VARCHAR2(240)
 ,CODE_ACTION VARCHAR2(200) NOT NULL
 ,ICON VARCHAR2(240)
 ,FLG_AVAILABLE VARCHAR2(1) DEFAULT 'Y' NOT NULL
 ,CREATE_USER TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,CREATE_TIME VARCHAR2(240)
 ,CREATE_INSTITUTION NUMBER(24)
 ,UPDATE_USER VARCHAR2(24)
 ,UPDATE_TIME TIMESTAMP(6) WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
);

COMMENT ON TABLE WF_WORKFLOW_ACTION IS 'Workflow action to change form one state to another';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.ID_WORKFLOW_ACTION IS 'Action primary key';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.INTERNAL_NAME IS 'Action internal name';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.DESCRIPTION IS 'Action internal description';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.CODE_ACTION IS 'Action name';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.ICON IS 'Action default icon';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.FLG_AVAILABLE IS 'Y if available, N otherwise';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN WF_WORKFLOW_ACTION.UPDATE_INSTITUTION IS 'Update Institution';
-- CHANGE END: Ana Monteiro