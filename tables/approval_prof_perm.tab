CREATE TABLE APPROVAL_PROF_PERM
 (ID_PROFESSIONAL NUMBER(24) NOT NULL
 ,ID_APPROVAL_TYPE NUMBER(24) NOT NULL
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,ID_DEPT NUMBER(24) NOT NULL
 ,ID_DEPARTMENT NUMBER(24) NOT NULL
 ,ID_CLINICAL_SERVICE NUMBER(12) NOT NULL
 );

COMMENT ON TABLE APPROVAL_PROF_PERM IS 'Professional approval permissions';

COMMENT ON COLUMN APPROVAL_PROF_PERM.ID_PROFESSIONAL IS 'Professional identifier';
COMMENT ON COLUMN APPROVAL_PROF_PERM.ID_APPROVAL_TYPE IS 'Approval type identifier';
COMMENT ON COLUMN APPROVAL_PROF_PERM.ID_INSTITUTION IS 'Institution identifier';
COMMENT ON COLUMN APPROVAL_PROF_PERM.ID_DEPT IS 'Department identifier';
COMMENT ON COLUMN APPROVAL_PROF_PERM.ID_DEPARTMENT IS 'Service identifier';
COMMENT ON COLUMN APPROVAL_PROF_PERM.ID_CLINICAL_SERVICE IS 'Clinical service identifier';

ALTER TABLE APPROVAL_PROF_PERM ADD (
   CREATE_USER VARCHAR2(24),
   CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
   CREATE_INSTITUTION NUMBER(24,0),
   UPDATE_USER VARCHAR2(24),
   UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE,
   UPDATE_INSTITUTION NUMBER(24,0)
);
 
COMMENT ON COLUMN APPROVAL_PROF_PERM.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN APPROVAL_PROF_PERM.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN APPROVAL_PROF_PERM.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN APPROVAL_PROF_PERM.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN APPROVAL_PROF_PERM.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN APPROVAL_PROF_PERM.UPDATE_INSTITUTION IS 'Update Institution';
