COMMENT ON COLUMN unit_measure_enum.code_unit_measure_enum2 IS 'Chave para uma descrição em função da língua' ;
COMMENT ON COLUMN unit_measure_enum.code_unit_measure_enum3 IS 'Chave para uma descrição em função da língua' ;
