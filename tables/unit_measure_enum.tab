
  CREATE TABLE UNIT_MEASURE_ENUM
   (	ID_UNIT_MEASURE_ENUM NUMBER(24,0) NOT NULL ENABLE,
	ID_UNIT_MEASURE NUMBER(24,0) NOT NULL ENABLE,
	CODE_UNIT_MEASURE_ENUM1 VARCHAR2(200) NOT NULL ENABLE,
	CODE_UNIT_MEASURE_ENUM2 VARCHAR2(200),
	CODE_UNIT_MEASURE_ENUM3 VARCHAR2(200),
	VALUE NUMBER(24,0) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	IMAGE_NAME VARCHAR2(200),
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );



COMMENT ON COLUMN unit_measure_enum.id_unit_measure_enum IS 'Chave primária' ;
COMMENT ON COLUMN unit_measure_enum.id_unit_measure IS 'Unidade de medida' ;
