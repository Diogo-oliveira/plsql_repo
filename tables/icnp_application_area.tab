
CREATE TABLE ICNP_APPLICATION_AREA
 (ID_APPLICATION_AREA NUMBER(24) NOT NULL
 ,AREA VARCHAR2(200) NOT NULL
 ,AREA_CODE VARCHAR2(50) NOT NULL
 ,PARAMETER_DESC VARCHAR2(200)
 ,ID_INSTITUTION NUMBER(24) NOT NULL
 ,ID_SOFTWARE NUMBER(24) NOT NULL
 ,CREATE_USER VARCHAR2(24) 
 ,CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE 
 ,CREATE_INSTITUTION NUMBER(24) 
 ,UPDATE_USER VARCHAR2(24) 
 ,UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE 
 ,UPDATE_INSTITUTION NUMBER(24)
 )
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.ID_APPLICATION_AREA IS 'Application area ID'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.AREA IS 'Application area REF'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.AREA_CODE IS 'Application area code for translation'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.PARAMETER_DESC IS 'Parameter description for translation'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.ID_INSTITUTION IS 'Institution ID'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.ID_SOFTWARE IS 'Software ID'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.CREATE_USER IS 'Creation User'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.CREATE_TIME IS 'Creation Time'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.CREATE_INSTITUTION IS 'Creation Institution'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.UPDATE_USER IS 'Update User'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.UPDATE_TIME IS 'Update Time'
/

COMMENT ON COLUMN ICNP_APPLICATION_AREA.UPDATE_INSTITUTION IS 'Update Institution'
/

