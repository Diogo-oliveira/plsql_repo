

  CREATE TABLE ANESTHESIA_TYPE
   (	ID_ANESTHESIA_TYPE NUMBER(24,0) NOT NULL ENABLE,
	RANK NUMBER(6,0),
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1),
	CODE_ANESTHESIA_TYPE VARCHAR2(200) NOT NULL ENABLE
   );


COMMENT ON COLUMN anesthesia_type.id_anesthesia_type IS 'ID único' ;
COMMENT ON COLUMN anesthesia_type.rank IS 'Ordem de visualização' ;
COMMENT ON COLUMN anesthesia_type.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN anesthesia_type.flg_available IS 'Disponível (Y/N)' ;
COMMENT ON COLUMN anesthesia_type.code_anesthesia_type IS 'Código para obter a descrição da tabela Translation' ;


