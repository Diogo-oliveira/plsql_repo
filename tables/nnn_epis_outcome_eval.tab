CREATE TABLE NNN_EPIS_OUTCOME_EVAL
  (
    ID_NNN_EPIS_OUTCOME_EVAL NUMBER (24) NOT NULL ,
    ID_NNN_EPIS_OUTCOME      NUMBER (24) NOT NULL ,
    ID_PATIENT               NUMBER (24) NOT NULL ,
    ID_EPISODE               NUMBER (24) ,
    ID_VISIT                 NUMBER (24) ,
    ID_PROFESSIONAL          NUMBER (24) ,
    ID_CANCEL_REASON         NUMBER (24) ,
    CANCEL_NOTES             VARCHAR2 (1000 CHAR) ,
    DT_PLAN                  TIMESTAMP WITH LOCAL TIME ZONE ,
    ID_ORDER_RECURR_PLAN     NUMBER (24) ,
    EXEC_NUMBER              NUMBER (24) ,
    FLG_STATUS               VARCHAR2 (1 CHAR) NOT NULL ,
    DT_EVALUATION            TIMESTAMP WITH LOCAL TIME ZONE ,
    TARGET_VALUE             NUMBER (24) ,
    OUTCOME_VALUE            NUMBER (24) ,
    CODE_NOTES               VARCHAR2 (200 CHAR) AS ( 'NNN_EPIS_OUTCOME_EVAL.CODE_NOTES.'
    ||TO_CHAR(ID_NNN_EPIS_OUTCOME_EVAL) ) VIRTUAL NOT NULL ,
    DT_TRS_TIME_START  TIMESTAMP WITH LOCAL TIME ZONE NOT NULL ,
    DT_TRS_TIME_END    TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_USER        VARCHAR2 (30 CHAR) ,
    CREATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    CREATE_INSTITUTION NUMBER (24) ,
    UPDATE_USER        VARCHAR2 (30 CHAR) ,
    UPDATE_TIME        TIMESTAMP WITH LOCAL TIME ZONE ,
    UPDATE_INSTITUTION NUMBER (24)
  )
  TABLESPACE ALERT_DATA ;

COMMENT ON TABLE NNN_EPIS_OUTCOME_EVAL
IS
  'Evaluations of NOC Outcomes in the context of a patient''s nursing care plan' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_NNN_EPIS_OUTCOME_EVAL
IS
  'Outcome Evaluation ID (Surrogate key)' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_NNN_EPIS_OUTCOME
IS
  'Episode''s NOC Outcome ID' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_PATIENT
IS
  'Patient ID' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_EPISODE
IS
  'Episode ID' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_VISIT
IS
  'Visit ID' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_PROFESSIONAL
IS
  'Professional ID who did the evaluation' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_CANCEL_REASON
IS
  'Cancel reason ID' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.CANCEL_NOTES
IS
  'Cancelation notes' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.DT_PLAN
IS
  'Planned date for the outcome evaluation' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.ID_ORDER_RECURR_PLAN
IS
  'Order recurrence plan ID for defined frequency in the instructions' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.EXEC_NUMBER
IS
  'The order of the execution within the plan as specified by the recurrence mechanism' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.FLG_STATUS
IS
  'Evaluation status:
C Cancelled
E Expired
F Finished
O Ongoing
P Suspended
R Requested' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.DT_EVALUATION
IS
  'Evaluation date' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.TARGET_VALUE
IS
  'Outcome Target rating: Likert scale (1 to 5)' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.OUTCOME_VALUE
IS
  'Outcome overall rating: Likert scale (1 to 5)' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.CODE_NOTES
IS
  'Notes about outcome evaluation performed by the professional' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.DT_TRS_TIME_START
IS
  'Transaction time start' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.DT_TRS_TIME_END
IS
  'Transaction time end' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.CREATE_USER
IS
  'Creation User' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.CREATE_TIME
IS
  'Creation Time' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.CREATE_INSTITUTION
IS
  'Creation Institution' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.UPDATE_USER
IS
  'Update User' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.UPDATE_TIME
IS
  'Update Time' ;
  COMMENT ON COLUMN NNN_EPIS_OUTCOME_EVAL.UPDATE_INSTITUTION
IS
  'Update Institution' ;
