

  CREATE TABLE SYS_FIELD
   (	ID_SYS_FIELD NUMBER(12,0) NOT NULL ENABLE,
	INTERN_NAME VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE
   );


COMMENT ON TABLE sys_field IS 'Campos contidos na parte principal (central) do ecrã e que podem ter funcionalidades próprias associadas. Por ex., uma coluna de uma grelha de tarefas que pode sofrer alterações.' ;

COMMENT ON COLUMN sys_field.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
