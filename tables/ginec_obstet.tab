

  CREATE TABLE GINEC_OBSTET
   (	ID_GINEC_OBSTET NUMBER(24,0) NOT NULL ENABLE,
	CODE_GINEC_OBSTET VARCHAR2(200) NOT NULL ENABLE,
	FLG_GINEC_OBSTET VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE ginec_obstet IS 'Eventos significativos para a história obstétrica: partos de termo, partos pré-termo, abortos expontâneos, abortos provocados, gravidez ectópica, filhos vivos + lista de antecedentes obstétricos do livro "Boletim de Saúde da Grávida".Ginecológicos: tensão pré-menstrual, infecção pélvica, dismenorreia (menstruação difícil e dolorosa), dispareunia (dor durante as relações sexuais), menorreia (escoamento da menstruação), metrorragia (hemorragia intra-uterina fora do período menstrual), leucorreia (corrimento esbranquiçado pela vagina), outros' ;

COMMENT ON COLUMN ginec_obstet.code_ginec_obstet IS 'Código para tradução' ;
COMMENT ON COLUMN ginec_obstet.flg_ginec_obstet IS 'Tipo: O - obstétrico, C - contraceptivo, G - ginecológico' ;
