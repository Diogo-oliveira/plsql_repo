CREATE TABLE GROUP_ACCESS_RECORD
 (ID_GROUP_ACCESS_RECORD NUMBER(24) NOT NULL
 ,ID_GROUP_ACCESS NUMBER(24) NOT NULL
 ,ID_RECORD NUMBER(24) NOT NULL
 ,FLG_TYPE VARCHAR2(2) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1 CHAR) DEFAULT 'Y' NOT NULL
 ,CREATE_USER VARCHAR2(24 CHAR)
 ,CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION NUMBER(24)
 ,UPDATE_USER VARCHAR2(24 CHAR)
 ,UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
 );
 
COMMENT ON TABLE GROUP_ACCESS_RECORD IS 'Relation between group access and a record ID (Analysis ID, Exams ID, Interventions ID, etc...)';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.ID_GROUP_ACCESS_RECORD IS 'Table ID';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.ID_GROUP_ACCESS IS 'Group access ID';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.ID_RECORD IS 'The record ID of specific area (ID_ANALYSIS, ID_EXAM, ID_INTERVENTION, etc...)';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.FLG_TYPE IS 'Flag that indicates the record type (A - Analysis, E - Exam, I - Intervention, etc...)';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.FLG_AVAILABLE IS 'Indicates if the record is available for the group access';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.CREATE_USER IS 'Create user';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.CREATE_TIME IS 'Create time';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.CREATE_INSTITUTION IS 'Create institution';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.UPDATE_USER IS 'Update user';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.UPDATE_TIME IS 'Update time';
COMMENT ON COLUMN GROUP_ACCESS_RECORD.UPDATE_INSTITUTION IS 'Update institution';