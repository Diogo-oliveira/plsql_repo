

  CREATE TABLE EPIS_DIAGNOSIS
   (	ID_EPIS_DIAGNOSIS NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	ID_DIAGNOSIS NUMBER(12,0) NOT NULL ENABLE,
	ID_PROFESSIONAL_DIAG NUMBER(24,0),
	ID_PROFESSIONAL_CANCEL NUMBER(24,0),
	DT_EPIS_DIAGNOSIS DATE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	NOTES VARCHAR2(4000),
	DT_CANCEL DATE,
	NOTES_CANCEL VARCHAR2(4000),

	DESC_EPIS_DIAGNOSIS VARCHAR2(200),
	DT_RULLED_OUT DATE,
	ID_PROF_CONFIRMED NUMBER(24,0),
	DT_CONFIRMED DATE,
	ID_PROF_RULLED_OUT NUMBER(24,0),
	ID_PROF_BASE NUMBER(24,0),
	DT_BASE DATE,
	FLG_FINAL_TYPE VARCHAR2(1),
	ID_EPIS_DIAGNOSIS_NOTES NUMBER
   );


COMMENT ON TABLE epis_diagnosis IS 'Diagnósticos registados no episódio' ;

COMMENT ON COLUMN epis_diagnosis.id_epis_diagnosis IS 'Chave primária' ;
COMMENT ON COLUMN epis_diagnosis.id_episode IS 'Episódio' ;
COMMENT ON COLUMN epis_diagnosis.id_diagnosis IS 'Diagnóstico' ;
COMMENT ON COLUMN epis_diagnosis.id_professional_diag IS 'Profissional do diagnóstico' ;
COMMENT ON COLUMN epis_diagnosis.id_professional_cancel IS 'Profissional que cancelou' ;
COMMENT ON COLUMN epis_diagnosis.dt_epis_diagnosis IS 'Data de registo' ;
COMMENT ON COLUMN epis_diagnosis.flg_type IS 'Tipo de diagnóstico: provável / definitivo. Os prováveis vão mudando ao longo do episódio, mas podem tornar-se definitivos. O definitivo tb pode ser alterado, mas geral/ é o de saída.Novo tipo de diagnóstico: B - Diagnóstico base, existe apenas para o ORIS.' ;
COMMENT ON COLUMN epis_diagnosis.flg_status IS 'Estado do episódio:A -Activo; C - Cancelar; D - Despiste; F - Confirmado; R - Declinar; B -Base' ;
COMMENT ON COLUMN epis_diagnosis.notes IS 'Notas' ;
COMMENT ON COLUMN epis_diagnosis.dt_cancel IS 'Data de cancelamento' ;
COMMENT ON COLUMN epis_diagnosis.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN epis_diagnosis.desc_epis_diagnosis IS 'Descrição do diagnóstico' ;
COMMENT ON COLUMN epis_diagnosis.dt_rulled_out IS 'Data em que se declinou um diagnóstico diferencial provisório' ;

COMMENT ON COLUMN epis_diagnosis.id_prof_confirmed IS 'Profissional que confirmou' ;
COMMENT ON COLUMN epis_diagnosis.dt_confirmed IS 'Data em que se confirmou um diagnóstico diferencial provisório' ;
COMMENT ON COLUMN epis_diagnosis.id_prof_rulled_out IS 'Profissional que declinou' ;
COMMENT ON COLUMN epis_diagnosis.id_prof_base IS 'Profissional que registou o diagnóstico base' ;
COMMENT ON COLUMN epis_diagnosis.dt_base IS 'Data do diagnóstico base' ;
COMMENT ON COLUMN epis_diagnosis.flg_final_type IS 'Tipo de diagnóstico final: P - Primary;S - Secondary' ;
COMMENT ON COLUMN epis_diagnosis.id_epis_diagnosis_notes IS 'Notas gerais do diagnóstico do episódio' ;

alter table epis_diagnosis 
add (dt_epis_diagnosis_tstz timestamp with local time zone,
     dt_cancel_tstz timestamp with local time zone,
     dt_rulled_out_tstz timestamp with local time zone,
     dt_confirmed_tstz timestamp with local time zone,
     dt_base_tstz timestamp with local time zone); 

