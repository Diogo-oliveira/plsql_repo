

  CREATE TABLE DOCUMENT_TYPE
   (	ID_DOCUMENT_TYPE NUMBER NOT NULL ENABLE,
	CODE_DOCUMENT_TYPE VARCHAR2(240) NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	ID_GLOBAL NUMBER,
	INTERNAL_NAME VARCHAR2(200)
   );


COMMENT ON COLUMN document_type.id_document_type IS 'Chave primária' ;
COMMENT ON COLUMN document_type.code_document_type IS 'Descrição do tipo de Documentation' ;
COMMENT ON COLUMN document_type.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN document_type.flg_available IS 'Disponibilidade do registo na instituição onde está instalada a BD' ;
COMMENT ON COLUMN document_type.id_global IS 'Chave de relacionamento' ;


