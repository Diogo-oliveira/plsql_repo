

  CREATE TABLE PROFILE_TEMPL_ACC_FUNC
   (	ID_PROFILE_TEMPL_ACCESS_FUNC NUMBER(24,0) NOT NULL ENABLE,
	ID_PROFILE_TEMPLATE NUMBER(12,0) NOT NULL ENABLE,
	ID_SYS_FIELD NUMBER(12,0),
	ID_FUNCTIONALITY NUMBER(12,0) NOT NULL ENABLE
   );


COMMENT ON TABLE profile_templ_acc_func IS 'Acessos dos templates às funcionalidades de cada campo.Se ID_SYS_FIELD ñ estiver preenchida, o acesso refere-se a uma funcionalidade q ñ está associada a nenhum campo.' ;
