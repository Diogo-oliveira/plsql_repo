create table SUPPLY_WORKFLOW
(
  ID_SUPPLY_WORKFLOW NUMBER(24) not null,
  ID_PROFESSIONAL    NUMBER(24) not null,
  ID_EPISODE         NUMBER(24) not null,
  ID_SUPPLY_REQUEST  NUMBER(24),
