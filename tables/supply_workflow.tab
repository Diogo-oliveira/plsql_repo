  ID_DEL_REASON      NUMBER(24),
  ID_SUPPLY_SET      NUMBER(24)
)
tablespace TABLE_L
  pctfree 10
  initrans 1
  maxtrans 255
  storage
  (
    initial 64K
    minextents 1
    maxextents unlimited
