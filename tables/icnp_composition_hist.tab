
CREATE TABLE ICNP_COMPOSITION_HIST
 (ID_COMPOSITION_HIST NUMBER(24) NOT NULL
 ,ID_COMPOSITION NUMBER(24) NOT NULL
 ,FLG_MOST_RECENT VARCHAR2(1) DEFAULT 'Y' NOT NULL
 ,DT_COMPOSITION_HIST TIMESTAMP WITH LOCAL TIME ZONE NOT NULL
 ,ID_PROFESSIONAL NUMBER(24)
 ,ID_CANCEL_REASON NUMBER(24)
 ,DT_CANCEL TIMESTAMP WITH LOCAL TIME ZONE
 ,REASON_NOTES VARCHAR2(4000)
 ,ID_PROF_CANCEL NUMBER(24)
 ,FLG_CANCEL VARCHAR2(1)
 ,CREATE_USER VARCHAR2(24) 
 ,CREATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE 
 ,CREATE_INSTITUTION NUMBER(24) 
 ,UPDATE_USER VARCHAR2(24) 
 ,UPDATE_TIME TIMESTAMP (6) WITH LOCAL TIME ZONE 
 ,UPDATE_INSTITUTION NUMBER(24)
 )
/

COMMENT ON TABLE ICNP_COMPOSITION_HIST IS 'Date when composition has changed.'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.ID_COMPOSITION_HIST IS 'Composition History identifier'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.ID_COMPOSITION IS 'Composition identifier'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.FLG_MOST_RECENT IS 'Flag for most recent composition (Y/N)'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.DT_COMPOSITION_HIST IS 'Date when composition has changed'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.ID_PROFESSIONAL IS 'Professional ID that has created the composition'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.ID_CANCEL_REASON IS 'ID for cancel reason'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.DT_CANCEL IS 'Date when composition was cancelled'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.REASON_NOTES IS 'Reason notes for cancel'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.ID_PROF_CANCEL IS 'ID of professional that cancelled the composition'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.FLG_CANCEL IS 'Flag to define if composition is cancelled'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.CREATE_USER IS 'Creation User'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.CREATE_TIME IS 'Creation Time'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.CREATE_INSTITUTION IS 'Creation Institution'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.UPDATE_USER IS 'Update User'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.UPDATE_TIME IS 'Update Time'
/

COMMENT ON COLUMN ICNP_COMPOSITION_HIST.UPDATE_INSTITUTION IS 'Update Institution'
/