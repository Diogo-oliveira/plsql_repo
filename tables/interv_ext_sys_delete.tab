

  CREATE TABLE INTERV_EXT_SYS_DELETE
   (	ID_INTERV_EXT_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_EXTERNAL_SYS NUMBER(12,0) NOT NULL ENABLE,
	ID_INTERVENTION NUMBER(24,0) NOT NULL ENABLE,
	VALUE VARCHAR2(30) NOT NULL ENABLE,
	ID_INSTITUTION NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE interv_ext_sys_delete IS 'Correspondência dos identificadores únicos dos procedimentos, no Alert e nos sistemas externos' ;

COMMENT ON COLUMN interv_ext_sys_delete.value IS 'Identificador único do procedimento, no sistema externo indicado' ;
