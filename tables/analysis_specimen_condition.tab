CREATE TABLE ANALYSIS_SPECIMEN_CONDITION
 (ID_SPECIMEN_CONDITION NUMBER(24) NOT NULL
 ,VALUE VARCHAR2(200 CHAR) NOT NULL
 ,CODE_SPECIMEN_CONDITION VARCHAR2(200 CHAR) NOT NULL
 ,ID_CONTENT VARCHAR2(200 CHAR) NOT NULL
 ,FLG_AVAILABLE VARCHAR2(1 CHAR) NOT NULL
 ,INST_OWNER NUMBER(24) NOT NULL
 ,CREATE_USER VARCHAR2(24 CHAR)
 ,CREATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,CREATE_INSTITUTION NUMBER(24)
 ,UPDATE_USER VARCHAR2(24 CHAR)
 ,UPDATE_TIME TIMESTAMP WITH LOCAL TIME ZONE
 ,UPDATE_INSTITUTION NUMBER(24)
 );


COMMENT ON TABLE ANALYSIS_SPECIMEN_CONDITION IS 'Analysis Specimen Condition (HL7)';

COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.ID_SPECIMEN_CONDITION IS 'Specimen condition ID';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.VALUE IS 'HL7 value';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.CODE_SPECIMEN_CONDITION IS 'Description code';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.ID_CONTENT IS 'Specimen condition content id';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.FLG_AVAILABLE IS 'Flag that indicates if this record is available';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.CREATE_USER IS 'Creation User';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.CREATE_TIME IS 'Creation Time';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.CREATE_INSTITUTION IS 'Creation Institution';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.UPDATE_USER IS 'Update User';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.UPDATE_TIME IS 'Update Time';
COMMENT ON COLUMN ANALYSIS_SPECIMEN_CONDITION.UPDATE_INSTITUTION IS 'Update Institution';
