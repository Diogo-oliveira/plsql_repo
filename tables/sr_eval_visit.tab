

  CREATE TABLE SR_EVAL_VISIT
   (	ID_SR_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	DT_REG DATE NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	FLG_STATUS VARCHAR2(1),
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	DT_CANCEL DATE,
	ID_PROF_CANCEL NUMBER(24,0),
	NOTES_CANCEL VARCHAR2(4000),
	ID_SR_EVAL_TYPE NUMBER(3,0)
   );



COMMENT ON TABLE sr_eval_visit IS 'Visitas de avaliação pré-operatória e pré-anestésica' ;

COMMENT ON COLUMN sr_eval_visit.id_sr_eval_visit IS 'ID único' ;
COMMENT ON COLUMN sr_eval_visit.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_eval_visit.dt_reg IS 'Data do registo' ;
COMMENT ON COLUMN sr_eval_visit.id_professional IS 'ID do profissional que efectuou a visita' ;
COMMENT ON COLUMN sr_eval_visit.flg_status IS 'Estado da visita: A- Activa, C- Cancelada' ;
COMMENT ON COLUMN sr_eval_visit.flg_type IS 'Tipo de Avaliação. Valores possíveis: A- Aspectos Gerais, B- Avaliação pré-operatória de cirurgia, C- Avaliação pré-operatória de anestesia, D- Avaliação no dia da cirurgia, E- Visita pré-anestésica, F- Avaliação antes da ida para o bloco' ;
COMMENT ON COLUMN sr_eval_visit.dt_cancel IS 'Data de cancelamento da visita pré-operatória' ;
COMMENT ON COLUMN sr_eval_visit.id_prof_cancel IS 'ID do profissional que cancelou a visita' ;
COMMENT ON COLUMN sr_eval_visit.notes_cancel IS 'Notas de cancelamento' ;
COMMENT ON COLUMN sr_eval_visit.id_sr_eval_type IS 'Tipo de avaliação' ;


