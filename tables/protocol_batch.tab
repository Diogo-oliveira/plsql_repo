
CREATE TABLE PROTOCOL_BATCH
 (ID_PROTOCOL_BATCH NUMBER(24) NOT NULL
 ,BATCH_DESC VARCHAR2(200)
 ,BATCH_TYPE VARCHAR2(1) NOT NULL
 ,DT_PROTOCOL_BATCH TIMESTAMP WITH LOCAL TIME ZONE NOT NULL
 );

COMMENT ON COLUMN PROTOCOL_BATCH.ID_PROTOCOL_BATCH IS 'ID of batch processing';
COMMENT ON COLUMN PROTOCOL_BATCH.BATCH_DESC IS 'Batch description';
COMMENT ON COLUMN PROTOCOL_BATCH.BATCH_TYPE IS 'Batch Type : Manual or automatic ...';
COMMENT ON COLUMN PROTOCOL_BATCH.DT_PROTOCOL_BATCH IS 'Date at which this batch was run';

