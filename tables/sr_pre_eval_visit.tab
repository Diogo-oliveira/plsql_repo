

  CREATE TABLE SR_PRE_EVAL_VISIT
   (	ID_SR_PRE_EVAL_VISIT NUMBER(24,0) NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	FLG_TYPE VARCHAR2(1) NOT NULL ENABLE,
	ID_PROFESSIONAL NUMBER(24,0) NOT NULL ENABLE,
	DT_REG DATE NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON TABLE sr_pre_eval_visit IS 'Visitas de avaliação pré-operatória e pré-anestésica' ;

COMMENT ON COLUMN sr_pre_eval_visit.id_sr_pre_eval_visit IS 'ID único' ;
COMMENT ON COLUMN sr_pre_eval_visit.id_episode IS 'ID do episódio' ;
COMMENT ON COLUMN sr_pre_eval_visit.flg_type IS 'Tipo de visita. Valores possíveis: S- Pré-operatória, A- Pré-anestésica (obter descrição através da sys_domain' ;
COMMENT ON COLUMN sr_pre_eval_visit.id_professional IS 'ID do profissional que efectuou a visita' ;
COMMENT ON COLUMN sr_pre_eval_visit.dt_reg IS 'Data do registo' ;
COMMENT ON COLUMN sr_pre_eval_visit.adw_last_update IS 'Data da última alteração do registo' ;
