

  CREATE TABLE DIAGRAM
   (	ID_DIAGRAM NUMBER(24,0) NOT NULL ENABLE,
	FLG_STATUS VARCHAR2(1) NOT NULL ENABLE,
	DT_CREATION DATE NOT NULL ENABLE,
	ADW_LAST_UPDATE DATE NOT NULL ENABLE,
	ID_EPISODE NUMBER(24,0) NOT NULL ENABLE,
	ID_DIAGRAM_EPIS NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE diagram IS 'Diagramas dos episódios' ;

COMMENT ON COLUMN diagram.id_diagram IS 'Chave primária' ;
COMMENT ON COLUMN diagram.flg_status IS 'Status do diagrama:O - Open; C -Close' ;
COMMENT ON COLUMN diagram.dt_creation IS 'Data da criação do diagrama' ;
COMMENT ON COLUMN diagram.adw_last_update IS 'Data da última alteração' ;
COMMENT ON COLUMN diagram.id_episode IS 'Episódio' ;
COMMENT ON COLUMN diagram.id_diagram_epis IS 'Diagrama do episódio' ;

comment on column DIAGRAM.ID_DIAGRAM_EPIS
  is 'ID do diagrama associado ao episódio';

drop table diagram;
