

  CREATE TABLE HEMO_TYPE
   (	ID_HEMO_TYPE NUMBER(24,0) NOT NULL ENABLE,
	CODE_HEMO_TYPE VARCHAR2(200) NOT NULL ENABLE,
	FLG_AVAILABLE VARCHAR2(1) NOT NULL ENABLE,
	RANK NUMBER(6,0),
	ADW_LAST_UPDATE DATE NOT NULL ENABLE
   );


COMMENT ON COLUMN hemo_type.id_hemo_type IS 'ID único' ;
COMMENT ON COLUMN hemo_type.code_hemo_type IS 'Código para obter descrição na tabela Translation' ;
COMMENT ON COLUMN hemo_type.flg_available IS 'Disponível? (Y/N)' ;
COMMENT ON COLUMN hemo_type.rank IS 'Ordem de visualização' ;
COMMENT ON COLUMN hemo_type.adw_last_update IS 'Data da última alteração' ;


