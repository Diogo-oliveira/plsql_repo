

  CREATE TABLE ANALYSIS_PARAM_INSTIT_SAMPLE
   (	ID_ANALYSIS_PARAM_INSTIT_SAMPL NUMBER(24,0) NOT NULL ENABLE,
	ID_ANALYSIS_PARAM_INSTIT NUMBER(24,0) NOT NULL ENABLE,
	ID_SAMPLE_RECIPIENT NUMBER(24,0) NOT NULL ENABLE,
	ID_SAMPLE_TYPE NUMBER(24,0) NOT NULL ENABLE
   );


COMMENT ON TABLE analysis_param_instit_sample IS 'Tabela de associação dos parâmetros às amostras e recipientes' ;

COMMENT ON COLUMN analysis_param_instit_sample.id_sample_recipient IS 'Recipiente seleccionável na colheita, para o parâmetro indicado' ;
COMMENT ON COLUMN analysis_param_instit_sample.id_sample_type IS 'Amostra' ;
