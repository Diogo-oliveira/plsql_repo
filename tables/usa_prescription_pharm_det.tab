
  CREATE TABLE USA_PRESCRIPTION_PHARM_DET
   (	ID_PRESCRIPTION_PHARM_DET NUMBER(24,0) NOT NULL ENABLE,
	ID_PRESCRIPTION_PHARM NUMBER(24,0) NOT NULL ENABLE,
	ID_INGREDIENT NUMBER(24,0) NOT NULL ENABLE,
	QTY NUMBER(6,3)
   );


COMMENT ON TABLE usa_prescription_pharm_det IS 'Constituição das prescrições de manipulados.' ;

COMMENT ON COLUMN usa_prescription_pharm_det.id_prescription_pharm_det IS 'Chave primária.' ;
COMMENT ON COLUMN usa_prescription_pharm_det.id_prescription_pharm IS 'ID da prescrição.' ;
COMMENT ON COLUMN usa_prescription_pharm_det.id_ingredient IS 'ID do constituinte.' ;
COMMENT ON COLUMN usa_prescription_pharm_det.qty IS 'Quantidade prescrita.' ;

