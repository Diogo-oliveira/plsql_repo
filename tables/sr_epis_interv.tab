-- CHANGE REASON: [ALERT-210978 ] DEMOS MX - OUT - Admission request- se preenche as áreas de Dx e lateralidade no pedido do procedimento cirúrgico dá erro.
DECLARE
    e_column_already_there EXCEPTION;
    
    PRAGMA EXCEPTION_INIT(e_column_already_there, -01430); -- add column
BEGIN
    BEGIN
        EXECUTE IMMEDIATE 'alter table sr_epis_interv add id_epis_diagnosis number(24)';
    EXCEPTION
        WHEN e_column_already_there THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;
END;
/
-- CHANGE END: Sofia Mendes

