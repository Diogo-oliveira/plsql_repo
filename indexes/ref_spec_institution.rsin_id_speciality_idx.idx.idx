DECLARE
    l_var PLS_INTEGER;
BEGIN
    SELECT COUNT(1)
      INTO l_var
      FROM user_indexes
     WHERE index_name = 'RSIN_ID_SPECIALITY_IDX'
       AND table_name = 'REF_SPEC_INSTITUTION';

    IF l_var <> 1
    THEN
        EXECUTE IMMEDIATE 'CREATE INDEX "RSIN_ID_SPECIALITY_IDX" ON "REF_SPEC_INSTITUTION" ("ID_SPECIALITY")';
    END IF;
END;
/
