--                DB Versioning - New tables
DECLARE
    e_name_already_exists EXCEPTION;

    PRAGMA EXCEPTION_INIT(e_name_already_exists, -955);
BEGIN
    BEGIN
           EXECUTE IMMEDIATE 'CREATE INDEX PHSF_VMIFS_IDX ON PRE_HOSP_SECTION_FIELDS (FLG_VISIBLE,ID_MARKET,ID_INSTITUTION,ID_PRE_HOSP_FORM,ID_PRE_HOSP_STEP,ID_PRE_HOSP_SECTION) TABLESPACE INDEX_S';
    EXCEPTION
        WHEN e_name_already_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;
END;
/
