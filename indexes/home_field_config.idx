DECLARE e_object_exists EXCEPTION;
PRAGMA EXCEPTION_INIT(e_object_exists, -00955);
BEGIN

    BEGIN
        EXECUTE IMMEDIATE 'CREATE unique INDEX hfc_composite_idx
  ON home_field_config (
id_home_field,
id_institution
  )
  TABLESPACE index_m';
    EXCEPTION
        WHEN e_object_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;

    BEGIN
        EXECUTE IMMEDIATE 'CREATE INDEX hfc_hf_idx
  ON home_field_config (id_home_field)
  TABLESPACE index_m';
    EXCEPTION
        WHEN e_object_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;

BEGIN
        EXECUTE IMMEDIATE 'CREATE INDEX hfc_inst_idx
  ON home_field_config (id_institution)
  TABLESPACE index_m';
    EXCEPTION
        WHEN e_object_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;


END;
/
