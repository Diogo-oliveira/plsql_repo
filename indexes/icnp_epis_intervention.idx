-- Drop indexes 
declare
   e_no_existent_index EXCEPTION;
 PRAGMA EXCEPTION_INIT(e_no_existent_index, -01418); -- table already there
begin
    BEGIN
        EXECUTE IMMEDIATE 'drop index EIPI_EPIS_DEST_FK_IDX';
    EXCEPTION
        WHEN e_no_existent_index THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;
    BEGIN
        EXECUTE IMMEDIATE 'drop index EIPI_EPIS_ORIG_FK_IDX';
    EXCEPTION
        WHEN e_no_existent_index THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;
end;
/
-- CHANGE END: Sérgio Santos