DECLARE
    l_var PLS_INTEGER;
BEGIN
    SELECT COUNT(1)
      INTO l_var
      FROM user_indexes
     WHERE index_name = 'PRCT_PK';
    IF l_var < 1
    THEN
        EXECUTE IMMEDIATE 'CREATE UNIQUE INDEX PRCT_PK ON P1_REASON_CODE_SOFT_INST (ID_REASON_CODE, ID_PROFILE_TEMPLATE, ID_SOFTWARE, ID_INSTITUTION)';
    END IF;
END;
/
