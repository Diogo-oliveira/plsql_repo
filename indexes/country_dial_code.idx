-- CHANGE REASON: [EMR-18187] - [ADT-DB] Patient ID - country dial code table
CREATE UNIQUE INDEX CDCODE_CNT_UK ON ALERT.country_dial_code (
CASE WHEN ID_CONTENT IS NOT NULL AND nvl(FLG_AVAILABLE, 'Y') <> 'N'
THEN ID_CONTENT END,
CASE WHEN ID_CONTENT IS NOT NULL AND nvl(FLG_AVAILABLE, 'Y') <> 'N'
THEN FLG_AVAILABLE END);
-- CHANGE END: Anna Kurowska