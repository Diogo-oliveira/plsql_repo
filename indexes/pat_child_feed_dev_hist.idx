DECLARE
    e_object_exists EXCEPTION;
    PRAGMA EXCEPTION_INIT(e_object_exists, -00955);
BEGIN
    BEGIN
        EXECUTE IMMEDIATE 'CREATE INDEX pcfh_epis_fk_idx
  ON pat_child_feed_dev_hist (
    id_episode
  )
  TABLESPACE index_m';
    EXCEPTION
        WHEN e_object_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;

    BEGIN
        EXECUTE IMMEDIATE 'CREATE INDEX pcfh_fdp_fk_i
  ON pat_child_feed_dev_hist (
    id_child_feed_dev
  )
  TABLESPACE index_m';
    EXCEPTION
        WHEN e_object_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;

    BEGIN
        EXECUTE IMMEDIATE 'CREATE INDEX pcfh_pat_fk_i
  ON pat_child_feed_dev_hist (
    id_patient
  )
  TABLESPACE index_m';
    EXCEPTION
        WHEN e_object_exists THEN
            dbms_output.put_line('AVISO: Operação já executada anteriormente.');
    END;
END;
/
